entries:
  Shout_Zelda_Text010:
    contents:
      - text: "Die energie rond Ganons lichaam is Haat.\nJe aanvallen kunnen hem zo niet deren."
  Shout_Zelda_Text020:
    contents:
      - text: "Ik zal de Haat tegenhouden,\nmaar mijn krachten zijn bijna uitgeput.\nVal de lichtgevende plekken aan!"
  Shout_Zelda_Text021:
    contents:
      - text: Zet hem op!
  Shout_Zelda_Text030:
    contents:
      - text: Ik houd de Haat tegen!
  Shout_Zelda_Text031:
    contents:
      - text: "Nu, Link!"
  Shout_Zelda_Text032:
    contents:
      - text: Nu!
  Shout_Zelda_Text033:
    contents:
      - text: Dit is je kans!
  Shout_Zelda_Text034:
    contents:
      - text: Nu snel!
  Shout_Zelda_Text035:
    contents:
      - text: "Schiet op, Link!"
  Shout_Zelda_Text036:
    contents:
      - text: Val de lichtgevende plek aan!
  Shout_Zelda_Text037:
    contents:
      - text: Richt op de lichtgevende plekken!
  Shout_Zelda_Text038:
    contents:
      - text: Schiet op een lichtgevende plek!
  Shout_Zelda_Text039:
    contents:
      - text: Richt op de lichtgevende plekken!
  Shout_Zelda_Text040:
    contents:
      - text: Het spijt me. Ik houd het niet langer vol.
  Shout_Zelda_Text041:
    contents:
      - text: "Ik kan 'm niet meer lang tegenhouden..."
  Shout_Zelda_Text042:
    contents:
      - text: "Alsjeblieft, schiet op!"
  Shout_Zelda_Text050:
    contents:
      - text: " "
  Shout_Zelda_Text051:
    contents:
      - text: " "
  Shout_Zelda_Text052:
    contents:
      - text: " "
  Shout_Zelda_Text070:
    contents:
      - text: Ik zal de Haat opnieuw tegenhouden!
  Shout_Zelda_Text071:
    contents:
      - text: "Bereid je voor, Link!"
  Shout_Zelda_Text072:
    contents:
      - text: "Zet je schrap, Link!"
  Shout_Zelda_Text080:
    contents:
      - text: Ganon wordt zwakker!
  Shout_Zelda_Text081:
    contents:
      - text: Het werkt!
  Shout_Zelda_Text082:
    contents:
      - text: Bijna!
  Shout_Zelda_Text090:
    contents:
      - text: "Link, kijk daar!"
  Shout_Zelda_Text091:
    contents:
      - text: "Dat is het diepste wezen van Ganon zelf.\nDoe wat je moet doen, Link!"
  Shout_Zelda_Text100:
    contents:
      - text: Gebruik zijn aanvallen in je voordeel!
  Shout_Zelda_Text101:
    contents:
      - text: "Gebruik de luchtstroom, Link!"
  Shout_Zelda_Text104:
    contents:
      - text: Schiet van bovenaf!
  Shout_Zelda_Text110:
    contents:
      - text: "Maak het af, Link!"
  Shout_Zelda_Text111:
    contents:
      - text: "Nu, Link!"
  Shout_Zelda_Text112:
    contents:
      - text: "Doe het, Link!"
