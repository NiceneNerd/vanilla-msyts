      - control:
          kind: set_colour
          colour: blue
      - text: Rito-dorp
      - control:
          kind: reset_colour
      - text: ", "
      - control:
          kind: set_colour
          colour: blue
      - text: Kaneli
      - control:
          kind: reset_colour
      - text: ", me verhalen over\nde Uitverkorene verteld."
