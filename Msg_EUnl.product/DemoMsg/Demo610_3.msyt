    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 12
      - text: "Het is net alsof de Uitverkorene het in\ndit lied tegen mij heeft en me vraagt om\nhet te voltooien..."
  Npc_Musician_AoC_HeroGerudoRelief_204:
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Gelukkig hebben "
      - control:
          kind: set_colour
          colour: blue
      - text: Riju
      - control:
          kind: reset_colour
      - text: " en haar lijfwacht,\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Buliara
      - control:
          kind: reset_colour
      - text: ", mooie verhalen over de\nUitverkorene met me gedeeld."
