  talk14:
    attributes: Npc_HatenoVillage011
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 21
      - text: "Je wilt me dus vertellen dat je er nog\nnooit bent geweest?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Goron-stad ligt blijkbaar ver ten noorden\nvan Hateno,"
      - control:
          kind: pause
          length: short
      - text: " in "
      - control:
          kind: set_colour
          colour: blue
      - text: Eldin
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 4
            - 19
      - text: Oeps...
      - control:
          kind: pause
          length: short
      - text: " Ik zei 'blijkbaar', dus nu weet je\ndat ik er nog nooit ben geweest."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Nou ja, maakt ook niet uit... Die plek is in\nieder geval "
      - control:
          kind: set_colour
          colour: red
      - text: zo heet
      - control:
          kind: reset_colour
      - text: ", dat het voelt alsof\nje levend verbrandt.\nOm die reden moest ik al omkeren\nvoordat ik er was."
  talk15:
    attributes: Npc_HatenoVillage011
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 20
      - text: "Ah,"
      - control:
          kind: pause
          length: short
      - text: " dus je bent er nog nooit geweest?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Het Gerudo-fort ligt ver ten westen van\nHateno..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Het gedijt zelfs in de "
      - control:
          kind: set_colour
          colour: blue
      - text: Gerudo-woestijn
      - control:
          kind: reset_colour
      - text: ", \ndus het moet wel een leuke plek zijn."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 10
            - 19
      - text: "Ah... "
      - control:
          kind: pause
          length: short
      - text: "Ik zei 'moet wel',"
      - control:
          kind: pause
          length: long
      - text: " dus nu weet je\ndat ik daar ook nog nooit ben geweest..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 9
            - 16
      - text: "Ik ben helemaal door de hete woestijn\ngereisd, maar ik werd weggestuurd bij de\npoort van het Gerudo-fort...\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Later hoorde ik dat alleen vrouwen daar\nwelkom zijn."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Het valt me een beetje laat in, maar ik\nhad natuurlijk wel kunnen binnenkomen\nals ik me had "
      - control:
          kind: set_colour
          colour: red
      - text: verkleed als vrouw
      - control:
          kind: reset_colour
      - text: "..."
  talk16:
    attributes: Npc_HatenoVillage011
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: Andere plaatsen?
      - control:
