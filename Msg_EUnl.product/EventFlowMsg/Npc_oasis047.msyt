  Npc_oasis047_019:
    attributes: Npc_oasis047
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Dit is het verblijf van Patricia, de\n"
      - control:
          kind: set_colour
          colour: blue
      - text: "woestijnwalrus "
      - control:
          kind: reset_colour
      - text: van onze matriach.
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Patricia is een heel bijzondere\n"
      - control:
          kind: set_colour
          colour: blue
      - text: "woestijnwalrus "
      - control:
          kind: reset_colour
      - text: met een speciale gave.
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ze kan je namelijk goddelijke\nboodschappen geven."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Op dit moment is ze onderweg met de\n"
      - control:
          kind: set_colour
          colour: blue
      - text: matriarch
      - control:
          kind: reset_colour
      - text: ", dus als je hier bent voor een\nboodschap, moet je later terugkomen.\n"
      - control:
          kind: sound2
          unknown:
            - 2
            - 205
      - text: "O, ja! Je moet Patricia een stuk fruit\ngeven als je een boodschap van haar\nwilt. Vergeet dat niet!"
