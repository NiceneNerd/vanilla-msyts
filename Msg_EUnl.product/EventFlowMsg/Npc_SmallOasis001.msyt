group_count: 17
atr1_unknown: 4
entries:
  "0000":
    attributes: ""
    contents:
      - text: Zandstorm?
  "0001":
    attributes: ""
    contents:
      - text: Tot ziens.
  Talk_01:
    attributes: Npc_SmallOasis001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ik houd die zandstorm in de gaten. Kijk je\nmet me mee?"
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Talk_02:
    attributes: Npc_SmallOasis001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Er zijn heel veel zandstormen in de\nwoestijn. Als je er in eentje terecht zou\nkomen, verdwaal je zeker weten."
  Talk_03:
    attributes: Npc_SmallOasis001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Ik wilde eigenlijk naar het "
      - control:
          kind: set_colour
          colour: blue
      - text: Gerudo-fort
      - control:
          kind: reset_colour
      - text: "..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Maar ze zeiden dat de weg ernaartoe is\ngeblokkeerd door een groot ding en dat\nik het een andere keer moest proberen.\n"
      - control:
          kind: sound
          unknown:
            - 9
            - 16
      - text: Dus ik zit hier vast...
  Talk_04:
    attributes: Npc_SmallOasis001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Als ik niet slaap, zit ik de hele tijd naar\ndie zandstorm te kijken... de hele tijd..."
  Talk_05:
    attributes: Npc_SmallOasis001
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 16
      - text: "Maar hij gaat maar niet weg. Probeert\niemand me nou voor de gek te houden?"
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Talk_06:
    attributes: Npc_SmallOasis001
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 16
      - text: "Urgh, wat krijgen we nou... Ik kan niet...\ntegen de kou... Ik haat nachten in de\nwoestijn..."
  Talk_07:
    attributes: Npc_SmallOasis001
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: Ik... heb vrienden gemaakt.
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Maar... zijn het nou mannen of vrouwen?
  Talk_08:
    attributes: Npc_SmallOasis001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: Hrm?
      - control:
          kind: pause
          length: longer
      - text: " Was jij..."
      - control:
          kind: pause
          length: longer
      - text: " Was jij niet bij de\nKara Kara-bazaar? Of was dat iemand\nanders?"
  Talk_09:
    attributes: Npc_SmallOasis001
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 16
      - text: "Ik heb gehoord dat de zandstorm soms\ngaat liggen. Ik hou hem al de hele tijd in\nde gaten, maar er is nog niets gebeurd.\nIk ga slapen..."
  Talk_10:
    attributes: Npc_SmallOasis001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Maar ik hoorde dat de zandstorm die je\nvanaf hier kunt zien, elke dag een paar\nuur verdwijnt.\nIk wil naar die grote toren, dus ik zit te\nwachten tot die zandstorm ophoepelt."
  Talk_11:
    attributes: Npc_SmallOasis001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ik heb gehoord dat er een zandstorm is\ndie soms verdwijnt. Vandaag ga ik daar\neindelijk getuige van zijn!"
  Talk_50:
    attributes: Npc_SmallOasis001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "De zandstorm is gaan liggen. Ik weet\nzeker dat ik de zandstorm vandaag ga\nzien."
  Talk_51:
    attributes: Npc_SmallOasis001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ik keek altijd naar die zandstorm, behalve\nals ik sliep."
  Talk_52:
    attributes: Npc_SmallOasis001
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 16
      - text: "Voorheen ging hij nooit liggen en nou\nsteekt hij de kop nooit meer op. Ik vraag\nme af hoe dat kan..."
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Talk_53:
    attributes: Npc_SmallOasis001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ik wil trouwens naar de grote toren. Nu\nde zandstorm is gaan liggen, kan ik er\nvast wel naartoe."
  Talk_54:
    attributes: Npc_SmallOasis001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ik wacht tot de zandstorm verschijnt...\nWil je met mij wachten?"
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Talk_55:
    attributes: Npc_SmallOasis001
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 16
      - text: "Ik heb gehoord dat de zandstorm af en\ntoe verdwijnt, maar hij lijkt nu helemaal\nniet meer terug te komen...\nIk hield hem de hele tijd in de gaten. Wat\nzou er aan de hand zijn?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Nou ja, ik ga slapen..."
  Talk_56:
    attributes: Npc_SmallOasis001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Goedemorgen, goro!"
  near00:
    attributes: Npc_SmallOasis001
    contents:
      - text: "Hij gaat niet weg, goro..."
