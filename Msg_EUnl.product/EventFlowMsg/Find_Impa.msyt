          colour: blue
      - text: Yiga-bende
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Hun enige doel is om iedereen uit te\nschakelen die "
      - control:
          kind: set_colour
