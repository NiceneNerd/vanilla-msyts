  talk_03:
    attributes: Npc_TabantaHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 9
      - text: "Er is een "
      - control:
          kind: set_colour
          colour: red
      - text: enorm doolhof
      - control:
          kind: reset_colour
      - text: " achter de grote\n"
      - control:
          kind: set_colour
          colour: blue
      - text: "Tabantha-vlakte "
      - control:
          kind: reset_colour
      - text: "ten noorden van\nhier.\nIk weet niet wie het heeft gebouwd en\nwanneer het is gebouwd."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "We mochten er van onze ouders niet\nnaartoe, omdat het er gevaarlijk was."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Op een dag heb ik "
      - control:
          kind: set_colour
          colour: blue
      - text: Hailo
      - control:
          kind: reset_colour
      - text: " uitgedaagd om\nhet doolhof in te gaan."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: Ik heb hem daarna nooit meer gezien.
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Het was er dus waarschijnlijk echt\ngevaarlijk. Maar kinderen beseffen zoiets\nvaak niet..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "Ik heb nog iedere dag spijt van wat ik\ntoen heb gedaan."
  talk_06:
    attributes: Npc_TabantaHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Op mijn leeftijd denk je vaak terug aan\nje jonge jaren. Daar zul je nog wel\nachter komen.\nMaar om eerlijk te zijn, waren er hier op\nhet platteland niet veel kinderen. Mijn\nenige vrienden waren "
      - control:
          kind: set_colour
          colour: blue
      - text: "Hailo "
      - control:
          kind: reset_colour
      - text: "en "
      - control:
          kind: set_colour
          colour: blue
      - text: Mali
      - control:
          kind: reset_colour
      - text: ".\n"
      - control:
          kind: sound
          unknown:
            - 11
            - 25
      - text: "Ik weet nog dat ik met "
      - control:
          kind: set_colour
          colour: blue
      - text: "Hailo "
      - control:
          kind: reset_colour
      - text: "speelde bij\nhet doolhof en met "
      - control:
          kind: set_colour
          colour: blue
      - text: "Mali "
      - control:
          kind: reset_colour
      - text: bij de ruïnes...
      - control:
          kind: choice
          choice_labels:
            - 2
            - 3
            - 4
          selected_index: 0
          cancel_index: 2
          unknown: 8
  talk_07:
    attributes: Npc_TabantaHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 18
      - text: "Misschien heb je het "
      - control:
          kind: set_colour
          colour: blue
      - text: "Tanagar-ravijn "
      - control:
          kind: reset_colour
      - text: "in\nhet zuidoosten wel gezien."
      - control:
          kind: pause
          length: short
      - text: " Onder in dat\nravijn bevonden zich een paar ruïnes.\nIk weet niet wie ze heeft gebouwd en\nwanneer ze zijn gebouwd."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "We mochten er van onze ouders niet\nnaartoe, omdat het er gevaarlijk was."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Maar op een dag besloten "
      - control:
          kind: set_colour
          colour: blue
      - text: Mali
      - control:
          kind: reset_colour
      - text: " en ik om\nde ruïnes te onderzoeken."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Helaas is "
      - control:
          kind: set_colour
          colour: blue
      - text: Mali
      - control:
          kind: reset_colour
      - text: " daarbij in het ravijn\ngestort. Ik heb haar daarna nooit meer\ngezien.\nKinderlijke nieuwsgierigheid leidt vaak\ntot ongelukken..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: Ik denk er nog iedere dag aan...
  talk_08:
    attributes: Npc_TabantaHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 0
      - text: Ik kan het niet loslaten...
  talk_09:
    attributes: Npc_TabantaHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: Maar goed...
      - control:
          kind: pause
          length: short
      - text: " We hoeven er ook niet te\nlang om te treuren."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound2
          unknown:
            - 2
            - 205
      - control:
          kind: set_colour
          colour: blue
      - text: "Hailo "
      - control:
          kind: reset_colour
      - text: "en "
      - control:
          kind: set_colour
          colour: blue
      - text: Mali
      - control:
          kind: reset_colour
      - text: " zijn nooit teruggekeerd,\nmaar twee van hun nakomelingen staan\nin de stal.\nMijn enige vrienden hier waren namelijk\npaarden."
