  Goron_Father_Near01:
    attributes: Npc_Goron010
    contents:
      - text: In de pot!
  Goron_Father_Near02:
    attributes: Npc_Goron010
    contents:
      - text: Waar is hij gebleven?!
  Goron_Father_Talk01:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 0
      - text: "Mijn roekeloze broer is nog steeds niet\nterug, goro!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 9
            - 1
      - text: "Wat denkt hij wel niet, goro?"
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
            - 4
          selected_index: 0
          cancel_index: 3
          unknown: 8
  Goron_Father_Talk02:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound2
          unknown:
            - 9
            - 205
      - text: "Broertje! Dus hier was je, goro?!"
  Goron_Father_Talk03:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ik heb geprobeerd om te denken en me\nte gedragen als mijn broer om hem te\nvinden, goro..."
  Goron_Father_Talk04:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Ik wil je om een gunst vragen, goro.\nIk ben zijn broer, dus ik moet hem\nhelpen, goro!"
  Goron_Father_Talk05:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Terwijl ik op je wacht, begin ik alvast met\nde voorbereidingen, goro."
  Goron_Father_Talk06:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: "Fantastisch, goro!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Dit is een geweldige "
      - control:
          kind: set_colour
          colour: red
      - text: rotsbief
      - control:
          kind: reset_colour
      - text: "! Ik ga hem\nmeteen bereiden, goro!"
  Goron_Father_Talk06_2:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: raw
          four:
            three:
              field_1: 0
      - text: "Het is me gelukt! De "
      - control:
          kind: set_colour
          colour: red
      - text: "gegrilde rotsbief "
      - control:
          kind: reset_colour
      - text: "is\nklaar, goro!"
  Goron_Father_Talk07:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound2
          unknown:
            - 1
            - 205
      - text: "Broertje! Het eten is klaar, goro. En jij\ngaat het allemaal opeten, goro!"
  Goron_Father_Talk08:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound2
          unknown:
            - 2
            - 205
      - text: "Dus dit is het "
      - control:
          kind: set_colour
          colour: red
      - text: geheim van de held
      - control:
          kind: reset_colour
      - text: ",\ngoro?!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound2
          unknown:
            - 4
            - 205
      - control:
          kind: text_size
          percent: 80
      - text: "Hm? Het komt me ergens bekend van voor, goro."
      - control:
          kind: text_size
          percent: 100
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound2
          unknown:
            - 9
            - 205
      - text: "Hoe dan ook... Wat een geweldige\nontdekking! En dat door mijn broer, goro!"
  Goron_Father_Talk09:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: "Hartelijk bedankt voor het helpen van\nmijn broer, goro."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Dan ga ik nu weer op weg naar\nGoron-stad met mijn geweldige broer,\ngoro."
  Goron_Father_Talk10:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 0
      - text: "Wanneer komt mijn broer weer thuis,\ngoro?!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 1
      - text: "Probeert hij nog altijd het "
      - control:
          kind: set_colour
          colour: red
      - text: "geheim van de\nheld"
      - control:
          kind: reset_colour
      - text: " te vinden dat in de "
      - control:
          kind: set_colour
          colour: blue
      - text: Gorko-tunnel
      - control:
          kind: reset_colour
      - text: "\nligt begraven?!"
      - control:
          kind: choice
          choice_labels:
            - 5
            - 3
            - 4
          selected_index: 0
          cancel_index: 3
          unknown: 8
  Goron_Father_Talk11:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "De "
      - control:
          kind: set_colour
          colour: blue
      - text: "Gorko-tunnel "
      - control:
          kind: reset_colour
      - text: "is een verlaten tunnel\nten zuidwesten van het "
      - control:
          kind: set_colour
          colour: blue
      - text: Gorko-meer
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "De tunnel eindigt in keihard gesteente,\ndus wordt er al heel lang niet meer\ngegraven, goro.\n"
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "En blijkbaar ligt het "
      - control:
          kind: set_colour
          colour: red
      - text: geheim van de held
      - control:
          kind: reset_colour
      - text: "\nverborgen achter al dat gesteente, goro!"
  Goron_Father_Talk12:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ga hier de "
      - control:
          kind: set_colour
          colour: red
      - text: heuvel omlaag
      - control:
          kind: reset_colour
      - text: ". Daar ligt de\nGortram-klif, waar je de beste "
      - control:
          kind: set_colour
          colour: red
      - text: rotsbief
      - control:
          kind: reset_colour
      - text: "\nvindt.\nBreng me er daar één van, goro."
  Goron_Father_Talk13:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 16
      - text: "Ik ging het liefst zelf, maar..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ik heb vandaag behoorlijk last van\nGoron-schouder, een chronische\naandoening, goro."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ik verwacht veel van je, dus help me\nalsjeblieft, goro!"
  Goron_Father_Talk14:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 0
      - text: "De broer waarop ik zo trots ben,\nGonguron!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 2
            - 0
      - text: "Hij zei dat hij ging graven naar het\n"
      - control:
          kind: set_colour
          colour: red
      - text: geheim van de held
      - control:
          kind: reset_colour
      - text: "... en is toen gewoon\nvertrokken, goro!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 9
            - 1
      - text: "Waarom doet hij zoiets, goro?"
      - control:
          kind: choice
          choice_labels:
            - 2
            - 3
            - 4
          selected_index: 0
          cancel_index: 3
          unknown: 8
  Goron_Father_Talk15:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 0
      - text: "Als hij van huis wilde weglopen, waarom\nzei hij dan voor vertrek dat hij het\n"
      - control:
          kind: set_colour
          colour: red
      - text: geheim van de held
      - control:
          kind: reset_colour
      - text: " ging opgraven?\n"
      - control:
          kind: sound
          unknown:
            - 9
            - 1
      - text: "Gonguron... Waar ben je mee bezig,\ngoro?!"
      - control:
          kind: choice
          choice_labels:
            - 2
            - 3
            - 4
          selected_index: 0
          cancel_index: 3
          unknown: 8
  Goron_Father_Talk16:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Ik ben er zeker van dat mijn broer naar\nde "
      - control:
          kind: set_colour
          colour: blue
      - text: "Gorko-tunnel "
      - control:
          kind: reset_colour
      - text: "is gegaan, ten\nzuidwesten van het "
      - control:
          kind: set_colour
          colour: blue
      - text: Gorko-meer
      - control:
          kind: reset_colour
      - text: ", goro.\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Het gesteente in die tunnel is keihard,\ndus we zijn er lang geleden al gestopt\nmet graven, goro."
  Goron_Father_Talk17:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Zodra je met een "
      - control:
          kind: set_colour
          colour: red
      - text: "rotsbief "
      - control:
          kind: reset_colour
      - text: "bent\nteruggekomen, mag je hem zo in de pan\nlaten vallen, goro."
  Goron_Father_Talk18:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 0
      - text: "Maar dat is maar een excuus om de\nkantjes ervanaf te lopen! En dat stoort\nme enorm, goro!"
  Goron_Father_Talk20:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Ten zuidwesten van het "
      - control:
          kind: set_colour
          colour: blue
      - text: Gorko-meer
      - control:
          kind: reset_colour
      - text: " ligt\neen verlaten tunnel, de zogenaamde\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Gorko-tunnel
      - control:
          kind: reset_colour
      - text: ", goro.\n"
      - control:
          kind: sound
          unknown:
            - 6
            - 1
      - text: "En achter het harde gesteente is geen\nerts te vinden, maar het "
      - control:
          kind: set_colour
          colour: red
      - text: "geheim\nvan de held"
      - control:
          kind: reset_colour
      - text: "!\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Nou ja, dat zegt men tenminste. Wie\nweet of het ook waar is, goro."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ik heb het gehoord van mijn vader en die\nheeft het van zijn neef, en die heeft het\nvan zijn achterbuurman, goro."
  Goron_Father_Talk21:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: "Ben je daar eindelijk, goro? Gooi de\nrotsfilet maar in de pan, goro!"
  Goron_Father_Talk22:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Het "
      - control:
          kind: set_colour
          colour: red
      - text: geheim van de held
      - control:
          kind: reset_colour
      - text: " is een\neeuwenoud sprookje."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ik heb het gehoord van mijn vader en die\nheeft het van zijn neef, en die van zijn\nachterbuurman, goro."
  Goron_Father_Talk23:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 0
      - text: "Je begrijpt hem wel, goro?! Jij weet wat\nmijn broer denkt, goro?!"
      - control:
          kind: text_size
          percent: 80
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 9
            - 1
      - text: "Ik snap echt niet wat er in het hoofd van mijn broer\nomgaat, goro. Als broer voel ik me daardoor maar\neenzaam, goro..."
      - control:
          kind: text_size
          percent: 100
  Goron_Father_Talk24:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "In plaats van dromen na te jagen, zie ik\nhem liever met beide benen in de realiteit\nstaan en aan de slag gaan, goro."
  Goron_Son_Near01:
    attributes: Npc_Goron009
    contents:
      - text: Het geheim van de held!
  Goron_Son_Near02:
    attributes: Npc_Goron009
    contents:
      - text: "Ik wil het snel eten, goro!"
  Goron_Son_Talk01:
    attributes: Npc_Goron009
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 10
      - text: "Ik moet graven, graven, graven en blijven\ngraven! Ik zal achter het "
      - control:
          kind: set_colour
          colour: red
      - text: "geheim "
      - control:
          kind: reset_colour
      - text: "komen,\ngoro!\nWaar ik naar graaf? Dat is een "
      - control:
          kind: set_colour
          colour: red
      - text: geheim
      - control:
          kind: reset_colour
      - text: ",\ngoro!"
  Goron_Son_Talk02:
    attributes: Npc_Goron009
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 43
      - text: Pfff...
      - control:
          kind: pause
          length: long
      - text: " Ik kan niet meer..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Achter al deze rotsen ligt het "
      - control:
          kind: set_colour
          colour: red
      - text: "geheim van\nde held"
      - control:
          kind: reset_colour
      - text: ", goro."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ik weet zeker dat iedereen naar me\nopkijkt als ik het geheim vind, goro..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound2
          unknown:
            - 40
            - 205
      - text: "Maar ik heb niet genoeg kracht om door\nte graven."
      - control:
          kind: pause
          length: short
      - text: " En ik heb honger, goro!"
  Goron_Son_Talk02b:
    attributes: Npc_Goron009
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 43
      - text: "Ik krijg mijn armen nog geen centimeter\nomhoog, goro."
  Goron_Son_Talk04:
    attributes: Npc_Goron009
    contents:
      - text: "Heerlijk, goro! Overheerlijk, goro!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Die structuur! Dat krokante korstje. Het\nschraperige gevoel in mijn keel!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "De heerlijke mineralen, die zijn gebraden\nop hoge temperatuur. Het voelt als een\nuitbarsting van smaak in mijn mond!\nIk ben de Berg des Doods, goro!"
  Goron_Son_Talk04_1:
    attributes: Npc_Goron009
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 6
      - text: "Hm?! Die geur... Dat lijkt wel "
      - control:
          kind: set_colour
          colour: red
      - text: rotsbief
      - control:
          kind: reset_colour
      - text: "..."
  Goron_Son_Talk04_2:
    attributes: Npc_Goron009
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 11
      - text: "Kijk nou! Dat ziet er verrukkelijk uit! Ik\nwil het nu meteen opeten, goro!"
  Goron_Son_Talk04_3:
    attributes: Npc_Goron009
    contents:
      - control:
          kind: sound2
          unknown:
            - 9
            - 205
      - text: "Niemand kookt zo lekker als jij, goro!"
  Goron_Son_Talk04_4:
    attributes: Npc_Goron009
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 18
      - text: "En nu is mijn maag propvol! Dus we\nkunnen aan de slag, goro!"
  Goron_Son_Talk05:
    attributes: Npc_Goron009
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 16
      - control:
          kind: text_size
          percent: 125
      - text: Haaaaaa!
      - control:
          kind: text_size
          percent: 100
      - control:
          kind: pause
          length: long
  Goron_Son_Talk05_2:
    attributes: Npc_Goron009
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 10
      - text: GORON-AANVAL!!!
      - control:
          kind: pause
          length: long
  Goron_Son_Talk06:
    attributes: Npc_Goron009
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 1
      - control:
          kind: animation
          name: Happy_00
      - text: "Daar is het, goro! "
      - control:
          kind: set_colour
          colour: red
      - text: "Het geheim van de\nheld"
      - control:
          kind: reset_colour
      - text: ", goro!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound2
          unknown:
            - 4
            - 205
      - control:
          kind: text_size
          percent: 80
      - text: "Huh? Maar volgens mij heb ik zoiets ook al eens in\nGoron-stad gezien..."
      - control:
          kind: text_size
          percent: 100
  Goron_Son_Talk07:
    attributes: Npc_Goron009
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 0
      - text: "Dank je wel, goro! Nu ga ik snel terug\nnaar Goron-stad om het aan iedereen te\nvertellen, goro!"
