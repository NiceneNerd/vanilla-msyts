  talk06:
    attributes: Npc_HyrulePlain005
    contents:
      - control:
          kind: set_colour
          colour: blue
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Eldin "
      - control:
          kind: reset_colour
      - text: ligt ten noordoosten van hier.
      - control:
          kind: pause
          length: short
      - text: " Het\nis een bergketen met in het midden de\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Berg des Doods
      - control:
          kind: reset_colour
      - text: ".\nJe kunt de "
      - control:
          kind: set_colour
          colour: blue
      - text: "Berg des Doods "
      - control:
          kind: reset_colour
      - text: "vanaf hier\nzien,"
      - control:
          kind: pause
          length: short
      - text: " maar je ziet alleen de achterkant."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Als je hem wilt beklimmen, moet je naar\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Akkala
      - control:
          kind: reset_colour
      - text: ", ten oosten van hier, gaan."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Je zou onderweg langs de "
      - control:
          kind: set_colour
          colour: blue
      - text: Bergvoetstal
      - control:
          kind: reset_colour
      - text: "\nmoeten komen..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Je kunt de berg vervolgens beklimmen\nvanaf de voet van de Berg des Doods in\nde buurt van de stal.\n"
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "De "
      - control:
          kind: set_colour
          colour: blue
      - text: "Berg des Doods "
      - control:
          kind: reset_colour
      - text: "is een gloeiendhete\nvulkaan..."
      - control:
          kind: pause
          length: long
      - text: " Als je er onvoorbereid naartoe\ngaat, komt dat je duur te staan.\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Ik weet zeker dat er in de "
      - control:
          kind: set_colour
          colour: blue
      - text: bergvoetstal
      - control:
          kind: reset_colour
      - text: "\niemand is die weet hoe je je op de hitte\nkunt voorbereiden."
  talk07:
    attributes: Npc_HyrulePlain005
    contents:
      - control:
          kind: set_colour
          colour: blue
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - control:
          kind: reset_colour
      - text: "Ten zuiden van hier, voorbij de\n"
      - control:
