              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: Hahaha.
      - control:
          kind: pause
          length: short
      - text: " Als je zo oud bent als ik, dan\nheb je mensen al heel snel door."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "De "
      - control:
