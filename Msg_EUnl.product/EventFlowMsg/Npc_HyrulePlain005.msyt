  talk12:
    attributes: Npc_HyrulePlain005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Ten noorden van hier vind je het\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Hyrule-woud
      - control:
          kind: reset_colour
      - text: "... Het is het grootste woud\nin Hyrule..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 1
      - text: "Het wordt ook wel het "
      - control:
          kind: set_colour
          colour: blue
      - text: Verdwaalwoud
      - control:
          kind: reset_colour
      - text: "\ngenoemd, en het is angstaanjagend."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Toen ik jong was, ben ik daar eens\nverdwaald tijdens het verzamelen van\nbrandhout.\nToen ik dieper het woud in ging, begon\ner ineens een "
      - control:
          kind: set_colour
          colour: red
      - text: "sterke wind "
      - control:
          kind: reset_colour
      - text: te waaien...
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Het was heel griezelig en ik was\ndoodsbang, dus ik ben er zo snel\nmogelijk weggegaan...\n"
      - control:
          kind: sound
          unknown:
            - 5
            - 12
      - text: "Plotseling trok de mist echter op en\nstond ik aan de rand van het bos..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Er woont iets onzichtbaars in het\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Verdwaalwoud
      - control:
          kind: reset_colour
      - text: "..."
      - control:
          kind: pause
          length: long
      - text: " Je kunt daar maar beter\nwegblijven."
