  talk04:
    attributes: Obj_DiaryMusician_A_01
    contents:
      - text: "Het oude lied van de "
      - control:
          kind: set_colour
          colour: blue
      - text: Gerudo-toren
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ik heb gehoord dat er een oud lied is\nover een toren, de "
      - control:
          kind: set_colour
          colour: blue
      - text: Gerudo-toren
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ik weet niet waar de "
      - control:
          kind: set_colour
          colour: blue
      - text: "Gerudo-toren "
      - control:
          kind: reset_colour
      - text: "is,\nmaar er zijn de laatste tijd overal\nvreemde torens uit de grond geschoten.\nDe "
      - control:
          kind: set_colour
          colour: blue
      - text: "Gerudo-toren "
      - control:
          kind: reset_colour
      - text: "is waarschijnlijk de\ntoren die is verschenen in Gerudo."
      - control:
          kind: choice
          choice_labels:
            - 3
            - 4
            - 5
          selected_index: 0
          cancel_index: 2
          unknown: 8
