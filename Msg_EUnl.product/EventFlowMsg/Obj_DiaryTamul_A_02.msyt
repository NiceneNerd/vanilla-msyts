  talk01:
    attributes: Obj_DiaryTamul_A_02
    contents:
      - text: "Titel: Schattige stempels"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Stempels zijn markeringen die je op je\nkaart kunt aanbrengen! Ze zijn heel\neenvoudig te gebruiken!\nOpen de kaart met je Sheikah-steen.\nBeweeg hem een beetje en druk op de\nknop als je iets interessants ziet.\nAls je in plaats van stempels "
      - control:
          kind: set_colour
          colour: red
      - text: runes
      - control:
          kind: reset_colour
      - text: "\ngebruikt, zie je een lichtstraal op de\ngemarkeerde locatie! Heel handig!"
  talk02:
    attributes: Obj_DiaryTamul_A_02
    contents:
      - text: "Titel: Stasis-functie"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Stasis houdt bewegende objecten niet\nalleen tegen..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "...maar zorgt er ook voor dat er "
      - control:
          kind: set_colour
          colour: red
      - text: "energie\nkan worden opgehoopt"
      - control:
          kind: reset_colour
      - text: " in stilgezette\nobjecten."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Sla hiervoor simpelweg een paar keer\ntegen het stilgezette object."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Zodra Stasis is uitgewerkt, zal de\nenergie zich ontladen!"
  talk03:
    attributes: Obj_DiaryTamul_A_02
    contents:
      - text: "Titel: Cryonis-functie"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "De ijszuilen die je met de Cryonis-functie\nkunt maken, zijn niet alleen om op te\nklimmen.\nJe kunt ze ook gebruiken om je te\n"
      - control:
          kind: set_colour
          colour: red
      - text: verstoppen
      - control:
          kind: reset_colour
      - text: " voor "
      - control:
          kind: set_colour
          colour: red
      - text: Wachters
      - control:
          kind: reset_colour
      - text: "!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Maak gewoon een ijszuil als je een\nWachter hoort piepen. Zo simpel is het!"
