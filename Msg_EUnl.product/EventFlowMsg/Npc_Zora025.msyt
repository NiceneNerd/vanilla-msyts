  talk12:
    attributes: Npc_Zora025
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 4
      - text: "Prins Sidon is het kleine broertje van de\nUitverkorene Mipha en de zoon van\nkoning Dorephan!\nPrins Sidon is ongelofelijk knap en ze\nzeggen dat zijn grote zus Mipha erg\nmooi was.\nDat een broer en zus zo knap kunnen\nzijn..."
      - control:
          kind: pause
          length: short
      - text: " Wat ben ik daar jaloers op!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "Ik zou willen dat ik ook zo'n broer en zus\nhad."
      - control:
          kind: pause
          length: short
      - text: " Ik heb alleen mijn broer Cleff..."
  talk13:
