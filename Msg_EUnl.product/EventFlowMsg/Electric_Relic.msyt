  Npc_GerudoDesert001_Defeat_001:
    attributes: Npc_GerudoDesert001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Als je de "
      - control:
          kind: set_colour
          colour: blue
      - text: matriarch
      - control:
          kind: reset_colour
      - text: " zoekt, zij is terug in\nhet "
      - control:
          kind: set_colour
          colour: blue
      - text: Gerudo-fort
      - control:
          kind: reset_colour
      - text: ". Ze zag er doodmoe uit."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Maar na zo'n gevecht kan ik haar dat niet\nkwalijk nemen..."
  Npc_GerudoDesert001_Finish_001:
    attributes: Npc_GerudoDesert001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Ik kon niet precies zien wat er is\ngebeurd, maar ik kan nauwelijks geloven\ndat je de Titaan hebt gekalmeerd..."
  Npc_GerudoDesert001_Invasion_001:
    attributes: Npc_GerudoDesert001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "De "
      - control:
          kind: set_colour
          colour: blue
      - text: matriarch
      - control:
          kind: reset_colour
      - text: " is boven. Ze was zo moe,\nmaar nu is ze weer opgeknapt."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ik erger me eraan dat ze zulke risico's\nmoest nemen en dat ik niet kon helpen."
  Npc_GerudoDesert001_Strategy_001:
    attributes: Npc_GerudoDesert001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Ik ben geschokt... Ik had niet verwacht\ndat je zo klein zou zijn. En jij wilt met de\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Titaan
      - control:
          kind: reset_colour
      - text: " vechten?\nMaar de "
      - control:
          kind: set_colour
          colour: blue
      - text: matriarch
      - control:
          kind: reset_colour
      - text: " heeft je haar zegen\ngegeven, dus daar zal ze wel een\nreden voor hebben."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "De "
      - control:
          kind: set_colour
          colour: blue
      - text: matriarch
      - control:
          kind: reset_colour
      - text: " wacht op je "
      - control:
          kind: set_colour
          colour: red
      - text: "boven op deze\nobservatiepost"
      - control:
          kind: reset_colour
      - text: ". Schiet op."
  Npc_GerudoDesert001_Strategy_002:
    attributes: Npc_GerudoDesert001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Als je de "
      - control:
          kind: set_colour
          colour: blue
      - text: matriarch
      - control:
          kind: reset_colour
      - text: " zoekt, ze is "
      - control:
          kind: set_colour
          colour: red
      - text: "boven op\ndeze observatiepost"
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: pause
          length: short
      - text: " Laat haar niet\nonnodig lang wachten!"
  Npc_oasis003_Defeat_001:
    attributes: Npc_oasis003
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 24
      - text: Weet je... dit is de eerste keer.
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "De eerste keer dat ik me echt nuttig voel\nen de problemen niet door iemand\nanders laat oplossen.\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "De rest laat ik aan jou over. Jij bent de\nenige die dit ding kan kalmeren."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Maar... beloof me. Beloof me dat je veilig\nterugkeert."
  Npc_oasis003_Finish_001:
    attributes: Npc_oasis003
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 11
      - text: "O, je bent terug!"
  Npc_oasis003_Finish_003:
    attributes: Npc_oasis003
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 18
      - text: Ik ben blij dat je niet gewond bent...
      - control:
          kind: pause
          length: longer
      - text: "\n"
      - control:
          kind: sound
          unknown:
            - 11
            - 1
      - control:
          kind: text_size
          percent: 80
      - text: Echt waar...
      - control:
          kind: text_size
          percent: 100
  Npc_oasis003_Finish_004:
    attributes: Npc_oasis003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - control:
          kind: set_colour
          colour: blue
      - text: Naboris
      - control:
          kind: reset_colour
      - text: " is naar zijn oude positie\nteruggekeerd en de zandstorm is gaan\nliggen.\nIk maak me nog steeds zorgen over de\n"
      - control:
          kind: set_colour
          colour: red
      - text: grote ramp
      - control:
          kind: reset_colour
      - text: ", maar "
      - control:
          kind: set_colour
          colour: blue
      - text: Naboris
      - control:
          kind: reset_colour
      - text: " is tenminste\ngekalmeerd."
  Npc_oasis003_Finish_005:
    attributes: Npc_oasis003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Deze uitrusting is de trots van onze\nstam. Ooit waren beide van\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Urbosa
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: "Jij was ooit bevriend met haar, dus ik\ndacht dat je dit wel zou willen hebben."
  Npc_oasis003_Finish_009:
    attributes: Npc_oasis003
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 0
      - text: "Dit is een bedankje voor je hulp. Ik neem\naan dat je het accepteert?"
  Npc_oasis003_GetBack_001:
    attributes: Npc_oasis003
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "Ik kan niet geloven dat onze "
      - control:
          kind: set_colour
          colour: blue
      - text: "nationale\nschat"
      - control:
          kind: reset_colour
      - text: " is gestolen. Dit is de eerste fout\nvan dit kaliber sinds we hier wonen.\nIk heb onze voorouders en Urbosa diep\nteleurgesteld. Het volk voelt nu hoe zwak\nzijn jonge matriarch eigenlijk is."
  Npc_oasis003_GetBack_002:
    attributes: Npc_oasis007
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Met alle respect, matriarch... maar de\ndiefstal van de nationale schat is een\nfalen van de wachters!\n"
      - control:
          kind: sound
          unknown:
            - 8
            - 1
      - text: Was ik maar oplettender geweest...
  Npc_oasis003_GetBack_003:
    attributes: Npc_oasis003
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "Maak het niet erger dan het al is,\nBuliara..."
  Npc_oasis003_GetBack_101:
    attributes: Npc_oasis003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Zoals je ziet, ben ik nog jong."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "De mensen zijn vriendelijk tegen mij,\nmaar ze behandelen me als een kind."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ik heb zo hard geprobeerd om ervoor te\nzorgen dat ze mij als hun matriarch\nzien.\nMaar nadat onze schat was gestolen,\nviel ik in een zwart gat."
  Npc_oasis003_GetBack_102:
    attributes: Npc_oasis051
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: En? H-hoe staat hij me?
      - control:
          kind: choice
          choice_labels:
            - 33
            - 34
          selected_index: 0
          cancel_index: 2
          unknown: 6
  Npc_oasis003_GetBack_103:
    attributes: Npc_oasis051
    contents:
      - control:
          kind: text_size
          percent: 80
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: H-hoe... zie ik eruit?
      - control:
          kind: text_size
          percent: 100
  Npc_oasis003_GetBack_104:
    attributes: Npc_oasis051
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 9
      - text: Hihi... Wat vreemd!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Het gevecht is nog niet begonnen, maar\ndoor jouw antwoord weet ik dat het goed\ngaat komen.\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ik vertrek alvast. Ontmoet me bij de \n"
      - control:
          kind: set_colour
          colour: red
      - text: observatiepost
      - control:
          kind: reset_colour
      - text: " "
      - control:
          kind: set_colour
          colour: blue
      - text: ten zuiden van het fort
      - control:
          kind: reset_colour
      - text: "."
  Npc_oasis003_GetBack_105:
    attributes: Npc_oasis051
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 1
      - text: O... Ik snap wat je bedoelt...
  Npc_oasis003_GetBack_106:
    attributes: Npc_oasis051
    contents:
      - control:
          kind: set_colour
          colour: blue
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - control:
          kind: reset_colour
      - text: "Sinds we op zoek zijn gegaan naar de\nschat, is "
      - control:
          kind: set_colour
          colour: blue
      - text: Vah Naboris
      - control:
          kind: reset_colour
      - text: " van locatie\nveranderd."
  Npc_oasis003_GetBack_107:
    attributes: Npc_oasis051
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ik wacht hier op je met mijn favoriete\nwoestijnwalrus, Patricia."
  Npc_oasis003_GetBack_108:
    attributes: Npc_oasis051
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "We kunnen "
      - control:
          kind: set_colour
          colour: blue
      - text: "Naboris "
      - control:
          kind: reset_colour
      - text: "alleen bereiken op\neen "
      - control:
          kind: set_colour
          colour: blue
      - text: woestijnwalrus
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "De "
      - control:
          kind: set_colour
          colour: red
      - text: observatiepost
      - control:
          kind: reset_colour
      - text: " ligt in de buurt van\nhet fort, maar om te oefenen kun je er\nook op een "
      - control:
          kind: set_colour
          colour: blue
      - text: "woestijnwalrus "
      - control:
          kind: reset_colour
      - text: heen gaan.
  Npc_oasis003_GetBack_109:
    attributes: Npc_oasis003
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: "Ik was aan het wachten... Hehe... Je\nhoeft niet eens iets te zeggen..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ik ben de matriarch van de Gerudo's. Ik\nkan de kracht van onze nationale schat\nuit je voelen stromen."
  Npc_oasis003_GetBack_111:
    attributes: Npc_oasis051
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 24
      - text: "Hihi... Je bent dan wel een Uitverkorene,\nmaar blijkbaar moet jij je ook mentaal\nvoorbereiden voor een taak als deze...\nIk vertrek alvast."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Kom zodra je er klaar voor bent naar de\n"
      - control:
          kind: set_colour
          colour: red
      - text: observatiepost
      - control:
          kind: reset_colour
      - text: " "
      - control:
          kind: set_colour
          colour: blue
      - text: ten zuiden van het fort
      - control:
          kind: reset_colour
      - text: "."
  Npc_oasis003_GetBack_112:
    attributes: Npc_oasis051
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Als we niet uitkijken, loopt het fort\nbinnenkort ook gevaar..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Het is mijn taak als matriarch, en als\nGerudo, om een manier te vinden om dat\nding tegen te houden. Wil jij me helpen?"
      - control:
          kind: choice
          choice_labels:
            - 122
            - 123
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Npc_oasis003_GetBack_113:
    attributes: Npc_oasis051
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 16
      - text: Maar...
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Eigenlijk is hij toch wel ietsjes te groot...
  Npc_oasis003_GetBack_114:
    attributes: Npc_oasis051
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "..."
  Npc_oasis003_GetBack_115:
    attributes: Npc_oasis051
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: Echt waar?
  Npc_oasis003_GetBack_117:
    attributes: Npc_oasis051
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 19
      - text: Wat is er? Waarom kijk je zo?
      - control:
          kind: pause
          length: longer
      - text: " En\nbelangrijker nog..."
      - control:
          kind: pause
          length: longer
  Npc_oasis003_GetBack_118:
    attributes: Npc_oasis003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Daarom denk ik dat Urbosa ervoor heeft\ngezorgd dat jij nu hier bent."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Kun je me de helm overhandigen?
  Npc_oasis003_Invasion_001:
    attributes: Npc_oasis003
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Als we ons niet hadden teruggetrokken,\nwas ik er waarschijnlijk geweest... Maar\nhet gaat wel weer.\nAlles ook in orde met jou? Vergeet niet\nom je "
      - control:
          kind: set_colour
          colour: blue
      - text: "boog "
      - control:
          kind: reset_colour
      - text: "en "
      - control:
          kind: set_colour
          colour: blue
      - text: bompijlen
      - control:
          kind: reset_colour
      - text: " te controleren."
  Npc_oasis003_Invasion_002:
    attributes: Npc_oasis003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 24
      - text: "We kunnen dat ding alleen kalmeren als\nwe samenwerken. Ik wacht hier tot je\nweer zover bent."
  Npc_oasis003_Invasion_003:
    attributes: Npc_oasis003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 24
      - text: "Het belangrijkste is dat we het overleven.\nDus ik leg het plan zo vaak uit als je maar\nwilt."
  Npc_oasis003_Invasion_004:
    attributes: Npc_oasis003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Eerst moeten we dichterbij komen met de\n"
      - control:
          kind: set_colour
          colour: blue
      - text: woestijnwalrussen
      - control:
          kind: reset_colour
      - text: ". Ik zal voorop gaan."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Zodra je een kans hebt, "
      - control:
          kind: set_colour
          colour: blue
      - text: schiet
      - control:
          kind: reset_colour
      - text: " dan op\nzijn voeten met "
      - control:
          kind: set_colour
          colour: blue
      - text: bompijlen
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Naboris onttrekt energie aan de bodem,\nmaar als je zijn voeten beschadigt, komt\nhij misschien wel tot stilstand.\nMaar makkelijk wordt het niet. De\nbliksemaanvallen uit zijn benen zijn\ndodelijk.\nAls je geraakt wordt, is de kans groot dat\nje het niet navertelt."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Met de "
      - control:
          kind: set_colour
          colour: blue
      - text: Bliksemhelm
      - control:
          kind: reset_colour
      - text: " kan ik je beschermen\ntegen de bliksemaanvallen."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 17
      - text: "Maar het bereik van de helm is niet erg\ngroot."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Houd er dus rekening mee dat ik je\nalleen kan beschermen als je in de buurt\nblijft.\n"
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "En als je te gewond raakt tijdens het\ngevecht, "
      - control:
          kind: set_colour
          colour: red
      - text: "trek je dan terug naar deze\nobservatiepost"
      - control:
          kind: reset_colour
      - text: ".\nNiemand is erbij gebaat als je roekeloos\nhet gevecht aangaat."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Dan vraag ik je nu voor de laatste keer:\nben je er klaar voor?"
      - control:
          kind: choice
          choice_labels:
            - 110
            - 111
            - 112
          selected_index: 0
          cancel_index: 2
          unknown: 8
  Npc_oasis003_Invasion_005:
    attributes: Npc_oasis003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Kom op!
  Npc_oasis003_Invasion_006:
    attributes: Npc_oasis033
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Als je nog verder weg gaat, verlies je\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Naboris
      - control:
          kind: reset_colour
      - text: " uit het oog!"
  Npc_oasis003_Invasion_007:
    attributes: Npc_oasis033
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 19
      - text: Wil je je terugtrekken?
      - control:
          kind: choice
          choice_labels:
            - 113
            - 114
          selected_index: 1
          cancel_index: 1
          unknown: 6
  Npc_oasis003_Invasion_008:
    attributes: Npc_oasis033
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 24
      - text: "Goed idee! Ik zie je bij de\nobservatiepost!"
  Npc_oasis003_Invasion_009:
    attributes: Npc_oasis033
