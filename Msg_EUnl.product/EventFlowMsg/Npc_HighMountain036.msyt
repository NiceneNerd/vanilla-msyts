entries:
  "0000":
    attributes: ""
    contents:
      - text: Mezelf.
  "0001":
    attributes: ""
    contents:
      - text: Toekomstige liefde.
  "0002":
    attributes: ""
    contents:
      - text: Wat is er?
  "0003":
    attributes: ""
    contents:
      - text: Tot ziens.
  "0004":
    attributes: Npc_HighMountain036
    contents:
      - text: Ja.
  "0005":
    attributes: Npc_HighMountain036
    contents:
      - text: Nee.
  Npc_HighMountain036_001:
    attributes: Npc_HighMountain036
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Kheel, doe alsjeblieft geen gevaarlijke\ndingen."
      - control:
          kind: single_choice
          label: 2
  Npc_HighMountain036_002:
    attributes: Npc_HighMountain036
    contents:
      - control:
          kind: animation
          name: SurpriseS_01
      - control:
          kind: sound
          unknown:
            - 8
            - 1
      - text: "Je kunt het dorp niet verlaten zolang\n"
      - control:
          kind: set_colour
          colour: blue
      - text: "Vah Medoh "
      - control:
          kind: reset_colour
      - text: in de buurt is!
  Npc_HighMountain036_002_2:
    attributes: Npc_HighMountain036
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 19
      - text: "O... sorry! Dat was niet voor jou bedoeld,\nreiziger."
  Npc_HighMountain036_003:
    attributes: Npc_HighMountain036
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 1
      - text: "Kheel, mijn jongste dochter, wil naar de\n"
      - control:
          kind: set_colour
          colour: red
      - text: Broederrotsen
      - control:
          kind: reset_colour
      - text: " gaan. Ze heeft het er al\ndagen over. Ik weet niet wat ik kan doen.\nIk heb namelijk vijf dochters en sinds dat\n"
      - control:
          kind: set_colour
          colour: blue
      - text: monster
      - control:
          kind: reset_colour
      - text: " verscheen, zijn ze beurtelings\ngedeprimeerd en enorm veeleisend...\n"
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "Was HIJ er maar, dan was de situatie niet\nzo ingewikkeld..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Maar ik kan niet, net als mijn dochters,\nalleen maar aan mezelf denken... Dit\ngezin is mijn verantwoordelijkheid."
  Npc_HighMountain036_004:
    attributes: Npc_HighMountain036
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Tot ziens. Zorg goed voor jezelf.
  Npc_HighMountain036_011:
    attributes: Npc_HighMountain036
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "O jee, mijn dochter is weer herrie aan\nhet schoppen. Ik hoop dat jij geen last\nvan haar hebt, reiziger..."
  Npc_HighMountain036_021:
    attributes: Npc_HighMountain036
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 16
      - text: "Reiziger, mag ik je wat vragen? Als je\neten kookt, voor wie kook je dan?"
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
          selected_index: 0
          cancel_index: 2
          unknown: 6
  Npc_HighMountain036_022:
    attributes: Npc_HighMountain036
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: Zalm... Boter... Tarwe...
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Tijdens het koken kan ik de "
      - control:
          kind: set_colour
          colour: blue
      - text: "Titaan "
      - control:
          kind: reset_colour
      - text: "even\nvergeten en stel ik me enkel voor hoe\nmijn dochters ervan zullen smullen...\nMaar ik hoop dat jij ook van je maaltijd\nzult genieten, reiziger!"
  Npc_HighMountain036_023:
    attributes: Npc_HighMountain036
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Sorry voor het stellen van zo'n vreemde\nvraag."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 9
            - 18
      - text: "Soms vraag ik me 's nachts af wat er met\ndit dorp zal gebeuren... en met mijn\ndochters..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Dat komt misschien wel gewoon door de\nhonger."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Maar ja, ga je gang en gebruik de pan om\nte koken tot je genoeg hebt gegeten."
  Npc_HighMountain036_024:
    attributes: Npc_HighMountain036
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: "Ja, natuurlijk! Het is het leukst om te\nkoken voor iemand waarvan je houdt! Ik\nkook zelf ook graag voor mijn dochters.\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Maar je kunt deze pan wel gebruiken om\nvoor je toekomstige liefde te koken!"
  Npc_HighMountain036_101:
    attributes: Npc_HighMountain036
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 6
      - text: "Volgens mijn dochters hebben zij Kaneli\nover het grote voorval verteld..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Maar wat is er gebeurd bij de\n"
      - control:
          kind: set_colour
          colour: red
      - text: Broederrotsen
      - control:
          kind: reset_colour
      - text: "?"
  Npc_HighMountain036_102:
    attributes: Npc_HighMountain036
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: "Ik zou me beter voelen als ik wist waar\niedereen is."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Kheels zussen verzamelen ingrediënten\nvoor "
      - control:
          kind: set_colour
          colour: blue
      - text: zalm met boter
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Genli wil niet zingen voordat ze\n"
      - control:
          kind: set_colour
          colour: blue
      - text: zalm met boter
      - control:
          kind: reset_colour
      - text: " heeft gegeten."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Daarom heb ik al mijn dochters eropuit\ngestuurd om de ingrediënten te kopen.\nIk hoop maar dat dat goed gaat."
  Npc_HighMountain036_103:
    attributes: Npc_HighMountain036
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Ik heb Kheels zussen eropuit gestuurd\nom de ingrediënten te zoeken voor\n"
      - control:
          kind: set_colour
          colour: blue
      - text: zalm met boter
      - control:
          kind: reset_colour
      - text: ".\nIk hoop maar dat dat goed gaat."
  Npc_HighMountain036_212:
    attributes: Npc_HighMountain036
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "O Link, het spijt me dat mijn dochters\nvandaag weer zo druk zijn."
  Npc_HighMountain036_223:
    attributes: Npc_HighMountain036
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Goedenavond, Link.\nWil je onze pan gebruiken?"
      - control:
          kind: choice
          choice_labels:
            - 4
            - 5
          selected_index: 0
          cancel_index: 2
          unknown: 6
  Npc_HighMountain036_224:
    attributes: Npc_HighMountain036
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "De zangkunsten van onze dochters zijn\nenorm verbeterd. Ik zou willen dat hij dat\nook kon horen...\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "O, sorry, Link! Ik was tegen mezelf aan\nhet praten..."
  Npc_HighMountain036_225:
    attributes: Npc_HighMountain036
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 9
      - text: "Gebruik 'm maar, het is geen probleem!"
  Npc_HighMountain036_226:
    attributes: Npc_HighMountain036
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: Dank je...
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Wanneer mijn dochters zangoefeningen\ndoen, krijgen ze meteen enorm veel\nhonger.\n"
      - control:
          kind: sound
          unknown:
            - 5
            - 1
      - text: "Als ik ze dan niet genoeg eten geef,\nzingen ze de volgende dag een liedje\nover hun gigantische honger!"
  Npc_HighMountain036_300:
    attributes: Npc_HighMountain036
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 4
      - text: "Dank je wel, Link!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Door jou is hij bij ons teruggekomen...\nMijn man is weer thuis!"
  Npc_HighMountain036_301:
    attributes: Npc_HighMountain036
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Hij is bij onze dochters. Die zijn op hun\nvaste plaats aan het zingen."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Als je ernaartoe gaat, kun je ze horen.\nWaarom ga je niet even luisteren?"
  Npc_HighMountain036_302:
    attributes: Npc_HighMountain036
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Mijn man maakt muziek op de plaats waar\nonze dochters normaal gesproken hun\nzangoefeningen doen.\nJe hoort ze vast wanneer je er in de\nbuurt komt. Waarom ga je niet even\nluisteren?"
  Npc_HighMountain036_Near_Rain:
    attributes: Npc_HighMountain036
    contents:
      - text: Het is vochtig...
