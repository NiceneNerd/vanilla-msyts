      - text: Jij bent Lin-lin!
      - control:
          kind: pause
          length: longer
      - text: "\nWauw, dat is lang geleden!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Wacht eens even, ik dacht dat jij en\nMipha"
      - control:
          kind: pause
          length: short
      - text: " gesneuveld waren tijdens de "
      - control:
          kind: set_colour
          colour: red
      - text: "grote\nramp"
      - control:
          kind: reset_colour
      - text: "!\nMaar als je nog leeft, waar heb je al die\ntijd dan gezeten?"
      - control:
          kind: pause
          length: longer
      - text: " En wat heb je gedaan?"
      - control:
          kind: pause
          length: short
      - text: "\nKom op, ik wil het weten!\n..."
      - control:
          kind: raw
          zero:
