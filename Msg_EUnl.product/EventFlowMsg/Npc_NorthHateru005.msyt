    attributes: Npc_NorthHateru005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Hm? "
      - control:
          kind: pause
          length: short
      - text: "O, jij bent het weer..."
      - control:
          kind: pause
          length: longer
      - text: " Wat is er?"
