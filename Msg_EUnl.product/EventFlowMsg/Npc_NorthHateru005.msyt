  talk010:
    attributes: Npc_NorthHateru005
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 12
      - text: "Het is een vieze stal met alleen maar\nmannen..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "En zelfs toen een Zora ons bezocht,"
      - control:
          kind: pause
          length: short
      - text: "\nbleek het iemand te zijn die zich de "
      - control:
          kind: set_colour
          colour: red
      - text: prins
      - control:
          kind: reset_colour
      - text: "\nnoemde..."
      - control:
          kind: pause
          length: short
      - text: " Alweer een man dus!\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 9
      - text: De enige die me troost biedt is Sato...
      - control:
          kind: choice
          choice_labels:
            - 3
            - 5
            - 4
          selected_index: 0
          cancel_index: 2
          unknown: 8
  talk020:
    attributes: Npc_NorthHateru005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 6
      - text: Kijk eens naar het oosten.
      - control:
          kind: pause
          length: longer
      - text: " Er hangen\ndaar wel erg veel wolken, hè?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Van regen krijg ik een slecht humeur.
      - control:
          kind: pause
          length: longer
      - text: "\nDaarom moet Sato me troosten..."
      - control:
          kind: choice
          choice_labels:
            - 5
            - 4
          selected_index: 0
          cancel_index: 1
          unknown: 6
  talk030:
    attributes: Npc_NorthHateru005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: Bespaar me je excuses...
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Jij en de "
      - control:
          kind: set_colour
          colour: red
      - text: "Zora-prins "
      - control:
          kind: reset_colour
      - text: "waren de\nafgelopen tijd de enige bezoekers..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Deze plek schijnt een vreemd soort\naantrekkingskracht uit te oefenen op\nmannen..."
      - control:
          kind: choice
          choice_labels:
            - 3
            - 4
          selected_index: 0
          cancel_index: 1
          unknown: 6
  talk040:
    attributes: Npc_NorthHateru005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 6
      - text: Ken je die niet?
      - control:
          kind: pause
          length: longer
      - text: " Dat zijn die glibberige,\nvisachtige figuren."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Ze leven ten oosten van hier...
      - control:
          kind: pause
          length: short
      - text: "\nstroomopwaarts aan de "
      - control:
          kind: set_colour
          colour: blue
      - text: Zora-rivier
      - control:
          kind: reset_colour
      - text: ","
      - control:
          kind: pause
          length: short
      - text: " in \nhet Zora-paleis.\nZe zeiden dat ik eruitzag als een \nkrachtige "
      - control:
          kind: set_colour
          colour: blue
      - text: Hyliaan
      - control:
          kind: reset_colour
      - text: ","
      - control:
          kind: pause
          length: short
      - text: " dus ze wilden dat ik\nlangskwam in het paleis, maar...\n"
      - control:
          kind: sound
          unknown:
            - 5
            - 12
      - text: "...ik wil niet dat Sato gevaar loopt, dus\nik heb het aanbod afgeslagen..."
      - control:
          kind: choice
          choice_labels:
            - 5
            - 4
          selected_index: 0
          cancel_index: 1
          unknown: 6
