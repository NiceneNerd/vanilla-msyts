  talk060:
    attributes: Npc_NorthHateru005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Tja, "
      - control:
          kind: set_colour
          colour: blue
      - text: Sato
      - control:
          kind: reset_colour
      - text: "... "
      - control:
          kind: pause
          length: short
      - text: "Ik heb mijn leven aan hem\nte danken. Het is een lang verhaal. Wil\nje het horen?"
      - control:
          kind: choice
          choice_labels:
            - 6
            - 7
          selected_index: 0
          cancel_index: 1
          unknown: 6
  talk061:
    attributes: Npc_NorthHateru005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Nou, luister goed..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Heb je weleens gehoord van die \n"
      - control:
          kind: set_colour
          colour: red
      - text: "vreemde berg "
      - control:
          kind: reset_colour
      - text: "waar dieren op bepaalde\ntijden verschijnen?\nToen ik dat gerucht hoorde, dacht ik dat\nhet een snelle manier zou zijn om geld\nte verdienen, dus ik ging ernaartoe... \nIk wilde de dieren vangen en hun vlees\nverkopen..."
      - control:
          kind: choice
          choice_labels:
            - 8
            - 9
          selected_index: 0
          cancel_index: 1
          unknown: 6
  talk062:
    attributes: Npc_NorthHateru005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 12
      - text: "Dus ik ging naar de berg en wachtte\ngeduldig tot de dieren zouden \nverschijnen...\nEn toen kwam deze vieze pup aanlopen..."
      - control:
          kind: pause
          length: longer
      - text: "\nWellicht was hij zijn ouders kwijtgeraakt\nof achtergelaten door zijn baasje...\nHij bedelde in ieder geval om voedsel,"
      - control:
          kind: pause
          length: longer
      - text: "\ndus ik gaf hem een plakje worst..."
      - control:
          kind: choice
          choice_labels:
            - 8
            - 9
          selected_index: 0
          cancel_index: 1
          unknown: 6
  talk063:
    attributes: Npc_NorthHateru005
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "Op een keer viel ik 's nachts in slaap..."
      - control:
          kind: pause
          length: short
      - text: "\nToen ik wakker werd, stond er\neen groot, gloeiend "
      - control:
          kind: set_colour
          colour: blue
      - text: ding
      - control:
          kind: reset_colour
      - text: " voor me...\nIk was volledig overrompeld, en had niet\neens tijd om mijn boog te spannen... Ik\nheb het "
      - control:
          kind: set_colour
          colour: blue
      - text: ding
      - control:
          kind: reset_colour
      - text: " alleen maar aangestaard...\nHet kwam op me af alsof het me wilde\nvertrappen, toen plotseling de hond die\nik had gevoerd tussen ons in sprong.\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Hij blafte tegen het "
      - control:
          kind: set_colour
          colour: blue
      - text: ding
      - control:
          kind: reset_colour
      - text: ", dat ongeveer\ntien keer zo groot was als hij..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Hij wilde me beschermen, hoewel hij zelf\nverschrikkelijk bang was..."
      - control:
          kind: choice
          choice_labels:
            - 10
            - 9
          selected_index: 0
          cancel_index: 1
          unknown: 6
  talk064:
    attributes: Npc_NorthHateru005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 12
      - text: "Het "
      - control:
          kind: set_colour
          colour: blue
      - text: ding
      - control:
          kind: reset_colour
      - text: " schrok een beetje terug..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ik zag mijn kans schoon, nam de hond in\nmijn armen en rende al struikelend de \nberg af..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Zo, dat was het hele verhaal..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Die "
      - control:
          kind: set_colour
          colour: red
      - text: vreemde berg
      - control:
          kind: reset_colour
      - text: " heet de "
      - control:
          kind: set_colour
          colour: blue
      - text: Satori-berg
      - control:
          kind: reset_colour
      - text: "...\nHij ligt ten westen van "
      - control:
          kind: set_colour
          colour: blue
      - text: Centraal Hyrule
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: "De pup die mijn leven heeft gered, heb ik\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Sato
      - control:
          kind: reset_colour
      - text: " genoemd..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Als je echt moedig bent, moet je eens op\nzoek gaan naar dat "
      - control:
          kind: set_colour
          colour: blue
      - text: ding
      - control:
          kind: reset_colour
      - text: "..."
