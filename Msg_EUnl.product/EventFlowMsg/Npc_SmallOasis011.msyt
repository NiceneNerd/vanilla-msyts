    attributes: Npc_SmallOasis011
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 6
      - text: "Heb je nog nooit van de "
      - control:
          kind: set_colour
          colour: blue
      - text: Titaan Naboris
      - control:
          kind: reset_colour
      - text: "\ngehoord?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Hij is de beschermgod van de Gerudo's."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Maar een tijdje terug sloeg hij ineens op\nhol..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "We kunnen niets doen, want de "
      - control:
