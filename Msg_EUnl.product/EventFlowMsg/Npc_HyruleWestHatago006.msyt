  talk00:
    attributes: Npc_HyruleWestHatago006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "O, dus je wilt hier overnachten?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Nu je er toch bent, kun je ook wel even\nluisteren naar mijn verhaal..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ten zuiden van hier ligt de "
      - control:
          kind: set_colour
          colour: blue
      - text: Washa-berg
      - control:
          kind: reset_colour
      - text: ".\n"
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Er is daar een mysterieus platform, en ik\nheb geen idee waarvoor het dient..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ik hoop dat Geggle en ik dat mysterie\nooit zullen ontrafelen..."
  talk01:
    attributes: Npc_HyruleWestHatago006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Als je het platform wilt zien, moet je\nin zuidelijke richting naar de "
      - control:
          kind: set_colour
          colour: blue
      - text: Washa-berg
      - control:
          kind: reset_colour
      - text: "\ngaan."
  talk02:
    attributes: Npc_HyruleWestHatago006
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 0
      - text: "Wat? Er is een "
      - control:
          kind: set_colour
          colour: blue
      - text: tempel
      - control:
          kind: reset_colour
      - text: " verschenen?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Wie had ooit gedacht dat er zo'n geheim\nzou schuilgaan achter het platform..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Nou, dan moet ik nu het mysterie van de\n"
      - control:
          kind: set_colour
          colour: blue
      - text: tempel
      - control:
          kind: reset_colour
      - text: " oplossen."
  talk03:
    attributes: Npc_HyruleWestHatago006
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "De "
      - control:
          kind: set_colour
          colour: blue
      - text: tempel
      - control:
          kind: reset_colour
      - text: "..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Het wordt steeds mysterieuzer...
  talk04:
    attributes: Npc_HyruleWestHatago006
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Ik heb helemaal geen tijd om witte geiten\nte hoeden..."
      - control:
          kind: pause
          length: long
  talk05:
    attributes: Npc_HyruleWestHatago006
    contents:
      - control:
          kind: sound
