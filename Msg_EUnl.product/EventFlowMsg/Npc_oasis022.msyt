      - text: "Ik weet niet wat er is gebeurd, maar ik\nhad nooit gedacht dat de "
      - control:
          kind: set_colour
          colour: blue
      - text: Titaan
      - control:
          kind: reset_colour
      - text: " zomaar\nzou kalmeren!\nMaar ik vertrouw het niet... Voor je het\nweet, stampt dat ding weer rond!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
