    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ik weet dat de "
      - control:
          kind: set_colour
          colour: red
      - text: "bol die ze van de Gerudo-\nstam hebben gestolen,"
      - control:
          kind: reset_colour
      - text: " op de tweede\netage wordt bewaard...\n"
      - control:
          kind: sound
          unknown:
            - 3
            - 12
      - text: "Maar het is gevaarlijk om er ook maar\nin de buurt te komen. In leven blijven is\nessentieel voor een schattenjager!"
  Npc_Attacked_006_DLC2nd_SD_001:
    attributes: Npc_Attacked_006_DLC2nd
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 0
      - text: "Ik heb gehoord dat de Yiga-bende is\nuitgeschakeld door een reizende\nzwaardvechter, maar...\n"
      - control:
          kind: sound
          unknown:
            - 4
            - 20
      - text: "Er lopen er nog zoveel rond! En het lijkt\nerop dat ze bananen aan het hamsteren\nzijn. Wat zouden ze van plan zijn?"
  Npc_Attacked_006_DLC2nd_SD_100:
    attributes: Npc_Attacked_006_DLC2nd
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 0
      - text: "Bedoel je echt dat je naar binnen\nbent gegaan...\n\n\n...en die "
      - control:
          kind: set_colour
          colour: red
      - text: bol
      - control:
          kind: reset_colour
      - text: " hebt meegenomen?\n\n\nDat je hem hebt gegapt?\n\n\nEcht!?\n\n\n"
      - control:
          kind: sound
          unknown:
            - 3
            - 0
      - text: "Als ze erachter komen, achtervolgen ze\nje tot de uiteinden van de wereld!"
  Npc_Attacked_006_DLC2nd_SD_200:
    attributes: Npc_Attacked_006_DLC2nd
