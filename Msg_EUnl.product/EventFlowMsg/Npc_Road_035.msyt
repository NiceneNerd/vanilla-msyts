  RetMSG_Kyu_00:
    attributes: Npc_Road_035
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: "Ik hoop niet dat je denkt dat ik ooit over\niets anders dan paarden ga praten..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Vrouwen hebben het vaak over de prins\nop het witte paard en zulk soort onzin..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Maar ik zie een wit paard liever zonder\nprins erop. Ik ben helemaal gek op witte\npaarden!\n"
      - control:
          kind: sound
          unknown:
            - 1
            - 11
      - text: "Gevlekt, bruin, zwart..."
      - control:
          kind: pause
          length: long
      - text: " Paarden hebben\nvele kleuren, maar wit is toch wel m'n\nfavoriet!"
  RetMSG_Kyu_00_1:
    attributes: Npc_Road_035
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 11
      - text: "Gevlekt, bruin, zwart..."
      - control:
          kind: pause
          length: long
      - text: " Paarden hebben\nvele kleuren, maar wit is toch wel m'n\nfavoriet!"
  RetMSG_Kyu_01:
    attributes: Npc_Road_035
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: "Ik kan absoluut niet overweg met\nmensen,"
      - control:
