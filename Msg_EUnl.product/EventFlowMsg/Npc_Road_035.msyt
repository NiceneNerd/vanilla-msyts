  Rest_00:
    attributes: Npc_Road_035
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: "Ik praat zo vaak over paarden, dat mijn\nvrouw bij me is weggegaan."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "O, ik moet je iets vertellen. Het is\nmisschien een beetje veel informatie, dus\nluister goed.\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "De Satori-berg, ten noordwesten van\nde Vergeten Hoogvlakte..."
      - control:
          kind: pause
          length: short
      - text: " Ten oosten\ndaarvan vind je het Sanidin-park...\nEr loopt daar ergens een paard rond dat\ngeschikt is voor een prins."
  Rest_01:
    attributes: Npc_Road_035
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: "Ik verveel me omdat het regent, dus ik ga\nje een vraag stellen over paarden..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Wit paard...
      - control:
          kind: pause
          length: short
      - text: " Reuzenpaard..."
      - control:
          kind: pause
          length: short
      - text: " Mechanisch\npaard... "
      - control:
          kind: pause
          length: short
      - text: "Skeletpaard... "
      - control:
          kind: pause
          length: longer
      - text: "Welk paard\nbestaat niet?"
      - control:
          kind: pause
          length: long
      - text: "\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Het antwoord zal ooit bekend worden\ngemaakt,"
      - control:
          kind: pause
          length: short
