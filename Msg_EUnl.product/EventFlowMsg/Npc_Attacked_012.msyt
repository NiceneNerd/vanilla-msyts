          unknown:
            - 9
            - 0
      - text: "D-dank je... Dat monster was best wel\ngevaarlijk."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Ik kan best goed overweg met een\nzwaard, maar... Hier is een bedankje!"
  SavedLv3_01:
    attributes: ""
    contents:
      - control:
