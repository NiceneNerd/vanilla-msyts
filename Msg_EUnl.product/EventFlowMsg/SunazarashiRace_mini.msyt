    attributes: Npc_oasis039
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Wat is er?
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ik hoop dat je niet hier bent om Tali\nnog eens onder de neus te wrijven dat jij\nde nieuwe kampioen bent!"
  Npc_oasis039_R_010:
    attributes: Npc_oasis038
    contents:
      - control:
