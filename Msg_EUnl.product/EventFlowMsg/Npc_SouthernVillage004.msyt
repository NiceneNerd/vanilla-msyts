  talk03:
    attributes: Npc_SouthernVillage004
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Nou, dan zal ik je er alles over vertellen."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Op zee is het belangrijk om efficiënt\ngebruik te maken van de wind."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Wij vissers hebben in de loop der jaren\neen instinct ontwikkeld voor het\nvoorspellen van weer en wind.\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 24
      - text: "Maar ik heb gehoord dat er ook een\nhandig hulpmiddel bestaat, genaamd\nde "
      - control:
          kind: set_colour
          colour: blue
      - text: Korok-waaier
      - control:
          kind: reset_colour
      - text: ".\nJe kunt die waaier gebruiken om de "
      - control:
