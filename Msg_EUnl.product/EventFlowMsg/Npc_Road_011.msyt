      - control:
          kind: sound
          unknown:
            - 1
            - 0
      - control:
          kind: sound2
          unknown:
            - 7
            - 205
      - text: "Haha. Ik vraag me af of ik ook zoiets zeg\nals ik zelf ooit kinderen heb."
  RetMSG_Gerud_04_1:
    attributes: Npc_Road_011
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
