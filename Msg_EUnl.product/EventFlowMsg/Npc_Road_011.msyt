  MSG_Gerud_02:
    attributes: Npc_Road_011
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Het is moeilijk om door de woestijn te\nlopen, dus ik zou liever met een\nwoestijnwalrus reizen...\n"
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "Helaas kan ik niet rijden op zo'n ding..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Kwam de man van mijn dromen maar\nlangs op een woestijnwalrus!"
  MSG_Gerud_02_1:
    attributes: Npc_Road_011
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "Het is moeilijk om door de woestijn te\nlopen, dus ik hoop dat er een prins op\neen woestijnwalrus langskomt."
  MSG_Gerud_03:
    attributes: Npc_Road_011
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "De Kara Kara-bazaar is een soort\nrecreatieoord voor Gerudo's."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ik heb al besloten dat ik naar de\nKara Kara-bazaar ga voor mijn\nhuwelijksreis.\nDus nu moet ik alleen nog even de man\nvan mijn dromen vinden."
  MSG_Gerud_03_1:
    attributes: Npc_Road_011
    contents:
