  MSG_Gerud_01:
    attributes: Npc_Road_011
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Als ik Dorrah of Merina zie, weet ik dat ik\nterug ben in het Gerudo-fort."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ze zijn keihard tegen mannen en ze zien\ner angstaanjagend uit, maar diep van\nbinnen zijn ze heel aardig."
  MSG_Gerud_01_1:
    attributes: Npc_Road_011
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
