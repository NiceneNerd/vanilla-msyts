          kind: sound
          unknown:
            - 5
            - 0
      - control:
          kind: sound2
          unknown:
            - 14
            - 205
      - text: "M'n grootmoedertje vertelde me dat er\nniets zo verdacht is als een man die\nmeteen over het 'lot' begint.\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Ze heeft vast gelijk.
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Sorry, ik heb het druk."
      - control:
          kind: pause
          length: long
      - text: " Tot ziens."
      - control:
          kind: pause
          length: short
      - text: " Savorq!"
  first_04_1:
