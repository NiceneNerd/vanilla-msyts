      - text: "Die monsters wil ik liever niet beter leren\nkennen..."
  SavedLv2_01:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 11
      - text: Dankzij jou leef ik nog...
  SavedLv3_00:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 18
      - text: "Oef... "
      - control:
          kind: pause
          length: long
      - text: "Er lopen hier tegenwoordig steeds\nmeer monsters rond..."
  SavedLv3_01:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 11
