  Guardian00:
    attributes: Npc_Road_034
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 31
      - text: Je kunt maar beter wegrennen!
      - control:
          kind: pause
          length: long
      - text: " Er komt\neen "
      - control:
          kind: set_colour
