  FirstTalk02:
    attributes: Npc_Kakariko013
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 0
      - text: "Die zwaardvechter trainde namelijk met\neen "
      - control:
          kind: set_colour
          colour: blue
      - text: sprintwortel
      - control:
          kind: reset_colour
      - text: " in plaats van een\nzwaard...\nHij kweekte spiermassa op een dieet van\nenkel "
      - control:
          kind: set_colour
          colour: blue
      - text: sprintwortels
      - control:
          kind: reset_colour
