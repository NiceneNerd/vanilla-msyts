      - text: " geweldig!"
  talk_showWeapon01:
    attributes: Npc_RiverSideHatago002
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: "Dank je! Je hebt me iets wonderbaarlijks\nlaten zien. Ik zou er eigenlijk zelf eentje\nmoeten gaan zoeken."
  talk_showWeapon02:
    attributes: Npc_RiverSideHatago002
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Dit is een teken van mijn waardering.
  talk_showWeaponBow:
    attributes: Npc_RiverSideHatago002
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 20
      - text: "W-wacht eens even! Kun je me de boog\nlaten zien die je daar hebt?!"
  talk_showWeaponLSword:
    attributes: Npc_RiverSideHatago002
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 20
      - text: "W-wacht eens even! Kun je me het grote\nzwaard laten zien dat je daar hebt?!"
  talk_showWeaponShield:
    attributes: Npc_RiverSideHatago002
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 20
      - text: "W-wacht eens even! Kun je me het\nschild laten zien dat je daar hebt?!"
  talk_showWeaponSpear:
    attributes: Npc_RiverSideHatago002
    contents:
