entries:
  QL_ZoraMini_ReliefSearch_Desc:
    contents:
      - text: "Op verzoek van Jiahto heb je toegestemd om\nop zoek te gaan naar de "
      - control:
          kind: set_colour
          colour: red
      - text: "stenen monumenten "
      - control:
          kind: reset_colour
      - text: "in\nhet gebied rond het Zora-paleis.\n\nZonder het monument in het Zora-paleis mee te\nrekenen, schijnen er "
      - control:
          kind: set_colour
          colour: red
      - text: tien stenen monumenten
      - control:
          kind: reset_colour
      - text: "\nte zijn...\n\nJe moet er nog "
      - control:
          kind: variable
          variable_kind: 14
          name: ZoraMini_ReliefSearch_Count
      - text: " vinden."
  QL_ZoraMini_ReliefSearch_Finish:
    contents:
      - text: "Je hebt de tien stenen monumenten gevonden\nen hebt dat aan Jiahto verteld."
  QL_ZoraMini_ReliefSearch_Name:
    contents:
      - text: De stenen monumenten
  QL_ZoraMini_ReliefSearch_SearchRelief:
    contents:
      - text: "Je hebt alle tien de stenen monumenten in de\nbuurt van het Zora-paleis gevonden.\n\nBreng verslag uit aan "
      - control:
          kind: set_colour
          colour: red
      - text: Jiahto
      - control:
          kind: reset_colour
      - text: "."
