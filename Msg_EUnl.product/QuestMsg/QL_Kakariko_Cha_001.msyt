entries:
  QL_Kakariko_Cha_001_Desc:
    contents:
      - text: "Vandaag staat er groentecrèmesoep op het\nmenu! Tenminste, dat wilde Koko maken, maar\nze heeft geen "
      - control:
          kind: set_colour
          colour: red
      - text: sprintwortel
      - control:
          kind: reset_colour
      - text: ". Haar zusje Cottla is\ndol op groentecrèmesoep. Koko wil Cottla niet\nteleurstellen. Had ze nou maar een\n"
      - control:
          kind: set_colour
          colour: red
      - text: sprintwortel
      - control:
          kind: reset_colour
      - text: "..."
  QL_Kakariko_Cha_001_Finish:
    contents:
      - text: "Koko was in paniek omdat ze geen sprintwortel\nhad, maar Link heeft haar er eentje gegeven\nzodat ze groentecrèmesoep kon maken! Koko\nis blij en Cottla straks ook!\n\n"
      - control:
          kind: set_colour
          colour: red
      - text: Ingrediënten
      - control:
          kind: reset_colour
      - text: ":\nsprintwortel, zoutkristal, melk."
  QL_Kakariko_Cha_001_Name:
    contents:
      - text: Koken met Koko
