entries:
  QL_HatenoMini_WhiteDragon_Desc:
    contents:
      - text: "Volgens Meddo uit Hateno kun je bovenop de\nLanayru-berg in het noorden de\n'"
      - control:
          kind: set_colour
          colour: red
      - text: Bron der Wijsheid
      - control:
          kind: reset_colour
      - text: "' vinden. Een prinses zou hier\nlang geleden een ritueel hebben ondergaan...\nVolgens de geruchten ligt er een schat in de\n"
      - control:
          kind: set_colour
          colour: red
      - text: Bron der Wijsheid
      - control:
          kind: reset_colour
      - text: "."
  QL_HatenoMini_WhiteDragon_Finish:
    contents:
      - text: "Toen je "
      - control:
          kind: set_colour
          colour: red
      - text: "Naydra's schub"
      - control:
          kind: reset_colour
      - text: " in het water van de\nBron der Wijsheid gooide, kreeg je toegang tot\neen tempel.\n\nBevrijd van het kwaad, vloog Naydra de\nhorizon tegemoet."
  QL_HatenoMini_WhiteDragon_Name:
    contents:
      - text: Schat op de Lanayru-berg
  QL_HatenoMini_WhiteDragon_Play:
    contents:
      - text: "Je hebt "
      - control:
          kind: set_colour
          colour: red
      - text: Naydra
      - control:
          kind: reset_colour
      - text: ", de draak die de bron op de\nLanayru-berg bewaakt, bevrijd uit de klauwen\nvan het kwaad.\nToen "
      - control:
          kind: set_colour
          colour: red
      - text: Naydra
      - control:
          kind: reset_colour
      - text: " wegvloog, liet hij "
      - control:
          kind: set_colour
          colour: red
      - text: "Naydra's schub"
      - control:
          kind: reset_colour
      - text: "\nachter."
  QL_HatenoMini_WhiteDragon_Step1:
    contents:
      - text: "Je hebt geen schat gevonden... Maar wel de\ngeest die de Bron der Wijsheid bewaakt, de\ndraak "
      - control:
          kind: set_colour
          colour: red
      - text: Naydra
      - control:
          kind: reset_colour
      - text: ", die wordt bezeten door een\nduistere macht.\n\nDoorboor het kwaad dat zich aan "
      - control:
          kind: set_colour
          colour: red
      - text: Naydra
      - control:
          kind: reset_colour
      - text: "\nheeft gehecht en bevrijd hem uit de klauwen\nvan de duistere macht!"
