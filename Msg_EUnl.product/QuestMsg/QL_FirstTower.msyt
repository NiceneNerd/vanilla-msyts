group_count: 2
atr1_unknown: 0
entries:
  QL_FirstTower_Desc:
    contents:
      - text: "Een mooie vrouwenstem begint plotseling tegen\nje te spreken. Ze draagt je op om naar de plek\nte gaan die op de"
      - control:
          kind: set_colour
          colour: red
      - text: " kaart van je Sheikah-steen "
      - control:
          kind: reset_colour
      - text: "is\naangeduid...\nJe ziet de steen voor het eerst, maar op de een\nof andere manier voelt hij vertrouwd.\nJe kunt de kaart bekijken door op "
      - control:
          kind: icon
          icon: minus
      - text: " te\ndrukken."
  QL_FirstTower_Finish:
    contents:
      - text: "Toen je de Sheikah-steen gebruikte op de plek\ndie stond aangegeven op de kaart, schoot er\nonder je voeten een enorme toren uit de grond.\n\nVlak daarna hoorde je uit de richting van het in\nde verte gelegen kasteel opnieuw die mooie\nvrouwenstem..."
  QL_FirstTower_Name:
    contents:
      - text: Volg de Sheikah-steen
