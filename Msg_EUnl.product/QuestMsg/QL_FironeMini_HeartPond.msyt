group_count: 3
atr1_unknown: 0
entries:
  QL_FironeMini_HeartPond_Desc:
    contents:
      - text: "Wabbin is op slag verliefd geworden op een\nGerudo-vrouw. Hij wil zijn gevoelens voor haar\nkenbaar maken door haar "
      - control:
          kind: set_colour
          colour: red
      - text: fluisterkruid
      - control:
          kind: reset_colour
      - text: " te\ngeven. Hij heeft echter geen "
      - control:
          kind: set_colour
          colour: red
      - text: fluisterkruid
      - control:
          kind: reset_colour
      - text: "!"
  QL_FironeMini_HeartPond_Finish:
    contents:
      - text: "Wabbin was verliefd geworden op Perda. Ze\nzat al te wachten tot Wabbin haar een keer uit\nzou vragen. Het was niet helemaal de ideale\nliefdesverklaring, maar ze hebben elkaar\ngelukkig gevonden."
  QL_FironeMini_HeartPond_GetFlower:
    contents:
      - text: "Wabbin wil de Gerudo-vrouw waar hij verliefd op\nis "
      - control:
          kind: set_colour
          colour: red
      - text: "fluisterkruid "
      - control:
          kind: reset_colour
      - text: "geven om zijn liefde kenbaar te\nmaken. Hij is echter zo zenuwachtig dat hij\ngeen voet kan verzetten.\n\nHij heeft jou gevraagd om het "
      - control:
          kind: set_colour
          colour: red
      - text: fluisterkruid
      - control:
          kind: reset_colour
      - text: "\nnamens hem aan de vrouw te geven."
  QL_FironeMini_HeartPond_Name:
    contents:
      - text: Stille liefde
  QL_FironeMini_HeartPond_PresentFlower:
    contents:
      - text: "Je hebt Perda het "
      - control:
          kind: set_colour
          colour: red
      - text: fluisterkruid
      - control:
          kind: reset_colour
      - text: " gegeven voor\nWabbin, omdat hij het niet durfde te geven."
