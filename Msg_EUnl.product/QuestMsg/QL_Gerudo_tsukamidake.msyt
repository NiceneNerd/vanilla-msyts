entries:
  QL_Gerudo_tsukamidake_Desc:
    contents:
      - text: "Pirou in de kloofstal beweert dat hij niet kan\nleven zonder sprintzwammen, maar omdat ze\nop rotswanden groeien, kan hij ze niet zelf\nplukken. Hij geeft je een diamant als je hem\n"
      - control:
          kind: set_colour
          colour: red
      - text: "55 sprintzwammen "
      - control:
          kind: reset_colour
      - text: brengt.
  QL_Gerudo_tsukamidake_Finish:
    contents:
      - text: "Je hebt Pirou in de kloofstal 55 sprintzwammen\ngegeven en daarvoor een diamant gekregen.\n\nHet lijkt er echter op dat hij er nog meer nodig\nheeft.\n\nAls je hem vanaf nu "
      - control:
          kind: set_colour
          colour: red
      - text: "55"
      - control:
          kind: reset_colour
      - text: " sprintzwammen brengt,\nkoopt hij ze voor "
      - control:
          kind: set_colour
          colour: red
      - text: twee keer
      - control:
          kind: reset_colour
      - text: " de marktwaarde."
  QL_Gerudo_tsukamidake_Name:
    contents:
      - text: "Sprint, zwam, sprint!"
