group_count: 3
atr1_unknown: 0
entries:
  QL_Hateno_SheikPad_PowerUp_Desc:
    contents:
      - text: "Purah bood aan om wat voor je te doen, als jij\nhaar wat "
      - control:
          kind: set_colour
          colour: red
      - text: geavanceerde onderdelen
      - control:
          kind: reset_colour
      - text: " bezorgt.\n\nMaar wat zijn "
      - control:
          kind: set_colour
          colour: red
      - text: geavanceerde onderdelen
      - control:
          kind: reset_colour
      - text: "? En\nwie is "
      - control:
          kind: set_colour
          colour: red
      - text: Robbie
      - control:
          kind: reset_colour
      - text: "? Vraag Purah om meer informatie\nhierover."
  QL_Hateno_SheikPad_PowerUp_Explain:
    contents:
      - text: "Als je Purah genoeg "
      - control:
          kind: set_colour
          colour: red
      - text: geavanceerde onderdelen
      - control:
          kind: reset_colour
      - text: "\ngeeft, verbetert zij de krachten van je\nSheikah-steen.\n\nDe verbeterde Sheikah-sensor kost je "
      - control:
          kind: set_colour
          colour: red
      - text: "drie\ngeavanceerde schroeven"
      - control:
          kind: reset_colour
      - text: ", de afstandsbommen\nkosten je "
      - control:
          kind: set_colour
          colour: red
      - text: drie geavanceerde assen
      - control:
          kind: reset_colour
      - text: " en Stasis\nkost je "
      - control:
          kind: set_colour
          colour: red
      - text: drie geavanceerde kernen
      - control:
          kind: reset_colour
      - text: "."
  QL_Hateno_SheikPad_PowerUp_Finish:
    contents:
      - text: "Purah heeft je Sheikah-sensor, je\nafstandsbommen en je Stasis verbeterd!\n\nVolgens Purah kun je je krachten nu niet\nmeer verder laten verbeteren."
  QL_Hateno_SheikPad_PowerUp_Name:
    contents:
      - text: Verbeteringen door Purah
