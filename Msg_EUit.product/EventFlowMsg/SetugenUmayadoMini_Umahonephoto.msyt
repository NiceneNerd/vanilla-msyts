  NPC_Umahonephoto001_R_001:
    attributes: Npc_Hunters_Lost
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ehi, come va? "
      - control:
          kind: pause
          length: short
      - text: "Hai trovato il "
      - control:
          kind: set_colour
          colour: red
      - text: "cavallo\nd'ossa"
      - control:
          kind: reset_colour
      - text: "?\n\nHo sentito dire che vaga tra il\n"
      - control:
          kind: set_colour
          colour: blue
      - text: "nevaio di Colbacco nord "
      - control:
          kind: reset_colour
      - text: "e quelle strane\n"
      - control:
