      - text: sacrario
      - control:
          kind: reset_colour
      - text: "!\n\nAllora quei "
      - control:
          kind: set_colour
          colour: blue
      - text: guardiani
      - control:
          kind: reset_colour
      - text: " avevano a che fare\ncon la prova!"
  Npc_Zora032_SD_101:
    attributes: Npc_Zora032
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "Anche se "
      - control:
          kind: set_colour
          colour: blue
      - text: Ruta
      - control:
          kind: reset_colour
      - text: " si è placato, non si può\nmai abbassare la guardia..."
      - control:
          kind: pause
          length: long
      - text: " Continuerò a\nproteggere il villaggio come sempre."
  Npc_ZoraB001_SD_000:
    attributes: Npc_ZoraB001
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 10
      - text: "Ah, Link, grazie di esserti occupato\ndel colosso sacro Vah Ruta!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 1
      - text: "Hai saputo confutare le argomentazioni\ndegli anziani..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 7
            - 1
      - text: "... e salvare la nostra terra.\nSenza contare che sei un Hylia!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Quel che hai fatto è davvero\nstraordinario!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 1
      - text: "Hai deviato il tuo viaggio per venire\nnel nostro regno, tormentato dai mostri\ne dalla pioggia incessante.\n"
      - control:
          kind: sound
          unknown:
            - 7
            - 1
      - text: "Hai schivato gli attacchi di "
      - control:
          kind: set_colour
          colour: blue
      - text: Ruta
      - control:
          kind: reset_colour
      - text: " e hai\nattivato i suoi generatori usando le\n"
      - control:
          kind: set_colour
          colour: blue
      - text: frecce elettriche
      - control:
          kind: reset_colour
      - text: ".\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Hai liberato "
      - control:
          kind: set_colour
          colour: blue
      - text: Ruta
      - control:
          kind: reset_colour
      - text: " e messo fine\nai tormenti di Mipha..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 9
      - text: Link...
      - control:
          kind: pause
          frames: 20
      - text: " non potrò mai ringraziarti\nabbastanza!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 7
            - 1
      - text: "Grazie per essere stato al mio fianco\nfino alla fine, nonostante i miei modi!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 7
            - 1
      - text: "Se non fossi arrivato tu, non so in che\nstato verserebbe il "
      - control:
          kind: set_colour
          colour: blue
      - text: villaggio degli Zora
      - control:
          kind: reset_colour
      - text: "!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 7
            - 1
      - text: Torna pure da noi se ti serve qualcosa.
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 7
            - 4
      - text: Saremo felicissimi di aiutarti!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: text_size
          percent: 125
      - control:
          kind: sound
          unknown:
            - 7
            - 9
      - text: "Sei un grande, Link!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: text_size
          percent: 100
      - control:
          kind: sound
          unknown:
            - 7
            - 10
      - text: Sei davvero il migliore tra gli Hylia!
      - control:
          kind: pause
          frames: 10
      - text: "\nE il mio migliore amico!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 1
      - text: Scusa...
      - control:
          kind: pause
          length: long
      - text: " come sempre, non so\ncontrollare la mia parlantina."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 7
            - 9
      - text: "Ma voglio farti capire davvero quanto\nti sono riconoscente...."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: Comunque...
      - control:
          kind: pause
          length: long
      - text: " Link, cosa ti porta qui oggi?"
      - control:
          kind: choice
          choice_labels:
            - 2
            - 0
            - 1
          selected_index: 0
          cancel_index: 2
          unknown: 8
