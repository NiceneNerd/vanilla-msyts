      - text: " sono apparse le\nstesse cose di quando la "
      - control:
          kind: set_colour
          colour: blue
      - text: "principessa\nMipha "
      - control:
          kind: reset_colour
      - text: si sottopose alla prova.
  Npc_Zora014_SD_004:
    attributes: Npc_Zora014
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 16
      - text: "All'epoca ci pensò lei a disfarsene,\nma questa volta chi dovrà affrontare\nla prova?"
  Npc_Zora014_SD_100:
    attributes: Npc_Zora014
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Hai sconfitto i "
      - control:
          kind: set_colour
          colour: blue
      - text: "guardiani "
      - control:
          kind: reset_colour
      - text: "apparsi\nsull'"
      - control:
          kind: set_colour
          colour: blue
      - text: altopiano Zorana
      - control:
          kind: reset_colour
      - text: "!\n\n"
      - control:
          kind: sound
          unknown:
            - 11
            - 16
      - text: "Non pensavo che saresti riuscito a\nsuperare la stessa prova svolta dalla\n"
      - control:
          kind: set_colour
          colour: blue
      - text: principessa Mipha
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: pause
          length: long
      - text: "\n"
      - control:
          kind: sound
          unknown:
            - 1
            - 11
      - text: "Ma dopotutto sei anche tu un campione!\nCongratulazioni, quindi!"
  Npc_Zora014_SD_101:
    attributes: Npc_Zora014
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: A proposito...
      - control:
          kind: pause
          length: long
      - text: " sei tu quello che ha\nsconfitto i "
      - control:
          kind: set_colour
          colour: blue
      - text: "guardiani "
      - control:
          kind: reset_colour
      - text: "che erano\nsull'"
      - control:
          kind: set_colour
          colour: blue
      - text: altopiano Zorana
      - control:
          kind: reset_colour
      - text: "?"
      - control:
          kind: pause
          length: long
      - text: "\n"
      - control:
          kind: sound
          unknown:
            - 11
            - 16
      - text: "Non pensavo che saresti riuscito a\nsuperare la stessa prova svolta dalla\n"
      - control:
          kind: set_colour
          colour: blue
      - text: principessa Mipha
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: pause
          length: long
      - text: "\n"
      - control:
          kind: sound
          unknown:
            - 1
            - 11
      - text: "Ma dopotutto sei anche tu un campione!\nCongratulazioni, quindi!"
  Npc_Zora032_SD_000:
    attributes: Npc_Zora032
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 6
      - text: Mh?
      - control:
          kind: pause
          length: long
      - text: " È raro vedere un Hylia da queste\nparti..."
      - control:
          kind: pause
          length: long
      - text: "\n\n"
      - control:
          kind: sound
          unknown:
            - 7
            - 11
      - text: "Oh, ma sei tu, maestro Link!"
      - control:
          kind: pause
          length: long
      - text: " Grazie per\naverci aiutati a risolvere il problema\ncon "
      - control:
          kind: set_colour
          colour: blue
      - text: Vah Ruta
      - control:
          kind: reset_colour
      - text: "!\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Purtroppo, si è presentata un'altra\nmagagna:"
      - control:
          kind: pause
          length: long
      - text: " sull'"
      - control:
          kind: set_colour
          colour: blue
      - text: altopiano Zorana
      - control:
          kind: reset_colour
      - text: " sono\napparsi dei "
      - control:
          kind: set_colour
          colour: red
      - text: "guardiani con l'occhio blu"
      - control:
          kind: reset_colour
      - text: "!\nL'ultima volta che quelle cose si sono\nfatte vedere da queste parti"
      - control:
          kind: pause
          length: long
      - text: " fu quando\nla principessa Mipha affrontò la prova.\nPapà ha detto che non c'è bisogno di\npreoccuparsi, ma secondo me è meglio\ntenere gli occhi aperti, non si sa mai."
  Npc_Zora032_SD_001:
    attributes: Npc_Zora032
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "A quanto pare,"
      - control:
          kind: pause
          length: long
      - text: " i "
      - control:
          kind: set_colour
          colour: red
      - text: "guardiani con l'occhio\nblu"
      - control:
          kind: reset_colour
      - text: " apparvero anche quando la\n"
      - control:
          kind: set_colour
