  Npc_ZoraB001_SD_010:
    attributes: Npc_ZoraB001
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 20
      - text: "Ah, vuoi sapere della prova?\nCapisco...\n\n"
      - control:
          kind: sound
          unknown:
            - 11
            - 1
      - text: "Ultimamente stanno succedendo strane\ncose, le stesse di quando "
      - control:
          kind: set_colour
          colour: blue
      - text: Mipha
      - control:
          kind: reset_colour
      - text: " dovette\nsuperare la prova per guidare "
      - control:
          kind: set_colour
          colour: blue
      - text: Ruta
      - control:
          kind: reset_colour
      - text: ".\nHo mandato "
      - control:
          kind: set_colour
          colour: blue
      - text: Morit
      - control:
          kind: reset_colour
      - text: " e "
      - control:
          kind: set_colour
          colour: blue
      - text: Takka
      - control:
          kind: reset_colour
      - text: " a controllare\nla zona a est del "
      - control:
          kind: set_colour
          colour: blue
      - text: burrone di Ulra
      - control:
          kind: reset_colour
      - text: ", che\nera una delle aree della prova."
      - control:
          kind: pause
          length: long
      - text: "\n"
      - control:
          kind: sound
          unknown:
            - 7
            - 10
      - text: "Ah, ma non dirmi... anche tu vuoi\naffrontare la prova!?"
  Npc_ZoraB001_SD_012:
    attributes: Npc_ZoraB001
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 1
      - text: "Se stai andando ad affrontare la prova,\nsappi che io sono sicuro che ce la farai!\n\n"
      - control:
          kind: sound
          unknown:
            - 7
            - 10
      - text: "Io faccio il tifo per te, perché siamo\namici del cuore!"
  Npc_ZoraB001_SD_013:
    attributes: Npc_ZoraB001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 16
      - text: "Vuoi sapere di mia sorella "
      - control:
          kind: set_colour
          colour: blue
      - text: Mipha
      - control:
          kind: reset_colour
      - text: "?\nSai, ci stavo pensando ultimamente...\n\nRicordo che cento anni fa..."
      - control:
          kind: pause
          length: long
      - text: " stava\nspesso con uno spadaccino che veniva\na trovarla.\n"
      - control:
          kind: sound
          unknown:
            - 11
            - 1
      - text: "Io ero molto arrabbiato con lui,"
      - control:
          kind: pause
          length: long
      - text: " perché\nera come se mi stesse portando via\nmia sorella.\n"
      - control:
          kind: sound
          unknown:
            - 7
            - 10
      - text: "Ma ora mi rendo conto che quello\nspadaccino eri proprio tu, vero Link?\n\nPensa un po', Link...\n\n\n"
      - control:
          kind: sound
          unknown:
            - 11
            - 1
      - text: Se ti fossi sposato con mia sorella...
      - control:
          kind: pause
          length: long
      - text: "\nadesso saresti mio cognato!\n\n..."
      - control:
          kind: pause
          frames: 60
      - text: "\n\n\n"
      - control:
          kind: sound
          unknown:
            - 7
            - 10
      - text: "Scusa, non volevo metterti in imbarazzo!"
      - control:
          kind: pause
          length: long
      - text: "\nMi sono lasciato trasportare\ndall'emozione!"
  Npc_ZoraB001_SD_020:
    attributes: Npc_ZoraB001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: Capisco...
      - control:
          kind: pause
          length: long
      - text: " ma torna pure a trovarmi\nquando hai un po' di tempo!\n\n"
      - control:
          kind: sound
          unknown:
            - 7
            - 10
      - text: Io faccio sempre il tifo per te!
