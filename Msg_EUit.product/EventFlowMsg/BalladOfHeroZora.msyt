entries:
  "0000":
    attributes: ""
    contents:
      - text: Dimmi di Mipha.
  "0001":
    attributes: ""
    contents:
      - text: Ciao.
  "0002":
    attributes: ""
    contents:
      - text: Dimmi della prova.
  "0003":
    attributes: ""
    contents:
      - text: Che diario?
  Npc_Zora001_SD_000:
    attributes: Npc_Zora001
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 20
      - text: Ooh...
      - control:
          kind: pause
          length: short
      - text: " ma tu sei il campione!"
      - control:
          kind: pause
          length: short
      - text: " Ma che\ncoincidenza!\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Io mi trovo qui come guardia del corpo\ndi "
      - control:
          kind: set_colour
          colour: blue
      - text: messer Morit
      - control:
          kind: reset_colour
      - text: ".\n\n"
      - control:
          kind: sound
          unknown:
            - 1
            - 1
      - text: "Pare che questo sia il posto in cui\nla "
      - control:
          kind: set_colour
          colour: blue
      - text: principessa
      - control:
          kind: reset_colour
      - text: " "
      - control:
          kind: set_colour
          colour: blue
      - text: Mipha
      - control:
          kind: reset_colour
      - text: " fu sottoposta\na una prova..."
      - control:
          kind: pause
          length: short
      - text: "\nMa "
      - control:
          kind: set_colour
          colour: blue
      - text: messer
      - control:
          kind: reset_colour
      - text: " "
      - control:
          kind: set_colour
          colour: blue
      - text: "Morit "
      - control:
          kind: reset_colour
      - text: "ne sa certamente più\ndi me!"
  Npc_Zora001_SD_100:
    attributes: Npc_Zora001
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 1
      - text: Non è stato facile arrivare qui.
      - control:
          kind: pause
          length: long
      - text: "\nPer la via siamo stati assaliti da un\nsacco di mostri..."
      - control:
          kind: pause
          length: long
      - text: "\n"
      - control:
          kind: sound
          unknown:
            - 1
            - 4
      - text: Ma messer Morit
      - control:
          kind: pause
          length: short
      - text: " continuava a ripetere\nche doveva dimostrare la sua utilità\nal principe Sidon."
  Npc_Zora001_SD_200:
    attributes: Npc_Zora001
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 1
      - text: "Messer Morit mi ha detto di prepararmi\nper tornare a casa...\n\n"
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "... ma se ripenso alla strada che abbiamo\nfatto per venire fin qui, mi sento male..."
      - control:
          kind: pause
          length: long
  Npc_Zora001_SD_300:
    attributes: Npc_Zora001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ho fatto un giro nei dintorni del\n"
      - control:
          kind: set_colour
          colour: blue
      - text: villaggio degli Zora
      - control:
          kind: reset_colour
      - text: ", ma la zona sembra\nancora molto pericolosa.\n"
      - control:
          kind: sound
          unknown:
            - 7
            - 4
      - text: "Difenderò il nostro villaggio con\nla massima attenzione!"
  Npc_Zora001_SD_301:
    attributes: Npc_Zora001
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: Messer Morit era così contento
      - control:
          kind: pause
          length: short
      - text: " di\nessersi dimostrato utile agli occhi\ndel principe Sidon."
  Npc_Zora003_SD_000:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 4
      - text: "Link, se dovessi mai trovarti nei guai,"
      - control:
          kind: pause
          frames: 20
      - text: "\nti prego, torna qui da noi.\n\n"
      - control:
          kind: sound
          unknown:
            - 1
            - 1
      - text: "Anche nel momento più buio, l'intero\npopolo Zora ti accoglierà come se tu\nfossi uno di noi.\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: A proposito...
      - control:
          kind: pause
          length: long
      - text: " c'è qualcosa che posso\nfare per te?"
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Npc_Zora003_SD_010:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: Bene...
      - control:
          kind: pause
          length: long
      - text: " lascia che ti parli di "
      - control:
          kind: set_colour
          colour: blue
      - text: Mipha
      - control:
          kind: reset_colour
      - text: ".\nAnche questo è un modo per onorare\nla sua memoria.\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Mipha
      - control:
          kind: reset_colour
      - text: " è sempre stata una ragazza\ngentile,"
      - control:
          kind: pause
          length: long
      - text: " sia con gli abitanti del villaggio,\nsia con i visitatori.\n"
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: "Quando venisti qui, da piccolino,\nti coccolava come fossi anche tu suo\nfratellino.\n"
      - control:
          kind: sound
          unknown:
            - 5
            - 0
      - text: "Ma voi Hylia crescete più velocemente\ndi noi, e presto il suo istinto di\nprotezione si trasformò in ammirazione."
      - control:
          kind: pause
          length: long
      - text: "\n"
      - control:
          kind: sound
          unknown:
            - 1
            - 1
      - text: "Il suo affetto per te era evidente,"
      - control:
          kind: pause
          length: long
      - text: " si\ncapisce anche leggendo il suo "
      - control:
          kind: set_colour
          colour: blue
      - text: diario
      - control:
          kind: reset_colour
      - text: "...\n\n"
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: "Una principessa Zora che si innamora\ndi un Hylia... questa storia era narrata\nanche nelle nostre leggende!\n"
      - control:
          kind: sound
          unknown:
            - 5
            - 1
      - text: "Per questo, l'idea che tu e "
      - control:
          kind: set_colour
          colour: blue
      - text: Mipha
      - control:
          kind: reset_colour
      - text: " un\ngiorno poteste essere marito e moglie\nnon mi dispiaceva."
      - control:
          kind: pause
          length: long
      - text: "\n"
      - control:
          kind: sound
          unknown:
            - 3
            - 12
      - text: "Ma poi, purtroppo..."
      - control:
          kind: pause
          length: long
      - control:
          kind: single_choice
          label: 3
  Npc_Zora003_SD_015:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 4
      - text: "Sì, il "
      - control:
          kind: set_colour
          colour: blue
      - text: diario di Mipha
      - control:
          kind: reset_colour
      - text: "... l'abbiamo\nritrovato di recente. È su "
      - control:
          kind: set_colour
          colour: blue
      - text: quel tavolo
      - control:
          kind: reset_colour
      - text: ".\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Per me puoi leggerlo. Secondo me,\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Mipha
      - control:
          kind: reset_colour
      - text: " non si arrabbierebbe..."
  Npc_Zora003_SD_020:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 11
      - text: "Se ti serve aiuto, vieni da me.\nOrmai sei di famiglia."
  Npc_Zora003_SD_100:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 4
      - text: "Se posso aiutarti in qualche modo,\nnon esitare a parlarmene."
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Npc_Zora004_SD_000:
    attributes: Npc_Zora004
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 11
      - text: "Ciao Link, come mai ti trovi da queste\nparti?\n\n"
      - control:
          kind: sound
          unknown:
            - 11
            - 1
      - text: "Noi siamo qui per ordine del principe\nSidon: dobbiamo perlustrare l'area."
  Npc_Zora004_SD_100:
    attributes: Npc_Zora004
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Sai, proprio qui la "
      - control:
          kind: set_colour
          colour: blue
      - text: principessa Mipha
      - control:
          kind: reset_colour
      - text: "\naffrontò la sua prova e adesso sono\nriapparsi gli stessi segni di cento anni fa."
  Npc_Zora004_SD_110:
    attributes: Npc_Zora004
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 0
      - text: "Nei documenti della "
      - control:
          kind: set_colour
          colour: blue
      - text: principessa Mipha
      - control:
          kind: reset_colour
      - text: "\nsi trovano le seguenti parole:"
      - control:
          kind: pause
          length: long
      - text: "\n\n\"Quando sorge il"
      - control:
          kind: set_colour
          colour: red
      - text: " sole del mattino"
      - control:
          kind: reset_colour
      - text: "\nsegui il "
      - control:
          kind: set_colour
          colour: red
      - text: "sentiero di luce "
      - control:
          kind: reset_colour
      - text: "fino alla prova.\"\n\nQuesta frase ricorre molte volte e\nsembra legata alla prova..."
      - control:
          kind: pause
          length: long
  Npc_Zora004_SD_111:
    attributes: Npc_Zora004
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 0
      - text: "Ah, sei tu, Link!\n\n\nSai, la "
      - control:
          kind: set_colour
          colour: blue
      - text: "principessa Mipha "
      - control:
          kind: reset_colour
      - text: "aveva trovato\nle seguenti parole legate alla prova...\n\n\"Quando sorge il"
      - control:
          kind: set_colour
          colour: red
      - text: " sole del mattino"
      - control:
          kind: reset_colour
      - text: "\nsegui il "
      - control:
          kind: set_colour
          colour: red
      - text: "sentiero di luce "
      - control:
          kind: reset_colour
      - text: "fino alla prova.\"\n\nA quanto pare si tratta di una citazione\ntestuale."
  Npc_Zora004_SD_200:
    attributes: Npc_Zora004
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 21
      - text: Accidenti...
      - control:
          kind: pause
          length: long
      - text: " quindi il "
      - control:
          kind: set_colour
          colour: red
      - text: sentiero di luce
      - control:
          kind: reset_colour
      - text: "\nera il riflesso del sole sulla superficie\ndel mare?\n"
      - control:
          kind: sound
          unknown:
            - 1
            - 10
      - text: Così hai risolto il mistero...
      - control:
          kind: pause
          length: short
      - text: "\nNon per niente sei il campione di cui\nsi innamorò la "
      - control:
          kind: set_colour
          colour: blue
      - text: principessa Mipha
      - control:
          kind: reset_colour
      - text: "!"
  Npc_Zora004_SD_201:
    attributes: Npc_Zora004
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Il "
      - control:
          kind: set_colour
          colour: red
      - text: sentiero di luce
      - control:
          kind: reset_colour
      - text: " era il riflesso del\nsole sulla superficie del mare?\n\n"
      - control:
          kind: sound
          unknown:
            - 1
            - 10
      - text: "Non per niente sei il campione di cui\nsi innamorò la "
      - control:
          kind: set_colour
          colour: blue
      - text: principessa Mipha
      - control:
          kind: reset_colour
      - text: "!"
  Npc_Zora004_SD_300:
    attributes: Npc_Zora004
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 11
      - text: Link!
      - control:
          kind: pause
          frames: 20
      - text: "\n\n\n"
      - control:
          kind: sound
          unknown:
            - 11
            - 1
      - text: "Non hai pinne né branchie, e sott'acqua\naffoghi!\n\n"
      - control:
          kind: sound
          unknown:
            - 1
            - 10
      - text: "Eppure hai superato la stessa prova\ndella "
      - control:
          kind: set_colour
          colour: blue
      - text: principessa Mipha
      - control:
          kind: reset_colour
      - text: "!\n\n"
      - control:
          kind: sound
          unknown:
            - 1
            - 1
      - text: "Non mi stupisce che si fosse\ninnamorata di te!"
  Npc_Zora004_SD_350:
    attributes: Npc_Zora004
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 10
      - text: "Hai superato la stessa prova della\n"
      - control:
          kind: set_colour
          colour: blue
      - text: principessa Mipha
      - control:
          kind: reset_colour
      - text: "! Non mi stupisce\nche si fosse innamorata di te!"
  Npc_Zora014_SD_000:
    attributes: Npc_Zora014
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Potresti dimenticare la mia scortesia e\nlasciarla scorrere come l'acqua\nsotto un ponte?"
      - control:
          kind: pause
          length: longer
      - text: "\n..."
      - control:
          kind: pause
          length: longer
      - text: " ..."
      - control:
          kind: pause
          length: longer
      - text: "\n\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 16
      - text: "Forse sto esagerando. Ma ti prego,\nmettiamoci una conchiglia sopra..."
      - control:
          kind: pause
          length: longer
      - text: "\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Comunque, passando a cose più serie!"
  Npc_Zora014_SD_001:
    attributes: Npc_Zora014
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Sull'"
      - control:
          kind: set_colour
          colour: blue
      - text: altopiano Zorana
      - control:
          kind: reset_colour
      - text: " sono apparse le\nstesse cose di quando la "
      - control:
          kind: set_colour
          colour: blue
      - text: "principessa\nMipha "
      - control:
          kind: reset_colour
      - text: "si sottopose alla prova.\n"
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "All'epoca ci pensò lei a disfarsene,\nma questa volta chi dovrà affrontare\nla prova?\n"
      - control:
          kind: sound
          unknown:
            - 11
            - 1
      - text: Spero non...
      - control:
          kind: pause
          length: longer
      - text: "\n\n\n"
      - control:
          kind: sound
          unknown:
            - 4
            - 1
      - text: IO!!!
  Npc_Zora014_SD_002:
    attributes: Npc_Zora014
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Link!
      - control:
          kind: pause
          length: longer
      - text: " Sei venuto al momento giusto!"
      - control:
          kind: pause
          length: long
      - text: "\nGuarda laggiù!"
  Npc_Zora014_SD_003:
    attributes: Npc_Zora014
    contents:
