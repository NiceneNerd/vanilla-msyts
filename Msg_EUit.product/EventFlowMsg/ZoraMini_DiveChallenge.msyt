            - 10
      - control:
          kind: text_size
          percent: 125
      - text: Fantastico!
      - control:
          kind: text_size
          percent: 100
      - text: "\n\n\nChe sensazione! Il mio spirito gioisce\ncome un pesce che guizza fuori\ndall'acqua!\n"
      - control:
          kind: sound
          unknown:
            - 1
            - 18
      - text: Mi hai mostrato la bellezza!
      - control:
          kind: pause
          length: short
      - text: " E io te ne\nsono grato."
      - control:
          kind: pause
          length: longer
      - text: " Adesso torna da me."
  talk13:
    attributes: Npc_Zora029
    contents:
      - control:
