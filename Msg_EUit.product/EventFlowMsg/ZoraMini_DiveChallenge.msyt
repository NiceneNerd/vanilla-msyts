          percent: 125
      - control:
          kind: sound
          unknown:
            - 1
            - 11
      - text: Che splendore!
      - control:
          kind: text_size
          percent: 100
      - text: "\n\n\nCotanta bellezza quasi mi acceca.\n\n\n"
      - control:
          kind: sound
