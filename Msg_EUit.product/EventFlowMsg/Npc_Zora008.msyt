      - text: "calamità\nGanon "
      - control:
          kind: reset_colour
      - text: "avesse ucciso "
      - control:
          kind: pause
          length: short
      - text: "te e Mipha?\n\nE invece sei rimasto in vita per tutto\nquesto tempo! Dove sei stato?"
      - control:
          kind: pause
          length: longer
      - text: " Che cosa\nhai fatto?"
      - control:
          kind: pause
          length: short
      - text: " Su, dimmi! Dimmi!\n...\n\n\n"
      - control:
          kind: sound
          unknown:
            - 9
            - 19
      - text: "Oh, scusa..."
      - control:
          kind: pause
          length: short
      - text: " Che comportamento\ninfantile! Non dovrei assillarti così...\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Sono veramente trascorsi cento anni\nda allora?\n\nChissà quante ne hai passate..."
      - control:
          kind: pause
          length: short
      - text: "\nMa anch'io, sai?"
      - control:
          kind: pause
          length: short
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 4
