      - control:
          kind: sound
          unknown:
            - 1
            - 11
      - text: Mipha era la nostra principessa!
      - control:
          kind: pause
          length: longer
      - text: "\nEra la figlia del re Dorefan e la sorella\nmaggiore del principe Sidon.\nAveva il potere di guarire le ferite"
      - control:
          kind: pause
          length: short
      - text: " ed era\nla campionessa del popolo Zora.\n\nCento anni fa,"
      - control:
          kind: pause
          length: short
      - text: " fu lei a pilotare\nil "
      - control:
          kind: set_colour
          colour: blue
      - text: "colosso sacro Vah Ruta "
      - control:
          kind: reset_colour
      - text: "per\ncombattere contro la "
      - control:
          kind: set_colour
          colour: red
      - text: calamità Ganon
      - control:
          kind: reset_colour
      - text: ".\n"
      - control:
          kind: sound
          unknown:
            - 3
            - 16
      - text: "Ma sai come andò a finire, "
      - control:
          kind: pause
          length: long
      - text: vero?
