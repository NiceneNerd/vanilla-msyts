    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ma allora... tu sei forse quello di cui\nparla sempre mia nonna?"
      - control:
          kind: pause
          length: longer
      - text: "\nLi..."
      - control:
          kind: pause
          length: short
      - text: " Lin..."
      - control:
          kind: pause
          length: short
      - text: " Hmmm..."
      - control:
          kind: pause
          length: short
  Talk28:
    attributes: Npc_Kakariko002
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Lo so, lo so. È il mio nome e dovrei\nricordarmelo, capisco..."
      - control:
          kind: pause
          length: short
      - text: " \n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Mi... mi spiace.
      - control:
          kind: pause
          length: short
      - text: "\nFin da quando ero bambina mia\nnonna mi ha raccontato di voi!\n\nPrego..."
      - control:
          kind: pause
          length: short
      - text: " prego, entrate. Mia nonna..."
      - control:
          kind: pause
          length: short
      - text: "\nvi sta aspettando da molto tempo."
  Talk29:
    attributes: Npc_Kakariko002
