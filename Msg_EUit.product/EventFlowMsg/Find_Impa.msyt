  Talk19:
    attributes: Npc_Kakariko002
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 21
      - text: Aaah!
      - control:
          kind: pause
          length: longer
      - text: "\n\n\nSiete così... vicino!"
      - control:
          kind: pause
          length: long
      - text: " Io..."
      - control:
          kind: pause
          length: short
      - text: " Non sono\nabituata a parlare con un giovane\nmaestro..."
      - control:
          kind: pause
          length: longer
      - text: " P-per favore...\nMia..."
      - control:
          kind: pause
          length: short
      - text: " mia nonna vi aspetta dentro."
      - control:
          kind: pause
          length: short
      - text: "\nPer favore,"
      - control:
          kind: pause
          length: short
      - text: " entrate subito."
  Talk21:
    attributes: Npc_Kakariko002
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Non è che non ricordo, è solo che non\nsono mai stata brava con i nomi..."
      - control:
          kind: pause
          length: long
      - text: "\n\nAnche con il mio..."
      - control:
          kind: pause
          length: short
      - text: " che è..."
      - control:
          kind: pause
          length: short
      - text: " Pa..."
      - control:
          kind: pause
          length: short
      - text: "\nPa..."
      - control:
          kind: pause
          length: short
      - text: " Papaya..."
      - control:
          kind: pause
          length: short
      - text: " Ah, no! Ecco!"
      - control:
          kind: pause
          length: short
      - text: "\nMi chiamo Paya."
      - control:
          kind: pause
          length: long
  Talk22:
    attributes: Npc_Kakariko002
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 8
      - text: "... Ehi! "
      - control:
          kind: pause
          length: short
      - text: "Aspetta... che cos'hai lì?"
      - control:
          kind: pause
          length: longer
  Talk23:
    attributes: Npc_Kakariko005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Eh!? "
      - control:
          kind: pause
          length: short
      - text: Ma quella...
      - control:
          kind: pause
          length: long
      - text: "\n\n\n"
      - control:
          kind: sound
          unknown:
            - 10
            - 0
      - text: "... è la "
      - control:
          kind: set_colour
          colour: blue
      - text: tavoletta Sheikah
      - control:
          kind: reset_colour
      - text: "?!\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Allora tu potresti...
      - control:
          kind: pause
          length: long
      - text: " No..."
      - control:
          kind: pause
          length: short
      - text: " Forse tu sei..."
      - control:
          kind: pause
          length: long
  Talk25:
    attributes: Npc_Kakariko007
    contents:
      - control:
