group_count: 37
atr1_unknown: 4
entries:
  "0000":
    attributes: ""
    contents:
      - text: La banda?
  "0001":
    attributes: ""
    contents:
      - text: Spada leggendaria?
  "0002":
    attributes: ""
    contents:
      - text: A presto
  "0003":
    attributes: ""
    contents:
      - text: Comunque...
  FR01_Talk_01:
    attributes: NPC_oasis004_fr01
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 42
      - text: "Non ne posso piùùù!\nQualcuno mi aiuti!"
  FR01_Talk_02:
    attributes: NPC_oasis004_fr01
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 18
      - text: Un attimo...
      - control:
          kind: pause
          length: longer
      - text: "\nSono... sana e salva?"
      - control:
          kind: single_choice
          label: 3
  FR01_Talk_03:
    attributes: NPC_oasis004_fr01
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Gomana è inaffidabile e non oso\nimmaginare cosa sarebbe successo\nse tu non fossi arrivato...\n"
      - control:
          kind: sound
          unknown:
            - 7
            - 9
      - text: Grazie.
      - control:
          kind: pause
          length: longer
      - text: "\n\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Non avrei mai immaginato che viaggiare\nfosse così pericoloso. Non c'era scritto\nnulla del genere nelle mie riviste...\n"
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Beh, immagino che non si debba dar\ncredito a nulla finché non lo si vive\nsulla propria pelle..."
  FR01_Talk_04:
    attributes: NPC_oasis004_fr01
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 0
      - text: Gomana?
      - control:
          kind: pause
          length: longer
      - text: "\nNon appena sono comparsi i mostri,\nquel tipo è scappato a gambe levate.\nCome può essere stato così crudele\nda abbandonare una ragazza?\n\n"
      - control:
          kind: sound2
          unknown:
            - 12
            - 205
      - text: "Non lo perdonerò mai!\n\n\nSi trova allo stallaggio del canyon\nGerudo, dici?"
      - control:
          kind: pause
          length: longer
      - text: " Ora vado a dirgli due\nparoline!"
  FR01_Talk_05:
    attributes: NPC_oasis004_fr01
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 4
      - text: "Non oso immaginare cosa sarebbe\nsuccesso se tu non fossi arrivato...\nGrazie...\n"
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Non avrei mai immaginato che viaggiare\nfosse così pericoloso. Non c'era scritto\nnulla del genere nelle mie riviste...\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Beh, immagino che non si debba dar\ncredito a nulla finché non lo si vive\nsulla propria pelle. Ora l'ho capito."
      - control:
          kind: pause
          length: short
      - text: "\nE ho capito che non sono fatta per\ni viaggi.\n\n"
      - control:
          kind: sound
          unknown:
            - 7
            - 9
      - text: "Tu invece continuerai, vero?\nLo so che sei forte, ma fai attenzione!"
  FR02_Talk_01:
    attributes: NPC_oasis004_fr02
    contents:
      - control:
          kind: sound
          unknown:
            - 2
            - 14
      - text: "Accidenti...\nNe hanno dato di filo da torcere,\nquei maledetti!"
  FR02_Talk_02:
    attributes: NPC_oasis004_fr02
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Grazie dell'aiuto, amico.\nEro davvero in un mare di guai."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 8
            - 0
      - text: "Aaah, mi fa male dappertutto.\nDannate bestiacce!"
      - control:
          kind: single_choice
          label: 3
  FR02_Talk_03:
    attributes: NPC_oasis004_fr02
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Volevo dirgliene quattro a Gomana\nper come si era comportato, ma..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Quando ha visto il mio viso, sembrava\nsul punto di scoppiare a piangere da\nun momento all'altro."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Penso che lo perdonerò... D'altronde\nlui è fatto così, si spaventa subito se\ni suoi amici non tornano da lui."
  FR02_Talk_04:
    attributes: NPC_oasis004_fr02
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 0
      - text: "Gomana? Se ne stava bello tranquillo\nnello stallaggio, senza nemmeno pensare\ndi venirmi a salvare..."
      - control:
          kind: pause
          length: longer
      - text: "\nAh, ma gliela faccio vedere io!"
  FR02_Talk_05:
    attributes: NPC_oasis004_fr02
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Volevo dirgliene quattro a Gomana\nper come si era comportato, ma..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Quando ha visto il mio viso, sembrava\nsul punto di scoppiare a piangere da\nun momento all'altro."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Penso che lo perdonerò... D'altronde\nlui è fatto così, si spaventa subito se\ni suoi amici non tornano da lui.\n"
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: "Quando sarò forte come te, ho\nintenzione di ritentare il viaggio.\n\nSpero di incontrarti di nuovo, in futuro.\nPerché allora sarò io a proteggere te!"
  FR03_Talk_01:
    attributes: NPC_oasis004_fr03
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 42
      - text: "Morirò?\nLa mia vita finirà così?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Oh, se solo fossi forte come Gomana...\nMi sbarazzerei di questi tizi in men\nche non si dica..."
  FR03_Talk_02:
    attributes: NPC_oasis004_fr03
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 21
      - text: "Oh, ma... "
      - control:
          kind: pause
          length: short
      - text: "sono...\n\n\n"
      - control:
          kind: sound
          unknown:
            - 7
            - 10
      - text: Sono
      - control:
          kind: pause
          length: longer
      - text: " vivo?"
      - control:
          kind: pause
          length: short
      - text: "\nSono davvero ancora vivo?\nHai sconfitto tu i mostri?"
  FR03_Talk_03:
    attributes: NPC_oasis004_fr03
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: Ho parlato con Gomana...
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Quando gli ho chiesto perché non era\nvenuto a salvarmi, ha farfugliato di una\n"
      - control:
          kind: set_colour
          colour: red
      - text: "spada leggendaria "
      - control:
          kind: reset_colour
      - text: "e altre sciocchezze...\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Non sarà mica che...
      - control:
          kind: pause
          length: longer
      - text: " in realtà\nGomana non è per niente forte?"
  FR03_Talk_04:
    attributes: NPC_oasis004_fr03
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: "È incredibile!\nSei FANTASTICO!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "La tua forza è seconda solo\na quella di Gomana!"
      - control:
          kind: single_choice
          label: 3
  FR03_Talk_05:
    attributes: NPC_oasis004_fr03
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 20
      - text: "Eh? Gomana si trova nello stallaggio?\nNon è venuto a salvarmi perché non\naveva un'arma?\n"
      - control:
          kind: sound
          unknown:
            - 11
            - 6
      - text: "Ma che stai dicendo? Lui è un maestro\ndi arti marziali e le sue tecniche sono\nimpareggiabili."
      - control:
          kind: pause
          length: longer
      - text: "\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Che... mi abbia mentito?
      - control:
          kind: pause
          length: longer
      - text: "\nD-Devo parlare subito con lui!"
  FR03_Talk_06:
    attributes: NPC_oasis004_fr03
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: Ho parlato con Gomana...
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Quando gli ho chiesto perché non era\nvenuto a salvarmi, ha farfugliato di una\n"
      - control:
          kind: set_colour
          colour: red
      - text: "spada leggendaria "
      - control:
          kind: reset_colour
      - text: "e altre sciocchezze...\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Non sarà mica che...
      - control:
          kind: pause
          length: longer
      - text: " in realtà\nGomana non è per niente forte?\n\n"
      - control:
          kind: sound
          unknown:
            - 9
            - 18
      - text: "Ma ho anche capito che il più debole\ndella compagnia era quello che contava\nsu Gomana per qualsiasi cosa: io...\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ecco perché ho deciso di andarmene\nper conto mio. Perché voglio diventare\nqualcuno su cui contare."
      - control:
          kind: pause
          length: longer
      - text: "\n"
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: Come te.
  FR04_Talk_01:
    attributes: NPC_oasis004_fr04
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 32
      - text: "È... "
      - control:
          kind: pause
          length: short
      - text: "È pericoloso qui...\nT-"
      - control:
          kind: pause
          length: short
      - text: Torna indietro...
  FR04_Talk_02:
    attributes: NPC_oasis004_fr04
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 11
      - text: "C-Che portamento!\nQual è il tuo stile con la spada?"
      - control:
          kind: pause
          length: longer
      - text: "\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 24
      - text: "Oh, ti chiedo scusa...\nMi sono fatto prendere dall'entusiasmo.\nIl mio nome è Veny.\nIn battaglia non me la cavo male, ma\nstavolta io e i miei amici ci siamo fatti\nsorprendere...\nE come se non bastasse, eravamo in\nnetta inferiorità numerica, quindi ci ho\npensato due volte prima di combattere.\n"
      - control:
          kind: sound
          unknown:
            - 3
            - 13
      - text: "Non voglio pensare a cosa sarebbe\nsuccesso se non fossi arrivato tu...\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Ti sono infinitamente grato!
      - control:
          kind: single_choice
          label: 3
  FR04_Talk_03:
    attributes: NPC_oasis004_fr04
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 13
      - text: "Non so se sia per la felicità, ma\nquando ho rivisto il volto di Gomana,\nhanno iniziato a tremarmi le gambe.\nPare che abbia ancora molta strada\nda fare, con il mio allenamento...\n\n"
      - control:
          kind: sound
          unknown:
            - 5
            - 4
      - text: "A proposito di bravura con la spada...\n\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Mi chiedo se la tua abilità non sia\nall'altezza del prode cavaliere che salvò\nquesta terra dalla grande calamità!"
      - control:
          kind: pause
          length: longer
      - text: "\nAh ah ah, in effetti non ha molto senso\nparlare di leggende di quasi cent'anni fa.\n\nPer ora resterò qui a riposarmi un po',\nse per te va bene."
  FR04_Talk_04:
    attributes: NPC_oasis004_fr04
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 23
      - text: È stato Gomana a mandarti?
      - control:
          kind: pause
          length: longer
      - text: "\n\n\n"
      - control:
          kind: sound
          unknown:
            - 1
            - 24
      - text: "Mi fa piacere saperlo.\nVuol dire che sta bene."
      - control:
          kind: pause
          length: longer
      - text: "\n\nAllora corro subito allo stallaggio\ndel canyon Gerudo dove aspetta."
  FR04_Talk_05:
    attributes: NPC_oasis004_fr04
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 24
      - text: "La tua abilità con la spada... Mi chiedo se\nsia all'altezza del prode cavaliere che\nsalvò questa terra dalla grande calamità!"
      - control:
          kind: pause
          length: longer
      - text: "\nAh ah ah, in effetti non ha molto senso\nparlare di leggende di quasi cent'anni fa.\n\nPer ora resterò qui a riposarmi un po',\nse per te va bene."
  Npc_oasis004_100:
    attributes: NPC_oasis004
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: Ehilà...
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Hai visto i miei amici, per caso?\nSono quattro viaggiatori di nome\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Veny
      - control:
          kind: reset_colour
      - text: ", "
      - control:
          kind: set_colour
          colour: blue
      - text: Amani
      - control:
          kind: reset_colour
      - text: ", "
      - control:
          kind: set_colour
          colour: blue
      - text: "Natane "
      - control:
          kind: reset_colour
      - text: "e "
      - control:
          kind: set_colour
          colour: blue
      - text: Pahm
      - control:
          kind: reset_colour
      - text: "..."
  Npc_oasis004_101:
    attributes: NPC_oasis004
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Non conosci la storia della "
      - control:
          kind: set_colour
          colour: red
      - text: "spada\nleggendaria "
      - control:
          kind: reset_colour
      - text: "che attende il suo padrone\nnella foresta di Hyrule?\nDeve trattarsi di una spada potentissima.\nD'altronde, ci sarà pure un motivo se la\nchiamano \"leggendaria\"...\n"
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Se fosse mia, nessun mostro potrebbe\nfarmi paura."
      - control:
          kind: pause
          length: longer
      - text: " Ma non è possibile che una\nsimile arma esista veramente..."
      - control:
          kind: pause
          length: longer
      - text: "\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Quindi...
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Se dovessi imbatterti nei miei amici,\npotresti avvertirli che Gomana li sta\naspettando qui?\nI loro nomi sono\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Veny
      - control:
          kind: reset_colour
      - text: ", "
      - control:
          kind: set_colour
          colour: blue
      - text: Amani
      - control:
          kind: reset_colour
      - text: ", "
      - control:
          kind: set_colour
          colour: blue
      - text: Natane
      - control:
          kind: reset_colour
      - text: " e "
      - control:
          kind: set_colour
          colour: blue
      - text: Pahm
      - control:
          kind: reset_colour
      - text: ".\nGrazie dell'aiuto."
  Npc_oasis004_102:
    attributes: NPC_oasis004
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "Io, ecco... non lo so..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 6
            - 1
      - text: "Ero in viaggio con loro, quando dei\nmostri sono sbucati fuori dal nulla\nlungo il "
      - control:
          kind: set_colour
          colour: blue
      - text: "sentiero dell'altopiano Kotume"
      - control:
          kind: reset_colour
      - text: ".\nIo ho pensato solo a scappare, finché\nnon sono arrivato qui..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 9
            - 16
      - text: "Tu non mi crederai, ma..."
      - control:
          kind: pause
          length: longer
      - text: "\nA un certo punto mi sono girato e..."
      - control:
          kind: pause
          length: longer
      - text: "\nI miei compagni, loro..."
      - control:
          kind: pause
          length: longer
      - text: "\nErano "
      - control:
          kind: pause
          length: longer
      - text: scomparsi...
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 1
      - text: Credi forse che quei mostri li abbiano...
      - control:
          kind: pause
          length: longer
      - text: "\n\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ah, ma cosa vado a pensare! Stanno\ntutti bene, ne sono certo.\n\nComunque, è per questo che mi trovo\nqui... Sto aspettando il loro ritorno."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Se dovessi incontrarli da queste parti,\npotresti avvertirli che Gomana è qui e\nche li sta cercando?"
  Npc_oasis004_103:
    attributes: NPC_oasis004
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 16
      - text: "I nomi dei miei amici sono\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Veny
      - control:
          kind: reset_colour
      - text: ", "
      - control:
          kind: set_colour
          colour: blue
      - text: Amani
      - control:
          kind: reset_colour
      - text: ", "
      - control:
          kind: set_colour
          colour: blue
      - text: Natane
      - control:
          kind: reset_colour
      - text: " e "
      - control:
          kind: set_colour
          colour: blue
      - text: Pahm
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Se dovessi incontrarli, potresti avvisarli\nche li sto aspettando qui?"
  Npc_oasis004_104:
    attributes: NPC_oasis004
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 16
      - text: "Forse sono stati davvero attaccati dai\nmostri dell'"
      - control:
          kind: set_colour
          colour: blue
      - text: altopiano Kotume
      - control:
          kind: reset_colour
      - text: "...\n\nSe potessi, andrei subito a salvarli.\n\n\nPurtroppo non possiedo nemmeno\nun'arma."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ah, se solo avessi la "
      - control:
          kind: set_colour
          colour: red
      - text: spada leggendaria
      - control:
          kind: reset_colour
      - text: "...\nA quel punto, potrei far fuori i mostri in\nun batter d'occhio, credimi!"
      - control:
          kind: choice
          choice_labels:
            - 1
            - 2
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Npc_oasis004_105:
    attributes: NPC_oasis004
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 16
      - text: Aaah...
  Npc_oasis004_106:
    attributes: NPC_oasis004
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 2
      - text: "Ehi, non pensare che non l'abbia\nsaputo."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Sei tu quello che ha salvato uno dei miei\ncompagni dai mostri. Troppo forte,\namico!\nMi hanno detto che anche se eri\nin svantaggio, ti sei sbarazzato dei\nnemici in un attimo. \n"
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Ah, se solo avessi la "
      - control:
          kind: set_colour
          colour: red
      - text: spada leggendaria
      - control:
          kind: reset_colour
      - text: "!\nAvrei combattuto al tuo fianco senza\npensarci un attimo!"
      - control:
          kind: choice
          choice_labels:
            - 1
            - 2
          selected_index: 0
          cancel_index: 1
          unknown: 6
