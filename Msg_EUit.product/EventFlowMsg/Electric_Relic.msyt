            - 102
            - 103
            - 106
          selected_index: 0
          cancel_index: 2
          unknown: 8
  Npc_oasis003_Strategy_024:
    attributes: Npc_oasis003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ecco qualche "
      - control:
          kind: set_colour
          colour: blue
      - control:
          kind: raw
          two_hundred_one:
            dynamic:
              - 0
              - len: 4
                field_2:
                  - 2
                  - 2
                  - 2
                  - 0
      - text: freccia bomba
      - control:
          kind: reset_colour
      - text: ".\nLe scorte non sono infinite, quindi\ncerca di prendere bene la mira."
  Npc_oasis003_Strategy_025:
    attributes: Npc_oasis003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 14
      - text: "... Aspetta."
      - control:
          kind: pause
          length: longer
      - text: "\n\n\nNon hai abbastanza "
      - control:
          kind: set_colour
          colour: blue
      - text: frecce bomba
      - control:
          kind: reset_colour
      - text: ".\nNon vorrai mica affrontare lo scontro\ncosì!"
      - control:
          kind: pause
          length: long
      - text: "\nIl problema è che anche io le ho finite..."
      - control:
          kind: pause
          length: longer
      - text: "\n\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Senti, vai a comprare qualche\n"
      - control:
          kind: set_colour
          colour: blue
      - control:
          kind: raw
          two_hundred_one:
            dynamic:
              - 0
              - len: 4
                field_2:
                  - 2
                  - 2
                  - 2
                  - 0
      - text: "freccia bomba "
      - control:
          kind: reset_colour
      - text: "alla cittadella.\n\nSecondo me è meglio se ti presenti\nben equipaggiato per uno scontro\ndel genere."
  Npc_oasis003_Strategy_026:
    attributes: Npc_oasis003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 14
      - text: "... Aspetta."
      - control:
          kind: pause
          length: longer
      - text: "\n\n\nNon hai abbastanza "
      - control:
          kind: set_colour
          colour: blue
      - text: frecce bomba
      - control:
          kind: reset_colour
      - text: ".\nNon vorrai mica affrontare lo scontro\ncosì!"
      - control:
          kind: pause
          length: long
      - text: "\nEcco. Avevo preparato delle "
      - control:
          kind: set_colour
          colour: blue
      - text: "frecce\nbomba"
      - control:
          kind: reset_colour
      - text: " di scorta."
      - control:
          kind: pause
          length: longer
      - text: " Prendile tu."
  Npc_oasis003_Strategy_027:
    attributes: Npc_oasis003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 14
      - text: "... Aspetta."
      - control:
          kind: pause
          length: longer
      - text: "\n\n\nNon hai abbastanza "
      - control:
          kind: set_colour
          colour: blue
      - text: frecce bomba
      - control:
          kind: reset_colour
      - text: ".\nNon vorrai mica affrontare lo scontro\ncosì!"
      - control:
          kind: pause
          length: long
      - text: "\nTieni, queste sono le ultime scorte..."
      - control:
          kind: pause
          length: longer
      - text: "\nLe affido a te."
  Npc_oasis004_GetBack_001:
    attributes: Npc_GerudoDesert004
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 19
      - text: "Ehi, tu! Che stai facendo qui?\nQuesto è il covo dei banditi che hanno\nrubato l'"
      - control:
          kind: set_colour
          colour: red
      - control:
          kind: raw
          two_hundred_one:
            dynamic:
              - 0
              - len: 4
                field_2:
                  - 1
                  - 3
                  - 0
                  - 0
      - text: elmo del tuono
      - control:
          kind: reset_colour
      - text: "!\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Se ti scoprono, "
      - control:
          kind: set_colour
          colour: red
      - text: chiameranno rinforzi
      - control:
          kind: reset_colour
      - text: "\ne ti ridurranno a granelli di sabbia.\n\n"
      - control:
          kind: sound
          unknown:
            - 11
            - 12
      - text: "Ah, se solo ci fosse un modo per\ndistrarli!\n\nNon fanno altro che pattugliare la zona\ne mangiare "
      - control:
          kind: set_colour
          colour: red
      - text: banane
      - control:
          kind: reset_colour
      - text: ". Sembrano non avere\nalcun punto debole da sfruttare...\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Comunque, è troppo pericoloso restare\nqui. Vattene via finché sei in tempo,\ncerco di risolvere la questione da sola!"
  Npc_oasis004_GetBack_004:
    attributes: Npc_GerudoDesert004
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 19
      - text: "Ehi, tu! Ma sei ancora qui?\n\n\nSe ti scoprono, "
      - control:
          kind: set_colour
          colour: red
      - text: chiameranno rinforzi
      - control:
          kind: reset_colour
      - text: "\ne ti ridurranno a granelli di sabbia.\n\n"
      - control:
          kind: sound
          unknown:
            - 11
            - 12
      - text: "Non fanno altro che girare di pattuglia e\nmangiare "
      - control:
          kind: set_colour
          colour: red
      - text: banane
      - control:
          kind: reset_colour
      - text: ". Sembrano non avere\nalcun punto debole da sfruttare...\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Comunque, è troppo pericoloso restare\nqui. Vattene via finché sei in tempo,\ncerco di risolvere la questione da sola!"
  Npc_oasis004_GetBack_Near_001:
    attributes: Npc_GerudoDesert004
    contents:
      - text: Devo fuggire...
  Npc_oasis007_Defeat_001:
    attributes: Npc_oasis007
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Ero così preoccupata che potesse\nsuccedere qualcosa alla "
      - control:
          kind: set_colour
          colour: blue
      - text: matriarca
      - control:
          kind: reset_colour
      - text: "\nche non riuscivo a star ferma.\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ma a quanto pare non avevo motivo\ndi preoccuparmi.\n\nDopo quello che è successo, sembra\nproprio che la "
      - control:
          kind: set_colour
          colour: blue
      - text: "matriarca "
      - control:
          kind: reset_colour
      - text: "sia maturata\nparecchio come regnante."
  Npc_oasis007_Finish_001:
    attributes: Npc_oasis007
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 35
      - text: "Ci sei riuscito alla fine!\n\n\nDa adesso potrai chiedere il nostro\naiuto in ogni momento."
  Npc_oasis007_GetBack_001:
    attributes: Npc_oasis007
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: "Che ti prende?\n\n\nSe sei un vero campione come dici,\nrecuperare il tesoro sacro del nostro\npopolo è un giochetto per te."
  Npc_oasis007_GetBack_002:
    attributes: Npc_oasis007
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Se ti servono informazioni sui banditi,\nparla con la "
      - control:
          kind: set_colour
          colour: blue
      - text: caposquadra Cik
      - control:
          kind: reset_colour
      - text: ".\n\nScendi per le scale e continua dritto."
  Npc_oasis007_GetBack_003:
    attributes: Npc_oasis007
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 9
      - text: "Cogli l'occasione per farci vedere\ndi cosa sei capace!"
  Npc_oasis007_GetBack_101:
    attributes: Npc_oasis007
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: "Se stai cercando la "
      - control:
          kind: set_colour
          colour: blue
      - text: matriarca Riju
      - control:
          kind: reset_colour
      - text: ",\nla trovi al "
      - control:
          kind: set_colour
          colour: red
      - text: secondo piano
      - control:
          kind: reset_colour
      - text: ".\n\nAspetta! Quindi ora sei stato autorizzato\nad andare al piano di sopra?\n\nAh, ma cosa passa per la testa alla\n"
      - control:
          kind: set_colour
          colour: blue
      - text: matriarca
      - control:
          kind: reset_colour
      - text: "...?\n\n"
      - control:
          kind: sound
          unknown:
            - 8
            - 0
      - text: "Ascoltami bene!\n\n\nSe manchi di rispetto alla "
      - control:
          kind: set_colour
          colour: blue
      - text: matriarca
      - control:
          kind: reset_colour
      - text: ",\nfarai una brutta fine!"
  Npc_oasis007_Strategy_001:
    attributes: Npc_oasis007
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "La "
      - control:
          kind: set_colour
          colour: blue
      - text: matriarca Riju
      - control:
          kind: reset_colour
      - text: " ti ha capito subito...\n\n\n"
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Anche io pensavo di saper guardare\nnel cuore della gente, ma evidentemente\nho ancora molto da imparare."
      - control:
          kind: pause
          length: longer
      - text: "\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Ora lascia a me le difese della cittadella.
      - control:
          kind: pause
          length: short
      - text: "\nPresto, dirigiti alla "
      - control:
          kind: set_colour
          colour: red
      - text: base di osservazione
      - control:
          kind: reset_colour
      - text: ".\n\n"
      - control:
          kind: sound
          unknown:
            - 8
            - 1
      - text: "Va' ad aiutare la "
      - control:
          kind: set_colour
          colour: blue
      - text: matriarca Riju
      - control:
          kind: reset_colour
      - text: "!"
  Npc_oasis007_Strategy_003:
    attributes: Npc_oasis007
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Perdonami se sono stata rude, ma non\npensavo che tu ce l'avresti fatta a\nrecuperare l'elmo.\n"
      - control:
          kind: sound
          unknown:
            - 11
            - 18
      - text: "Per proteggere il mio popolo e la\nmatriarca, mi sono addestrata anni e\nanni con la lancia.\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Eppure, tutto il mio addestramento\nsarebbe inutile contro il "
      - control:
          kind: set_colour
          colour: blue
      - text: colosso sacro
      - control:
          kind: reset_colour
      - text: ".\n\nFacciamo tutte affidamento su di te.\n\n\nPresto, vai alla "
      - control:
          kind: set_colour
          colour: red
      - text: base di osservazione
      - control:
          kind: reset_colour
      - text: "!\n\n\n"
      - control:
          kind: sound
          unknown:
            - 8
            - 0
      - text: "Aiuta la "
      - control:
          kind: set_colour
          colour: blue
      - text: matriarca Riju
      - control:
          kind: reset_colour
      - text: "."
  Npc_oasis024_Ready_001:
    attributes: Npc_oasis024
    contents:
      - control:
          kind: sound2
          unknown:
            - 5
            - 205
      - text: "È un voy! C'è un voy dentro le mura!\nCatturatelo!"
  Npc_oasis024_Ready_005:
    attributes: Npc_oasis024
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 0
      - text: "Per i voy l'accesso alla cittadella\nè proibito! Questa è la legge\ndelle Gerudo.\nNon farti vedere mai più da queste parti,\nci siamo intesi?"
  Npc_oasis024_Ready_006:
    attributes: Npc_oasis024
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 0
      - text: "Ancora tu!\n\n\nMi sembra di averti già detto che i voy\nnon sono ammessi all'interno della cinta\nmuraria.\nTe lo ripeto ancora una volta: non farti\npiù vedere qui! Capito?"
  Npc_oasis024_Ready_007:
    attributes: Npc_oasis024
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 0
      - text: "Ah, sei più fastidioso della sabbia nelle\nscarpe! Quante volte devo ripetertelo?\n\nI voy non sono ammessi all'interno della\ncinta muraria della cittadella!\n\nÈ una legge e va rispettata!\nGira al largo e non tornare mai più\nper il resto dei tuoi giorni!"
  Npc_oasis042_GetBack_001:
    attributes: Npc_oasis042
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "La caposquadra ha dato istruzioni di\ncollaborare con te su questa faccenda,\nma non abbiamo molte informazioni..."
      - control:
          kind: choice
          choice_labels:
            - 30
            - 31
            - 32
          selected_index: 0
          cancel_index: 2
          unknown: 8
  Npc_oasis042_GetBack_004:
    attributes: Npc_oasis042
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "I banditi si sono intrufolati nel palazzo\ncol favore della notte, hanno rubato il\n"
      - control:
          kind: set_colour
          colour: blue
      - text: tesoro sacro
      - control:
          kind: reset_colour
      - text: " e sono svaniti nel nulla.\n"
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Gli unici ad essere così abili nelle\ntecniche dell'agire invisibili sono\ngli Sheikah.\nMa perché mai avrebbero voluto\nrubare l'"
      - control:
          kind: set_colour
          colour: blue
      - control:
          kind: raw
          two_hundred_one:
            dynamic:
              - 0
              - len: 4
                field_2:
                  - 1
                  - 3
                  - 0
                  - 0
      - text: elmo del tuono
      - control:
          kind: reset_colour
      - text: "?"
  Npc_oasis042_GetBack_005:
    attributes: Npc_oasis042
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Sappiamo che il covo dei banditi si trova\nnella "
      - control:
          kind: set_colour
          colour: red
      - text: valle Calcita
      - control:
          kind: reset_colour
      - text: ".\n\nMa la strada che attraversa quella valle\nè molto pericolosa...\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Abbiamo provato più volte un assalto,\nusando le "
      - control:
          kind: set_colour
          colour: blue
      - text: foche delle sabbie
      - control:
          kind: reset_colour
      - text: ", ma...\n\n... ancora non siamo riuscite ad arrivare\nal loro covo.\n\n"
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "Il pensiero che nel deserto viva un altro\npopolo capace di tenerci testa ci\nriempie di vergogna."
      - control:
          kind: choice
          choice_labels:
            - 30
            - 32
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Npc_oasis042_GetBack_006:
    attributes: Npc_oasis042
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Le nostre indagini sono ancora in corso,\ngiorno e notte. Per adesso non abbiamo\naltre informazioni."
      - control:
          kind: choice
          choice_labels:
            - 30
            - 31
            - 32
          selected_index: 0
          cancel_index: 2
          unknown: 8
  Npc_oasis042_GetBack_007:
    attributes: Npc_oasis042
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Se ti serve aiuto per localizzare il covo\ndei banditi, chiedi alla "
      - control:
          kind: set_colour
          colour: blue
      - text: "guardia della\nporta nord-ovest"
      - control:
          kind: reset_colour
      - text: ".\nLa "
      - control:
          kind: set_colour
          colour: blue
      - text: porta nord-ovest
      - control:
          kind: reset_colour
      - text: " è alla tua sinistra\nappena esci dalla caserma.\n\nScusa ma non posso scortarti, ho altre\nfaccende a cui badare qui..."
      - control:
          kind: pause
          length: longer
      - text: "\n\n"
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "A dire il vero... "
      - control:
          kind: pause
          length: short
      - text: "ci sono giorni in cui\nnon so nemmeno da dove iniziare..."
  Npc_oasis042_GetBack_008:
    attributes: Npc_oasis042
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "I banditi fanno parte di una banda\nchiamata Yiga. Sono maestri nel\ntravestirsi e nell'agire furtivamente.\nHanno approfittato del buio per entrare\nnel palazzo, rubare il tesoro sacro\ne scomparire come fantasmi...\n"
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Sono discendenti di alcuni Sheikah che\nin passato si diedero al crimine e si\nrivoltarono contro il re.\nMa perché mai avrebbero voluto\nrubare l'"
      - control:
          kind: set_colour
          colour: blue
      - control:
          kind: raw
          two_hundred_one:
            dynamic:
              - 0
              - len: 4
                field_2:
                  - 1
                  - 3
                  - 0
                  - 0
      - text: elmo del tuono
      - control:
          kind: reset_colour
      - text: "?"
      - control:
          kind: choice
          choice_labels:
            - 31
            - 32
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Npc_oasis042_GetBack_009:
    attributes: Npc_oasis042
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Sappiamo che il covo dei banditi si trova\nnella "
      - control:
          kind: set_colour
          colour: red
      - text: valle Calcita
      - control:
          kind: reset_colour
      - text: ".\n\nMa la strada che attraversa quella valle\nè molto pericolosa...\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Abbiamo provato più volte un assalto,\nusando le "
      - control:
          kind: set_colour
          colour: blue
      - text: foche delle sabbie
      - control:
          kind: reset_colour
      - text: ", ma...\n\n... ancora non siamo riuscite ad arrivare\nal loro covo.\n\n"
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "Il pensiero che nel deserto viva un altro\npopolo capace di tenerci testa ci\nriempie di vergogna."
  Npc_oasis042_GetBack_201:
    attributes: Npc_oasis042
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 19
      - text: "È da un po' che non vedo Barta.\nChissà dove si sarà cacciata."
  Npc_oasis042_GetBack_202:
    attributes: Npc_oasis040
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Barta è partita dicendo che sarebbe\nandata a spiare il covo dei banditi...\n\n"
      - control:
          kind: sound
          unknown:
            - 9
            - 16
      - text: "Ma ora sono due giorni che non si vede\ne mi chiedo che ne sia stato di lei!"
  Npc_oasis042_GetBack_203:
    attributes: Npc_oasis042
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 20
      - control:
          kind: text_size
          percent: 125
      - text: Tu... sei...
      - control:
          kind: pause
          length: short
      - text: " un'idiota!"
      - control:
          kind: text_size
          percent: 100
      - text: "\n\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Perché l'hai lasciata andare da sola?\nE perché hai aspettato due giorni interi\nprima di riportare l'accaduto?"
  Npc_oasis042_GetBack_204:
    attributes: Npc_oasis040
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 4
      - text: "Mi... mi spiace... Ha detto che voleva\nsolo dare un'occhiata e così ho pensato\nche non ci fossero problemi..."
  Npc_oasis042_GetBack_205:
    attributes: Npc_oasis042
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 16
      - text: "Grrr... adesso oltre al "
      - control:
          kind: set_colour
          colour: blue
      - text: tesoro sacro
      - control:
          kind: reset_colour
      - text: "\ndobbiamo pure metterci a cercare "
      - control:
          kind: set_colour
          colour: blue
      - text: Barta
      - control:
          kind: reset_colour
      - text: "!\n\n"
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "Se lo viene a sapere la caposquadra,\nper punizione ci toccherà fare cento\ngiri del deserto Gerudo di corsa!"
  Npc_oasis042_GetBack_206:
    attributes: Npc_oasis041
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: Accipicchia! E adesso che facciamo?
  Npc_oasis042_GetBack_207:
    attributes: Npc_oasis042
    contents:
      - control:
          kind: text_size
          percent: 125
      - control:
          kind: sound
          unknown:
            - 8
            - 0
      - text: Ci sto pensando!
      - control:
          kind: text_size
          percent: 100
  talk00:
    attributes: Npc_oasis007
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 0
      - text: "Ti trovi al cospetto di "
      - control:
          kind: set_colour
          colour: blue
      - text: Riju
      - control:
          kind: reset_colour
      - text: ", matriarca\ndelle Gerudo! Se hai qualcosa da dire,\nparla, ma mantieni la dovuta distanza!"
  talk01:
    attributes: Npc_oasis007
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 0
      - text: "Vuoi salire a bordo e prendere\nil controllo di Naboris?!"
      - control:
          kind: pause
          length: longer
      - text: "\n\nLe uniche persone capaci di un'impresa\ndel genere furono i campioni come\nUrbosa.\nE tutti loro persero la vita cento anni\nfa lottando contro la "
      - control:
          kind: set_colour
          colour: red
      - text: grande calamità
      - control:
          kind: reset_colour
      - text: ".\n\nChe sciocchezze mi tocca sentire..."
  talk02:
    attributes: Npc_oasis007
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 14
      - text: Non dirmi che tu... sei un voy!
  talk04:
    attributes: Npc_oasis007
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Però... ancora non possiamo fidarci.\n\n\nCome facciamo ad affidare una missione\ntanto importante a una persona di cui\nnon sappiamo niente?"
      - control:
          kind: pause
          length: longer
  talk05:
    attributes: Npc_oasis007
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Le nostre milizie conoscono bene\ni banditi che ci hanno sottratto\nil tesoro sacro."
  talk06:
    attributes: Npc_oasis007
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: M-Ma...
      - control:
          kind: pause
          length: longer
      - text: "\n\n\nIl campione Hylia dovrebbe essere\nin possesso della "
      - control:
          kind: set_colour
          colour: red
      - text: spada leggendaria
      - control:
          kind: reset_colour
      - text: "!\n\nIo qui non vedo spade leggendarie."
  talk07:
    attributes: Npc_oasis007
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Dirigiti in caserma.
      - control:
          kind: pause
          length: longer
      - text: " Parla con la\n"
      - control:
          kind: set_colour
          colour: blue
      - text: caposquadra Cik
      - control:
          kind: reset_colour
      - text: " e chiedile tutte\nle informazioni che può darti."
  talk08:
    attributes: Npc_oasis007
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "Eppure... non ricordo nessuna \nvay Hylia tra i campioni..."
      - control:
          kind: pause
          length: longer
  talk13:
    attributes: Npc_oasis007
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Certo. Per un vero campione dovrebbe\nessere facile come costruire un castello\ndi sabbia."
  talk15:
    attributes: Npc_oasis007
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Per prima cosa... perché non la\nmandiamo a recuperare l'elmo?"
