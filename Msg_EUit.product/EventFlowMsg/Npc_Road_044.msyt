  RetMSG_Ichika_00:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Sono Matts, il cuoco ambulante!"
      - control:
          kind: pause
          length: short
      - text: "\n\n\nIn questo mondo,"
      - control:
          kind: pause
          length: short
