  Rest_01:
    attributes: Npc_Road_044
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "La pioggia aiuta la vegetazione.\nLa vegetazione aiuta gli animali."
      - control:
          kind: pause
          length: long
      - text: "\n\n"
      - control:
          kind: sound
          unknown:
            - 1
            - 10
      - text: "I miei piatti aiutano le persone.\nLe persone e i loro soldi aiutano me!\nPerfetto, non credi? Ah ah ah!"
  Rest_02:
    attributes: Npc_Road_044
    contents:
      - control:
          kind: sound
