  MSG_Futa_00:
    attributes: Npc_Road_018
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "Mmm... "
      - control:
          kind: pause
          length: short
      - text: "Non so quanto possa\ninteressarti, ma..."
      - control:
          kind: pause
          length: short
      - text: "\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "... volevo parlarti di pietre preziose."
      - control:
          kind: pause
          length: short
      - text: "\nNon ti interessano?\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 6
      - text: E se ti dicessi
      - control:
          kind: pause
          length: short
      - text: " che le gemme possono\nessere vendute a prezzi esorbitanti?"
  MSG_Futa_00_1:
    attributes: Npc_Road_018
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Anche il meno interessato alle pietre\npreziose lo diventa subito"
      - control:
          kind: pause
          length: longer
      - text: " quando scopre\na che prezzi possono essere vendute..."
  MSG_Futa_01:
    attributes: Npc_Road_018
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: "Senti,"
      - control:
          kind: pause
          length: short
      - text: " sei mai stato sui\nMonti Gemelli?\n\nPuoi trovarci un sacco di pietre preziose!"
      - control:
          kind: pause
          length: short
      - text: "\nVorrei imbracciare un martello e cercarne\nun po' anch'io!\n"
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: E poi vorrei...
      - control:
          kind: pause
          length: short
      - text: " rivenderle a prezzi\nesagerati! Non importa a chi!\n\n"
      - control:
          kind: sound
          unknown:
            - 5
            - 1
      - text: "Ti chiedo scusa...\n\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Quando penso alle pietre preziose,\nvado subito su di giri!"
  MSG_Futa_01_1:
    attributes: Npc_Road_018
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Scavando sui Monti Gemelli si possono\ntrovare un sacco di pietre preziose!"
      - control:
          kind: pause
          length: short
      - text: "\nDovrei prendere uno scalpello e provare!"
  MSG_Futa_02:
    attributes: Npc_Road_018
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: Per caso vuoi raggiungere il
      - control:
          kind: pause
          length: short
      - text: "\n"
      - control:
          kind: set_colour
          colour: blue
      - text: villaggio Calbarico
      - control:
          kind: reset_colour
      - text: "?\n\nTi basta seguire la strada che va\nverso nord.\n\n"
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: "Ci vado spesso anch'io per vendere\npietre preziose."
  MSG_Futa_02_1:
    attributes: Npc_Road_018
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Per arrivare al "
      - control:
          kind: set_colour
          colour: blue
      - text: "villaggio Calbarico "
      - control:
          kind: reset_colour
      - text: "basta\nseguire la strada che va verso nord."
  MSG_Futa_03:
    attributes: Npc_Road_018
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "..."
      - control:
          kind: pause
          length: long
      - text: "\n\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Oh, ti chiedo scusa!"
      - control:
          kind: pause
          length: short
      - text: " Stavo sognando\nad occhi aperti, per cui avevo la testa\nper aria...\nStavo sognando "
      - control:
          kind: pause
          length: short
      - control:
          kind: set_colour
          colour: blue
      - text: rubini
      - control:
          kind: reset_colour
      - text: " e "
      - control:
          kind: set_colour
          colour: blue
      - text: zaffiri
      - control:
          kind: reset_colour
      - text: "...\n\n\nSono due tipi di pietre preziose che\nvengono vendute a prezzi davvero\nelevati!"
      - control:
          kind: pause
          length: short
      - text: " Le ideali per fare soldi facili!!!"
      - control:
          kind: pause
          length: long
      - text: "\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 19
      - text: "Scusa...\n\n\nQuando penso alle pietre preziose,\nperdo completamente la testa..."
  MSG_Futa_03_1:
    attributes: Npc_Road_018
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "È facile guadagnare un sacco vendendo\ngemme!"
