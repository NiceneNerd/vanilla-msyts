          length: longer
      - text: " Specialmente "
      - control:
          kind: set_colour
          colour: blue
      - text: rubini
      - control:
          kind: reset_colour
      - text: " e "
      - control:
          kind: set_colour
          colour: blue
      - text: zaffiri
      - control:
          kind: reset_colour
      - text: "...\nMeglio venderli che tenerseli!"
  MSG_Futa_04:
    attributes: Npc_Road_018
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: "Per determinare se una pietra è preziosa,\ndi solito si valuta la sua bellezza e la sua\nqualità naturale...\nMa non si pensa mai al brivido che dà"
      - control:
          kind: pause
          length: short
      - text: "\nquando la si rivende a carissimo prezzo!\n\n"
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: "E se dipendesse da me, metterei questa\ncaratteristica in cima alla classifica!"
  MSG_Futa_04_1:
    attributes: Npc_Road_018
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "La bellezza e la purezza di una gemma\nsono importanti..."
      - control:
          kind: pause
          length: longer
      - text: " ma ancora di più lo\nè il prezzo a cui la si vende!"
