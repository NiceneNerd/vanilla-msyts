    attributes: Npc_SouthernVillage006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Allora ci pensi tu, eh?\nStanno al "
      - control:
          kind: set_colour
          colour: blue
      - text: lido Bagnacciuga
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: pause
          length: short
