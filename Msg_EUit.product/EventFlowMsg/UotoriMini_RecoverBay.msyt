      - control:
          kind: set_colour
          colour: blue
      - text: lido Bagnacciuga
      - control:
          kind: reset_colour
      - text: ", prima ci\nandavamo perché si trovavano un sacco\ndi pesci, ma ora..."
  talk01:
    attributes: Npc_SouthernVillage006
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
