      - text: pozione contro il calore
      - control:
          kind: reset_colour
      - text: "\ncomposta da "
      - control:
          kind: set_colour
          colour: blue
      - text: "un insetto con effetto\nrinfrescante "
      - control:
          kind: reset_colour
      - text: "e "
      - control:
          kind: set_colour
          colour: blue
      - text: ingredienti mostruosi
      - control:
          kind: reset_colour
      - text: "...\n"
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "Ma non la vendono da nessuna parte...\n\n\nL'"
      - control:
          kind: set_colour
          colour: blue
      - text: insetto con effetto rinfrescante
      - control:
          kind: reset_colour
      - text: " vive\nsoltanto nelle aree fredde del "
      - control:
          kind: set_colour
          colour: red
      - text: "grande\naltopiano Gerudo"
      - control:
          kind: reset_colour
      - text: "...\nMa lì è pieno di mostri...\n\n\n"
      - control:
          kind: sound
          unknown:
            - 11
            - 12
      - text: Quindi preferisco...
      - control:
          kind: pause
          length: short
      - text: " starmene qui\na pensare sul da farsi..."
