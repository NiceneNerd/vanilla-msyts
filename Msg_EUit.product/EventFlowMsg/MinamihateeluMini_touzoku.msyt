          cancel_index: 1
          unknown: 6
  Talk18:
    attributes: subchallnpc000
    contents:
      - control:
          kind: sound
          unknown:
            - 2
            - 1
      - text: "Come ti permetti? Non si origliano\nle conversazioni degli altri!"
  Talk19:
    attributes: subchallnpc000_twin
    contents:
      - control:
          kind: sound
          unknown:
            - 2
            - 0
      - text: "Noi siamo cacciatori di tesori!\nQuindi il "
      - control:
          kind: set_colour
          colour: red
      - text: "tesoro del grande bandito\nLambda "
      - control:
          kind: reset_colour
      - text: appartiene a noi!
  Talk20:
    attributes: subchallnpc000_twin
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: "Il fiuto sopraffino del più grande\ncacciatore di tesori al mondo non\nfallisce mai! Ottimo lavoro, fratello!"
  Talk22:
    attributes: subchallnpc000
    contents:
      - control:
          kind: sound
          unknown:
            - 2
            - 0
      - text: "Stiamo cercando il tesoro del "
      - control:
