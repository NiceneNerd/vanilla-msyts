atr1_unknown: 4
entries:
  Atacked00:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 32
      - text: "Hah... "
      - control:
          kind: pause
          length: short
      - text: "Fiuuu... "
      - control:
          kind: pause
          length: short
      - text: Uff...
      - control:
          kind: pause
          length: long
      - text: "\nCredevo di essere spacciato..."
  Atacked01:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 42
      - text: Sono in trappola...
  Failed00:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 12
      - text: "Urgh... "
      - control:
          kind: pause
          length: short
      - text: Mi fa male dappertutto...
      - control:
          kind: pause
          length: long
      - text: "\nAccidenti a quei mostri..."
  Failed01:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 12
      - text: "Alla mia età è dura trovarsi in situazioni\nsimili."
      - control:
          kind: pause
          length: long
      - text: " Accidenti a quei mostri... "
      - control:
          kind: pause
          length: short
      - text: "Non\nhanno rispetto per nessuno!"
  Guardian00:
    attributes: Npc_Road_021
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 31
      - text: "Non vale la pena rischiare la mia vita\ne quella del mio cavallo!"
      - control:
          kind: pause
          length: short
      - text: " Devo fuggire\nda qui!"
  MSG_Futa_00:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Posso insegnarti qualcosa solo a\nproposito dei cavalli."
      - control:
          kind: pause
          length: short
      - text: " A te sta bene,\ngiusto?\nDunque: quando acchiappi un cavallo\nselvaggio, la prima cosa da fare è\ncondurlo allo stallaggio."
      - control:
          kind: pause
          length: long
      - text: "\nDagli un nome e otterrai sella e redini."
  MSG_Futa_00_1:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Quando acchiappi un cavallo selvaggio,\nla prima cosa da fare è condurlo allo\nstallaggio.\nDagli un nome e otterrai sella e redini."
  MSG_Futa_01:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Sapevi già che
      - control:
          kind: pause
          length: short
      - text: " i cavalli non sono gli unici\nanimali che si possono cavalcare?\n\nA me non interessano,"
      - control:
          kind: pause
          length: short
      - text: " ma tu sembri un\ntipo con molta curiosità addosso...\nho indovinato?"
  MSG_Futa_01_1:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "I cavalli non sono gli unici animali che\npuoi cavalcare."
      - control:
          kind: pause
          length: longer
      - text: " Fai una prova, se ne\nvedi altri che ti sembrano adatti!"
  MSG_Futa_02:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ti darò un'informazione molto utile."
      - control:
          kind: pause
          length: long
      - text: "\nI cavalli adorano mangiare... "
      - control:
          kind: pause
          length: long
      - text: le carote.
      - control:
          kind: pause
          length: long
      - text: "\nScusa, è una cosa ovvia."
      - control:
          kind: pause
          length: long
      - text: "\nA proposito, puoi comprare le carote\nal "
      - control:
          kind: set_colour
          colour: blue
      - text: villaggio Calbarico
      - control:
          kind: reset_colour
      - text: "."
  MSG_Futa_02_1:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "I cavalli adorano le carote.\n\n\nPuoi comprarne un po' al "
      - control:
          kind: set_colour
          colour: blue
      - text: "villaggio\nCalbarico "
      - control:
          kind: reset_colour
      - control:
          kind: pause
          length: short
      - text: "e provare a darle al tuo cavallo.\nPotrebbe essere una buona idea."
  MSG_Futa_03:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Te lo dirò chiaro e tondo:"
      - control:
          kind: pause
          length: long
      - text: " l'unico\nargomento di conversazione in cui sono\nspecializzato sono i cavalli!\nSapevi che puoi scoccare frecce\nquando sei in groppa al tuo destriero?\n\nÈ una tecnica molto efficace per colpire\ni mostri ed evitare i loro attacchi al\ntempo stesso."
  MSG_Futa_03_1:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Puoi lanciare frecce quando sei in sella\nal tuo cavallo,"
      - control:
          kind: pause
          length: longer
      - text: "così da evitare gli attacchi\ndei mostri e bersagliarli con le frecce."
  MSG_Futa_04:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Posso solo parlarti di cavalli.
      - control:
          kind: pause
          length: short
      - text: "\nPer te non è un problema, giusto?\n\nÈ troppo divertente andare al galoppo"
      - control:
          kind: pause
          length: short
      - text: "\ne sfrecciare a tutta velocità, anche tra\ni villaggi...\nMa tu non farlo, eh!"
      - control:
          kind: pause
          length: long
      - text: " Sarebbe una bella\nseccatura sia per gli abitanti che per\nil cavallo."
  MSG_Futa_04_1:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Cerca di non correre troppo quando\nattraversi i villaggi in groppa al tuo\ncavallo.\nNon c'è nulla di peggio che causare\nseccature alla gente."
  Near00:
    attributes: Npc_Road_021
    contents:
      - text: Con il mio cavallo...
  Near01:
    attributes: Npc_Road_021
    contents:
      - text: Il mio cavallo è sazio...
  Near02:
    attributes: Npc_Road_021
    contents:
      - text: Devo pulire la sella...
  Near03:
    attributes: Npc_Road_021
    contents:
      - text: Guardiamo le stelle...
  Near04:
    attributes: Npc_Road_021
    contents:
      - text: Il mio cavallo si bagnerà...
  Rest_00:
    attributes: Npc_Road_021
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Adoro i cavalli.
      - control:
          kind: pause
          length: short
      - text: "\n\n\nAl punto da influenzare anche i miei\ngusti in fatto di donne..."
  Rest_01:
    attributes: Npc_Road_021
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 0
      - text: "Quando piove cerco di frenare la mia\nvoglia di cavalcare e non esco con il mio\ncavallo."
      - control:
          kind: pause
          length: long
      - text: " Odiamo bagnarci."
  Rest_02:
    attributes: Npc_Road_021
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 0
      - text: Il mio cavallo è molto sensibile...
      - control:
          kind: pause
          length: long
      - text: "\nLa pioggia lo rende di cattivo umore."
  RetMSG_Futa_00:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Posso parlarti solo di cavalli, sai? Se a\nte va bene, ascolta un po'...\nCi sono tanti tipi di cavalli.\nAlcuni sono veloci... "
      - control:
          kind: pause
          length: short
      - text: "altri sono molto\npotenti."
      - control:
          kind: pause
          length: short
      - text: " Scegliere quello più adatto alle\ntue esigenze è molto importante."
  RetMSG_Futa_00_1:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Ci sono tanti tipi di cavalli.
      - control:
          kind: pause
          length: longer
      - text: " È una buona\nidea cercare quello più adatto a te."
  RetMSG_Futa_01:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 26
      - text: "♪ Cavallo bianco, cavallo gigante e\ncavallo d'ossa..."
      - control:
          kind: pause
          length: long
      - text: " Se esistete davvero,\nvorrei salirvi in groppa."
      - control:
          kind: pause
          length: long
      - text: "\nTi incuriosisce questa canzone?"
      - control:
          kind: pause
          length: short
      - text: " L'ho\nscritta io e parla dei cavalli leggendari."
  RetMSG_Futa_01_1:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 26
      - text: "♪ Cavallo bianco, cavallo gigante e\ncavallo d'ossa..."
      - control:
          kind: pause
          length: long
      - text: " Se esistete davvero,\nvorrei salirvi in groppa..."
      - control:
          kind: pause
          length: long
      - text: "\nCosa ne pensi di questa canzone?"
      - control:
          kind: pause
          length: longer
      - text: "\nSi chiama \"Cavalli leggendari\"! "
      - control:
          kind: pause
          length: longer
      - text: "\nRicordatela."
  RetMSG_Futa_02:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Vuoi qualche informazione?
      - control:
          kind: pause
          length: short
      - text: " Allora ti\nparlerò dei cavalli. Anche loro si\nstancano dopo una bella corsa.\nNon puoi spronare il tuo destriero\nall'infinito per andare più veloce. Tieni gli\nocchi aperti e controlla le sue condizioni."
  RetMSG_Futa_02_1:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Anche i cavalli si stancano dopo una\ngaloppata."
      - control:
          kind: pause
          length: longer
      - text: " Fai attenzione alle condizioni\ndel tuo cavallo e prenditene cura."
  RetMSG_Futa_03:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Vuoi che ti dia qualche punto di\nriferimento? Guarda che l'unica cosa di\ncui posso parlarti sono i cavalli.\nOra che ci penso, a est di qui trovi le\nrovine del vecchio campo di\naddestramento per cavalli.\nÈ un sito molto importante ed è anche\nsegnato sulle mappe."
      - control:
          kind: pause
          length: short
      - text: " Ma al momento\ni mostri se ne sono impossessati."
  RetMSG_Futa_03_1:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Verso est ci sono delle rovine di un\nvecchio campo di addestramento per\ncavalli.\nPurtroppo è diventato un campo di\naddestramento per mostri."
  RetMSG_Futa_04:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ignorerò ciò che vuoi sapere veramente\ne ti insegnerò un trucco per acchiappare\ni cavalli.\nAvvicinati con cautela da dietro per non\nfarti notare, poi sali loro in groppa e\nriempili di carezze."
  RetMSG_Futa_04_1:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Per acchiappare un cavallo, avvicinati da\ndietro senza farti notare e,"
      - control:
          kind: pause
          length: longer
      - text: " una volta in\ngroppa, accarezzalo per calmarlo!"
  SavedLv1_00:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 11
      - text: "In un modo o nell'altro, sono salvo!"
  SavedLv1_01:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 11
      - text: "Il viaggio non ha alcun valore se non\nsopravvivo abbastanza per godermelo!"
      - control:
          kind: pause
          length: short
      - text: "\nGrazie per avermi salvato!"
  SavedLv2_00:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 11
      - text: "Sono riuscito a fuggire grazie al mio\ncavallo..."
      - control:
          kind: pause
          length: long
      - text: " Ma ovviamente il tuo aiuto\nè stato molto ben accetto."
  SavedLv2_01:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 11
      - text: Hai salvato sia me che il mio cavallo!
      - control:
          kind: pause
          length: longer
      - text: "\nNon dimenticherò mai ciò che hai fatto."
  SavedLv3_00:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 11
      - text: Grazie...
      - control:
          kind: pause
          length: short
      - text: "\nPer merito tuo vivrò più a lungo."
  SavedLv3_01:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 18
      - text: "Mi sono ricordato di quanto possono\nessere terrificanti quei mostri..."
      - control:
          kind: pause
          length: long
      - text: "\nGrazie dell'aiuto."
  Thanks00:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 11
      - text: "Non è molto, ma ti prego, accetta\nquesto come ringraziamento."
