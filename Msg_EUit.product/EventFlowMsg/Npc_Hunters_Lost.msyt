  Talk_300:
    attributes: Npc_Hunters_Lost
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: Cosa ti interessa sapere?
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ho delle informazioni sulle rovine\noscure e su ciò che si trova al limite\ndelle montagne di Hebra."
      - control:
