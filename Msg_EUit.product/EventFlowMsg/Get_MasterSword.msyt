    attributes: TwnObj_Village_Korok_DekuTree_A_01
    contents:
      - control:
          kind: sound2
          unknown:
            - 14
            - 205
      - text: Questa lama...
      - control:
          kind: pause
          length: long
      - text: " porta il nome di\n"
