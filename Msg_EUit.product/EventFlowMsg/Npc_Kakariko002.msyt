              field_1: 0
      - control:
          kind: sound
          unknown:
            - 5
            - 0
      - text: "È un cimelio e il nostro popolo\nha il compito di custodirlo..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "\""
      - control:
          kind: set_colour
          colour: red
      - text: "La protezione degli antenati sarà\nconcessa al prescelto dalla sfera."
      - control:
          kind: reset_colour
      - text: "\""
      - control:
          kind: pause
          length: longer
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Queste parole si tramandano dai tempi\ndella nonna della nonna di mia nonna...\nfino alla più nonna di tutte."
      - control:
          kind: pause
          length: short
      - text: "\nNon riesco a capire il loro significato\npreciso, ma..."
      - control:
          kind: pause
          length: short
      - text: " questo \""
      - control:
          kind: set_colour
          colour: red
      - text: "prescelto dalla\nsfera"
      - control:
          kind: reset_colour
      - text: "\" non sarà..."
  Talk03:
    attributes: Npc_Kakariko002
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 6
      - text: "Noi "
      - control:
          kind: set_colour
          colour: blue
      - text: Sheikah
      - control:
          kind: reset_colour
      - control:
          kind: pause
          length: short
      - text: " siamo un popolo segreto da\nsempre al servizio della famiglia reale."
      - control:
