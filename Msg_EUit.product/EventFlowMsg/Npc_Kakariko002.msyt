              field_1: 0
      - control:
          kind: sound
          unknown:
            - 3
            - 0
      - text: "Ma la "
      - control:
          kind: set_colour
          colour: red
      - text: grande calamità
      - control:
          kind: reset_colour
      - text: " che si abbatté su\nqueste terre cento anni fa"
      - control:
          kind: pause
          length: short
      - text: " mandò in\nrovina la famiglia reale e il nostro popolo.\n"
      - control:
          kind: sound
          unknown:
            - 3
            - 1
      - text: Purtroppo...
      - control:
          kind: pause
          length: short
      - text: " molte persone giurarono\nfedeltà a "
      - control:
          kind: set_colour
          colour: red
      - text: Ganon
