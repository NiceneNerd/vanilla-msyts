  RetMSG_Akka_03_1:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 11
      - text: "La Gerudo che arriva dalle Terre\nGerudo..."
      - control:
          kind: pause
          length: longer
      - text: " Davvero una bellezza!"
  RetMSG_Akka_04:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "Lo "
      - control:
          kind: set_colour
          colour: blue
      - text: "stallaggio di Estakkala "
      - control:
          kind: reset_colour
      - text: "e lo "
      - control:
          kind: set_colour
          colour: blue
      - text: "stallaggio\ndi Sudakkala"
      - control:
          kind: reset_colour
      - text: "..."
      - control:
          kind: pause
          length: long
      - text: " Mi confondo sempre!\nQui siamo allo stallaggio..."
      - control:
          kind: pause
          length: long
      - text: " Ehm..."
  RetMSG_Akka_04_1:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "C'è lo "
      - control:
          kind: set_colour
          colour: blue
      - text: stallaggio di Estakkala
      - control:
          kind: reset_colour
      - text: " e lo\n"
      - control:
          kind: set_colour
          colour: blue
      - text: stallaggio di Sudakkala
      - control:
          kind: reset_colour
      - text: "..."
      - control:
          kind: pause
          length: longer
      - text: "\nMa come faccio a distinguerli?"
  SavedLv1_00:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 18
      - text: "Ehi, "
      - control:
          kind: pause
          length: short
      - text: mi hai davvero aiutato!
  SavedLv1_01:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 18
      - text: "Sei fortissimo! "
      - control:
          kind: pause
          length: short
      - text: "Che grande aiuto che\nmi hai dato, roccia!"
  SavedLv2_00:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 11
      - text: "Incredibile, "
      - control:
          kind: pause
          length: short
      - text: mi hai salvato!
  SavedLv2_01:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 11
      - text: "Sei forte, roccia!"
      - control:
          kind: pause
