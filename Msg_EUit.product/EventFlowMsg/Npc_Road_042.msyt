    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Sono Naddon!
      - control:
          kind: pause
          length: longer
      - text: "\nHo un fratello di nome Boldon!\n\nDeve essere in viaggio sulla "
      - control:
          kind: set_colour
          colour: blue
      - text: "strada di\nOldin "
      - control:
          kind: reset_colour
