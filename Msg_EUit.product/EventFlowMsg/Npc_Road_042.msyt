  Rest_01:
    attributes: Npc_Road_042
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: Il tempo è davvero brutto oggi...
      - control:
          kind: pause
          length: longer
      - text: " Meglio\nnon sfidare la sorte ed evitare di\ncamminare con questa pioggia!"
  Rest_02:
    attributes: Npc_Road_042
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "Booom...!\n\n\nNo, non ero io! "
      - control:
