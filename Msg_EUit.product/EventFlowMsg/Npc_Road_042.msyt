  MSG_Akka_00:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: Hai qualche minerale?
      - control:
          kind: pause
          length: longer
      - text: " Se ne hai, vendili\nai negozi. Quelli rari possono fruttare\nun bel gruzzolo, roccia!"
  MSG_Akka_00_1:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Se hai dei minerali che non ti servono,\nvendili ai negozi."
      - control:
          kind: pause
          length: longer
      - text: " Le gemme luccicanti\npossono fruttarti un bel gruzzolo!"
  MSG_Akka_01:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: "Sei mai stato alla "
      - control:
          kind: set_colour
          colour: blue
      - text: città dei Goron
      - control:
          kind: reset_colour
      - text: "?"
      - control:
          kind: pause
          length: long
      - text: "\nSi trova sul "
      - control:
          kind: set_colour
          colour: blue
      - text: "Monte Morte "
      - control:
          kind: reset_colour
      - text: "a ovest!\n\nPerò ho sentito che è un viaggio troppo\ncaldo"
      - control:
          kind: pause
          length: short
      - text: " per chi non è un Goron!"
  MSG_Akka_01_1:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "La "
      - control:
          kind: set_colour
          colour: blue
      - text: città dei Goron
      - control:
          kind: reset_colour
      - text: " si trova sul\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Monte Morte
      - control:
          kind: reset_colour
      - text: " a ovest!\n\nÈ un viaggio molto caldo e difficile,\nma ci sono dei negozi..."
      - control:
          kind: pause
          length: longer
      - text: " Sarebbe bello\nse tu li visitassi almeno una volta, roccia!"
  MSG_Akka_02:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: Conosci un Goron chiamato Boldon?
      - control:
          kind: pause
          length: longer
      - text: "\nÈ mio fratello!\n\nHa un cuore grande e non si perde mai\nnelle piccole cose!"
      - control:
          kind: pause
          length: longer
      - text: " Chissà dove starà\nviaggiando adesso..."
  MSG_Akka_02_1:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: Sono sicuro che mio fratello Boldon
      - control:
          kind: pause
          length: short
      - text: "\nsia in viaggio da qualche parte.\n\nSe lo incontri, fai pure quattro\nchiacchiere con lui. Lo apprezzerei\nmolto."
  MSG_Akka_03:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: "Puoi vendere le gemme a un prezzo\nmolto alto, roccia!\n\nSe trovi depositi di minerali lungo il\ncammino,"
      - control:
          kind: pause
          length: short
      - text: " prendi l'abitudine di scavarli\ne raccoglierli, mi raccomando!"
  MSG_Akka_03_1:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Quando trovo un deposito di minerali\nlungo la strada, lo uso per scavare i\ngioielli!"
      - control:
          kind: pause
          length: longer
      - text: " Posso venderli a buon prezzo!"
  MSG_Akka_04:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: "Questo è lo "
      - control:
          kind: set_colour
          colour: blue
      - text: stallaggio di Estakkala
      - control:
          kind: reset_colour
      - text: "!\n\n\nSe hai qualche minerale, non mangiarlo,"
      - control:
          kind: pause
          length: short
      - text: "\nma vendilo a questo stallaggio!\n\nCerto, non essendo un Goron, anche\na volerlo mangiare ti risulterebbe\nindigesto..."
  MSG_Akka_04_1:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Quando arrivi a uno stallaggio dovresti\nvendere le cose che non ti servono!\n\nIo mangerei le rocce che non mi\nservono,"
      - control:
          kind: pause
          length: short
      - text: " ma un "
      - control:
          kind: set_colour
          colour: blue
      - text: "Hylia "
      - control:
          kind: reset_colour
      - text: "come te...\n"
      - control:
          kind: pause
          length: short
      - text: "farebbe meglio a venderle, no?"
  Near00:
    attributes: Npc_Road_042
    contents:
      - text: "'Giorno..."
  Near01:
    attributes: Npc_Road_042
    contents:
      - text: È ora di pranzo?
  Near02:
