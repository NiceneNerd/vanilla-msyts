            four:
              field_1: 0
      - control:
          kind: text_size
          percent: 100
      - text: "Il numero di vittime di incidenti\nsulle "
      - control:
          kind: set_colour
          colour: blue
      - text: montagne di Hebra
      - control:
          kind: reset_colour
      - text: " è infinito."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Per prima cosa, questo non è luogo in\ncui avventurarsi vestiti come se si fosse\nnella "
      - control:
