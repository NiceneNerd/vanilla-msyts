---
group_count: 29
atr1_unknown: 4
entries:
  "0000":
    attributes: ""
    contents:
      - text: Fratello?
  "0001":
    attributes: ""
    contents:
      - text: È rotolato via?
  "0002":
    attributes: ""
    contents:
      - text: "Dov'è andato?"
  "0003":
    attributes: ""
    contents:
      - text: "Il segreto dell'eroe?"
  "0004":
    attributes: ""
    contents:
      - text: Capisco.
  "0005":
    attributes: ""
    contents:
      - text: Galleria Pigoron?
  Goron_Father_Near01:
    attributes: Npc_Goron010
    contents:
      - text: Gorotola nel pentolon!
  Goron_Father_Near02:
    attributes: Npc_Goron010
    contents:
      - text: "Dov'è andato?"
  Goron_Father_Talk01:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 0
      - text: "Quello smineralizzato di mio fratello\nnon è ancora tornato a casa!\n\n"
      - control:
          kind: sound
          unknown:
            - 9
            - 1
      - text: Dove si è andato a cacciare...?!
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
            - 4
          selected_index: 0
          cancel_index: 3
          unknown: 8
  Goron_Father_Talk02:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound2
          unknown:
            - 9
            - 205
      - text: Fratello mio! Ecco dove stavi!
  Goron_Father_Talk03:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Per trovarlo, ho provato a pensare come\nmio fratello e a fare quello che avrebbe\nfatto lui."
  Goron_Father_Talk04:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Ti devo chiedere un favore.\nSono suo fratello, mica posso stare qui\na sedimentarmi. Lo voglio aiutare."
  Goron_Father_Talk05:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ti aspetto qui e intanto mi preparo\na cucinare, roccia."
  Goron_Father_Talk06:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: Eh!
      - control:
          kind: pause
          length: longer
      - text: "\nChe meraviglia, roccia!\n\nQuesto è un "
      - control:
          kind: set_colour
          colour: red
      - text: arrocciosto
      - control:
          kind: reset_colour
      - text: " coi fiocchi!\nLo preparo subito."
  Goron_Father_Talk06_2:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: raw
          four:
            three:
              field_1: 0
      - text: "Ce l'ho fatta, roccia! Ho finito la "
      - control:
          kind: set_colour
          colour: red
      - text: "delizia\ndi arrocciosto grigliato"
      - control:
          kind: reset_colour
      - text: "!"
  Goron_Father_Talk07:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound2
          unknown:
            - 1
            - 205
      - text: "Fratello mio, è pronto!\nMangiatelo tutto, roccia!"
  Goron_Father_Talk08:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound2
          unknown:
            - 2
            - 205
      - text: "Allora, questo era il "
      - control:
          kind: set_colour
          colour: red
      - text: "segreto dell'eroe"
      - control:
          kind: reset_colour
      - text: ",\nroccia?!\n\n"
      - control:
          kind: sound2
          unknown:
            - 4
            - 205
      - control:
          kind: text_size
          percent: 80
      - text: "... A me mi pare di averlo già visto da qualche\nparte però."
      - control:
          kind: text_size
          percent: 100
      - text: "\n\n"
      - control:
          kind: sound2
          unknown:
            - 9
            - 205
      - text: "Comunque... che scoperta fantastica!\nBravo fratello mio, bravo!"
  Goron_Father_Talk09:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: "Grazie per aver aiutato mio fratello,\nroccia.\n\nAdesso ce ne torniamo assieme alla\ncittà dei Goron."
  Goron_Father_Talk10:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 0
      - text: "Ma quando torna a casa mio fratello?!\n\n\n"
      - control:
          kind: sound
          unknown:
            - 11
            - 1
      - text: "Se ti trovi alla "
      - control:
          kind: set_colour
          colour: blue
      - text: galleria Pigoron
      - control:
          kind: reset_colour
      - text: ",\ndiresti a mio fratello di smetterla di\ncercare il "
      - control:
          kind: set_colour
          colour: red
      - text: "segreto dell'eroe"
      - control:
          kind: reset_colour
      - text: "?"
      - control:
          kind: choice
          choice_labels:
            - 5
            - 3
            - 4
          selected_index: 0
          cancel_index: 3
          unknown: 8
  Goron_Father_Talk11:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "La "
      - control:
          kind: set_colour
          colour: blue
      - text: "galleria Pigoron "
      - control:
          kind: reset_colour
      - text: "è una galleria\nabbandonata a sud-ovest dello\n"
      - control:
          kind: set_colour
          colour: blue
      - text: stagno Pigoron
      - control:
          kind: reset_colour
      - text: ", roccia.\nFinisce su un muro di roccia durissima\ne perciò abbiamo smesso di scavarla\ntanto tempo fa.\n"
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Si dice che dietro quel muro di roccia\ndurissima ci sia questo "
      - control:
          kind: set_colour
          colour: red
      - text: "segreto dell'eroe"
      - control:
          kind: reset_colour
      - text: ",\nroccia."
  Goron_Father_Talk12:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Esci da qui e "
      - control:
          kind: set_colour
          colour: red
      - text: "rotola giù "
      - control:
          kind: reset_colour
      - text: "fino al\ndirupo Vagoron. Lì in fondo\nci trovi gli "
      - control:
          kind: set_colour
          colour: red
      - text: arrocciosti
      - control:
          kind: reset_colour
      - text: ".\nVai e portamene uno, roccia!"
  Goron_Father_Talk13:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 16
      - text: "Non sai quanto vorrei poterci andare\nio da solo, roccia...\n\nMa ho un dolore cronico alla spalla che\nmi impedisce di fare sforzi goronici.\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Perciò lo chiedo a te! Aiutami!
  Goron_Father_Talk14:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 0
      - text: "Mio fratello Gongron, che mi rende\ncosì orgoglioso.\n\n"
      - control:
          kind: sound
          unknown:
            - 2
            - 0
      - text: "Ha detto che voleva scavare\nil "
      - control:
          kind: set_colour
          colour: red
      - text: "segreto dell'eroe"
      - control:
          kind: reset_colour
      - text: "... e se n'è andato,\nroccia!\n"
      - control:
          kind: sound
          unknown:
            - 9
            - 1
      - text: Ma che gli è saltato in testa?!
      - control:
          kind: choice
          choice_labels:
            - 2
            - 3
            - 4
          selected_index: 0
          cancel_index: 3
          unknown: 8
  Goron_Father_Talk15:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 0
      - text: "Ha detto che voleva scavare\nil "
      - control:
          kind: set_colour
          colour: red
      - text: "segreto dell'eroe"
      - control:
          kind: reset_colour
      - text: "... e se n'è andato,\nroccia!\n"
      - control:
          kind: sound
          unknown:
            - 9
            - 1
      - text: Ma che gli è saltato in testa?!
      - control:
          kind: choice
          choice_labels:
            - 2
            - 3
            - 4
          selected_index: 0
          cancel_index: 3
          unknown: 8
  Goron_Father_Talk16:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Sarà alla "
      - control:
          kind: set_colour
          colour: blue
      - text: galleria Pigoron
      - control:
          kind: reset_colour
      - text: ", che sta a \nsud-ovest dello "
      - control:
          kind: set_colour
          colour: blue
      - text: stagno Pigoron
      - control:
          kind: reset_colour
      - text: ", roccia.\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Là dentro la roccia è durissima, perciò\nnon ci abbiamo più scavato."
  Goron_Father_Talk17:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Quando hai preso l'"
      - control:
          kind: set_colour
          colour: red
      - text: arrocciosto
      - control:
          kind: reset_colour
      - text: ",\nbuttalo proprio qui, roccia."
  Goron_Father_Talk18:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 0
      - text: "E così ora con quella scusa se n'è\nandato a perdere tempo!\nChe nervi, roccia!"
  Goron_Father_Talk20:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "A sud-ovest dello "
      - control:
          kind: set_colour
          colour: blue
      - text: stagno Pigoron
      - control:
          kind: reset_colour
      - text: " c'è\nuna galleria abbandonata che si chiama\n"
      - control:
          kind: set_colour
          colour: blue
      - text: galleria Pigoron
      - control:
          kind: reset_colour
      - text: ".\n"
      - control:
          kind: sound
          unknown:
            - 6
            - 1
      - text: "E dietro la dura pietra non ci sono più\nminerali, ma il "
      - control:
          kind: set_colour
          colour: red
      - text: "segreto dell'eroe"
      - control:
          kind: reset_colour
      - text: "!\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "... Almeno così si dice, ma chi lo sa se\nè vero, roccia.\n\nIo l'ho sentito dal vicino di casa del\ncugino di secondo grado del cugino\ndi mio padre."
  Goron_Father_Talk21:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: "Ti aspettavo, roccia!\nDai, buttalo nel pentolone!"
  Goron_Father_Talk22:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Il "
      - control:
          kind: set_colour
          colour: red
      - text: "segreto dell'eroe"
      - control:
          kind: reset_colour
      - text: " è una specie di \nfavola che si racconta tra noi Goron\nda tanto tempo.\nL'ho sentita dal vicino del cugino\ndi secondo grado del cugino di papà..."
  Goron_Father_Talk23:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 0
      - text: "Tu lo capisci,"
      - control:
          kind: pause
          length: short
      - text: " roccia?\nTra voi sassetti vi capite.\n\n"
      - control:
          kind: text_size
          percent: 80
      - control:
          kind: sound
          unknown:
            - 9
            - 1
      - text: Io non lo capisco proprio mio fratello...
      - control:
          kind: text_size
          percent: 100
  Goron_Father_Talk24:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "Ah, quanto vorrei che si mettesse a\nlavorare e a dare una mano a tutti i\nGoron invece di inseguire stupidi sogni!"
  Goron_Son_Near01:
    attributes: Npc_Goron009
    contents:
      - text: "È il segreto dell'eroe!"
  Goron_Son_Near02:
    attributes: Npc_Goron009
    contents:
      - text: Ho bisogno di mangiare!
  Goron_Son_Talk01:
    attributes: Npc_Goron009
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 10
      - text: "Oooh! Scavo, stacco e scasso!\nC'è il "
      - control:
          kind: set_colour
          colour: red
      - text: segreto
      - control:
          kind: reset_colour
      - text: " dietro il sasso!\n\nVuoi saper che scavo a fare?\nMa è un "
      - control:
          kind: set_colour
          colour: red
      - text: segreto
      - control:
          kind: reset_colour
      - text: ", non domandare!"
  Goron_Son_Talk02:
    attributes: Npc_Goron009
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 43
      - text: "Puff... puff... "
      - control:
          kind: pause
          length: long
      - text: "Non ce la faccio più...\n\n\nRoccia, da qualche parte dietro questi\nsassi c'è il "
      - control:
          kind: set_colour
          colour: red
      - text: "segreto dell'eroe"
      - control:
          kind: reset_colour
      - text: "!\n\nEro sicuro che sarei diventato chissà\ncosa, a svelare quel segreto..."
      - control:
          kind: pause
          length: short
      - text: "\n\n"
      - control:
          kind: sound2
          unknown:
            - 40
            - 205
      - text: Ma non ce la faccio più a scavare...
      - control:
          kind: pause
          length: short
      - text: "\nLa pancia mi fa \"gorogoron\" dalla fame,\nroccia..."
  Goron_Son_Talk02b:
    attributes: Npc_Goron009
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 43
      - text: Non mi sento più le braccia.
  Goron_Son_Talk04:
    attributes: Npc_Goron009
    contents:
      - text: "Delizia deliziosaaa, roccia!\n\n\nChe croccantezza! Questa consistenza\nsotto i denti che senti croc croc croc!\n\nI minerali riscaldati ad alta temperatura\nsono un'eruzione di sapori nella mia\nbocca!\nSono diventato il Monte Morte!!!"
  Goron_Son_Talk04_1:
    attributes: Npc_Goron009
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 6
      - text: "Oh! Sento odore di "
      - control:
          kind: set_colour
          colour: red
      - text: "delizia di arrocciosto\ngrigliato"
      - control:
          kind: reset_colour
      - text: "..."
  Goron_Son_Talk04_2:
    attributes: Npc_Goron009
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 11
      - text: "Guarda qua, mi fa venire fame!\nOra mi mangio tutto!"
  Goron_Son_Talk04_3:
    attributes: Npc_Goron009
    contents:
      - control:
          kind: sound2
          unknown:
            - 9
            - 205
      - text: "La cucina di mio fratello è la migliore,\nroccia!"
  Goron_Son_Talk04_4:
    attributes: Npc_Goron009
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 18
      - text: "E ora che ho la pancia piena, mettiamoci\nsotto, roccia!"
  Goron_Son_Talk05:
    attributes: Npc_Goron009
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 16
      - control:
          kind: text_size
          percent: 125
      - text: SBRAAAH!
      - control:
          kind: text_size
          percent: 100
  Goron_Son_Talk05_2:
    attributes: Npc_Goron009
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 10
      - control:
          kind: text_size
          percent: 125
      - text: GOROATTACCO!
      - control:
          kind: text_size
          percent: 100
  Goron_Son_Talk06:
    attributes: Npc_Goron009
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 1
      - control:
          kind: animation
          name: Happy_00
      - text: "Eccolo!!! Grande Goron!!!\nÈ il "
      - control:
          kind: set_colour
          colour: red
      - text: "segreto dell'eroe"
      - control:
          kind: reset_colour
      - text: ", roccia!\n\n"
      - control:
          kind: sound2
          unknown:
            - 4
            - 205
      - control:
          kind: text_size
          percent: 80
      - text: "Però... mi pare di averla già vista una cosa\nsimile alla città dei Goron..."
      - control:
          kind: text_size
          percent: 100
  Goron_Son_Talk07:
    attributes: Npc_Goron009
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 0
      - text: "Grazie, roccia! Adesso dobbiamo\ntornare alla città dei Goron e dirlo\na tutti!"
