          unknown:
            - 4
            - 21
      - text: "Aspetta,"
      - control:
          kind: pause
          length: short
      - text: " stai cercando di dirmi\nche non ci sei mai stato?\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "La città dei Goron si trova molto più\na nord del villaggio Finterra,"
      - control:
          kind: pause
          length: short
      - text: " nelle\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Terre di Oldin
      - control:
          kind: reset_colour
      - text: "... O almeno così pare...\n"
      - control:
          kind: sound
