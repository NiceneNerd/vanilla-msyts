            - 4
            - 19
      - text: "Aaah... "
      - control:
          kind: pause
          length: short
      - text: "Ho detto \"almeno così pare\"!\n"
      - control:
          kind: pause
          length: long
      - text: "Allora avrai capito che non ci sono stato\nneanche io... Però mi ci sono avvicinato."
      - control:
          kind: pause
          length: short
      - text: "\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: È solo che quel posto è così caldo che
      - control:
          kind: set_colour
          colour: red
      - text: "\nsembra di bruciare"
      - control:
          kind: reset_colour
      - text: ", perciò me ne sono\ntornato indietro ancora prima di arrivare."
  talk15:
    attributes: Npc_HatenoVillage011
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 20
      - text: "Oooh, "
      - control:
          kind: pause
          length: short
      - text: "intendi dire che non ci sei mai\nstato? \n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "La cittadella Gerudo si trova\na ovest del villaggio Finterra..."
      - control:
          kind: pause
          length: short
      - text: "\n\nSorge nel "
      - control:
          kind: set_colour
