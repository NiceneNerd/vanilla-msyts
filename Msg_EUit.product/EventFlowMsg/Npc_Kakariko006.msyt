          unknown: 6
  Talk21:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: "Questa è la specialità del villaggio\nCalbarico: la "
      - control:
          kind: set_colour
          colour: blue
      - control:
          kind: raw
          two_hundred_one:
            dynamic:
              - 0
              - len: 4
                field_2:
                  - 2
                  - 2
                  - 2
                  - 0
      - text: corazzucca
      - control:
          kind: reset_colour
      - text: "!"
      - control:
          kind: pause
          length: longer
      - text: "\n\nColtivo questo ortaggio con tanto amore\ne sudore, ed è veramente delizioso."
      - control:
          kind: pause
          length: short
      - text: "\n\nSe ne vuoi qualcuna, parla con me\nquando mi vedi lavorare nel campo."
