      - text: "è bellissimo, roccia!\n\n\n"
      - control:
          kind: text_size
          percent: 80
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Però a me pare uguale al sacrario che si trova\nqui in città..."
