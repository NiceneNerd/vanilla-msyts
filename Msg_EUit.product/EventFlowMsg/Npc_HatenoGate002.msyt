  talk02:
    attributes: Npc_HatenoGate002
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: "Oh, anch'io..."
      - control:
          kind: pause
          length: longer
      - text: "\n\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Ma non si fa vivo...
      - control:
          kind: pause
          length: longer
      - text: "\n\n\n... nessuno..."
      - control:
          kind: pause
          length: longer
      - text: "\n\n\n"
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "... proprio nessuno!"
      - control:
          kind: pause
          length: longer
      - text: "\n\n\nCredevo che questo fosse il leggendario\n\"laghetto degli innamorati\". Che mi sia\nsbagliato?"
  talk03:
    attributes: Npc_HatenoGate002
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: "Non ti vergognerai mica ad ammetterlo?\n\n\nPer quale altro motivo dovresti trovarti\nqui?\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Comunque, a dirti la verità io sto\naspettando da un bel pezzo,"
      - control:
          kind: pause
          length: longer
      - text: "\nma non è venuto..."
      - control:
          kind: pause
          length: longer
      - text: "\n... nessuno..."
      - control:
          kind: pause
          length: longer
      - text: "\n\n\n"
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "... proprio nessuno!"
      - control:
          kind: pause
          length: longer
      - text: "\n\n\nChe mi sia sbagliato? Forse questo non\nè il \"laghetto degli innamorati\"?"
  talk04:
    attributes: Npc_HatenoGate002
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Chissà perché non si fa vivo nessuno...\nForse è un segno del destino che l'anima\ngemella me la devo cercare da solo?"
      - control:
          kind: choice
          choice_labels:
            - 2
            - 3
          selected_index: 0
          cancel_index: 1
          unknown: 6
  talk05:
    attributes: Npc_HatenoGate002
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: Cerchi di tirarmi su di morale?
      - control:
          kind: pause
          length: longer
      - text: " \n\n\n"
      - control:
          kind: sound
          unknown:
            - 7
            - 9
      - text: Non ti devi preoccupare per me!
      - control:
          kind: pause
          length: longer
      - text: "\n\n\nMia mamma mi dice sempre che so\ncavarmela in ogni situazione.\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Piuttosto... non dovresti preoccuparti\nper te stesso?\n\nSe continui a credere nelle favole,\nnon troverai mai l'anima gemella!"
  talk06:
    attributes: Npc_HatenoGate002
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "... ... ..."
      - control:
          kind: pause
          length: longer
      - text: "\n\n\n"
      - control:
          kind: sound
          unknown:
            - 10
            - 0
      - text: Dici davvero?
      - control:
          kind: pause
          length: longer
  talk07:
    attributes: Npc_HatenoGate002
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ad ogni modo...\n\n\n... devi essere proprio disperato per\nvenire in un posto come questo"
      - control:
          kind: pause
          length: long
      - text: "\ncon un tempo del genere..."
