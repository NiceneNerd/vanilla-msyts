  Action5_01:
    attributes: Npc_FaronWoods006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 6
      - text: "Che c'è?"
      - control:
          kind: pause
          length: long
      - text: " Anche tu in pausa?"
      - control:
          kind: choice
          choice_labels:
            - 1
            - 2
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Colony00:
    attributes: Npc_FaronWoods006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 6
      - text: "L'altopiano delle origini?\n\n\nE che ci vai a fare?"
      - control:
          kind: pause
          length: long
      - text: " Lassù non ci vive\nnessuno! E poi non c'è nessun sentiero\ne le pareti sono troppo ripide."
  Colony01:
    attributes: Npc_FaronWoods006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Comunque, è laggiù. Attento però che\nai suoi piedi vivono parecchi mostri."
  GoodBye00:
    attributes: Npc_FaronWoods006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: Ci vediamo!
      - control:
          kind: pause
          length: long
      - text: "\nBuona fortuna a te e a me!"
  GoodBye01:
    attributes: Npc_FaronWoods006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Qui non è male per riposarsi un po'!"
  Hyrule00:
    attributes: Npc_FaronWoods006
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 0
      - text: "Vuoi andare al castello di Hyrule?!\n\n\nSenti, io non so quante vite hai...\nma con tutti quei guardiani che stanno\nlì, sono sicuro che non ti basteranno."
  Hyrule01:
    attributes: Npc_FaronWoods006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Lo vedi, laggiù in lontananza?"
      - control:
          kind: pause
          length: long
      - text: "\nÈ meglio se continui a guardarlo\nsenza avvicinarti."
  Kakariko00:
    attributes: Npc_FaronWoods006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: Il villaggio Calbarico è da quella parte.
  Kakariko01:
    attributes: Npc_FaronWoods006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Dall'altra parte dei Monti Gemelli,\nc'è uno stallaggio. Fermati lì e\nchiedi alla gente del posto."
  Near00:
    attributes: Npc_FaronWoods006
    contents:
      - control:
          kind: sound2
          unknown:
            - 10
            - 205
      - text: La fine del mondo è vicina!
  Near01:
    attributes: Npc_FaronWoods006
    contents:
      - control:
          kind: sound2
          unknown:
            - 24
            - 205
      - text: Du da dì! ♪
  RunRain00:
    attributes: Npc_FaronWoods006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 14
      - text: "Tsk, piove a dirotto!\nParliamo dopo!"
  Talk00:
    attributes: Npc_FaronWoods006
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 12
      - text: "Sono abituato a vedere presagi nefasti,"
      - control:
          kind: pause
          length: long
      - text: "\nma questo era il più nefasto di tutti."
  Talk01_1:
    attributes: Npc_FaronWoods006
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 6
      - text: E tu da dove spunti fuori?
      - control:
          kind: pause
          length: long
      - text: "\nNon ti ho mai visto da queste parti.\n\nMah, non importa..."
      - control:
          kind: pause
          length: long
      - text: "\ncapiti al momento giusto, sono\nstufo di parlare da solo.\nHai visto"
      - control:
          kind: pause
          length: long
      - text: " quelle cose che sono spuntate\ndal terreno?"
  Talk01_2:
    attributes: Npc_FaronWoods006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Ma no, non dico i funghi! Le torri!\n\n\nE pensa un po', mica solo qui!\nÈ successo in tutto il mondo!"
  Talk01_3:
    attributes: Npc_FaronWoods006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: E le cose strane non finiscono qui!
      - control:
          kind: pause
          length: long
      - text: "\nQuei sacrari abbandonati da lungo tempo\nhanno cominciato a brillare...\nCapisci cosa significa?"
      - control:
          kind: pause
          length: long
      - text: "\n"
      - control:
          kind: sound2
          unknown:
            - 10
            - 205
      - text: Sta per arrivare la fine del mondo.
  Talk02:
    attributes: Npc_FaronWoods006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Quindi insomma, considerando tutto\nquesto sono venuto a tenere d'occhio\nquell'affare.\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Non si sa mai che magari si metta\nin moto all'improvviso."
      - control:
          kind: choice
          choice_labels:
            - 1000
            - 1001
            - 1002
          selected_index: 0
          cancel_index: 2
          unknown: 8
  Talk03:
    attributes: Npc_FaronWoods006
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 20
      - text: "Ma veramente non sai neanche cos'è un\n"
      - control:
          kind: set_colour
          colour: blue
      - text: guardiano
      - control:
          kind: reset_colour
      - text: "?"
      - control:
          kind: pause
          length: long
      - text: " Non conosci le leggende\ndi Hyrule?"
  Talk04:
    attributes: Npc_FaronWoods006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Sì, quel "
      - control:
          kind: set_colour
          colour: blue
      - text: guardiano
      - control:
          kind: reset_colour
      - text: ".\nDai, uno di quelli delle leggende!"
  Talk05:
    attributes: Npc_FaronWoods006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Vedi quell'affare laggiù?"
      - control:
          kind: pause
          length: long
      - text: "\nQuello a forma di urna rovesciata."
  Talk06:
    attributes: Npc_FaronWoods006
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Oh, ma lo sai che una volta"
      - control:
          kind: pause
          length: long
      - text: " uno di quei\ncosi mi ha visto e ha iniziato a seguirmi?"
      - control:
          kind: choice
          choice_labels:
            - 1003
            - 1002
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Talk07:
    attributes: Npc_FaronWoods006
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 1
      - text: "No no, un altro!\n\n\nÈ successo davanti al bosco che\nbisogna attraversare per arrivare\nal castello.\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Quando uno di quei cosi ti vede,\nparte una lucina rossa...\n\nAppena l'ho vista, credevo che per me\nfosse giunta la fine.\n\nE invece in qualche modo sono riuscito\na mettermi in salvo nel bosco.\n\n"
      - control:
          kind: sound
          unknown:
            - 11
            - 6
      - text: "Eh? Dici che ho buoni riflessi?\n\n\nMa no, è stata solo fortuna.\n\n\nComunque intorno al castello di Hyrule\nè pieno di quei guardiani..."
  Talk08:
    attributes: Npc_FaronWoods006
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "Era tanto che aspettavo qualcuno con\ncui parlare, ma devo aver detto qualcosa\ndi sbagliato..."
  Talk09:
    attributes: Npc_FaronWoods006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Sta' attento!"
  TalkLoop00:
    attributes: Npc_FaronWoods006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: Chiedimi quello che vuoi.
      - control:
          kind: pause
          length: long
      - text: " Rimarrò qui\na parlare con te ancora un po'."
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
            - 2
          selected_index: 0
          cancel_index: 2
          unknown: 8
  Wander00:
    attributes: Npc_FaronWoods006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 6
      - text: "Che c'è?"
      - control:
          kind: pause
          length: long
      - text: " Anche tu hai voglia di\nchiacchierare?"
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
            - 2
          selected_index: 0
          cancel_index: 2
          unknown: 8
  Wander01:
    attributes: Npc_FaronWoods006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Sono di pattuglia, tutto qua."
      - control:
          kind: pause
          length: long
      - text: "\nQuesto ponte è un punto nevralgico\nper il traffico delle merci.\nIl mio compito è tenerlo sgombero\ndai mostri."
  Wander02:
    attributes: Npc_FaronWoods006
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 1
      - text: Dove sei diretto?
      - control:
          kind: choice
          choice_labels:
            - 3
            - 4
            - 5
            - 2
          selected_index: 0
          cancel_index: 3
          unknown: 10
  WanderRain00:
    attributes: Npc_FaronWoods006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: Dici che smette?
      - control:
          kind: choice
          choice_labels:
            - 1
            - 2
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Warning00:
    attributes: Npc_FaronWoods006
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 0
      - text: "Ehi, aspetta!"
  Warning01:
    attributes: Npc_FaronWoods006
    contents:
      - control:
          kind: text_size
          percent: 125
      - control:
          kind: sound
          unknown:
            - 10
            - 0
      - text: Fermo! Pensa a quello che fai!
      - control:
          kind: text_size
          percent: 100
  Warning02:
    attributes: Npc_FaronWoods006
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 1
      - text: "Io sono ancora giovane, ma tu di più!\n\n\nIo sto facendo del mio meglio, ma\nmi pare che tu abbia intenzione\ndi spingerti ancora oltre...\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: Te lo leggo in faccia...
      - control:
          kind: pause
          length: long
      - text: " è scritto nei\ntuoi occhi."
  Warning03:
    attributes: Npc_FaronWoods006
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 1
      - text: "Cosa pensi di ottenere? Guarda\nche sparendo dalla circolazione\nnon cambierai il mondo. \nSolo una cosa cambierà di sicuro..."
      - control:
          kind: pause
          length: long
      - text: "\ne cioè...\n\n"
      - control:
          kind: sound
          unknown:
            - 8
            - 0
      - text: "... Il mio umore!\n\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Sul serio, voglio dire... pensa che razza\ndi shock subirei da un'esperienza simile!"
  Warning04:
    attributes: Npc_FaronWoods006
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 0
      - text: Adesso calmati e scendi da lì.
      - control:
          kind: pause
          length: long
      - text: "\nScendi da questa parte, intendo, non\ndall'altra parte!"
  Warning05:
    attributes: Npc_FaronWoods006
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 0
      - text: Non prendere decisioni affrettate!
      - control:
          kind: pause
          length: long
      - text: "\nLa vita ha ancora tante sorprese in\nserbo per te!"
  WarningClear00:
    attributes: Npc_FaronWoods006
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 18
      - text: Ahhh! Per favore non farlo mai più!
      - control:
          kind: pause
          length: long
      - text: "\nMi hai fatto davvero sudare freddo!"
  WarningClear01:
    attributes: Npc_FaronWoods006
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 18
      - text: "Fiuuu! Mi hai fatto rizzare i capelli!\nNon è un bel modo di scherzare, sai!"
  WarningClear02:
    attributes: Npc_FaronWoods006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 6
      - text: "Ma senti,"
      - control:
          kind: pause
          length: long
      - text: " se vuoi puoi confidarti con me."
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
            - 2
          selected_index: 0
          cancel_index: 2
          unknown: 8
