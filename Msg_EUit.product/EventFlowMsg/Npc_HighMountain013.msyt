    contents:
      - text: Arrivederci!
  "0006":
    attributes: ""
    contents:
      - text: Chi è Revali?
  near00:
    attributes: Npc_HighMountain013
    contents:
      - text: Abiti caldi!
  near01:
    attributes: Npc_HighMountain013
    contents:
      - text: Una pausa...
  near02:
    attributes: Npc_HighMountain013
    contents:
      - text: Dunque...
  near03:
    attributes: Npc_HighMountain013
    contents:
      - text: Oh oh...
  near04:
    attributes: Npc_HighMountain013
    contents:
      - text: Vero?
  near05:
    attributes: Npc_HighMountain013
    contents:
      - text: No...
  talk00:
    attributes: Npc_HighMountain013
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 19
      - text: "Mmmh? "
      - control:
          kind: pause
          length: short
      - text: "Ehi, tu! Sei un viandante?"
  talk01:
    attributes: Npc_HighMountain013
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 25
      - text: "Se stai pensando di andare in un "
      - control:
          kind: set_colour
          colour: blue
      - text: "posto\nfreddo"
      - control:
          kind: reset_colour
      - text: ", ti conviene rifornirti qui di abiti\nben caldi!"
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
            - 2
            - 3
          selected_index: 0
          cancel_index: 3
          unknown: 10
  talk02:
    attributes: Npc_HighMountain013
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 16
      - text: "Se un viandante inesperto si spinge sulle\n"
      - control:
          kind: set_colour
          colour: blue
      - text: "montagne innevate del nord "
      - control:
          kind: reset_colour
      - text: "o in "
      - control:
          kind: set_colour
          colour: blue
      - text: "luoghi\npiù elevati di questo"
      - control:
          kind: reset_colour
      - text: "... può congelare!\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Ma grazie ai miei "
      - control:
          kind: set_colour
          colour: blue
      - text: indumenti
      - control:
          kind: reset_colour
      - text: " anche i più\nsprovveduti possono resistere al gelo.\n\nQuindi su,"
      - control:
          kind: pause
          length: short
      - text: " entra e da' un'occhiata\nalla merce!"
  talk03:
    attributes: Npc_HighMountain013
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 11
      - text: Ottimo!
      - control:
          kind: pause
          length: short
      - text: " Allora accomodati pure\ne inizia a scegliere!"
  talk04:
    attributes: Npc_HighMountain013
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: Torna presto!
  talk05:
    attributes: Npc_HighMountain013
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: "Ehi, sai che ho sentito parlare di te?"
      - control:
          kind: pause
          length: long
      - text: "\nTu sei quello che ha sconfitto quel\n"
      - control:
          kind: set_colour
          colour: blue
      - text: bestione dei cieli
      - control:
          kind: reset_colour
      - text: ", vero?"
  talk06:
    attributes: Npc_HighMountain013
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Oh, amico!"
      - control:
          kind: pause
          length: longer
      - text: " Sempre in viaggio?"
  talk07:
    attributes: Npc_HighMountain013
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Grazie per essere passato di qua, anche\nse è tardi!"
  talk08:
    attributes: Npc_HighMountain013
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Questa è la bottega di articoli per la\ndifesa "
      - control:
          kind: set_colour
          colour: blue
      - text: Il pavone scarlatto
      - control:
          kind: reset_colour
      - text: ".\n\nPerché non entri a dare un'occhiata?"
      - control:
          kind: pause
          length: short
      - text: "\nAbbiamo tantissimi prodotti tessuti con\nil preziosissimo piumaggio dei Rito!\n"
      - control:
          kind: sound
          unknown:
            - 10
            - 0
      - text: "Un attimo! Non crederai mica che "
      - control:
          kind: pause
          length: short
      - text: "mi\nabbiano strappato le piume per poter\nrealizzare quella merce, vero?\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Vengono usate solo le piume morbide\ne soffici dei Rito neonati che cadono\nloro al mutar delle stagioni.\nNon si crea niente con piume vecchie."
      - control:
          kind: pause
          length: longer
      - text: "\nSe ti procuri "
      - control:
          kind: set_colour
          colour: blue
      - text: "indumenti "
      - control:
          kind: reset_colour
      - text: "belli caldi, potrai\nresistere alle temperature più rigide!"
  talk11:
    attributes: Npc_HighMountain013
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Grazie di essere passato, anche\ncon questa pioggia!"
  talk14:
    attributes: Npc_HighMountain013
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 0
      - text: Ah!
      - control:
          kind: pause
          length: long
      - text: " Forse tu sei quello che era\nandato con Teba?"
  talk15:
    attributes: Npc_HighMountain013
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Forse non ti importa granché, ma ci\ntenevo a ringraziarti per l'altra volta.\n\n"
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "Quel "
      - control:
          kind: set_colour
          colour: blue
      - text: bestione
      - control:
          kind: reset_colour
      - text: "..."
      - control:
          kind: pause
          length: short
      - text: " a dire la verità mi ha\nsempre fatto una paura tremenda...\n\n"
      - control:
          kind: sound
          unknown:
            - 6
            - 1
      - text: "Tu hai intenzione di continuare il tuo\nviaggio?"
  talk16:
    attributes: Npc_HighMountain013
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: "Ciao, amico!"
      - control:
          kind: pause
          length: short
      - text: " Hai preso tutto ciò che ti\nserviva nella mia bottega?"
      - control:
          kind: pause
          length: longer
      - text: " Allora grazie!\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Se indossi tutto il completo, potrai\nsopravvivere sia sulle "
      - control:
          kind: set_colour
          colour: blue
      - text: montagne innevate
      - control:
          kind: reset_colour
      - text: "\nche "
      - control:
          kind: set_colour
          colour: blue
      - text: in alto nel cielo
      - control:
          kind: reset_colour
      - text: "!"
  talk17:
    attributes: Npc_HighMountain013
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 13
      - text: Amico...
      - control:
          kind: pause
          length: short
      - text: " Come fai ad andare in giro così\ncombinato?"
      - control:
          kind: pause
          length: longer
      - text: " Dovresti comprare qualche\n"
      - control:
          kind: set_colour
          colour: blue
      - text: vestito
      - control:
          kind: reset_colour
      - text: " nella mia bottega!"
  talk18:
    attributes: Npc_HighMountain013
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 17
      - text: "Uff... "
      - control:
          kind: pause
          length: short
      - text: "Veramente...\n\n\n"
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "Con quella "
      - control:
          kind: set_colour
          colour: blue
      - text: "bestia "
      - control:
          kind: reset_colour
      - text: "che gironzola sul\nnostro borgo, il turismo e gli affari\ncontinueranno a soffrire.\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ciò mi porta a pensare che questo\npotrebbe essere il momento più adatto\nper fare una bella pausa!"
  talk19:
    attributes: Npc_HighMountain013
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Il cielo in questi giorni non promette\nniente di buono, ma non serve a nulla\nlamentarsi.\nLascerò che sia il "
      - control:
          kind: set_colour
          colour: blue
      - text: "patriarca "
      - control:
          kind: reset_colour
      - text: "a risolvere\nquesta situazione complicata e io mi\ndedicherò soltanto al mio lavoro."
  talk20:
    attributes: Npc_HighMountain013
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 2
      - text: Oh oh!
      - control:
          kind: pause
          length: longer
      - text: " Cioè, volevo dire... Buongiorno!\nSi stava proprio parlando di quel\n"
      - control:
          kind: set_colour
          colour: blue
      - text: bestione nei cieli
      - control:
          kind: reset_colour
      - text: "."
  talk21:
    attributes: Npc_HighMountain025
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 4
      - text: "Quel "
      - control:
          kind: set_colour
          colour: blue
      - text: "bestione "
      - control:
          kind: reset_colour
      - control:
          kind: pause
          length: short
      - text: "in realtà è l'"
      - control:
          kind: set_colour
          colour: blue
      - text: essere
      - control:
          kind: reset_colour
      - text: "\ncavalcato un tempo dal campione Revali\nper proteggere il nostro borgo!\nIn pratica, era nostro alleato. Almeno,\ncosì mi hanno detto."
  talk22:
    attributes: Npc_HighMountain013
    contents:
      - control:
          kind: sound
          unknown:
            - 2
            - 14
      - text: "Ehi! "
      - control:
          kind: pause
          length: short
      - text: Glielo volevo dire io!
  talk23:
    attributes: Npc_HighMountain025
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 0
      - text: Ma che importa!
      - control:
          kind: pause
          length: longer
      - text: "\nTanto tu non fai altro che riportare\nquello che dice il "
      - control:
          kind: set_colour
          colour: blue
      - text: patriarca
      - control:
          kind: reset_colour
      - text: "!"
  talk24:
    attributes: Npc_HighMountain013
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 19
      - text: Eh?
      - control:
          kind: pause
          length: short
      - text: " Che altro vuoi?"
      - control:
          kind: choice
          choice_labels:
            - 6
            - 3
          selected_index: 0
          cancel_index: 1
          unknown: 6
  talk25:
    attributes: Npc_HighMountain013
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 9
      - text: "Ehm... Volevo dire... "
      - control:
          kind: pause
          length: short
      - text: Torna presto!
  talk26:
    attributes: Npc_HighMountain013
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Non che io fossi lì, a dire il vero, ma...\n\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Ho sentito dire che Revali era uno di\nquelli che avevano lottato cento anni fa\ndurante la "
      - control:
          kind: set_colour
          colour: blue
      - text: grande calamità
      - control:
          kind: reset_colour
      - text: ".\nA tutti i giovani Rito viene ricordato\ncontinuamente di diventare dei guerrieri\ncoraggiosi come Revali.\nSi direbbe che lui fosse il guerriero più\nvaloroso."
  talk27:
    attributes: Npc_HighMountain025
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 25
      - text: "Le sue frecce raggiungevano sempre\nl'obiettivo per quanto fosse piccolo...\n\nRiusciva a librarsi in cielo con la rapidità\ndi una folata di vento... Questo è quanto\nsi dice di Revali.\nTu sei un Hylia, quindi dovresti sapere\ncos'è successo durante la "
      - control:
          kind: set_colour
          colour: blue
      - text: "grande\ncalamità"
      - control:
          kind: reset_colour
      - text: ", vero?"
