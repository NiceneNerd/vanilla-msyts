    attributes: Npc_GerudoDesert002
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: Proprio ora che ho trovato il sacrario...
      - control:
          kind: pause
          length: longer
      - text: "\n\n\nUgh..."
      - control:
          kind: pause
          length: longer
      - text: "\nHo sete..."
      - control:
          kind: pause
          length: longer
      - text: "\nAnf..."
      - control:
          kind: pause
          length: longer
      - text: " Non riesco a muovermi..."
      - control:
          kind: pause
          length: longer
      - text: "\nVorrei tanto..."
      - control:
          kind: pause
          length: longer
      - text: "\ncome ultimo desiderio..."
      - control:
          kind: pause
          length: longer
      - text: "\nun sorso...\nUn sorso di "
      - control:
          kind: set_colour
          colour: red
      - text: vay meets voy
      - control:
          kind: reset_colour
      - text: "..."
  Npc_GerudoMilkflost_016:
    attributes: Npc_OasisMilk_E
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Grazie a te ho potuto preparare\nun delizioso "
      - control:
          kind: set_colour
          colour: blue
      - text: vay meets voy
      - control:
          kind: reset_colour
      - text: ".\n\nPer favore, vai a chiamare "
      - control:
          kind: set_colour
          colour: blue
      - text: Pakut
      - control:
          kind: reset_colour
      - text: ".\nDille che appena torna qui le preparo\nil "
      - control:
          kind: set_colour
          colour: blue
      - text: vay meets voy
      - control:
          kind: reset_colour
      - text: " più buono della storia.\nDai retta a me! Appena sentirà parlare\ndi cocktail, correrà qui alla velocità\ndella luce!"
  Npc_GerudoMilkflost_017:
    attributes: Npc_OasisMilk_E
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 0
      - text: "Che cosa?\n\n\nPakut è nei guai?\n\n\n"
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Vorrei aiutarla, ma senza ghiaccio\nnon posso preparare quel cocktail...\n\nAlla "
      - control:
          kind: set_colour
          colour: red
      - text: "ghiacciaia "
      - control:
          kind: reset_colour
      - text: "dovrebbe essercene\nabbastanza, ma io non ce la faccio\na camminare fin là..."
      - control:
          kind: single_choice
          label: 22
  Npc_GerudoMilkflost_018:
    attributes: Npc_OasisMilk_E
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "In effetti è da un po' che non la vedo...\n\n\nNon pensavo Pakut si trovasse in una\nsituazione del genere...\n\n"
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "E io dovevo finire il ghiaccio proprio\nadesso..."
      - control:
          kind: choice
          choice_labels:
            - 20
            - 21
          selected_index: 0
          cancel_index: 1
          unknown: 6
