      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Roccia che rocce!
      - control:
          kind: pause
          length: longer
      - text: "\nCioè ci siamo battuti contro quell'affare!\n\nE non ho nemmeno avuto tanta fifa come\npensavo..."
  talk21:
    attributes: Npc_Goron020_2
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
