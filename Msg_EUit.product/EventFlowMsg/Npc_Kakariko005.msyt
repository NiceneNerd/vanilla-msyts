  Talk08:
    attributes: Npc_Kakariko005
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: "Che sorpresa, Link."
      - control:
          kind: pause
          length: short
      - text: "\nEh sì, io adoro i coccò."
      - control:
          kind: pause
          length: long
      - text: "\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Forse non te l'ho mai detto..."
      - control:
