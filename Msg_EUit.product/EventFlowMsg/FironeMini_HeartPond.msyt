  Lady19:
    attributes: Npc_SouthernVillage015
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: "Non avrei mai pensato di incontrarti nel\nbel mezzo di una tempesta.\n\nSei tu..."
      - control:
          kind: pause
          length: longer
      - text: " il voy dei miei miraggi?"
