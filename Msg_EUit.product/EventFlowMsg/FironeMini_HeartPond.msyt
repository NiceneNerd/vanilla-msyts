          length: longer
      - text: "\n\n"
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - control:
          kind: pause
          length: longer
      - text: "No, non può essere."
      - control:
          kind: pause
          length: longer
      - text: "\n\n\nQuesto è il famoso laghetto degli\ninnamorati..."
      - control:
          kind: pause
