          kind: pause
          length: short
      - text: " prega di non\ndoverli mai incontrare."
  RetMSG_Futa_00_1:
    attributes: Npc_Road_017
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "I mostri che appaiono solo di notte\nsono pericolosi."
      - control:
          kind: pause
          length: longer
      - text: " Scheletri e pipistrelli..."
      - control:
          kind: pause
          length: longer
      - text: "\nSolo a pensarci..."
      - control:
          kind: pause
          length: short
      - text: " Brrr...!"
