      - text: parco Sadino
      - control:
          kind: reset_colour
      - text: ", dove si trova la\nstatua di un... cavallo."
  MSG_Futa_00_1:
    attributes: Npc_Road_017
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Mi piace cavalcare con questo cavallo\nfino al "
      - control:
          kind: set_colour
          colour: blue
      - text: parco Sadino
      - control:
          kind: reset_colour
      - text: ", dove si trova la\nstatua di un... cavallo. Ci vado pure oggi!"
  MSG_Futa_01:
    attributes: Npc_Road_017
