  Quest_finish_03:
    attributes: Npc_oasis003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 24
      - text: "Papizia è un regalo dalla precedente...\nda mia madre.\n\n"
      - control:
          kind: sound
