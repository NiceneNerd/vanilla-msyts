      - control:
          kind: set_colour
          colour: blue
      - text: Ruta
      - control:
          kind: reset_colour
      - text: "!\n\n...\n\n\n... ... ...\n\n\n"
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: "Ma guardati, sei così modesto che non\napri nemmeno bocca!"
      - control:
          kind: pause
          length: longer
      - text: " Hai tutto il diritto\ndi vantarti...\nDopotutto"
      - control:
