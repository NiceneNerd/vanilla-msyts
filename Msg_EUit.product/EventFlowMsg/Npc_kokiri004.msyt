  Npc_kokiri004_Near000:
    attributes: Npc_kokiri004
    contents:
      - text: "Funghettini, micetini!"
  Npc_kokiri004_Near020:
    attributes: Npc_kokiri004
    contents:
      - text: Vado a caccia di funghi!
  Npc_kokiri004_Sell000:
    attributes: Npc_kokiri004
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Il grande albero Deku mi aveva già\nsussurrato che presto saresti arrivato,\nsignor Eroe.\nPensavo che saresti stato affamato,"
      - control:
          kind: pause
          length: short
      - text: "\nquindi sono svolazzato nel bosco a\nraccogliere tanti bei funghetti per te!\nSe vuoi vendermi qualcosa, vieni a\nparlarmi."
  Npc_kokiri004_Sell010:
    attributes: Npc_kokiri004
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: "Grazie, signor Eroe!"
  Npc_kokiri004_Sell010b:
    attributes: Npc_kokiri004
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: "Grazie dell'acquisto, signor Eroe!"
  Npc_kokiri004_Sell010c:
    attributes: Npc_kokiri004
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: "Per fortuna ho raccolto tutti questi\nfunghetti!"
  Npc_kokiri004_Sell010d:
    attributes: Npc_kokiri004
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: Mangia più che puoi!
  Npc_kokiri004_Sell020:
    attributes: Npc_kokiri004
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: "Oh, pescolina! Grazie che mi compri\ntutti i funghetti!"
      - control:
          kind: pause
          length: long
      - text: " Vado subito a\nraccoglierne altri.\nIl grande albero Deku"
      - control:
          kind: pause
          length: short
      - text: " mi aveva già\nsussurrato che presto saresti arrivato,\nsignor Eroe.\nPensavo che saresti stato affamato,"
      - control:
          kind: pause
          length: short
      - text: "\nquindi sono svolazzato nel bosco e ho\nraccolto tanti bei funghetti per te!\nSe vuoi vendermi qualcosa, fammelo\nsapere, va bene?"
  Npc_kokiri004_Sell030:
    attributes: Npc_kokiri004
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 9
      - text: "Oh, pescolina! Grazie che mi compri\ntutti i funghetti! Vado subito a\nraccoglierne altri."
  Npc_kokiri004_Talk000:
    attributes: Npc_kokiri004
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: "Oh, pescolina! "
      - control:
          kind: pause
          length: short
      - text: "Sai che ti aspettavo?!\n\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Il grande albero Deku mi aveva già\nsussurrato che presto saresti arrivato,\nsignor Eroe.\nPensavo che saresti stato affamato,"
      - control:
          kind: pause
          length: short
      - text: "\nquindi sono svolazzato nel bosco a\nraccogliere tanti bei funghetti per te!"
  Npc_kokiri004_Talk010:
    attributes: Npc_kokiri004
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 0
      - text: "Questa è la "
      - control:
          kind: set_colour
          colour: blue
      - text: bottega del fungo
      - control:
          kind: reset_colour
      - text: "! I miei\nmiceti sono deliziosi, "
      - control:
          kind: pause
          length: short
      - text: "sospettosi, ma\nsoprattutto molto "
      - control:
          kind: pause
          length: short
      - text: "preziosi!\n"
      - control:
          kind: sound
          unknown:
            - 7
            - 1
      - text: "Guarda che belli! "
      - control:
          kind: pause
          length: short
      - text: "Perché non compri\nqualcosa? Oppure preferisci forse\nsbarazzarti di qualcosa?"
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
            - 2
          selected_index: 0
          cancel_index: 2
          unknown: 8
  Npc_kokiri004_Talk020:
    attributes: Npc_kokiri004
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 0
      - text: "Questa è la "
      - control:
          kind: set_colour
          colour: blue
      - text: bottega del fungo
      - control:
          kind: reset_colour
      - text: "! Il sole\nè tramontato, ma i miei miceti sono\nancora in ottimo stato!\n"
      - control:
          kind: sound
          unknown:
            - 7
            - 1
      - text: "Guarda che belli! "
      - control:
          kind: pause
          length: short
      - text: "Vuoi comprare\nqualcosa? O forse preferisci vendermi\nqualcosa?"
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
            - 2
          selected_index: 0
          cancel_index: 2
          unknown: 8
  Npc_kokiri004_Talk030:
    attributes: Npc_kokiri004
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 18
      - text: "Nessuno mi aveva mormorato che vai\nmatto per i funghi, signor Eroe!\n\n"
      - control:
          kind: sound
          unknown:
            - 7
            - 1
      - text: "Dovrò volare di nuovo nel bosco a\nraccogliere dei funghi freschi!\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Sarei felice di comprare qualcosa da te.\nC'è qualcosa che desideri vendere?"
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
            - 2
          selected_index: 0
          cancel_index: 2
          unknown: 8
  Npc_kokiri004_Talk040:
    attributes: Npc_kokiri004
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Portami qualsiasi cosa che non ti serve!\nAccetto tutti gli oggetti, anche se non\nsono funghetti!"
  Npc_kokiri004_Talk050:
    attributes: Npc_kokiri004
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Eh, sì! "
      - control:
          kind: pause
          length: short
      - text: "C'è sempre qualche funghetto che\nsi cela nel buio, in attesa di qualcuno\nche lo colga e lo faccia suo! Ricordalo!"
  Npc_kokiri004_Talk060:
    attributes: Npc_kokiri004
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: "Grazie di aver comprato "
      - control:
          kind: pause
          length: short
      - text: "così tanti funghi!\nSei un vero signor Eroe!"
  Npc_kokiri004_Talk070:
    attributes: Npc_kokiri004
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 4
      - text: "Qui siamo proprio dentro lo stomaco\ndel grande albero Deku. Non lo trovi\nanche tu molto affascinante?\n"
      - control:
          kind: sound
          unknown:
