  NewText_00:
    attributes: Npc_NorthHatelHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: "Oh, scusami."
      - control:
          kind: pause
          length: short
      - text: "\nEro perso nei miei pensieri."
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
          selected_index: 0
          cancel_index: 1
          unknown: 6
  NewText_01:
    attributes: Npc_NorthHatelHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 12
      - text: "L'idea di raccontarlo a qualcuno\nnon mi esalta, comunque..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Sul letto di questo fiume si intravede\nqualcosa..."
      - control:
          kind: pause
          length: short
      - text: " A giudicare dalla forma, sono\nquasi certo che si tratti di uno scrigno.\nMi sto arrovellando il cervello per\ncapire come tirarlo fuori dal fiume."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Immagino che mi servirebbe una\nspecie di "
      - control:
          kind: pause
          length: short
      - text: "macchinario o qualcosa\ndel genere... "
      - control:
          kind: pause
          length: short
      - text: Non credi anche tu?
  NewText_03:
    attributes: Npc_NorthHatelHatago001
    contents:
      - text: "Se la vista non mi inganna,"
      - control:
          kind: pause
          length: short
      - text: " la forma\ncorrisponde a quella di uno scrigno..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Immagino che per tirarlo fuori da lì mi\nservirebbe una specie di "
      - control:
          kind: pause
          length: short
      - text: "macchinario,\no qualcosa del genere... "
      - control:
          kind: pause
          length: short
      - text: Tu che dici?
  NewText_07:
    attributes: Npc_NorthHatelHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 0
      - text: Un tesoro?
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 8
            - 0
      - text: "Diamine! "
      - control:
          kind: pause
          length: longer
      - text: Che rabbia!
  NewText_08:
    attributes: Npc_NorthHatelHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 6
      - text: "Ah, sì?"
      - control:
          kind: pause
          length: longer
      - text: " E ne è valsa la pena?"
  NewText_09:
    attributes: Npc_NorthHatelHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Non so che razza di poteri hai, ma\n"
      - control:
          kind: pause
          length: short
      - text: "non avevo mai visto niente di simile\nprima d'ora!\nA proposito..."
      - control:
          kind: pause
          length: longer
      - text: " Non vuoi dirmi cosa\nconteneva lo scrigno?"
      - control:
          kind: choice
          choice_labels:
            - 4
            - 5
          selected_index: 0
          cancel_index: 2
          unknown: 6
  NewText_10:
    attributes: Npc_NorthHatelHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: Non so proprio che fare...
      - control:
          kind: pause
          length: longer
      - text: " Io e l'acqua\nnon andiamo molto d'accordo."
  NewText_11:
    attributes: Npc_NorthHatelHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 18
      - text: "Sono così curioso di sapere cosa\ncontiene..."
      - control:
          kind: pause
          length: short
      - text: " Tanto da non riuscire\nnemmeno a chiudere occhio."
  NewText_12:
    attributes: Npc_NorthHatelHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Se solo fossi riuscito a tirarlo fuori,\nora sarei ricco sfondato"
      - control:
          kind: pause
          length: short
      - text: " e le donne\nmi guarderebbero con altri occhi..."
  NewText_13:
    attributes: Npc_NorthHatelHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Beh, ora che so cosa c'era dentro, posso\ndormire sonni tranquilli. Grazie!"
  NewText_14:
    attributes: Npc_NorthHatelHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Beh, meglio così. Io non avrei saputo\nche farmene..."
  NewText_15:
    attributes: Npc_NorthHatelHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Finalmente non perderò più il sonno,\nora che so cosa conteneva."
      - control:
          kind: pause
          length: longer
      - text: " Grazie..."
  NewText_16:
    attributes: Npc_NorthHatelHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Oh! "
      - control:
          kind: pause
          length: longer
      - text: Lo scrigno!
  NewText_17:
    attributes: Npc_NorthHatelHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: È stato incredibile!
  NewText_18:
    attributes: Npc_NorthHatelHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Non posso crederci...\n"
      - control:
          kind: pause
          length: short
      - text: Lo scrigno?!
  NewText_19:
    attributes: Npc_NorthHatelHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 0
      - text: Ma che diamine...?!
  NewText_20:
    attributes: Npc_NorthHatelHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Eh?
  NewText_23:
    attributes: Npc_NorthHatelHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 0
      - text: Geniale!
      - control:
          kind: pause
          length: longer
      - text: " Io non sarei mai riuscito a\ncapire come recuperare quello scrigno!\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Forza, vediamo cosa contiene!"
  NewText_26:
    attributes: Npc_NorthHatelHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Mmh...
      - control:
          kind: pause
          length: short
      - text: " Come posso fare?"
  NewText_27:
    attributes: Npc_NorthHatelHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Aaah. "
      - control:
          kind: pause
          length: long
      - text: Arrivederci.
  NewText_28:
    attributes: Npc_NorthHatelHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ho destato la tua curiosità, eh?"
  NewText_29:
    attributes: Npc_NorthHatelHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: "Uao, ma chi sei?! Come hai fatto\na spostare quello scrigno?\n\n"
      - control:
          kind: sound
          unknown:
            - 5
            - 0
      - text: "Davvero... Sono rimasto qui giorni interi\na spremermi le meningi per capire come\ntirare fuori quello scrigno..."
  NewText_30:
    attributes: Npc_NorthHatelHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 0
      - text: "Uao, ma chi sei?! Che poteri hai?!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: Non importa... è fantastico!
      - control:
          kind: pause
          length: short
      - text: " Super\nfantastico!"
      - control:
          kind: pause
          length: short
      - text: " Ora sbrigati ad aprire quello\nscrigno e guarda cosa c'è dentro!"
  NewText_31:
    attributes: Npc_NorthHatelHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 0
      - text: "Uao, ma chi sei?!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Davvero... Sono rimasto qui giorni interi\na spremermi le meningi per capire come\ntirare fuori questo scrigno..."
