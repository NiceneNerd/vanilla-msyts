  Appear02:
    attributes: Npc_King_Parasail002
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 13
      - text: "Guarda quella torre, spuntata fuori\ndal terreno! E come quella ce ne sono\nmolte altre!\nChissà se..."
      - control:
