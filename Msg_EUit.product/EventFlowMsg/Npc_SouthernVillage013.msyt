  talk19:
    attributes: Npc_SouthernVillage013
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 4
      - text: "Potrebbero esserci ancora dei\n"
      - control:
          kind: set_colour
          colour: red
      - text: "tesori "
      - control:
