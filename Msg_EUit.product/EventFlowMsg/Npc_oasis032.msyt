          cancel_index: 1
          unknown: 6
  Talk_05:
    attributes: Npc_oasis032
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "Che stanchezza...\nLasciami in pace un attimo..."
  Talk_06:
    attributes: Npc_oasis032
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "La "
      - control:
          kind: set_colour
          colour: blue
      - text: corsa di foche delle sabbie
      - control:
          kind: reset_colour
      - text: "...\n\n\n... è una gara di velocità alla guida\ndelle "
      - control:
          kind: set_colour
          colour: blue
      - text: foche delle sabbie
      - control:
          kind: reset_colour
      - text: ". È una\ntradizione per noi Gerudo.\nSe esci dalla porta "
      - control:
          kind: set_colour
          colour: blue
      - text: "sud-est "
      - control:
          kind: reset_colour
      - text: "e continui\nverso destra arrivi alla pista.\n\nSe ti interessa, vai a dare un'occhiata."
  Talk_07:
    attributes: Npc_oasis032
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "A proposito...\nUltimamente Fraji...\n\n... ha cominciato a parlare in modo\nun po' strano! Chissà che è successo..."
