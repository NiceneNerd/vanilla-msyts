  talk00:
    attributes: Obj_DiaryGerudo_A_01
    contents:
      - text: "Questa è la testimonianza delle ricerche\nsulla "
      - control:
          kind: set_colour
          colour: red
      - text: "prova dell'eroe"
      - control:
