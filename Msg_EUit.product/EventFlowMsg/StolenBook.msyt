  Npc_Assassin_1:
    attributes: NPC_PublicVoice
    contents:
      - control:
          kind: sound2
          unknown:
            - 0
            - 205
      - text: Sono qui.
      - control:
          kind: pause
          length: short
      - text: "\nNon c'è bisogno di gridare."
  Npc_Assassin_2:
    attributes: NPC_PublicVoice
    contents:
      - text: Mi sorprendi con questa domanda...
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Dorian... Proprio tu che hai\nabbandonato la nostra banda..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Sai meglio di tutti cosa significa\nuna scelta del genere."
  Npc_Assassin_3:
    attributes: NPC_PublicVoice
    contents:
      - text: Però...
      - control:
          kind: pause
          length: longer
      - text: "\nDevo ammettere che hai fatto un\nottimo lavoro fino all'ultimo."
  Npc_Assassin_4:
    attributes: NPC_PublicVoice
    contents:
      - text: Ma penserò dopo a sistemarti.
      - control:
          kind: pause
          length: longer
      - text: "\nPrima di tutto..."
      - control:
          kind: pause
          length: longer
  Npc_Assassin_5:
    attributes: NPC_PublicVoice
    contents:
      - text: "... annienterò questo spadaccino\nche ama nascondersi!"
  Npc_Assassin_6:
    attributes: NPC_PublicVoice
    contents:
      - text: "... annienterò questo spadaccino\nche ama nascondersi!\n\nPagherà amaramente per quello che ha\nfatto passare al mio padrone!!!"
