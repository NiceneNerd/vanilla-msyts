  Npc_Kakariko002_R_004:
    attributes: Npc_Kakariko001
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 4
      - text: "\""
      - control:
          kind: set_colour
          colour: red
      - text: "La protezione degli antenati sarà\nconcessa al prescelto dalla sfera."
      - control:
          kind: reset_colour
      - text: "\""
      - control:
          kind: pause
          length: longer
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Secondo Paya,"
      - control:
          kind: pause
          length: long
      - text: " questo \""
      - control:
          kind: set_colour
          colour: red
      - text: "prescelto dalla\nsfera"
      - control:
          kind: reset_colour
      - text: "\" "
      - control:
          kind: pause
          length: short
      - text: sei proprio tu.
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Così ho passato la notte a studiare\nil mistero della "
      - control:
          kind: set_colour
          colour: blue
      - text: sfera
      - control:
          kind: reset_colour
      - text: ","
      - control:
          kind: pause
          length: short
      - text: " senza\nchiudere occhio e..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "... ho concluso che questa \""
      - control:
          kind: set_colour
          colour: red
      - text: "protezione\ndegli antenati"
      - control:
          kind: reset_colour
      - text: "\" si riferisce a un sacrario\nancestrale!"
      - control:
          kind: pause
          length: short
      - text: "\nSì, proprio come quello che sovrasta\nil villaggio."
      - control:
          kind: pause
          length: short
      - text: " E la"
      - control:
          kind: set_colour
          colour: blue
      - text: " sfera "
      - control:
          kind: reset_colour
      - text: "dev'essere\nla "
      - control:
          kind: set_colour
          colour: red
      - text: "chiave che apre "
      - control:
          kind: reset_colour
      - text: "quel sacrario.\n"
      - control:
          kind: sound
          unknown:
            - 9
            - 17
      - text: Proprio quando volevo dirti tutto ciò...
      - control:
          kind: pause
          length: short
      - text: "\nè capitata..."
      - control:
          kind: pause
          length: long
      - text: " questa disgrazia..."
      - control:
          kind: pause
          length: longer
  Npc_Kakariko002_R_006:
    attributes: Npc_Kakariko002
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 37
      - text: "Non facevo altro che lucidare la "
      - control:
          kind: set_colour
          colour: blue
      - text: sfera
      - control:
          kind: reset_colour
      - text: ","
      - control:
          kind: pause
          length: short
      - text: "\nda mattina a sera, senza sosta."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "La tenevo d'occhio tutto il giorno!"
      - control:
          kind: pause
          length: short
      - text: "\nChi l'ha rubata deve essersi\nindubbiamente intrufolato in piena notte."
      - control:
          kind: pause
          length: longer
      - text: "\n"
      - control:
          kind: sound
          unknown:
            - 11
            - 1
      - text: "Il fatto è che nessun estraneo ha\nil permesso di entrare qui."
      - control:
          kind: pause
          length: longer
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "E nessuno di questo villaggio oserebbe\nmai compiere un atto simile..."
      - control:
          kind: pause
          length: short
      - text: "\nAlmeno spero!"
      - control:
          kind: pause
          length: longer
  Npc_Kakariko002_R_008:
    attributes: Npc_Kakariko002
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 12
      - text: "Tenevo sempre d'occhio la "
      - control:
          kind: set_colour
          colour: blue
      - text: sfera
      - control:
          kind: reset_colour
      - text: ","
      - control:
          kind: pause
          length: short
      - text: "\nda mattina a sera, quindi il ladro deve\nessersi intrufolato nel cuore della notte.\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Dorian e Vogado mi hanno confermato\nche non hanno visto nessuno andare in\ngiro, oltre agli abitanti del villaggio."
      - control:
          kind: pause
          length: longer
      - text: "\n"
      - control:
          kind: sound
          unknown:
            - 11
            - 12
      - text: "Ma la gente che vive qui non farebbe\nmai una cosa del genere!"
      - control:
          kind: pause
          length: longer
      - text: "\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Comunque, non è passato molto tempo\ndal furto..."
      - control:
          kind: pause
          length: short
      - text: "\n\nQuindi il colpevole potrebbe essere\nancora nascosto nei paraggi."
      - control:
          kind: pause
          length: short
      - text: "\n\n"
      - control:
          kind: sound
          unknown:
            - 3
            - 1
      - text: "Ma se vedete dei movimenti sospetti\ndurante la notte, "
      - control:
          kind: set_colour
          colour: red
      - text: "mi raccomando, non vi\navvicinate troppo"
      - control:
          kind: reset_colour
      - text: "!"
      - control:
          kind: pause
          length: longer
  Npc_Kakariko002_R_009:
    attributes: Npc_Kakariko002
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 1
      - text: "Qualcuno con cattive intenzioni si è\nintrufolato in questo luogo sacro.\n\n"
      - control:
          kind: sound
          unknown:
            - 5
            - 1
      - text: "L'importante è che la nonna stia bene,\nma ho tanta paura..."
  Npc_Kakariko002_R_010:
    attributes: Npc_Kakariko001
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 17
      - text: Link!
      - control:
          kind: pause
          length: short
      - text: " Paya ha tanta paura. I ladri\npotrebbero essere ancora da queste\nparti!\nSe puoi, resta con lei per un po'!"
      - control:
          kind: pause
          length: longer
  Npc_Kakariko002_R_011:
    attributes: Npc_Kakariko002
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 18
      - text: Maestro Link...
      - control:
          kind: pause
          length: short
      - text: "\nNon so come ringraziarvi per avermi\nfatto compagnia tutto il giorno.\nOra sento di avere anche io il coraggio\ndi affrontare i ladri! Non posso stare\na piangere tutto il tempo!\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ho chiesto anche a Dorian e Vogado, ma\nhanno detto di non aver visto nessuno a\nparte gli abitanti del villaggio..."
      - control:
          kind: pause
          length: longer
      - text: "\n"
      - control:
          kind: sound
          unknown:
            - 5
            - 12
      - text: "Cercherò di ricordare tutto quello\nche è successo la notte in cui è stata\nrubata la "
      - control:
          kind: set_colour
          colour: blue
      - text: sfera
      - control:
          kind: reset_colour
      - text: "...\nComunque, il colpevole potrebbe\nessere ancora nei paraggi."
      - control:
          kind: pause
          length: short
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Se vedete dei movimenti sospetti\ndurante la notte, "
      - control:
          kind: set_colour
          colour: red
      - text: "mi raccomando, non\nvi avvicinate troppo"
      - control:
          kind: reset_colour
      - text: "!"
      - control:
          kind: pause
          length: longer
  Npc_Kakariko006_01:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: Anche tu qui per osservare il castello?
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 16
      - text: "Quella luce è proprio sinistra, lo so...\nma di notte emana un bagliore così\nbello che ogni tanto vengo ad ammirarlo.\n"
      - control:
          kind: sound
          unknown:
            - 11
            - 13
      - text: "Eh, ma non lo dire a nessuno al villaggio,\nsoprattutto alle vecchie pettegole!"
  Npc_Kakariko006_02:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 6
      - text: "Anche tu qui per osservare il castello?\n\n\nQuella luce è proprio sinistra, lo so...\nma di notte emana un bagliore così\nbello che ogni tanto vengo ad ammirarlo.\n"
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Che strano però, stanotte c'è un sacco di\ngente in giro... \n\nHo appena visto "
      - control:
          kind: set_colour
          colour: blue
      - text: Dorian
      - control:
          kind: reset_colour
      - text: " che correva\nverso le "
      - control:
          kind: set_colour
          colour: red
      - text: colline là dietro
      - control:
          kind: reset_colour
      - text: ".\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "No, non ci ho parlato, sembra avere\nmolta fretta!"
  Npc_Kakariko006_03:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: "Che splendida giornata.\n\n\nMi sa che vado a fare un giro prima di\ntornare a casa."
  Npc_Kakariko006_Near:
    attributes: Npc_Kakariko006
    contents:
      - text: Ehm...
  Npc_Kakariko007_101:
    attributes: Npc_Kakariko007
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 14
      - text: Lo so che sei là!
      - control:
          kind: pause
          length: longer
      - text: "\nEsci fuori, non nasconderti!"
  Npc_Kakariko007_102:
    attributes: Npc_Kakariko007
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 0
      - text: Siete stati voi a rubare la sfera!
  Npc_Kakariko007_103:
    attributes: Npc_Kakariko007
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 14
      - text: "Perché l'avete rubata?"
  Npc_Kakariko007_104:
    attributes: Npc_Kakariko007
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 44
      - text: Ugh...
      - control:
          kind: pause
          length: longer
  Npc_Kakariko007_105:
    attributes: Npc_Kakariko007
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "Mi stavano seguendo e non mi sono\naccorto di niente...\n\nTutto quello che è successo è colpa mia!"
  Npc_Kakariko007_106:
    attributes: Npc_Kakariko007
    contents:
      - control:
          kind: sound
          unknown:
            - 2
            - 12
      - text: Ma poi ho deciso di darci un taglio!
      - control:
          kind: pause
          length: short
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ho pensato di poter proteggere da\nsolo le mie bambine."
      - control:
          kind: pause
          length: short
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 8
            - 45
      - text: Ma quella è gente orribile.
      - control:
          kind: pause
          length: short
      - text: "\nSe la sono presa anche con Impa e Paya."
  Npc_Kakariko007_107:
    attributes: Npc_Kakariko007
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Già lo saprai, ma questa "
      - control:
          kind: set_colour
          colour: blue
      - text: sfera
      - control:
          kind: reset_colour
      - control:
          kind: pause
          length: short
      - text: " è\nuna "
      - control:
          kind: set_colour
          colour: red
      - text: "chiave in grado di rivelare\nun sacrario ancestrale"
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Noi non siamo riusciti a usarla,"
      - control:
          kind: pause
          length: short
      - text: "\nma tu sicuramente..."
  Npc_Kakariko007_108:
    attributes: Npc_Kakariko007
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "Scusa la sfrontatezza,"
      - control:
          kind: pause
          length: short
      - text: " ma ti prego\ndi mantenere il segreto su ciò che è\nsuccesso.\nNon voglio far soffrire ulteriormente\nle mie innocenti bambine"
      - control:
          kind: pause
          length: short
      - text: " Coco e Prisca!"
      - control:
          kind: pause
          length: short
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "So che non basta a espiare le mie colpe,"
      - control:
          kind: pause
          length: short
      - text: "\nma da oggi in poi proteggerò il villaggio\ne la mia famiglia a costo della vita!"
      - control:
          kind: pause
          length: longer
  Npc_Kakariko007_109:
    attributes: Npc_Kakariko007
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 1
      - text: "A questo punto non c'è motivo di\nnasconderlo...\n\n"
      - control:
          kind: sound
          unknown:
            - 11
            - 12
      - text: "In passato...\n\n\nFacevo parte della banda degli Yiga."
      - control:
          kind: pause
          length: longer
      - text: "\n\n\n"
      - control:
          kind: sound
          unknown:
            - 5
            - 1
      - text: "Ma poi ho incontrato mia moglie "
      - control:
          kind: pause
          length: short
      - text: "e sono\nnate le mie figlie...\n\nCosì ho deciso di lasciare\nl'organizzazione."
      - control:
          kind: pause
          length: longer
      - text: "\n\n"
      - control:
          kind: sound
          unknown:
            - 9
            - 12
      - text: "E per questo, mia moglie ha perso\nla vita..."
      - control:
          kind: pause
          length: longer
  Npc_Kakariko007_110:
    attributes: Npc_Kakariko007
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 14
      - text: "È un osso duro, ma è da solo!\nNon ci vorrà molto a farlo fuori!"
  Npc_Kakariko007_111:
    attributes: Npc_Kakariko007
    contents:
      - control:
          kind: sound
