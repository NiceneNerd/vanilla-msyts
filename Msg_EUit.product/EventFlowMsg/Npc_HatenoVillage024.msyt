  talk09:
    attributes: Npc_HatenoVillage024
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 6
      - text: "Questa?\n\n\nÈ una "
      - control:
          kind: set_colour
          colour: blue
      - text: lanterna
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: pause
          length: longer
      - text: " Sai, quelli del "
      - control:
