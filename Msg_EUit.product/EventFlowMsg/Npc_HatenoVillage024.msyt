          unknown:
            - 3
            - 16
      - text: "Mi spiace, ma..."
      - control:
          kind: pause
          length: short
      - text: " adesso devo prepararmi\nper andare a dormire."
  talk15:
    attributes: Npc_HatenoVillage024
    contents:
