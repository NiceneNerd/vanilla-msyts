  Rest_00:
    attributes: Npc_Road_012
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Hai sentito parlare della leggendaria\ncacciatrice di tesori?"
      - control:
          kind: pause
          length: long
      - text: " Non c'è bisogno\ndi rispondere, sono io!"
      - control:
          kind: pause
          length: long
      - text: "\nI migliori ricordi delle mie ricerche\nsi trovano al "
      - control:
          kind: set_colour
          colour: blue
      - text: "bazar Arsur "
      - control:
          kind: reset_colour
      - text: "e allo\n"
      - control:
          kind: set_colour
          colour: blue
      - text: stallaggio del canyon Gerudo
      - control:
          kind: reset_colour
      - text: "."
  RetMSG_Gerud_00:
    attributes: Npc_Road_012
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: Io mi chiamo
      - control:
          kind: pause
          length: short
      - text: " W-a-s-k!\n\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Sono una leggendaria cacciatrice\ndi tesori!"
      - control:
          kind: pause
          length: long
      - text: "\n\n"
      - control:
          kind: sound
          unknown:
            - 1
            - 11
      - text: Come mi capita un tesoro tra le mani...
      - control:
          kind: pause
          length: short
      - text: "\no un voy..."
      - control:
          kind: pause
          length: long
      - text: " eh eh eh!"
  RetMSG_Gerud_00_1:
    attributes: Npc_Road_012
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 11
      - text: "Come mi capita un tesoro o un voy\ntra le mani..."
      - control:
          kind: pause
          length: long
      - text: " eh eh eh!"
  RetMSG_Gerud_01:
    attributes: Npc_Road_012
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 6
      - text: "Hai intenzione di attraversare il deserto\ncon quel tuo fisichino?"
      - control:
          kind: pause
          length: long
      - text: " Temerario,\nproprio come le Gerudo!\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Mi sei simpatico, perciò lascia che ti\ndica una cosa che ho saputo..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Gira voce che ci siano dei tesori nascosti\nsotterrati da qualche parte nel deserto."
  RetMSG_Gerud_01_1:
    attributes: Npc_Road_012
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Gira voce che ci siano dei tesori nascosti\nsotterrati da qualche parte nel deserto."
  RetMSG_Gerud_02:
