    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "Credo che attraversare il deserto sia\narduo anche per me..."
      - control:
          kind: pause
          length: long
      - text: "\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Se solo avessimo le "
      - control:
          kind: set_colour
          colour: red
      - text: foche delle sabbie
      - control:
          kind: reset_colour
      - text: "..."
      - control:
          kind: pause
          length: short
      - text: "\nPotremmo viaggiare più facilmente sulla\nsabbia..."
  RetMSG_Gerud_02_1:
    attributes: Npc_Road_012
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Se avessimo le "
      - control:
          kind: set_colour
          colour: red
      - text: foche delle sabbie
      - control:
          kind: reset_colour
      - text: ",\npotremmo viaggiare più facilmente\nsulla sabbia..."
  RetMSG_Gerud_03:
    attributes: Npc_Road_012
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 6
      - text: "Come fai ad avere così tanta forza e\ndeterminazione con quel tuo corpicino?\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Pochi viandanti riescono a trovare il\n"
      - control:
          kind: set_colour
          colour: red
      - text: fossile di balena gigante
      - control:
          kind: reset_colour
      - text: ", "
      - control:
          kind: pause
          length: short
      - text: "ma tu potresti\nriuscirci camminando nel deserto!"
  RetMSG_Gerud_03_1:
    attributes: Npc_Road_012
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Pochi viandanti riescono a trovare il\n"
      - control:
          kind: set_colour
          colour: red
      - text: fossile di balena gigante
      - control:
          kind: reset_colour
      - text: ", "
      - control:
          kind: pause
