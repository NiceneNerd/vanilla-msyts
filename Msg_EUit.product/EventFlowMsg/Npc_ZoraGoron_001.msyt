  talk07:
    attributes: Npc_ZoraGoron_001
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "Devo dire che il villaggio degli Zora è\nun posto proprio tosto!"
      - control:
          kind: pause
          length: longer
      - text: " Con tutta\nquest'acqua che ci cade addosso!"
  talk08:
    attributes: Npc_ZoraGoron_001
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 14
      - text: "A differenza di qua, lassù fa caldissimo."
  talk09:
    attributes: Npc_ZoraGoron_001
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 22
      - text: "Ma che dici?!\nNon conosci il "
      - control:
          kind: set_colour
          colour: blue
      - text: Monte Morte
      - control:
          kind: reset_colour
      - text: "?\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "In una bella giornata,"
      - control:
          kind: pause
          length: short
      - text: " il vulcano si vede\nda tutta Hyrule!\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Sai, noi Goron mangiamo i sassi."
      - control:
          kind: pause
          length: longer
      - text: " Perciò\nci conviene vivere vicino al "
      - control:
          kind: set_colour
          colour: blue
      - text: Monte Morte
      - control:
          kind: reset_colour
      - text: "."
