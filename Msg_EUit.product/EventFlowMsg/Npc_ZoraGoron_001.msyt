          kind: sound
          unknown:
            - 7
            - 11
      - text: "Proprio così, roccia! "
      - control:
          kind: pause
          length: longer
      - text: "Per noi i sassi\nsono deliziosi.\n\n"
      - control:
          kind: sound
          unknown:
            - 0
