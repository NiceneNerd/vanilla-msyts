      - control:
          kind: pause
          length: longer
      - text: "\n\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Se ti avvicini camminando accucciato,"
      - control:
          kind: pause
          length: short
      - text: "\nper il nemico sarà ancora più difficile\nnotarti!"
      - control:
          kind: choice
          choice_labels:
            - 16
            - 17
          selected_index: 0
          cancel_index: 1
          unknown: 6
