    attributes: Npc_Kakariko013
    contents:
      - control:
          kind: sound
          unknown:
            - 2
            - 14
      - text: "Sì, un vecchietto che coltiva "
      - control:
          kind: set_colour
          colour: blue
      - text: corazzucche
      - control:
          kind: pause
          length: short
      - control:
          kind: reset_colour
      - text: "\ndall'altra parte del ruscello.\n\nNon siamo mai andati molto d'accordo.\n\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Blatera sempre che la difesa è il miglior\nattacco.\n\n"
      - control:
          kind: sound
          unknown:
            - 8
            - 0
      - text: "Quando andiamo a caccia insieme,"
      - control:
          kind: pause
          length: short
      - text: " lui\nsi avvicina alla preda così piano che si\nfa notte prima di raggiungerla."
      - control:
          kind: pause
          length: long
      - text: "\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Dai retta a me, l'attacco è la miglior\ndifesa!"
  Talk02:
    attributes: Npc_Kakariko013
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
