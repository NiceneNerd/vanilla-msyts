  Npc_kokiri005_After_Demo_Talk000:
    attributes: Npc_kokiri005
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 0
      - text: "Benvenuto! Benvenuto! "
      - control:
          kind: pause
          length: short
      - text: "Maccome sono\nfelice di vederti, signor Eroe!\n\nIo sono Maca, l'esperto di questo bosco.\nSe vuoi sapere qualcosa, non esitare!\nMacchiedi a me!\n\nPer parlare con il "
      - control:
          kind: set_colour
          colour: blue
      - text: grande albero Deku
      - control:
          kind: reset_colour
      - text: ",\ndevi salire quassù!"
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
            - 2
            - 3
          selected_index: 0
          cancel_index: 3
          unknown: 10
  Npc_kokiri005_After_Demo_Talk001:
    attributes: Npc_kokiri005
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 0
      - text: Macché ti posso aiutare ancora?
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
            - 2
            - 3
          selected_index: 0
          cancel_index: 3
          unknown: 10
  Npc_kokiri005_After_Demo_Talk010:
    attributes: Npc_kokiri005
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: "Il grande albero Deku e il piedistallo\ndella spada sono le attrazioni\ndel "
      - control:
          kind: set_colour
          colour: blue
      - text: bosco dei Korogu
      - control:
          kind: reset_colour
      - text: ".\n"
      - control:
          kind: sound
          unknown:
            - 9
            - 1
      - control:
          kind: set_colour
          colour: grey
      - control:
          kind: text_size
          percent: 80
      - text: "Se mi sentono che li chiamo attrazioni si\narrabbiano..."
      - control:
          kind: text_size
          percent: 100
      - control:
          kind: reset_colour
      - text: "\n\n"
      - control:
          kind: sound
          unknown:
            - 6
            - 1
      - text: "Dentro al "
      - control:
          kind: set_colour
          colour: blue
      - text: grande albero Deku
      - control:
          kind: reset_colour
      - text: " c'è un\nposticino dove il signor Eroe si può\nriposare e trovare oggetti utili.\nE poi c'è il "
      - control:
          kind: set_colour
          colour: blue
      - text: piedistallo della spada
      - control:
          kind: reset_colour
      - text: "!\nNon servono presentazioni per\nla "
      - control:
          kind: set_colour
          colour: red
      - text: spada che esorcizza il male
      - control:
          kind: reset_colour
      - text: "!\n"
      - control:
          kind: sound
          unknown:
            - 11
            - 18
      - text: "Inoltre il "
      - control:
          kind: set_colour
          colour: blue
      - text: piedistallo della spada
      - control:
          kind: reset_colour
      - text: "\nè anche il centro di questo bosco..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Se sali sul piedistallo e ti guardi\nintorno, non dovresti avere difficoltà\na orientarti in mezzo al bosco.\nCome punti di riferimento ci sono\nanche le"
      - control:
          kind: set_colour
          colour: blue
      - text: " lanterne fagiolo"
      - control:
          kind: reset_colour
      - text: " su cui\npuoi fare affidamento per girovagare.\n"
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: "Insomma, se ti perdi basta che torni\nal "
      - control:
          kind: set_colour
          colour: blue
      - text: piedistallo
      - control:
          kind: reset_colour
      - text: "!"
  Npc_kokiri005_After_Demo_Talk020:
    attributes: Npc_kokiri005
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: "Finalmente hai trovato la "
      - control:
          kind: set_colour
          colour: red
      - text: "spada che\nesorcizza il male"
      - control:
          kind: reset_colour
      - text: "! La spada che puoi\nimpugnare soltanto tu..."
      - control:
          kind: pause
          length: long
      - text: "\nNessun altro ha quest'onore! Abbiamo\nlavorato sodo affinché nessun altro\npotesse allungarci sopra le mani.\n"
      - control:
          kind: sound
          unknown:
            - 11
            - 1
      - text: "Com'era il bosco perduto?\nHai"
      - control:
          kind: pause
          length: short
      - text: " perduto la strada?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 10
            - 6
      - text: "Oh, macché! "
      - control:
          kind: pause
          length: long
      - text: "È ovvio che no, visto che\nora ti trovi qui! Scusa, signor Eroe, "
      - control:
          kind: pause
          length: long
      - text: "\nnon era mia intenzione offenderti!"
  Npc_kokiri005_After_Demo_Talk030:
    attributes: Npc_kokiri005
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 4
      - text: Per uscire dal bosco?
      - control:
          kind: pause
          length: short
      - text: " Beh certo,\nprima o poi potresti voler uscire..."
      - control:
          kind: pause
          length: long
      - text: "\n\n"
      - control:
          kind: sound
          unknown:
            - 7
            - 1
      - text: "Non ti preoccupare, siamo pronti\nanche a questa evenienza!\n\n"
      - control:
          kind: sound
          unknown:
            - 6
            - 1
      - text: "Dirigiti a est del "
      - control:
          kind: set_colour
          colour: red
      - text: piedistallo della spada
      - control:
          kind: reset_colour
      - text: "\ne sulla destra troverai un segnale.\n\n"
      - control:
          kind: sound
          unknown:
            - 6
            - 1
      - text: "Da lì sali ancora sulla collina a destra e\nvedrai un "
      - control:
          kind: set_colour
          colour: red
      - text: albero spettrale
      - control:
          kind: reset_colour
      - text: "!\n\nEntra nella sua bocca e... maccarà!\nEccoti fuori dal bosco!\n\n"
      - control:
          kind: sound
          unknown:
            - 7
            - 9
      - text: "Ci siamo resi conto che sarebbe stato\ntroppo complicato attraversare di nuovo\ntutto il "
      - control:
          kind: set_colour
          colour: blue
      - text: bosco perduto
      - control:
          kind: reset_colour
      - text: "! "
      - control:
          kind: pause
          length: short
      - text: Eheh!
  Npc_kokiri005_After_Demo_Talk040:
    attributes: Npc_kokiri005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Se hai qualche dubbio riguardo al bosco,\nmacchiedi a me, Maca!"
  Npc_kokiri005_GotMSword_Talk000:
    attributes: Npc_kokiri005
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: "Ah! "
      - control:
          kind: pause
          length: short
      - text: "Signor Eroe!\nTi ho visto estrarre la "
      - control:
          kind: set_colour
          colour: red
      - text: "spada che\nesorcizza il male"
      - control:
          kind: reset_colour
      - text: "!\n"
      - control:
          kind: sound
          unknown:
            - 6
            - 0
      - text: "Se vuoi saperne di più sulla "
      - control:
          kind: set_colour
          colour: red
      - text: spada
      - control:
          kind: reset_colour
      - text: ",\nmacchiedi a me, Maca!"
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
            - 2
            - 3
          selected_index: 0
          cancel_index: 3
          unknown: 10
  Npc_kokiri005_GotMSword_Talk010:
    attributes: Npc_kokiri005
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 6
      - text: "La spada che esorcizza il male...\nmaccheppoi è la "
      - control:
          kind: set_colour
          colour: blue
      - control:
          kind: raw
          two_hundred_one:
            dynamic:
              - 0
              - len: 4
                field_2:
                  - 2
                  - 2
                  - 2
                  - 0
      - text: Spada suprema
      - control:
          kind: reset_colour
      - text: "!\n\nLa "
      - control:
          kind: set_colour
          colour: blue
      - control:
          kind: raw
          two_hundred_one:
            dynamic:
              - 0
              - len: 4
                field_2:
                  - 2
                  - 2
                  - 2
                  - 0
      - text: spada suprema
      - control:
          kind: reset_colour
      - text: " è molto molto\nspeciale ma, se la usi tanto, dopo\nun po' si stanca.\nSe succede,"
      - control:
          kind: pause
          length: short
      - text: " stai tranquillo, "
      - control:
          kind: pause
          length: short
      - text: "non ti\nagitare, basta che aspetti un pochino,\ncosì si riposa e riprende le forze!\nForse perché lei lo sa che ci sono\nmomenti in cui serve tutta l'energia\npossibile.\n"
      - control:
          kind: sound
          unknown:
            - 7
            - 9
      - text: "Massecchiedi al grande albero Deku, \nti saprà dare altre informazioni!"
  Npc_kokiri005_Talk000:
    attributes: Npc_kokiri005
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 0
      - text: "Oh, signor Eroe!"
      - control:
          kind: pause
          length: short
      - text: " Io..."
      - control:
          kind: pause
          length: short
      - text: " ti..."
      - control:
          kind: pause
          length: short
      - text: " ho aspettato\ncosì a lungo!"
  Npc_kokiri005_Talk010:
    attributes: Npc_kokiri005
    contents:
      - control:
          kind: sound
