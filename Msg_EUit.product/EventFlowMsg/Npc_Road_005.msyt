      - text: "\nBrrr..."
      - control:
          kind: pause
          length: short
      - text: " Io e il mio cavallo stiamo per\nfinire congelati."
  MSG_Taba_03:
    attributes: Npc_Road_005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Sei stato bravo ad arrivare così lontano!\n\n\nSe sei diretto a nord, non dimenticare\ndi portarti dei vestiti pesanti che ti\ntengano al caldo."
  MSG_Taba_03_1:
    attributes: Npc_Road_005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
