    attributes: Npc_Road_005
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 4
      - text: Evviva!
      - control:
          kind: pause
          length: long
      - text: " Quanti ne ho venduti di questi\nfunghi speciali cresciuti nella terra\ndel ghiaccio e della neve!\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Crescere nel freddo li aiuta a mantenere\nun sapore... freschissimo!\n\nOra devo dirigermi verso lo\n"
      - control:
          kind: set_colour
          colour: blue
      - text: "stallaggio del nevaio "
      - control:
          kind: reset_colour
      - text: "per\nfare di nuovo scorta di funghi."
  MSG_Taba_00_1:
    attributes: Npc_Road_005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ho venduto tutti i miei funghi della terra\ndel ghiaccio e della neve."
      - control:
          kind: pause
          length: short
      - text: " Ora torno allo\n"
      - control:
          kind: set_colour
          colour: blue
      - text: "stallaggio del nevaio "
      - control:
          kind: reset_colour
      - text: "per un'altra scorta."
  MSG_Taba_01:
    attributes: Npc_Road_005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Sì, la strada che va dallo "
      - control:
