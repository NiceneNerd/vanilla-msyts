      - control:
          kind: pause
          length: long
      - text: " Dovresti provarli\nalmeno una volta."
  RetMSG_Taba_00_1:
    attributes: Npc_Road_005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
