      - text: "cannone "
      - control:
          kind: reset_colour
      - text: "per\ncercare di farlo finire nel cratere, "
      - control:
          kind: pause
          length: short
      - text: "ma\nniente."
      - control:
          kind: pause
          length: short
      - text: " Quello continua a tornare."
  talk16:
    attributes: Npc_goron003
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 0
      - text: "Il "
      - control:
          kind: set_colour
          colour: blue
      - text: "capo "
      - control:
          kind: reset_colour
      - text: "amministra la città dei Goron.\nÈ molto molto importante!\n\nVive nella casa con la grande insegna."
      - control:
          kind: pause
          length: short
      - text: "\nUno di questi giorni dovresti andare a\nconoscerlo."
  talk17:
    attributes: Npc_goron003
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 0
      - text: "Gironzolo spesso intorno al "
      - control:
          kind: set_colour
          colour: blue
      - text: Monte Morte
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: pause
          length: short
      - text: "\nPare che il capo e Yunobo finalmente\nci abbiano messo una bella pietra sopra!\n"
      - control:
          kind: sound
          unknown:
            - 1
            - 0
      - text: "Fu per colpa di "
      - control:
          kind: set_colour
          colour: blue
      - text: "Rudania "
      - control:
          kind: reset_colour
      - text: "che persi\nil lavoro."
      - control:
          kind: pause
          length: short
      - text: " Mi hanno proprio levato\nun macigno dallo stomaco."
  talk18:
    attributes: Npc_goron003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: "Ehi, "
      - control:
          kind: set_colour
          colour: blue
      - text: Hylia
      - control:
          kind: reset_colour
      - text: ".\nCadi proprio a sassolino.\n\nUltimamente non si è vista più nessuna\n"
      - control:
