      - text: "."
  talk12:
    attributes: Npc_goron003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Fino a poco tempo fa,"
      - control:
          kind: pause
          length: short
      - text: " lavoravo come\nminatore sul "
      - control:
          kind: set_colour
          colour: blue
      - text: Monte Morte
      - control:
          kind: reset_colour
      - text: "...\n\nPurtroppo, dopo l'apparizione del\n"
      - control:
          kind: set_colour
          colour: blue
      - text: colosso sacro Vah Rudania
      - control:
          kind: reset_colour
      - text: ","
      - control:
          kind: pause
          length: short
      - text: " non è stato\npiù possibile continuare gli scavi.\nPer questo ho perso il lavoro..."
  talk13:
    attributes: Npc_goron003
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "Ah... "
      - control:
          kind: pause
          length: short
      - text: I bei tempi sono andati...
  talk14:
    attributes: Npc_goron003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Hai visto cosa ne è stato del\n"
      - control:
          kind: set_colour
          colour: blue
      - text: colosso sacro Vah Rudania
      - control:
          kind: reset_colour
      - text: "?"
      - control:
          kind: pause
          length: short
      - text: " Ben gli sta!\n\n"
      - control:
          kind: sound
          unknown:
            - 1
            - 0
      - text: "Adesso che si può anche raggiungere\nla cima,"
      - control:
          kind: pause
          length: short
      - text: " non vedo l'ora di tornare sul\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Monte Morte
      - control:
          kind: reset_colour
      - text: ", piccone in mano. "
  talk15:
    attributes: Npc_goron003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Con quel mostro in giro
      - control:
          kind: pause
          length: short
      - text: " non possiamo\nneanche avvicinarci al "
      - control:
          kind: set_colour
          colour: blue
      - text: Monte Morte
      - control:
          kind: reset_colour
      - text: ".\n\nOgni giorno il "
      - control:
