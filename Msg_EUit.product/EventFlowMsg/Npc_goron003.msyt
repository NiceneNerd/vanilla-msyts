          kind: set_colour
          colour: blue
      - text: Monte Morte
      - control:
          kind: reset_colour
      - text: "\nè apparso un mostro chiamato\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Vah Rudania
      - control:
          kind: reset_colour
      - text: ".\nTutta l'area è diventata estremamente\ncalda,"
      - control:
          kind: pause
