          colour: red
      - text: "sfera che hanno rubato alle\nGerudo "
      - control:
          kind: reset_colour
      - text: "è nel magazzino al primo piano...\n\n"
      - control:
          kind: sound
          unknown:
            - 3
            - 12
      - text: "Ma io non posso avvicinarmi più di così,\nè troppo pericoloso. Mica ci voglio\nrimettere la pelle!"
  Npc_Attacked_006_DLC2nd_SD_001:
    attributes: Npc_Attacked_006_DLC2nd
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 0
      - text: "Ho sentito dire che gli Yiga sono stati\nsconfitti da uno spadaccino giramondo.\n\n"
      - control:
          kind: sound
          unknown:
            - 4
            - 20
      - text: "Ma in realtà ce ne sono ancora tanti!\nE vanno in giro a raccogliere banane...\nChe stiano riformando il clan?"
  Npc_Attacked_006_DLC2nd_SD_100:
    attributes: Npc_Attacked_006_DLC2nd
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 0
      - text: "Non mi dire che...\n\n\n... sei andato lì dentro...\n\n\n... e hai recuperato...\n\n\nla "
      - control:
          kind: set_colour
          colour: red
      - text: sfera
      - control:
          kind: reset_colour
      - text: "?!\n\n\n"
      - control:
          kind: sound
          unknown:
            - 3
            - 0
      - text: "Se ti scoprono, ti inseguiranno in\ncapo al mondo!"
  Npc_Attacked_006_DLC2nd_SD_200:
    attributes: Npc_Attacked_006_DLC2nd
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 1
      - control:
          kind: animation
          name: Trouble_00
      - text: "Aaah... hai gettato la preziosa sfera\nnella voragine!\n\n"
