      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: "Yuppi!\nGrazie dell'acquisto!\n\nUn fruscio mi aveva suggerito che saresti\nvenuto, per questo ho deciso di avviare\nquest'attività, la "
      - control:
          kind: pause
          length: short
      - control:
          kind: set_colour
          colour: blue
      - text: bottega Fogliesparse
      - control:
          kind: reset_colour
      - text: "!\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Se vuoi vendere qualcosa, sarò\nstrafeliceci di comprarlo!"
  Npc_kokiri003_Sell030:
    attributes: Npc_kokiri003
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: "Grazie dell'acquisto, signor Eroe!\n\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Volerò a raccogliere tanti bei prodotti\nfreschi per la prossima volta che torni!"
  Npc_kokiri003_Talk000:
    attributes: Npc_kokiri003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Guarda chi c'èci! Il signor Eroe!"
      - control:
          kind: pause
          length: short
      - text: "\nBenvenuto nella "
      - control:
          kind: set_colour
          colour: blue
      - text: bottega Fogliesparse
      - control:
          kind: reset_colour
      - text: "!\n\nIo vendo tantissime cose che raccolgo\nnella foresta..."
      - control:
          kind: pause
          length: short
      - text: " e anche tantissime cose\nche non raccolgo nella foresta.\nPuoi scegliere quello che ti sussurra\nil vento! "
      - control:
          kind: pause
          length: short
      - text: "O forse preferisci vendermi\nqualcosa?"
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
            - 2
          selected_index: 0
          cancel_index: 2
          unknown: 8
  Npc_kokiri003_Talk010:
    attributes: Npc_kokiri003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Benvenuto nella "
      - control:
