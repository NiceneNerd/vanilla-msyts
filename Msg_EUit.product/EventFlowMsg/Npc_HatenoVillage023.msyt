      - text: "\nnell'epoca dei campi ardenti.\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Ricordo i primi germogli sulle piante qui\na Finterra."
      - control:
          kind: pause
          length: short
      - text: " Ero già ragazzina ed eravamo\nappena diventati autosufficienti.\nEppure,"
      - control:
          kind: pause
          length: short
      - text: " il "
      - control:
          kind: set_colour
          colour: blue
      - text: castello di Hyrule
      - control:
          kind: reset_colour
      - text: " e il suo\n"
      - control:
