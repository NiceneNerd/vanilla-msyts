      - control:
          kind: set_colour
          colour: blue
      - text: rovine del borgo
      - control:
          kind: reset_colour
      - text: ".\nA giudicare dall'estensione di quelle\nmacerie, il "
      - control:
          kind: set_colour
          colour: blue
      - text: "borgo del castello "
      - control:
