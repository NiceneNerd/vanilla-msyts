      - text: "castello di\nHyrule"
      - control:
          kind: reset_colour
      - control:
          kind: pause
          length: short
      - text: " fosse un centro ricchissimo\ndi commerci e conoscenze.\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 6
      - text: "A proposito... "
      - control:
          kind: pause
          length: short
      - text: Per caso
      - control:
