    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Le "
      - control:
          kind: set_colour
          colour: blue
      - text: Terre di Oldin
      - control:
          kind: reset_colour
      - text: " si trovano a nord-est."
      - control:
          kind: pause
          length: short
      - text: "\nSono una regione montagnosa con al\ncentro il "
      - control:
          kind: set_colour
          colour: blue
      - text: Monte Morte
      - control:
          kind: reset_colour
      - text: ".\nIl "
      - control:
          kind: set_colour
          colour: blue
      - text: Monte Morte
      - control:
          kind: reset_colour
      - text: " si vede anche da qui,"
      - control:
          kind: pause
          length: short
      - text: "\nma è il versante occidentale.\n\nSe vuoi scalarlo, devi andare verso\nle "
      - control:
          kind: set_colour
          colour: blue
      - text: Terre di Akkala
      - control:
          kind: reset_colour
      - text: ", che si trovano\na est di qui.\nSul cammino incontrerai"
