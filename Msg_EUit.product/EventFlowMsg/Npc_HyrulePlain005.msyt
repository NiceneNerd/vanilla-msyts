    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "La "
      - control:
          kind: set_colour
          colour: blue
      - text: foresta di Hyrule
      - control:
          kind: reset_colour
      - text: " è qui a nord.\nÈ la macchia boschiva più grande di\ntutta Hyrule."
      - control:
          kind: pause
          length: long
      - text: "\n"
      - control:
          kind: sound
          unknown:
            - 11
            - 1
      - text: "Al suo interno c'è il "
      - control:
          kind: set_colour
          colour: blue
      - text: Bosco Perduto
      - control:
          kind: reset_colour
      - text: ",\nun luogo davvero pauroso.\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Quando ero giovane, andai a raccogliervi\ndella legna e mi persi tra gli alberi.\n\nPiù mi addentravo e più si alzava\nun "
      - control:
          kind: set_colour
          colour: red
      - text: vento forte
      - control:
          kind: reset_colour
      - text: "..."
      - control:
          kind: pause
          length: long
      - text: "\nMe la stavo facendo sotto dalla paura..."
      - control:
          kind: pause
          length: long
      - text: "\nFinché non mi ritrovai intrappolato nella\nrete di un ragno.\n\n"
      - control:
          kind: sound
          unknown:
            - 5
            - 12
      - text: "Fu allora che la nebbia scomparve\ne tutto d'un tratto mi ritrovai fuori\ndal bosco."
      - control:
          kind: pause
          length: long
      - text: "\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "C'è qualcosa di minaccioso nel\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Bosco Perduto
      - control:
          kind: reset_colour
      - text: ", qualcosa di invisibile..."
      - control:
          kind: pause
          length: long
      - text: "\nMi raccomando, stanne lontano."
