              - 4
              - field_1: 0
      - text: "accesso\nal Monte Morte.\n"
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "Ma il "
      - control:
          kind: set_colour
          colour: blue
      - text: Monte Morte
      - control:
          kind: reset_colour
      - text: " è un vulcano molto\npericoloso."
      - control:
          kind: pause
          length: long
      - text: " Devi equipaggiarti al meglio\nse vuoi avventurarti lassù.\n"
      - control:
          kind: sound
