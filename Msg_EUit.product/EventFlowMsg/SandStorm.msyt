    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 19
      - text: Eh?
      - control:
          kind: pause
          length: long
      - text: " Cosa?!"
      - control:
          kind: pause
          length: short
      - text: "\nNon mi dire che la tempesta di sabbia\nsi è fermata!\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Non l'ho mai persa d'occhio..."
      - control:
          kind: pause
          length: long
      - text: "\nAllora perché ha smesso così\nall'improvviso?"
      - control:
          kind: pause
          length: short
      - text: " Che romantico mistero..."
