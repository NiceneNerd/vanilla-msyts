    contents:
      - text: Aspetta un attimo...
      - control:
          kind: pause
          length: longer
      - text: " Ma tu chi sei?\nScusa ma ho una pessima memoria\nper i nomi."
  Npc_SmallOasis007_F_008:
    attributes: Npc_SmallOasis007
    contents:
      - control:
          kind: sound
