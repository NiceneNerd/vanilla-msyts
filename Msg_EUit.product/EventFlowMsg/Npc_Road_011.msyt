  "0001":
    attributes: ""
    contents:
      - text: "Mi spiace, ma no."
  Atacked00:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 42
      - text: Aaagh...
      - control:
          kind: pause
          length: short
      - text: " Vattene via, per favore..."
  Atacked01:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 42
      - text: "Ecco perché non voglio mai\nuscire dalla cittadella..."
  Failed00:
    attributes: Npc_Road_011
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 1
      - control:
          kind: sound2
