  Demo661_0_Text000:
    contents:
      - text: "Certo, sarò felicissimo di aiutarti.\nSono il grande Daruk e non ho paura di niente!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Se il regno di Hyrule è in pericolo,\nnon sarò certo io a tirarmi indietro!"
  Demo661_0_Text001:
