    contents:
      - control:
          kind: auto_advance
          frames: 275
      - control:
          kind: raw
          one:
            one:
              field_1: 4
              field_2: 1053609165
      - text: "Come fiamma s'inerpica"
      - control:
          kind: pause
          length: long
      - text: " "
      - control:
          kind: set_colour
          colour: blue
      - text: Rudania
      - control:
          kind: reset_colour
