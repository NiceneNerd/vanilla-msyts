          length: long
      - text: " nel "
      - control:
          kind: set_colour
          colour: blue
      - text: monumento
      - control:
          kind: reset_colour
      - text: " è la tua speranza."
  Npc_Musician_AoC_HeroGoronRelief_102:
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "Forse tu... ti sei spalmato addosso\nun po' di "
      - control:
          kind: set_colour
          colour: blue
      - text: pozione ignifuga
      - control:
