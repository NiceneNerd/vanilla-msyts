  Npc_Musician_AoC_HeroGoronRelief_101:
    contents:
      - control:
          kind: auto_advance
          frames: 300
      - control:
          kind: raw
          one:
            one:
              field_1: 4
              field_2: 1053609165
      - text: O tu che supplichi
      - control:
          kind: pause
          length: long
      - text: " di rafforzar la sacra\npossanza,"
      - control:
          kind: pause
          frames: 90
      - text: " percorri le lande, cerca tre\nprove,"
