    contents:
      - control:
          kind: auto_advance
          frames: 275
      - control:
          kind: raw
          one:
            one:
              field_1: 4
              field_2: 1053609165
      - text: Con scopo benigno
      - control:
          kind: pause
          length: long
      - text: " "
      - control:
          kind: set_colour
          colour: red
      - text: "sconfiggi l'infuocato\nmacigno"
      - control:
          kind: reset_colour
      - text: "!"
      - control:
          kind: pause
          frames: 75
      - text: " Con occhi alla meta protesi,"
      - control:
          kind: pause
          length: long
      - text: "\n"
      - control:
          kind: set_colour
          colour: red
      - text: passa nei cerchi sul vulcano sospesi
      - control:
          kind: reset_colour
      - text: "!"
  Npc_Musician_AoC_HeroGoronRelief_109:
    contents:
      - control:
          kind: auto_advance
          frames: 330
      - control:
          kind: raw
          one:
            one:
              field_1: 4
              field_2: 1053609165
      - text: "Mostra una testa non cava,"
      - control:
          kind: pause
          length: long
      - text: " "
      - control:
          kind: set_colour
          colour: red
      - text: "ergi il corpo\nsopra la lava"
      - control:
          kind: reset_colour
      - text: "!"
      - control:
          kind: pause
          frames: 84
      - text: " Se migliorare vuoi il sacro\npotere,"
      - control:
          kind: pause
          length: long
      - text: " affronta le prove senza temere!"
  Npc_Musician_AoC_HeroGoronRelief_201:
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 0
      - text: "Posso solo immaginare il valore del\ncampione di un secolo fa!"
  Npc_Musician_AoC_HeroGoronRelief_202:
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 24
      - text: "Il mio maestro diceva sempre che, per\nscrivere delle belle canzoni, bisogna\nviaggiar molto e incontrar la gente."
  Npc_Musician_AoC_HeroGoronRelief_203:
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Perciò io adesso seguo il suo consiglio\ne mi intrattengo con le persone\nincontrate nel mio peregrinar.\nAlla "
      - control:
          kind: set_colour
          colour: blue
      - text: città dei Goron
      - control:
          kind: reset_colour
      - text: " ho incontrato\n"
      - control:
          kind: set_colour
          colour: blue
      - text: "Yunobo "
      - control:
          kind: reset_colour
      - text: "e "
      - control:
          kind: set_colour
          colour: blue
      - text: Bludo
      - control:
          kind: reset_colour
      - text: " che sapevano molte\ncose sul conto del campione.\nMi hanno raccontato delle storie\ndavvero interessanti."
  Npc_Musician_AoC_HeroGoronRelief_204:
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Sono certo che i loro racconti mi saranno\ndi grande aiuto nello scrivere la "
      - control:
          kind: set_colour
          colour: blue
      - text: "parte\nessenziale"
      - control:
          kind: reset_colour
      - text: " della ballata."
