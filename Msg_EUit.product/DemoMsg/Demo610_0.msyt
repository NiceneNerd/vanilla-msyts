  Npc_Musician_AoC_HeroGoronRelief_103:
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 11
      - text: "Oh, cielo! Quindi sei capace di resistere\nal calore proprio come i "
      - control:
          kind: set_colour
          colour: blue
      - text: Goron
      - control:
          kind: reset_colour
      - text: "!\n\nIo se non mi metto la "
      - control:
          kind: set_colour
          colour: blue
      - text: pozione ignifuga
      - control:
          kind: reset_colour
      - text: "\ndivento un pollo arrosto!"
  Npc_Musician_AoC_HeroGoronRelief_104:
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 11
      - text: "Lo sapevo!\n\n\nIo se non mi metto la "
      - control:
          kind: set_colour
          colour: blue
      - text: pozione ignifuga
      - control:
          kind: reset_colour
      - text: "\ndivento un pollo arrosto!"
  Npc_Musician_AoC_HeroGoronRelief_105:
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 1
      - control:
          kind: animation
          name: Ease_00
      - text: "Ahhh... chissà se il mio maestro venne\nanche qui per contemplare i versi della\nsua canzone!"
  Npc_Musician_AoC_HeroGoronRelief_106:
    contents:
      - control:
          kind: auto_advance
          frames: 140
      - control:
          kind: sound
          unknown:
            - 1
            - 0
      - text: "Nel canto incompiuto del mio maestro\nc'è una "
      - control:
          kind: set_colour
          colour: blue
      - text: parte dedicata
      - control:
          kind: reset_colour
      - text: " al "
      - control:
          kind: set_colour
          colour: blue
      - text: "campione\nDaruk"
      - control:
          kind: reset_colour
      - text: " e alle prove che affrontò.\n"
      - control:
          kind: auto_advance
          frames: 40
      - text: Lascia che te la accenni.
