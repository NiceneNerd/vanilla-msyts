  QL_Gerudo_Ch_SnowMT_Desc:
    contents:
      - text: "Una leggenda narra di sette paladine Gerudo,\nma pare che ne esistesse un'"
      - control:
          kind: set_colour
          colour: red
      - text: ottava
      - control:
          kind: reset_colour
      - text: ", tuttora\nvenerata sul "
      - control:
          kind: set_colour
          colour: red
      - text: grande altopiano Gerudo
      - control:
          kind: reset_colour
      - text: ".\nSe trovi l'ottava paladina e ne mostri a Boten\n"
      - control:
          kind: set_colour
          colour: red
      - text: "un'immagine del busto"
      - control:
          kind: reset_colour
      - text: ", otterrai gli stivali da\nsabbia. Hai sentito dire che alla cittadella\nGerudo c'è qualcuno molto ben informato sulla\nleggenda delle \"sette paladine\"."
  QL_Gerudo_Ch_SnowMT_Finish:
    contents:
      - text: "L'ottava paladina è ancora venerata\nsul grande altopiano Gerudo. Le hai scattato\nun'immagine e, dopo averla mostrata a Boten,\nhai ricevuto da lui gli "
      - control:
          kind: raw
          two_hundred_one:
            dynamic:
              - 0
              - len: 4
                field_2:
                  - 1
                  - 5
                  - 5
                  - 1
      - text: stivali da sabbia.
  QL_Gerudo_Ch_SnowMT_Name:
    contents:
      - text: "L'ottava paladina"
