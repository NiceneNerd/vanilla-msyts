  QL_SanrokuMini_Lizard_Desc:
    contents:
      - text: "Masaki, alla miniera a sud, ti ha chiesto di\ncatturare dieci esemplari di "
      - control:
          kind: set_colour
          colour: red
      - text: lucertole focus
      - control:
          kind: reset_colour
      - text: ",\nche vivono sul Monte Morte.\n\nPare che si trovino con più facilità proprio\nnei pressi della miniera a sud.\n\nCerca "
      - control:
          kind: set_colour
          colour: red
      - text: 10 lucertole focus
      - control:
          kind: reset_colour
      - text: " e portale a Masaki."
  QL_SanrokuMini_Lizard_Finish:
    contents:
      - text: "Per ringraziarti delle "
      - control:
          kind: set_colour
          colour: red
      - text: lucertole focus
      - control:
          kind: reset_colour
      - text: ", Masaki\nti ha regalato la sua "
      - control:
          kind: raw
          two_hundred_one:
            dynamic:
              - 0
              - len: 4
                field_2:
                  - 2
                  - 3
                  - 3
                  - 0
      - text: "armaturoccia ignifuga.\n\nDa questo momento in poi, inoltre, ogni volta\nche porterai a Masaki "
      - control:
          kind: set_colour
          colour: red
      - text: tre lucertole focus
      - control:
          kind: reset_colour
      - text: ",\nriceverai da lui "
      - control:
          kind: set_colour
          colour: red
      - text: 20 rupie
      - control:
          kind: reset_colour
      - text: "."
  QL_SanrokuMini_Lizard_Name:
    contents:
      - text: Lucertole focus a volontà!
