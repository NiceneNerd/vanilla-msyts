  QL_Get_MasterSword_Desc:
    contents:
      - text: "Oltre il Bosco Perduto, nei meandri del\nbosco dei Korogu, riposa la "
      - control:
          kind: set_colour
          colour: red
      - text: "spada che\nesorcizza il male"
      - control:
          kind: reset_colour
      - text: ", l'unica arma in grado di\nsconfiggere la calamità.\nTuttavia, il grande albero Deku dice che la\nspada metterà alla prova chiunque tenti di\nafferrarla: cento anni fa eri in grado di brandirla,\nma non è detto che ci riuscirai anche adesso."
  QL_Get_MasterSword_Finish:
    contents:
      - text: "Hai conquistato la leggendaria "
      - control:
          kind: set_colour
          colour: red
      - control:
          kind: raw
          two_hundred_one:
            dynamic:
              - 0
              - len: 4
                field_2:
