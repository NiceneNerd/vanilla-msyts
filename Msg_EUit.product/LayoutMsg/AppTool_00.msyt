entries:
  "0000":
    contents:
      - text: "Crea un pilastro di ghiaccio sull'acqua"
  "0001":
    contents:
      - control:
          kind: set_colour
          colour: light_green4
      - text: "I pilastri di ghiaccio sono molto resistenti e non si sciolgono.\nPuoi usarli come "
      - control:
          kind: set_colour
          colour: orange
      - text: "pedane "
      - control:
          kind: set_colour
          colour: light_green4
      - text: "o "
      - control:
          kind: set_colour
          colour: orange
      - text: barriere
      - control:
          kind: set_colour
          colour: light_green4
      - text: ". Usa di nuovo il glacyor \nsu un pilastro di ghiaccio per "
      - control:
          kind: set_colour
          colour: orange
      - text: distruggerlo
      - control:
          kind: set_colour
          colour: light_green4
      - text: "."
      - control:
          kind: reset_colour
  "0002":
    contents:
      - text: FifthItem
  "0003":
    contents:
      - text: "Crea un'immagine di ciò che stai osservando"
  "0004":
    contents:
      - control:
          kind: set_colour
          colour: light_green4
      - text: "Le immagini scattate vengono salvate nell'album. Ogni volta che\nsi riproduce un nuovo oggetto, questo viene registrato nel\ncompendio di Hyrule automaticamente."
      - control:
          kind: reset_colour
  "0005":
    contents:
      - text: SixthItem
  "0006":
    contents:
      - text: Usa un amiibo per evocare qualcosa di utile
  "0007":
    contents:
      - control:
          kind: set_colour
          colour: light_green4
      - text: "Puoi usare lo stesso amiibo solo una volta al giorno\ne ciò che viene evocato è affidato alla sorte."
      - control:
          kind: reset_colour
  "0008":
    contents:
      - text: SeventhItem
  "0009":
    contents:
      - text: Manipola gli oggetti metallici
  "0010":
    contents:
      - control:
          kind: set_colour
          colour: light_green4
      - text: "Puoi agganciare, "
      - control:
          kind: set_colour
          colour: orange
      - text: "sollevare e spostare liberamente "
      - control:
          kind: set_colour
          colour: light_green4
      - text: "gli\noggetti metallici grazie al raggio magnetico emanato\nda questo strumento."
      - control:
          kind: reset_colour
  "0011":
    contents:
      - text: ThirdItem
  "0012":
    contents:
      - text: Fai scoppiare le bombe a distanza
  "0013":
    contents:
      - control:
          kind: set_colour
          colour: light_green4
      - text: "Infliggi enormi "
      - control:
          kind: set_colour
          colour: orange
      - text: "danni "
      - control:
          kind: set_colour
          colour: light_green4
      - text: "ai nemici e "
      - control:
          kind: set_colour
          colour: orange
      - text: distruggi gli ostacoli
      - control:
          kind: set_colour
          colour: light_green4
      - text: "\ncon queste bombe a forma di sfera o di cubo.\nUsa il tipo più adatto alla situazione."
      - control:
          kind: reset_colour
  "0014":
    contents:
      - text: Fai scoppiare le bombe a distanza
  "0015":
    contents:
      - control:
          kind: set_colour
          colour: light_green4
      - text: "Esplosivi potenziati e con minor tempo di ricarica.\nInfliggi enormi "
      - control:
          kind: set_colour
          colour: orange
      - text: "danni "
      - control:
          kind: set_colour
          colour: light_green4
      - text: "ai nemici e "
      - control:
          kind: set_colour
          colour: orange
      - text: distruggi gli ostacoli
      - control:
          kind: set_colour
          colour: light_green4
      - text: "\ncon queste bombe a forma di sfera o di cubo.\nUsa il tipo più adatto alla situazione."
      - control:
          kind: reset_colour
  "0016":
    contents:
      - text: FirstItem
  "0017":
    contents:
      - text: SecondItem
  "0018":
    contents:
      - text: Ferma un oggetto nel tempo
  "0019":
    contents:
      - control:
          kind: set_colour
          colour: light_green4
      - text: "Colpendo gli oggetti fermi nel tempo, puoi accumulare al loro\ninterno dell'"
      - control:
          kind: set_colour
          colour: orange
      - text: energia cinetica
      - control:
          kind: set_colour
          colour: light_green4
      - text: ". L'energia accumulata si libera non\nappena il tempo riprende a scorrere. Sfrutta questa abilità per\n"
      - control:
          kind: set_colour
          colour: orange
      - text: spostare oggetti enormi
      - control:
          kind: set_colour
          colour: light_green4
      - text: "."
      - control:
          kind: reset_colour
  "0020":
    contents:
      - text: Ferma un oggetto nel tempo
  "0021":
    contents:
      - control:
          kind: set_colour
          colour: light_green4
      - text: "Ora puoi bloccare nel tempo anche i nemici. Se colpisci oggetti\nfermi nel tempo, questi accumulano "
      - control:
          kind: set_colour
          colour: orange
      - text: energia cinetica
      - control:
          kind: set_colour
          colour: light_green4
      - text: ". Sfruttando\ntale energia accumulata puoi "
      - control:
          kind: set_colour
          colour: orange
      - text: spostare anche oggetti enormi
      - control:
          kind: set_colour
          colour: light_green4
      - text: "."
      - control:
          kind: reset_colour
  "0022":
    contents:
      - text: FourthItem
  "0023":
    contents:
      - text: "Funzione non utilizzabile: non\nhai trovato lo strumento adatto."
  "0024":
    contents:
      - text: Evoca il destriero di Hyrule Zero
  "0025":
    contents:
      - control:
          kind: set_colour
          colour: light_green4
      - text: "Per alimentare il veicolo, "
      - control:
          kind: set_colour
          colour: orange
      - text: inserisci materiali nel serbatoio
      - control:
          kind: set_colour
          colour: light_green4
      - text: ".\nPuoi evocarlo ogni volta che vuoi, tranne in alcune zone dove\nnon è possibile utilizzarlo."
      - control:
          kind: reset_colour
  "0026":
    contents:
      - text: EighthItem
