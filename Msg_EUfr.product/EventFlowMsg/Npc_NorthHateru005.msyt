    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 6
      - text: "Mate un peu les nuages à l'est."
      - control:
          kind: pause
          length: longer
      - text: "\nChelou, non ?\n\nJ'aime pas la flotte, ça m'file le cafard."
      - control:
          kind: pause
          length: longer
      - text: "\nHeureusement que Satori est là pour\nme consoler..."
