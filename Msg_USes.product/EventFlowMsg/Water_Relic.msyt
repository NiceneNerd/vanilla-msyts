            - 4
            - 1
      - text: ¿Eres hyliano?
      - control:
          kind: pause
          frames: 20
      - text: " ¿Qué hace alguien\ncomo tú en nuestra región?"
  Npc_Zora004_S1_001:
    attributes: Npc_Zora004
    contents:
      - control:
          kind: sound
          unknown:
            - 2
            - 13
      - text: No sé por qué viniste hasta aquí;
