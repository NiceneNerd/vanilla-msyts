          cancel_index: 1
          unknown: 6
  talk08:
    attributes: Npc_UMiiVillage031
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: No te molestes.
      - control:
          kind: pause
          length: short
      - text: " Este es mi problema."
      - control:
