          unknown: 6
  talk09:
    attributes: Npc_UMiiVillage031
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 18
      - text: "Bueno, si tú te ofreces..."
  talk10:
    attributes: Npc_UMiiVillage031
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "No es por apurarte, pero"
      - control:
          kind: pause
          length: short
      - text: " si quiero\npedirle a los demás que se muden aquí,\nvoy a necesitar casas primero.\nSi me traes "
      - control:
