          cancel_index: 1
          unknown: 6
  talk05:
    attributes: Npc_UMiiVillage031
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Eso creía yo, pero me equivocaba..."
      - control:
