  talk001:
    attributes: Npc_NorthHateru005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: ¿Otro hombre?
      - control:
          kind: pause
          length: short
      - text: " ¡Qué mala suerte la mía!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "¿Por qué será que a este rancho\nsolo vienen hombres?"
      - control:
