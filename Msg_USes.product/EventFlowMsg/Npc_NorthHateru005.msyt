          cancel_index: 2
          unknown: 8
  talk002:
    attributes: Npc_NorthHateru005
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 0
      - text: ¡Caray!
      - control:
          kind: pause
          length: longer
      - text: " Por este rancho no se ven\nmuchas mujeres, y menos tan\nelegantes como tú.\n"
      - control:
          kind: sound
          unknown:
            - 3
            - 0
      - text: "Un segundo... "
      - control:
          kind: pause
          length: short
      - text: ¡Tú no eres una mujer!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Puede que logres engañar a una gerudo\ncon ese atuendo, pero reconozco un\n"
      - control:
          kind: set_colour
          colour: blue
      - text: hyliano
      - control:
          kind: reset_colour
      - text: " cuando lo veo."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: En fin...
      - control:
          kind: pause
          length: short
      - text: " Supongo que este rancho no\nes el sitio adecuado para conocer a\nmujeres."
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
            - 2
          selected_index: 0
          cancel_index: 2
          unknown: 8
  talk005:
    attributes: Npc_NorthHateru005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Tú otra vez...
      - control:
          kind: pause
          length: longer
      - text: " ¿Qué pasa?"
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
            - 2
