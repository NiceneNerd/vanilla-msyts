            - 4
          selected_index: 0
          cancel_index: 2
          unknown: 6
  Npc_Musician_AoC_HeroXxxx_Demo610_First_030:
    attributes: Npc_Musician_AoC_HeroZoraRelief
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "Cuando encuentro a otro viajero en\nun lugar relacionado con una "
      - control:
          kind: set_colour
          colour: blue
      - text: canción
      - control:
          kind: reset_colour
      - text: ",\nno puedo evitar cantarla.\nAh, casi se me olvida."
  Npc_Musician_AoC_HeroXxxx_Demo610_First_050:
    attributes: Npc_Musician_AoC_HeroZoraRelief
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Sí, está casi completada..."
  Npc_Musician_AoC_HeroXxxx_Demo610_First_060:
    attributes: Npc_Musician_AoC_HeroZoraRelief
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 1
      - text: "Pero, por desgracia, mi maestro falleció\nantes de poder terminarla."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 6
            - 1
      - text: "Como su discípulo, me propuse\nescribir los "
      - control:
          kind: set_colour
          colour: blue
      - text: versos
      - control:
          kind: reset_colour
      - text: " que faltan y darle\nuna conclusión.\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Ese es el motivo por el cual visito\nlos lugares que se mencionan en ella."
  Npc_Musician_AoC_HeroXxxx_Demo610_HeroBallad_001:
    attributes: Npc_Musician_AoC_HeroZoraRelief
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "¿Y bien? ¿Te conmovió el canto?\nFue escrito en honor al "
      - control:
          kind: set_colour
          colour: blue
      - text: campeón
      - control:
          kind: reset_colour
      - text: "\nde esta tierra."
  Npc_Musician_AoC_HeroXxxx_Demo610_HeroBallad_002:
    attributes: Npc_Musician_AoC_HeroZoraRelief
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: ¿Qué te pareció?
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Gracias a las historias que me contaron\nsobre los "
      - control:
          kind: set_colour
          colour: blue
      - text: campeones
