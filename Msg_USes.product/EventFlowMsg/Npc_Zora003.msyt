  talk00:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 11
      - text: ¡Link!
      - control:
          kind: pause
          frames: 10
      - text: " Estamos en deuda contigo por\nhaber salvado la región de los zora."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 1
      - text: "Todos los miembros de la tribu están\ncontentos."
      - control:
          kind: pause
          frames: 20
      - text: " ¡Nos has quitado un gran\npesar y nos das nuevas esperanzas!"
  talk01:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 6
      - text: No obstante...
      - control:
          kind: pause
          frames: 20
      - text: " Todavía quisiera pedirte\nun pequeño favor, Link."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Lograste regresar sano y salvo\ndel interior de la bestia divina del\nagua, "
      - control:
          kind: set_colour
          colour: blue
      - text: Vah Ruta
      - control:
          kind: reset_colour
      - text: ".\n"
      - control:
          kind: sound
          unknown:
            - 3
            - 1
      - text: Pero Mipha no volvió contigo.
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Siento que mis peores temores\nhan sido confirmados."
      - control:
          kind: pause
          frames: 20
      - text: "\n"
      - control:
          kind: sound
          unknown:
            - 3
            - 16
      - text: "Dime, ¿era ya demasiado tarde?"
      - control:
          kind: single_choice
          label: 0
  talk02:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 6
      - text: ¿Su espíritu?
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Entonces Mipha está...
      - control:
          kind: pause
          frames: 20
      - text: "\n¿Llegó a decirte algo?"
  talk04:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 12
      - text: Entiendo...
      - control:
          kind: pause
          frames: 20
      - text: "\nEs difícil de creer."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Pero no hay nada que pudiera hacer.
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 5
            - 1
      - text: "No obstante,"
      - control:
          kind: pause
          frames: 20
      - text: " Mipha cumplió con\nsu deber como campeona. Es una\nauténtica heroína.\nY, como padre suyo que soy,"
      - control:
          kind: pause
          frames: 20
      - text: " mi deber\nes velar por su bienestar, incluso ahora."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: Link...
      - control:
          kind: pause
          frames: 20
      - text: " Te quería mucho.\nLo sabes, ¿verdad?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 1
      - text: "Incluso en sus circunstancias actuales,\ndebe estar contenta porque al fin tiene\nla oportunidad de ayudarte.\n"
      - control:
          kind: sound
          unknown:
            - 1
            - 6
      - text: "Me gustaría que la recordaras\nsiempre, Link."
      - control:
          kind: choice
          choice_labels:
            - 2
            - 3
          selected_index: 0
          cancel_index: 2
          unknown: 6
  talk05:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: Agradezco enormemente tus palabras.
      - control:
          kind: pause
          frames: 20
      - text: "\nEres un buen muchacho."
  talk06:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: Gracias por tus palabras.
      - control:
          kind: pause
          frames: 20
      - text: "\nSon como un bálsamo."
  talk07:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 4
      - text: "Link, no dudes en acudir a nosotros"
      - control:
          kind: pause
          frames: 20
      - text: "\nsi algún día te encuentras en apuros."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 1
      - text: "Todos los zora te recibiremos gustosos\nsi vuelves a visitarnos."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Gracias por todo,"
      - control:
          kind: pause
          frames: 20
      - text: " Link."
  talk08:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 11
      - text: "Gracias, Link."
      - control:
          kind: pause
          frames: 20
      - text: " Nos has salvado."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Estoy seguro de que Mipha se sintió\nmuy feliz por haber podido verte de\nnuevo.\nEstaré al pendiente de la lucha\nque ambos habrán de librar."
