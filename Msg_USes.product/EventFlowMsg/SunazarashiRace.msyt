          variable_kind: 14
          name: SunazarashiRace_BestTime_L
      - text: ":"
      - control:
          kind: variable
          variable_kind: 14
          name: SunazarashiRace_BestTime_M
      - control:
          kind: reset_colour
      - text: ",\nte alzarás con la victoria."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound2
          unknown:
            - 2
            - 205
      - text: "Y ahora basta de cháchara, que no\nsomos cotorras. Si quieres intentarlo,\nserán "
      - control:
          kind: set_colour
          colour: red
      - text: 50 rupias
      - control:
          kind: reset_colour
      - text: " por carrera.\n¿Te ves capaz de retar a la campeona?"
      - control:
          kind: choice
          choice_labels:
            - 8
            - 18
            - 9
          selected_index: 0
          cancel_index: 2
          unknown: 8
