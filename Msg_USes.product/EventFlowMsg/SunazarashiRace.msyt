  Npc_oasis039_R_Near:
    attributes: Npc_oasis039
    contents:
      - text: ¡Esa es mi Asyré!
  Npc_oasis039_S1_001:
    attributes: Npc_oasis039
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - control:
          kind: sound2
          unknown:
            - 0
            - 205
      - text: "¡La "
      - control:
          kind: set_colour
          colour: blue
      - text: carrera con morsa del desierto
      - control:
          kind: reset_colour
      - text: "\nes el deporte gerudo por excelencia!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Si superas la marca de Asyré, la vigente\ncampeona, te llevarás un trofeo único\ne inimitable.\nPuedes probar tu suerte por tan solo\n"
      - control:
          kind: set_colour
          colour: red
      - text: 50 rupias
      - control:
          kind: reset_colour
      - text: ". ¿Qué opinas?"
      - control:
          kind: choice
