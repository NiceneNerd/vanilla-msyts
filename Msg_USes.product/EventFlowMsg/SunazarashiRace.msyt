            - 8
            - 18
            - 9
          selected_index: 0
          cancel_index: 2
          unknown: 8
  Npc_oasis039_S1_002:
    attributes: Npc_oasis039
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: "¡Fabuloso!\nNo todo el mundo le echa tanto valor."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Dame "
      - control:
          kind: set_colour
          colour: red
      - text: 50 rupias
      - control:
          kind: reset_colour
      - text: " y empezamos."
  Npc_oasis039_S1_003:
    attributes: Npc_oasis039
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Ah, esta sensación de tensión justo\nantes de que empiece la carrera...\nJamás me cansaré de esto."
  Npc_oasis039_S1_007:
    attributes: Npc_oasis039
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 0
      - text: "¡Alto ahí! ¡No tienes un "
      - control:
          kind: set_colour
          colour: blue
      - text: escudo
      - control:
          kind: reset_colour
      - text: "!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "¿Cómo pretendes usar una morsa si no\ntienes un "
      - control:
          kind: set_colour
          colour: blue
      - text: escudo
      - control:
          kind: reset_colour
      - text: "? ¿Es que nunca fuiste\na la escuela?\nEquípate como se debe si quieres\ncompetir contra la campeona."
  Npc_oasis039_S1_100:
    attributes: Npc_oasis039
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Debes guiar a tu "
      - control:
          kind: set_colour
          colour: blue
      - text: morsa del desierto
      - control:
          kind: reset_colour
      - text: "\ny tratar de conseguir la mejor marca,\npasando por debajo de todos los arcos.\nSi te sueltas de la "
      - control:
          kind: set_colour
          colour: blue
      - text: morsa del desierto
      - control:
          kind: reset_colour
      - text: "\no te sales de los límites, se te descalifica\nautomáticamente.\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Si superas la marca actual, "
      - control:
          kind: set_colour
          colour: red
      - control:
