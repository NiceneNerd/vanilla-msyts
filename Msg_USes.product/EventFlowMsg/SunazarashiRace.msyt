    attributes: Npc_oasis039
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: ¡Al fin podemos salir a correr de nuevo!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Cuando la "
      - control:
          kind: set_colour
          colour: blue
      - text: bestia divina
      - control:
          kind: reset_colour
      - text: " andaba echando\nrayos por ahí, no se podía hacer nada."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
