  Book00:
    attributes: TwnObj_UmayadoHeroBook_03
    contents:
      - text: "¿Quieres leer el "
      - control:
          kind: set_colour
          colour: blue
      - text: diario de Revali
      - control:
          kind: reset_colour
      - text: "?"
      - control:
