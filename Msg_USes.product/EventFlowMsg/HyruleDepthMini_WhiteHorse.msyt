      - text: colina de Saffus
      - control:
          kind: reset_colour
      - text: "...\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "¿Podría ser del mismo linaje que el\n"
      - control:
          kind: set_colour
          colour: red
      - text: caballo blanco
      - control:
          kind: reset_colour
      - text: " que tenía la familia real\ncuando mi abuelo era pequeño?\nOye... Si ves un "
      - control:
          kind: set_colour
          colour: red
      - text: "caballo blanco "
      - control:
          kind: reset_colour
      - text: "durante\ntus viajes,"
      - control:
          kind: pause
          length: long
