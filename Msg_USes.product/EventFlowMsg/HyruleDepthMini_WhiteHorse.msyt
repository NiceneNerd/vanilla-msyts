      - text: " de la "
      - control:
          kind: set_colour
          colour: blue
      - text: "princesa\nZelda"
      - control:
          kind: reset_colour
      - text: ", del que tanto me habló mi abuelo!\n"
      - control:
          kind: sound
          unknown:
            - 1
            - 1
      - text: Es una auténtica hermosura...
      - control:
          kind: pause
          length: short
      - text: "\nSiento como si estuviese soñando."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: "No cabe duda de que desciende del\ncaballo que mencionaba mi abuelo."
      - control:
          kind: pause
          length: long
      - text: "\n¡Ah, qué sorpresas nos da la vida!"
  talk17:
    attributes: Npc_HyruleDepthHatago007
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "En el mostrador del rancho puedes\n"
