  talk20:
    attributes: Npc_HyruleDepthHatago007
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 11
      - text: "Como muestra de agradecimiento,\nte entregaré la silla y la brida de\nla familia real.\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "No obstante, te advierto de que solo\npodrás usarlas con este "
      - control:
          kind: set_colour
          colour: red
      - text: caballo blanco
      - control:
          kind: reset_colour
