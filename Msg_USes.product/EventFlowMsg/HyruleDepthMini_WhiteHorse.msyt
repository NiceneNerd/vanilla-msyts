          unknown: 6
  talk03:
    attributes: Npc_HyruleDepthHatago007
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 20
      - text: "¡Qué amable, gracias!"
      - control:
          kind: pause
          length: long
      - text: " Pues lo dicho:\nsi das con algún "
      - control:
          kind: set_colour
          colour: red
      - text: caballo blanco
      - control:
          kind: reset_colour
      - text: ", trata\nde "
      - control:
