            - 11
            - 0
      - control:
          kind: text_size
          percent: 80
      - text: "Mi hija ha recuperado la salud,"
      - control:
          kind: pause
          length: short
      - text: " así que ya no sé\nqué hacer con esta residencia adicional."
      - control:
          kind: pause
          length: longer
      - text: " Qué\npreocupación..."
      - control:
