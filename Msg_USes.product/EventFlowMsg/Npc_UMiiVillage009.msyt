          percent: 100
  talk26:
    attributes: Npc_UMiiVillage009
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 25
      - text: "Voy con la familia a tomar un rato el\nsol sin hacer nada."
      - control:
