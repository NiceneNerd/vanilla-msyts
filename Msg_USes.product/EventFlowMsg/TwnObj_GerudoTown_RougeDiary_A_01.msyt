      - control:
          kind: raw
          one:
            two:
              field_1: 0
      - text: "El voe regresó, y quiere que le\nprestemos el "
      - control:
          kind: raw
          two_hundred_one:
            dynamic:
              - 0
              - len: 4
                field_2:
                  - 1
                  - 0
                  - 0
