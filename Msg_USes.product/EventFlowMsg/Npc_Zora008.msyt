      - text: Linkitín...
      - control:
          kind: pause
          length: longer
      - text: " ¿Fuiste tú quien\npor fin aplacó a la bestia divina?"
  talk15:
    attributes: Npc_Zora008
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 11
      - text: "Gracias por aplacar a la bestia divina,\nLinkitín."
      - control:
          kind: raw
          one:
            eight:
              unknown_1: []
              field_1:
                - 12
                - 0
