      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Ay, lo siento,"
      - control:
          kind: pause
          length: longer
      - text: " Linkitín."
  talk14:
    attributes: Npc_Zora008
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 20
      - text: "Ah, "
      - control:
