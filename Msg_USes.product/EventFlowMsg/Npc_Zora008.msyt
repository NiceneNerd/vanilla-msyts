    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 18
      - text: Ya dejó de llover...
      - control:
          kind: pause
          length: longer
      - text: " ¿Será que "
      - control:
          kind: set_colour
          colour: blue
      - text: Ruta
      - control:
          kind: reset_colour
      - text: "\nse quedó quieta al fin?"
      - control:
