group_count: 53
atr1_unknown: 4
entries:
  Atacked00:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 32
      - text: Uf...
      - control:
          kind: pause
          length: short
      - text: "Uf... "
      - control:
          kind: pause
          length: long
      - text: Faltó poco.
      - control:
          kind: pause
          length: short
      - text: "\nCasi no la contamos..."
  Atacked01:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 42
      - text: "Todavía hay algunos "
      - control:
          kind: raw
          two_hundred_one:
            dynamic:
              - 0
              - len: 4
                field_2:
                  - 1
                  - 0
                  - 0
                  - 0
      - text: "monstruos\npor aquí..."
  Failed00:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 32
      - text: "Ay, ay, ay... "
      - control:
          kind: pause
          length: short
      - text: "Me duele todo el cuerpo...\n¿Será este mi final?"
  Failed01:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 12
      - text: "Me estoy muriendo de los nervios... "
      - control:
          kind: pause
          length: short
      - text: "\n¿Crees que nos atacarán de nuevo?"
  Guardian00:
    attributes: Npc_Road_003
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 31
      - text: Salgamos de aquí inmediatamente...
      - control:
          kind: pause
          length: long
      - text: "\nHay un "
      - control:
          kind: set_colour
          colour: blue
      - text: guardián
      - control:
          kind: reset_colour
      - text: " buscándonos."
  MSG_Rane_00:
    attributes: Npc_Road_003
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "Ay, me preocupa el caudal del Río Zora..."
      - control:
          kind: pause
          length: long
      - text: "\nÚltimamente ha llovido a raudales..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Los pobres zora
      - control:
          kind: pause
          length: short
      - text: " ni siquiera pueden\ncolgar la ropa para que se seque."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 5
            - 6
      - text: Un momento.
      - control:
          kind: pause
          length: short
      - text: " ¿Sí usan ropa?"
      - control:
          kind: pause
          length: long
      - text: "\nQué confuso..."
      - control:
          kind: pause
          length: long
      - text: " ¿Llevan ropa o no?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ay, ¿estaré perdiendo la memoria?..."
  MSG_Rane_00_1:
    attributes: Npc_Road_003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: Está cayendo un tormentón río arriba...
      - control:
          kind: pause
          length: short
      - text: "\nY no parece que tenga intención\nde parar."
  MSG_Rane_01:
    attributes: Npc_Road_003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 6
      - text: ¿Alguna vez has visto a algún zora?
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Es una tribu que vive en una región\nmuy húmeda."
      - control:
          kind: pause
          length: short
      - text: " Sería interesante\nconocer el lugar en el que viven...\nPero me preocupa que el camino sea\npeligroso. Todo aquel que vaya a ese\nlugar debe tener verdaderas agallas..."
  MSG_Rane_01_1:
    attributes: Npc_Road_003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 6
      - text: ¿Ya has conocido a algún zora?
      - control:
          kind: pause
          length: longer
      - text: "\nSon fácilmente reconocibles por..."
      - control:
          kind: pause
          length: short
      - text: "\nya sabes..."
      - control:
          kind: pause
          length: short
      - text: " las aletas y eso."
  MSG_Rane_02:
    attributes: Npc_Road_003
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "No estoy seguro de que puedas llegar\nhasta allí..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "En cualquier caso, "
      - control:
          kind: pause
          length: short
      - text: "te daré indicaciones\nde cómo llegar a la "
      - control:
          kind: set_colour
          colour: blue
      - text: aldea Kakariko
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Es muy sencillo:"
      - control:
          kind: pause
          length: long
      - text: " ve hacia el este en\nel siguiente cruce."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Y ya está...
      - control:
          kind: pause
          length: short
      - text: " Pero te repito que no estoy\nseguro de que puedas llegar."
  MSG_Rane_02_1:
    attributes: Npc_Road_003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Para llegar a la "
      - control:
          kind: set_colour
          colour: blue
      - text: aldea Kakariko
      - control:
          kind: reset_colour
      - text: ", ve hacia\nel este en el siguiente cruce."
      - control:
          kind: pause
          length: longer
      - text: " Espero\nque no te pase nada por el camino..."
  MSG_Rane_03:
    attributes: Npc_Road_003
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "Me preocupan los granjeros\nde la "
      - control:
          kind: set_colour
          colour: blue
      - text: aldea Kakariko
      - control:
          kind: reset_colour
      - text: "..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Son conocidos por sus calabazas y\nsus zanahorias, pero parece que han\ntenido problemas últimamente."
  MSG_Rane_03_1:
    attributes: Npc_Road_003
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "Los granjeros de "
      - control:
          kind: set_colour
          colour: blue
      - text: Kakariko
      - control:
          kind: reset_colour
      - text: " están\ndiscutiendo por cuál debería ser\nel producto estrella de la aldea.\n¿Las zanahorias o las calabazas?"
      - control:
          kind: pause
          length: longer
      - text: "\nNo es una decisión fácil..."
  MSG_Rane_04:
    attributes: Npc_Road_003
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "Me preocupa la jefa\nde la "
      - control:
          kind: set_colour
          colour: blue
      - text: aldea Kakariko
      - control:
          kind: reset_colour
      - text: "..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Seguro que ha vivido tanto tiempo
      - control:
          kind: pause
          length: short
      - text: "\nque su pastel de cumpleaños está\nenterrado bajo una montaña de velas..."
  MSG_Rane_04_1:
    attributes: Npc_Road_003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "¿Qué edad tendrá la jefa de la "
      - control:
          kind: set_colour
          colour: blue
      - text: "aldea\nKakariko"
      - control:
          kind: reset_colour
      - text: "?"
      - control:
          kind: pause
          length: short
      - text: " Lo que debe tardar en apagar\ntodas la velas de cumpleaños..."
  Near00:
    attributes: Npc_Road_003
    contents:
      - text: No soy madrugador...
  Near01:
    attributes: Npc_Road_003
    contents:
      - text: Así no se puede vivir...
  Near02:
    attributes: Npc_Road_003
    contents:
      - text: Podría ser peor...
  Near03:
    attributes: Npc_Road_003
    contents:
      - text: ¿Y ahora qué?
  Near04:
    attributes: Npc_Road_003
    contents:
      - text: Estoy empapado...
  Rest_00:
    attributes: Npc_Road_003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: "A diferencia de ti, yo soy un simple\nviajero,"
      - control:
          kind: pause
          length: short
      - text: " así que nunca se me ocurriría ir\na lugares como la llanura de Hyrule.\nIncluso si están un poco lejos, prefiero\nexplorar lugares como la "
      - control:
          kind: set_colour
          colour: blue
      - text: aldea Kakariko
      - control:
          kind: reset_colour
      - text: "\no el "
      - control:
          kind: set_colour
          colour: blue
      - text: rancho del pantano
      - control:
          kind: reset_colour
      - text: "."
  Rest_01:
    attributes: Npc_Road_003
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "Me preocupa mucho que estés tanto\nrato bajo la lluvia."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ay, ya me imagino lo molesto que estarás"
      - control:
          kind: pause
          length: short
      - text: "\ncuando intentes escalar una colina pero\nno puedas por culpa de la lluvia."
  Rest_02:
    attributes: Npc_Road_003
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "Dicen que es imposible que llueva\nsin parar,"
      - control:
          kind: pause
          length: short
      - text: " pero ¿y si esta es la primera\nvez que ocurre?"
  RetMSG_Rane_00:
    attributes: Npc_Road_003
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: No gano para disgustos...
      - control:
          kind: pause
          length: short
      - text: "\nIrás hacia el noroeste diga\nlo que te diga, ¿verdad?\nSi es así, ve con cuidado cuando te\nacerques al castillo de Hyrule."
      - control:
          kind: pause
          length: short
      - text: "\n¡Y procura equiparte adecuadamente!"
  RetMSG_Rane_00_1:
    attributes: Npc_Road_003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "La zona hacia el noroeste, cerca del\n"
      - control:
          kind: set_colour
          colour: blue
      - text: castillo de Hyrule
      - control:
          kind: reset_colour
      - text: ", es muy peligrosa.\nEquípate bien si tienes que ir por allí."
  RetMSG_Rane_01:
    attributes: Npc_Road_003
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: Tengo un mal presentimiento...
      - control:
          kind: pause
          length: short
      - text: "\nTal vez sepa demasiado sobre\nel castillo de Hyrule...\nPuedes colarte en el castillo por un\npasadizo secreto situado en la\n"
      - control:
          kind: set_colour
          colour: blue
      - text: galería oeste
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: pause
          length: long
      - text: " Dicen que hay un tesoro..."
  RetMSG_Rane_01_1:
    attributes: Npc_Road_003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Se puede entrar en el castillo por un\npasadizo secreto situado en la "
      - control:
          kind: set_colour
          colour: blue
      - text: "galería\noeste"
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: pause
          length: long
      - text: " ¿Habrá un tesoro dentro?..."
  RetMSG_Rane_02:
    attributes: Npc_Road_003
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "Ay, creo que hablé de más..."
      - control:
          kind: pause
          length: short
      - text: " Seguro que\nahora vas a ir derechito por el tesoro,\npor muy peligroso que sea...\nCuentan que hay armas muy poderosas,"
      - control:
          kind: pause
          length: short
      - text: "\npero que por el camino..."
      - control:
          kind: pause
          length: short
      - text: "\nAy, que me va a dar algo."
  RetMSG_Rane_02_1:
    attributes: Npc_Road_003
    contents:
      - text: "Dicen que en el castillo hay armas muy\npoderosas, pero..."
      - control:
          kind: pause
          length: longer
      - text: " "
      - control:
          kind: sound
          unknown:
            - 0
            - 6
      - text: "Irás a buscar el tesoro\ndiga lo que diga, ¿no? Ay..."
  RetMSG_Rane_03:
    attributes: Npc_Road_003
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "Cocinar zanahorias no me da buena\nespina, la verdad..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Se ve que si mezclas dos tipos diferentes\nde zanahoria,"
      - control:
          kind: pause
          length: short
      - text: " te sale un bodrio de\nplato.\nPero seguro que ahora vas a probarlo\npor diversión..."
      - control:
          kind: pause
          length: short
      - text: " Luego no digas que no\nte lo advertí."
  RetMSG_Rane_03_1:
    attributes: Npc_Road_003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Al parecer, no es aconsejable mezclar\nzanahorias de dos tipos diferentes en\nuna receta."
      - control:
          kind: pause
          length: short
      - text: " Te saldrá un bodrio de plato."
  RetMSG_Rane_04:
    attributes: Npc_Road_003
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "Creo que le echo demasiada sal\na la comida."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Pero es que es tan fácil conseguir\n"
      - control:
          kind: set_colour
          colour: blue
      - control:
          kind: raw
          two_hundred_one:
            dynamic:
              - 0
              - len: 4
                field_2:
                  - 2
                  - 1
                  - 1
                  - 0
      - text: "halita "
      - control:
          kind: reset_colour
      - text: "en la "
      - control:
          kind: set_colour
          colour: blue
      - text: aldea Kakariko
      - control:
          kind: reset_colour
      - text: "..."
      - control:
          kind: pause
          length: short
      - text: "\nY va bien con todo."
  RetMSG_Rane_04_1:
    attributes: Npc_Road_003
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "Echar un poco de "
      - control:
          kind: set_colour
          colour: blue
      - control:
          kind: raw
          two_hundred_one:
            dynamic:
              - 0
              - len: 4
                field_2:
                  - 2
                  - 1
                  - 1
                  - 0
      - text: halita
      - control:
          kind: reset_colour
      - text: " a los platos\nles da tan buen sabor... Y en "
      - control:
          kind: set_colour
          colour: blue
      - text: Kakariko
      - control:
          kind: reset_colour
      - text: "\nes muy fácil conseguirla."
  SavedLv1_00:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 18
      - text: "¿Ya se fueron todos los "
      - control:
          kind: raw
          two_hundred_one:
            dynamic:
              - 0
              - len: 4
                field_2:
                  - 1
                  - 0
                  - 0
                  - 0
      - text: monstruos?
      - control:
          kind: pause
          length: short
      - text: "\n¿Y bien?"
  SavedLv1_01:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 18
      - text: "Estaba seguro de que no saldría\nde esta..."
  SavedLv2_00:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 18
      - text: ¡Mi vida entera pasó frente a mis ojos!
  SavedLv2_01:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 18
      - text: No soy
      - control:
          kind: pause
          length: short
      - text: " un aventurero ni nada\nparecido, ¿sabes?"
  SavedLv3_00:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 18
      - text: "Ahora hay muchos más "
      - control:
          kind: raw
          two_hundred_one:
            dynamic:
              - 0
              - len: 4
                field_2:
                  - 1
                  - 0
                  - 0
                  - 0
      - text: "monstruos\nrondando la zona..."
  SavedLv3_01:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 18
      - text: "Debemos aprovechar y salir corriendo\nahora que podemos."
  Thanks00:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 11
      - text: Gracias por salvarme.
      - control:
          kind: pause
          length: short
      - text: " Toma, acepta\nesto como recompensa."
