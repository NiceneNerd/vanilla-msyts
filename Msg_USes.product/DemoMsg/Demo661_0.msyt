  Demo661_0_Text017:
    contents:
      - text: Ay... ¡Qué vergüenza!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Sentí pavor al ver que era un pulgoso.\nDe niño siempre me asustaban."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: No creí que me seguirían dando miedo.
  Demo661_0_Text018:
    contents:
      - text: "Todos tenemos una debilidad, Daruk.\n¡Incluso tú!"
  Demo661_0_Text019:
    contents:
      - text: " "
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Dime, princesa..."
  Demo661_0_Text020:
    contents:
      - text: ¿Sí?
  Demo661_0_Text021:
    contents:
      - text: Ese tal Ganon...
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "No tomará una forma canina, ¿verdad?"
  Demo661_0_Text022:
    contents:
      - text: " "
