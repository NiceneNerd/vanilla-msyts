  Demo646_0_Text001:
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 4
      - text: "¿Es solo mi impresión o cambiaste\ndesde la última vez que nos vimos?\nTu determinación parece mayor.\nTengo una canción que me gustaría\ninterpretarte."
  Demo646_0_Text002:
    contents:
      - text: "¿Te acuerdas de la "
      - control:
          kind: set_colour
          colour: blue
      - text: balada inacabada
      - control:
          kind: reset_colour
      - text: "\nque dejó mi maestro?"
      - control:
