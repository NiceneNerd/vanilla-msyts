group_count: 2
atr1_unknown: 0
entries:
  QL_Gerudo_Ch_SnowBoots_Desc:
    contents:
      - text: "여덟 번째 영웅은 사실 "
      - control:
          kind: set_colour
          colour: red
      - text: 검
      - control:
          kind: reset_colour
      - text: "을 가지고 있었다!\n\n보텐사의 추측일지도 모르지만\n찾아볼 가치는 있는 것 같다\n여덟 번째 영웅이 가지고 있었다는 "
      - control:
          kind: set_colour
          colour: red
      - text: 검
      - control:
          kind: reset_colour
      - text: "은\n겔드 고지의 어딘가에 잠들어 있을 가능성이 높다\n여덟 번째 영웅의 검의 사진을 보텐사에게 보여주고\n"
      - control:
          kind: set_colour
          colour: red
      - text: 스노 부츠
      - control:
          kind: reset_colour
      - text: 를 받자
  QL_Gerudo_Ch_SnowBoots_Finish:
    contents:
      - text: "겔드 고지의 정상에서\n여덟 번째 영웅의 검을 발견했다\n\n그 검을 사진에 담고\n약속대로 보텐사에게 스노 부츠를 받았다"
  QL_Gerudo_Ch_SnowBoots_Name:
    contents:
      - text: 잊혀진 검
