  talk22:
    attributes: Npc_HighMountain013
    contents:
      - control:
          kind: sound
          unknown:
            - 2
            - 14
      - text: "야, 그거"
      - control:
          kind: pause
          length: short
      - text: "\n내가 지금 말하려고 했던 거야!"
  talk23:
    attributes: Npc_HighMountain025
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 0
      - text: 뭐야!
      - control:
          kind: pause
          length: longer
      - text: "\n어차피 너도 "
      - control:
          kind: set_colour
          colour: blue
      - text: 족장님
      - control:
          kind: reset_colour
      - text: 께 들은 얘기잖아
  talk24:
    attributes: Npc_HighMountain013
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 19
      - text: 응?
      - control:
          kind: pause
          length: short
