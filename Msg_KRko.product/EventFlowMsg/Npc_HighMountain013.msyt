  talk02:
    attributes: Npc_HighMountain013
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 16
      - text: "예를 들면 "
      - control:
          kind: set_colour
          colour: blue
      - text: 북쪽 설산
      - control:
          kind: reset_colour
      - text: "이나 "
      - control:
          kind: set_colour
          colour: blue
      - text: 여기보다 높은 곳
      - control:
          kind: reset_colour
      - text: 은
      - control:
          kind: pause
          length: short
      - text: "\n익숙지 않으면 동사할 정도로 춥거든\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "하지만 우리집 "
      - control:
          kind: set_colour
          colour: blue
      - text: 옷
      - control:
          kind: reset_colour
      - text: 을 입으면
      - control:
          kind: pause
          length: short
      - text: "\n익숙지 않은 사람도 추위를 견딜 수 있게 되지\n자, 일단"
      - control:
          kind: pause
          length: short
      - text: "\n안에 들어가서 구경해 봐"
  talk03:
    attributes: Npc_HighMountain013
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 11
      - text: 고마워!
      - control:
          kind: pause
          length: short
      - text: " 그럼 안에서 골라 봐"
  talk04:
    attributes: Npc_HighMountain013
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: 또 와!
  talk05:
    attributes: Npc_HighMountain013
