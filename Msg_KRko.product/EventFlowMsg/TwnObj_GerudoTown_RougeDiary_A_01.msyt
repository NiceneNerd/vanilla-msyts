  DiaryFirst:
    attributes: TwnObj_GerudoTown_RougeDiary_A_01
    contents:
      - text: "『족장 일기』\n마키·아·루쥬"
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
            - 2
          selected_index: 0
          cancel_index: 2
