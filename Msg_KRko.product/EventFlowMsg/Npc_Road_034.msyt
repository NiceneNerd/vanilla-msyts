  SavedLv3_00:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 18
      - text: "후우… "
      - control:
          kind: pause
          length: long
      - text: 이 부근도 몬스터가 늘었어…
  SavedLv3_01:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 11
      - text: 네 덕분에 목숨을 건졌어…
  Thanks00:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 11
      - text: "이건 보답이야! "
      - control:
          kind: pause
          length: long
