          kind: set_colour
          colour: red
      - control:
          kind: raw
          two_hundred_one:
            dynamic:
              - 0
              - len: 4
                field_2:
                  - 2
                  - 1
                  - 1
                  - 0
      - text: libélula ígnea
      - control:
          kind: reset_colour
      - text: ", una "
      - control:
          kind: set_colour
          colour: red
      - control:
          kind: raw
          two_hundred_one:
            dynamic:
              - 0
              - len: 4
                field_2:
                  - 2
                  - 1
                  - 1
                  - 0
      - text: libélula gélida
      - control:
          kind: reset_colour
      - text: "\ny una "
      - control:
          kind: set_colour
          colour: red
      - control:
          kind: raw
          two_hundred_one:
