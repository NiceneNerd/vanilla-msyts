  QL_Gerudo_tsukamidake_Desc:
    contents:
      - text: "Has conocido a Pilou en la posta del cañón,\ny te ha dicho que no es capaz de vivir sin\ncomer setas raudas a menudo.\n\nComo crecen en zonas de acantilados,\nya no está en condiciones de ir a buscarlas,\npero te dará un diamante a cambio si le llevas\n"
      - control:
          kind: set_colour
          colour: red
      - text: 55 setas raudas
      - control:
          kind: reset_colour
      - text: "."
  QL_Gerudo_tsukamidake_Finish:
    contents:
      - text: "Has ido a la posta del cañón a entregarle\n55 setas raudas a Pilou, y te ha pagado\ncon un "
      - control:
          kind: raw
          two_hundred_one:
            dynamic:
              - 0
              - len: 4
                field_2:
                  - 1
                  - 0
                  - 0
                  - 0
      - text: "diamante.\n\nCon todo, parece que no se ha conformado\ncon tal cantidad, y dice que está dispuesto\na comprarte otras "
      - control:
          kind: set_colour
          colour: red
      - text: "55 "
      - control:
          kind: reset_colour
      - text: "por el "
      - control:
          kind: set_colour
          colour: red
      - text: "doble de su\nprecio habitual"
      - control:
          kind: reset_colour
      - text: "."
  QL_Gerudo_tsukamidake_Name:
    contents:
      - text: Setas a raudales
