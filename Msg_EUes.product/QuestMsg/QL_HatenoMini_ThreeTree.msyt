          kind: set_colour
          colour: red
      - text: "tres cedros que\nse hacen uno"
      - control:
          kind: reset_colour
      - text: ", dirígete "
      - control:
          kind: set_colour
          colour: red
      - text: "hacia el mar "
      - control:
          kind: reset_colour
      - text: "y\nsupera la prueba encerrada en la roca»."
  QL_HatenoMini_ThreeTree_Finished:
