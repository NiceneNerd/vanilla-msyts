group_count: 3
atr1_unknown: 0
entries:
  QL_Hateno_SheikPad_PowerUp_Desc:
    contents:
      - text: "Prunia, la jefa del laboratorio de Hatelia,\nte ha contado que podría darte algo útil\nsi le traes "
      - control:
          kind: set_colour
          colour: red
      - text: materiales ancestrales
      - control:
          kind: reset_colour
      - text: ".\n\n¿Qué serán esos "
      - control:
          kind: set_colour
          colour: red
      - text: materiales
      - control:
          kind: reset_colour
      - text: "? ¿Y quién\nes "
      - control:
          kind: set_colour
          colour: red
      - text: Rotver
      - control:
          kind: reset_colour
      - text: "? Habla con Prunia para obtener\nmás información."
  QL_Hateno_SheikPad_PowerUp_Explain:
    contents:
      - text: "Prunia podría mejorar estos módulos\nde la "
      - control:
          kind: raw
          two_hundred_one:
            dynamic:
              - 0
              - len: 4
                field_2:
                  - 2
                  - 1
                  - 1
                  - 0
