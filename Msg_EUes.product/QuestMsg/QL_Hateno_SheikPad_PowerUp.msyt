      - control:
          kind: set_colour
          colour: red
      - text: "materiales\nancestrales"
      - control:
          kind: reset_colour
      - text: ":\n\n"
      - control:
          kind: raw
          two_hundred_one:
            dynamic:
              - 0
              - len: 4
                field_2:
                  - 1
                  - 0
                  - 0
                  - 0
      - text: "Sensor sheikah: "
      - control:
          kind: set_colour
          colour: red
      - text: 3 tornillos ancestrales
      - control:
          kind: reset_colour
      - text: "\n"
      - control:
          kind: raw
          two_hundred_one:
            dynamic:
              - 0
              - len: 4
                field_2:
                  - 2
                  - 3
                  - 3
                  - 1
      - text: "Bombas remotas: "
      - control:
          kind: set_colour
          colour: red
      - text: 3 ejes ancestrales
      - control:
          kind: reset_colour
      - text: "\n"
      - control:
          kind: raw
          two_hundred_one:
            dynamic:
              - 0
              - len: 4
                field_2:
                  - 1
                  - 0
                  - 0
                  - 0
      - text: "Paralizador: "
      - control:
          kind: set_colour
          colour: red
      - text: 3 núcleos ancestrales
      - control:
          kind: reset_colour
  QL_Hateno_SheikPad_PowerUp_Finish:
    contents:
      - text: "¡Prunia te ha mejorado el "
      - control:
          kind: raw
          two_hundred_one:
            dynamic:
              - 0
              - len: 4
                field_2:
                  - 1
                  - 0
                  - 0
                  - 0
      - text: "sensor sheikah,\nlas "
      - control:
          kind: raw
          two_hundred_one:
            dynamic:
              - 0
              - len: 4
                field_2:
                  - 2
                  - 3
                  - 3
                  - 1
      - text: "bombas remotas y el "
      - control:
          kind: raw
          two_hundred_one:
            dynamic:
              - 0
              - len: 4
                field_2:
                  - 1
                  - 0
                  - 0
                  - 0
      - text: "paralizador!\n\nParece que ya no puede mejorarlos más,\npero seguro que te serán muy útiles."
