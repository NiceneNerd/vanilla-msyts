entries:
  DragonFirstEncounter00:
    contents:
      - text: "Bienvenido,"
      - control:
          kind: pause
          length: short
      - text: " joven..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Te felicito por haber llegado\nhasta esta fuente."
  DragonFirstEncounter01:
    contents:
      - text: "Tú que has superado los desafíos de\nlos santuarios y recibido "
      - control:
          kind: set_colour
          colour: blue
      - text: "símbolos\nde valía"
      - control:
          kind: reset_colour
      - text: "..."
  DragonFirstEncounter02:
    contents:
      - text: "Este es "
      - control:
          kind: set_colour
          colour: blue
      - text: Nayen
      - control:
          kind: reset_colour
      - text: ", espíritu azul que\nreside en Lanayru, y guardián de\nla "
      - control:
          kind: set_colour
          colour: blue
      - text: Fuente de la Sabiduría
      - control:
          kind: reset_colour
      - text: ".\nDesde tiempos remotos, este siervo\nde la Diosa ha velado por los espíritus\nque pueblan estas tierras...\nSin embargo, ahora está corrompido\npor el poder maligno que surgió de\nGanon, el Cataclismo."
  DragonFirstEncounter03:
    contents:
      - text: Tú que has recibido símbolos de valía...
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Libera al espíritu azul, "
      - control:
          kind: set_colour
          colour: blue
      - text: Nayen
      - control:
          kind: reset_colour
      - text: ", del mal\nque lo corrompe. Muéstrame hasta\ndónde llega tu poder."
