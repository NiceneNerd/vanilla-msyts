group_count: 17
atr1_unknown: 0
entries:
  "0000":
    contents:
      - text: "Fija un enemigo y usa "
      - control:
          kind: icon
          icon:
            a: 10
      - text: " justo\nantes de que te golpee para\nrealizar una "
      - control:
          kind: set_colour
          colour: red
      - text: guardia perfecta
      - control:
          kind: reset_colour
      - text: "."
  "0001":
    contents:
      - text: "Salta hacia atrás o a los lados\nen el momento justo para\nrealizar una "
      - control:
          kind: set_colour
          colour: red
      - text: elusión perfecta
      - control:
          kind: reset_colour
      - text: "."
  "0002":
    contents:
      - text: "Con un escudo equipado, salta\ncon "
      - control:
          kind: icon
          icon:
            x: 12
      - text: " y pulsa "
      - control:
          kind: icon
          icon:
            a: 10
      - text: " para\ndeslizarte con el escudo."
  "0003":
    contents:
      - text: "Los disparos a la cabeza se\nconsideran "
      - control:
          kind: set_colour
          colour: red
      - text: golpes críticos
      - control:
          kind: reset_colour
      - text: "\ne infligen el doble de daño."
  "0004":
    contents:
      - text: "Si se te agota la resistencia, te\nmoverás más despacio hasta\nque el indicador se rellene."
  "0005":
    contents:
      - text: "Hace demasiado frío.\nSi no le pones remedio,\nempezarás a perder energía."
  "0006":
    contents:
      - text: "Hace demasiado calor.\nSi no le pones remedio,\nempezarás a perder energía."
  "0007":
    contents:
      - text: "Si te acercas a un enemigo\ny lo atacas por sorpresa,\nle infligirás más daño."
  "0008":
    contents:
      - text: "El golpe de un arma justo antes\nde romperse siempre será\n"
      - control:
          kind: set_colour
          colour: red
      - text: crítico
      - control:
          kind: reset_colour
      - text: " y hará el doble de daño."
  "0009":
    contents:
      - text: "Lanza armas a los enemigos\npara causar "
      - control:
          kind: set_colour
          colour: red
      - text: golpes críticos
      - control:
          kind: reset_colour
      - text: "\nque infligen el doble de daño."
  "0010":
    contents:
      - text: "El aire está demasiado caliente.\nSi te quedas aquí más tiempo,\nempezarás a perder energía."
  "0013":
    contents:
      - text: "Es más difícil escalar cuando\nllueve porque las superficies\nse vuelven resbaladizas."
  "0014":
    contents:
      - text: "Las armas y los escudos de\nmadera empezarán a arder\nsi los usas en esta zona."
  "0015":
    contents:
      - text: "Agáchate para acercarte a\nlos enemigos y a los animales\n"
      - control:
          kind: set_colour
          colour: red
      - text: sigilosamente
      - control:
          kind: reset_colour
      - text: "."
  "0016":
    contents:
      - text: "Debes equipar un escudo\npara poder viajar en una\nmorsa del desierto."
  "0017":
    contents:
      - text: "Si el "
      - control:
          kind: raw
          two_hundred_one:
            dynamic:
              - 0
              - len: 4
                field_2:
                  - 1
                  - 0
                  - 0
                  - 0
      - text: "sensor sheikah detecta\nalgo, Link Lobo te guiará\nhasta el lugar exacto."
  "0018":
    contents:
      - text: "En la alforja, elige "
      - control:
          kind: set_colour
          colour: red
      - text: Ingredientes
      - control:
          kind: reset_colour
      - text: "\npara ver qué materiales has\nusado al cocinar un plato."
  "0019":
    contents:
      - text: "Si te electrocutan, dejarás caer\nel "
      - control:
          kind: set_colour
          colour: red
      - text: arma
      - control:
          kind: reset_colour
      - text: " que lleves en la mano."
  "0020":
    contents:
      - text: "Hace mucho "
      - control:
          kind: set_colour
          colour: red
      - text: frío
      - control:
          kind: reset_colour
      - text: ". Necesitarás\nmás protección para soportarlo."
  "0021":
    contents:
      - text: "Hace mucho "
      - control:
          kind: set_colour
          colour: red
      - text: calor
      - control:
          kind: reset_colour
      - text: ". Necesitarás\nmás protección para soportarlo."
  "0022":
    contents:
      - text: "En esta zona el aire está muy\ncaliente. Necesitarás más\nprotección para soportarlo."
  "0023":
    contents:
      - text: "Puedes controlar ciertos\nelementos de la bestia divina\ndesde la "
      - control:
          kind: set_colour
          colour: red
      - text: pantalla del mapa
      - control:
          kind: reset_colour
      - text: "."
  "0024":
    contents:
      - text: "La Moto Hyliana Alfa recupera\nla "
      - control:
          kind: set_colour
          colour: red
      - text: energía
      - control:
          kind: reset_colour
      - text: " si se se añaden\nmateriales en su depósito."
  "0025":
    contents:
      - text: "Cuando uses el visor VR,\nprocura descansar a menudo."
  T_Guide_00:
    contents:
      - text: Controles del juego
  TextBox_00:
    contents:
      - text: 
