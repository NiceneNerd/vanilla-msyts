      - text: "Set 1: Las pruebas legendarias"
  "0010":
    contents:
      - text: Teletransportador
  "0011":
    contents:
      - text: "A partir de ahora puedes activar el "
      - control:
          kind: raw
          two_hundred_one:
            dynamic:
              - 0
              - len: 4
                field_2:
                  - 1
