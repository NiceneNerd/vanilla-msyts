    attributes: Npc_oasis003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 18
      - text: "Creo que por hoy ya he cumplido con\nmis deberes como matriarca."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Cuando "
