    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 18
      - text: "Has dado buena cuenta de esos\nhorribles "
      - control:
          kind: raw
          two_hundred_one:
            dynamic:
              - 0
              - len: 4
                field_2:
                  - 1
                  - 0
                  - 0
                  - 0
      - text: "monstruos, ¿verdad?"
      - control:
          kind: pause
          length: long
      - text: "\n¡Dime que sí, por favor!"
  SavedLv1_01:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 18
      - text: "Qué alivio... "
      - control:
          kind: pause
          length: short
      - text: "¡Casi no puedo creer\nque siga de una pieza!"
  SavedLv2_00:
    attributes: ""
    contents:
      - control:
