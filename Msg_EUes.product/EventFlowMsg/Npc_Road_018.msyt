          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 19
      - text: "Si andas corto de rupias y tienes alguna\nde esas bellezas, ¡te puedes hacer de\noro en un periquete!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ay, lo siento."
      - control:
          kind: pause
          length: short
      - text: " En cuanto me lanzo a\nhablar de piedras preciosas, no paro."
  MSG_Futa_03_1:
    attributes: Npc_Road_018
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Por los "
      - control:
          kind: set_colour
          colour: blue
      - control:
