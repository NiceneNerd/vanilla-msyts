      - text: aldea Kakariko
      - control:
          kind: reset_colour
      - text: " por un casual,\n¿verdad?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Porque no tienes más que tirar hacia\nel norte."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: "Yo voy allí de vez en cuando a vender\nminerales."
  MSG_Futa_02_1:
    attributes: Npc_Road_018
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Para ir a la "
      - control:
          kind: set_colour
          colour: blue
      - text: aldea Kakariko
      - control:
          kind: reset_colour
      - text: " no tienes\nmás que seguir el camino hacia el norte."
  MSG_Futa_03:
    attributes: Npc_Road_018
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: Mmm...
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "¡Uy, perdona!"
      - control:
          kind: pause
          length: short
      - text: " Estaba embobada soñando\ndespierta y ni me había fijado que\nandabas por aquí..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: ¿Que en qué estaba pensando?
      - control:
          kind: pause
          length: short
      - text: "\n¡En "
      - control:
          kind: set_colour
          colour: blue
      - control:
          kind: raw
          two_hundred_one:
            dynamic:
              - 0
              - len: 4
                field_2:
                  - 1
                  - 0
                  - 0
                  - 0
      - text: rubís
