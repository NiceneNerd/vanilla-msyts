    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 11
      - text: "Si sigo viva, es sin duda gracias a ti."
  SavedLv3_01:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 18
      - text: "De no haber aparecido tú, ahora sería\npasto de..."
      - control:
          kind: pause
          length: long
      - text: " Mejor borro eso de mi mente."
  Thanks00:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 11
      - text: "Déjame darte las gracias como es\ndebido y acepta esto, por favor."
