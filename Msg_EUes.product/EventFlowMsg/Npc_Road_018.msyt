  MSG_Futa_00:
    attributes: Npc_Road_018
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "Hum... "
      - control:
          kind: pause
          length: short
      - text: "No tienes cara de que te vaya\na interesar el tema..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Me refiero a las piedras preciosas.\nEstaba a punto de hablarte de ellas,\npero tampoco quiero aburrirte."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Aunque, si te dijera que puedes\nvenderlas a precios desorbitados,\n"
      - control:
          kind: sound
