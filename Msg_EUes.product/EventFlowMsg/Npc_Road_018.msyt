      - text: " y "
      - control:
          kind: set_colour
          colour: blue
      - control:
          kind: raw
          two_hundred_one:
            dynamic:
              - 0
              - len: 4
                field_2:
                  - 1
                  - 0
                  - 0
                  - 0
      - text: zafiros
      - control:
          kind: reset_colour
      - text: ", obviamente!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "¡Esas gemas en particular se pueden\nvender a precios de lo más abultados!"
      - control:
