      - text: " A ver si me hago con\nun martillo y voy yo misma para allá."
  MSG_Futa_02:
    attributes: Npc_Road_018
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: "Oye, no estarás pensando en ir a\nla "
      - control:
