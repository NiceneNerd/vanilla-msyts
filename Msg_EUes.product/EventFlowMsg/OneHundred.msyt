          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Qué va, seguro que están todos bien."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Sí, fijo. Por eso estoy aquí esperando,\n¿sabes?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Si te los encuentras por el camino,\ndiles que aguardo aquí, ¿vale?"
