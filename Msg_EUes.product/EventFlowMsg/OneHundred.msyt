                field_2:
                  - 1
                  - 0
                  - 0
                  - 0
      - text: "monstruos.\nMe dieron tal susto que empecé a\ncorrer y no paré hasta llegar aquí."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 9
            - 16
      - text: Y luego...
      - control:
