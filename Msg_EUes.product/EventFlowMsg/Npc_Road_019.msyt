          cancel_index: 2
          unknown: 8
  Thanks00:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 1
      - text: "Acepta esta pequeña muestra de\nagradecimiento, hazme el favor."
