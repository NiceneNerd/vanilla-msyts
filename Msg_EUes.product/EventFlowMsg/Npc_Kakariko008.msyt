      - control:
          kind: sound
          unknown:
            - 4
            - 19
      - text: Vaya...
      - control:
          kind: pause
          length: short
      - text: " Eras tú. "
      - control:
          kind: pause
          length: longer
      - text: "Pensaba que\neras Lemm."
