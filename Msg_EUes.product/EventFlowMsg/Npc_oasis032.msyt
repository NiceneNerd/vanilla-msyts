    attributes: Npc_oasis032
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "¿No ves que estoy agotada?\nDéjame descansar."
  Talk_06:
    attributes: Npc_oasis032
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Las "
      - control:
          kind: set_colour
          colour: blue
      - text: carreras
      - control:
          kind: reset_colour
      - text: " consisten en correr\nen "
      - control:
          kind: set_colour
          colour: blue
      - text: morsa del desierto
      - control:
          kind: reset_colour
      - text: " y ver quién\nhace el recorrido en menos tiempo.\nEs un deporte tradicional gerudo."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Si te interesa, ve a echar un vistazo.\nSolo debes salir por la "
      - control:
          kind: set_colour
          colour: blue
      - text: puerta sureste
      - control:
          kind: reset_colour
