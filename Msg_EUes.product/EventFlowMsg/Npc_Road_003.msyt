      - text: "No soy un aventurero ni nada\nparecido, ¿sabes? "
      - control:
          kind: pause
          length: short
      - text: "Cuanto más\nlejos esté del peligro, mejor."
  SavedLv3_00:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 18
      - text: "Ahora hay muchos más "
      - control:
          kind: raw
          two_hundred_one:
            dynamic:
              - 0
              - len: 4
