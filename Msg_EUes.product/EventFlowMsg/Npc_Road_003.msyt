  SavedLv3_01:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 18
      - text: "Debemos aprovechar y salir pitando\nahora que podemos."
  Thanks00:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 11
      - text: Gracias por salvarme.
      - control:
          kind: pause
          length: short
      - text: " Toma, acepta\nesto como recompensa."
