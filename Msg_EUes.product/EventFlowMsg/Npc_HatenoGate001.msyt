  talk01:
    attributes: Npc_HatenoGate001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - control:
          kind: sound2
          unknown:
            - 17
            - 205
      - text: ¿Y a ti qué se te ha perdido aquí?
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Soy Carill, y estoy investigando\nsantuarios ancestrales en la zona."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Por si no estás muy puesto en el tema,\nsoy lo que los profesionales llaman\nun arqueólogo.\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: A mí no es que me entusiasme la palabra.
      - control:
          kind: pause
          length: longer
      - text: "\nMás que nada porque no me gusta\ndármelas de listo.\n"
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Por eso me conformo con que me llamen\n«eminencia», y todos contentos."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - control:
          kind: sound2
          unknown:
            - 23
            - 205
      - text: "Ahora mismo estoy ocupadísimo\nanalizando una escritura ancestral.\nSi necesitas algo, tendrá que esperar."
      - control:
          kind: choice
          choice_labels:
            - 6
            - 1
          selected_index: 0
          cancel_index: 1
          unknown: 6
  talk03:
    attributes: Npc_HatenoGate001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - control:
          kind: sound2
          unknown:
            - 12
            - 205
      - text: ¿Cómo dices?
      - control:
          kind: pause
          length: longer
      - text: " ¿Que quieres venir\nconmigo a la investigación de campo?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Estos novatos encuentran una piedra\ny ya se creen que son el no va más..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Bueno,"
      - control:
          kind: pause
          length: short
      - text: " supongo que puedo dejar que\nme acompañes en mi expedición."
      - control:
          kind: pause
          length: longer
      - text: "\nIntenta no quedarte atrás."
  talk06:
    attributes: Npc_HatenoGate001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ah, eres tú."
      - control:
          kind: pause
          length: short
      - text: " Ya te había dicho que\nestoy ocupado."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Tengo que averiguar el significado\nde este escrito"
      - control:
          kind: pause
          length: short
      - text: " como sea."
      - control:
          kind: choice
          choice_labels:
            - 6
            - 1
