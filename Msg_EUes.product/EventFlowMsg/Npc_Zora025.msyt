    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 20
      - text: "¡Me he llevado una sorpresa\nmuy grata contigo!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Has ayudado al príncipe Sidon
      - control:
