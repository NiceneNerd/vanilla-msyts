          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: ¿No tienes nada que decir?
      - control:
          kind: pause
          length: longer
      - text: "\n¡Pero qué modesto eres!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Lo que has hecho no ha sido nada\nfácil, así que puedes presumir\ncon todo el derecho."
  talk21:
