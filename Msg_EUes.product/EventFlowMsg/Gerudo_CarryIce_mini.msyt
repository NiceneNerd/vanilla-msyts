    attributes: Npc_GerudoDesert003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 6
      - text: "No pasa nada, pero ten en cuenta que\nel hielo es una mercancía muy valiosa."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Si quieres volver a intentarlo, necesito\nque me vuelvas a pagar "
      - control:
          kind: set_colour
          colour: red
      - text: 50 rupias
      - control:
          kind: reset_colour
      - text: ".\n¿Te parece bien o no?"
