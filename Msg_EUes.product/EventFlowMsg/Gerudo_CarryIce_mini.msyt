  NewText_00:
    attributes: Npc_OasisMilk_E
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: ¿Ahora te dedicas a ayudar a Anere?
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Bueno, mientras me sigan trayendo\nhielo, me da igual quién lo haga."
      - control:
          kind: pause
          length: long
      - text: "\n¿Tienes algo para mí?"
  NewText_01:
    attributes: Npc_OasisMilk_E
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "No es por ser pesada, pero\n¿te importaría traerme el hielo?"
