          cancel_index: 1
          unknown: 6
  Talk_001:
    attributes: Npc_OasisMilk_E
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "¡Sahku! Tráeme el hielo cuanto antes.\nNo me moveré de aquí."
  Talk_002:
    attributes: Npc_OasisMilk_E
    contents:
      - control:
          kind: sound
          unknown:
            - 0
