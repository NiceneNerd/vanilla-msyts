            - 11
            - 4
      - text: "¡Espera, Pristen!"
  Talk01:
    attributes: subchallnpc000_twin
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 0
      - text: "Tengo una pista de dónde está enterrado\nel "
      - control:
          kind: set_colour
