            - 11
            - 16
      - text: Por cierto...
      - control:
          kind: pause
          length: longer
      - text: " No consigo localizar\na Rinley, mi hija."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Creo que la última vez que hablé\ncon ella mencionó algo sobre hablar\ncon un amigo...\n"
      - control:
          kind: sound
          unknown:
            - 5
            - 6
      - text: "Me pregunto si estará de camino al\n"
      - control:
