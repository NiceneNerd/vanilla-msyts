  talk03:
    attributes: Npc_Zora008
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 11
      - text: "¡Sí, estoy completamente segura!"
      - control:
          kind: pause
          length: short
      - text: "\n¡Eres Linkitín!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Ha pasado tanto tiempo...
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Pero estaba convencida de que Mipha y\ntú habíais sucumbido al "
      - control:
          kind: set_colour
          colour: red
      - text: gran cataclismo
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Si has sobrevivido... ¿dónde habías\nestado metido hasta ahora?"
      - control:
          kind: pause
          length: longer
      - text: "\n¿Qué has estado haciendo?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 9
            - 19
      - text: "Supongo que habrás tenido tus razones\npara desaparecer..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "No me puedo creer que hayan pasado\nya cien años. Seguro que has vivido\nmillones de cosas.\n"
      - control:
          kind: sound
          unknown:
            - 1
            - 4
      - text: "Si tienes tiempo, me gustaría que nos\npusiésemos al día, ¿vale?"
  talk04:
    attributes: Npc_Zora008
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 20
