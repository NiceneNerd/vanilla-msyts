  talk24:
    attributes: Npc_Zora008
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 12
      - text: "Lo que le ocurrió a Mipha fue\nuna tragedia."
      - control:
          kind: pause
          length: short
      - text: " Pero en el fondo\nme diste más pena tú..."
  talk25:
    attributes: Npc_Zora008
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 1
      - text: Estaba... pensando.
  talk26:
    attributes: Npc_Zora008
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: Siempre me cuesta horrores
      - control:
          kind: pause
          length: short
      - text: " pensar\nqué puedo hacer de cena..."
      - control:
          kind: pause
          length: longer
      - text: " Ay."
