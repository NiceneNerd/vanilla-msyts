    attributes: Npc_kokiri009
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 21
      - text: ¿Quién eres tú?
      - control:
          kind: pause
          length: long
      - text: " ¡Espera!"
      - control:
          kind: pause
          length: long
      - text: "\n¿No serás el señor héroe?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "¿Sabes que he conseguido superar\nla "
      - control:
          kind: set_colour
          colour: red
      - text: prueba
      - control:
          kind: reset_colour
      - text: " yo solito?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 10
            - 19
      - text: "Ah, ahora que lo pienso..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 7
            - 1
      - text: El Gran Árbol Deku
      - control:
          kind: pause
          length: long
      - text: " dijo algo de que\nel héroe podría recibir una bendición\nen el santuario.\n¿No te dan como ganas de ir a mirar?"
  Npc_Kokiri009_R_001:
    attributes: Npc_kokiri009
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 0
      - control:
          kind: text_size
          percent: 125
      - text: ¡Aaah!
      - control:
          kind: text_size
          percent: 100
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 8
            - 0
      - text: ¡¿Qué quieres?! ¡No me des esos sustos!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "¡Dije que haría la "
      - control:
          kind: set_colour
          colour: red
      - text: prueba
      - control:
          kind: reset_colour
      - text: " solo y no\npienso cambiar de idea!"
  Npc_Kokiri009_R_002:
    attributes: Npc_kokiri009
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 0
      - control:
          kind: text_size
          percent: 125
      - text: ¡Aaah!
      - control:
          kind: text_size
          percent: 100
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "¡¿Pero no me has oído?!\n¡Que no me asustes!"
  Npc_Kokiri009_R_Near:
    attributes: Npc_kokiri009
    contents:
      - control:
          kind: sound2
          unknown:
            - 10
            - 205
      - text: No tengo miedo...
  Npc_Kokiri009_S1_001:
    attributes: Npc_kokiri009
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 22
      - control:
