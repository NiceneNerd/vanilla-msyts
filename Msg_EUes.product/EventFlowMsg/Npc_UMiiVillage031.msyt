      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Aunque no las veo yo muy interesadas\npor la costura y la "
      - control:
          kind: set_colour
          colour: red
      - text: moda
      - control:
          kind: reset_colour
      - text: "..."
  talk26:
    attributes: Npc_UMiiVillage031
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Doy el callo día y noche, "
      - control:
          kind: pause
          length: short
      - text: "así que\ntengo el uniforme hecho polvo."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
