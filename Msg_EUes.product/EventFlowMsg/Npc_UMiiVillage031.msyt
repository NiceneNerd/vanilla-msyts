  talk34:
    attributes: Npc_UMiiVillage031
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Pues no sé a qué esperas para\ntraerme esa "
      - control:
          kind: set_colour
          colour: red
      - text: leña
      - control:
          kind: reset_colour
      - text: ". ¿O es que ya\nse te ha pasado el interés?"
  talk35:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "En lo que a "
      - control:
