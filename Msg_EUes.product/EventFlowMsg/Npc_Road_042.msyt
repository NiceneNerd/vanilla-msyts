  RetMSG_Akka_04_1:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "Hay una "
      - control:
          kind: set_colour
          colour: blue
      - text: posta de Akkala este
      - control:
          kind: reset_colour
      - text: " y\nuna "
      - control:
          kind: set_colour
          colour: blue
      - text: posta de Akkala sur
      - control:
          kind: reset_colour
      - text: ", sí, eso lo\ntengo goroclaro."
      - control:
