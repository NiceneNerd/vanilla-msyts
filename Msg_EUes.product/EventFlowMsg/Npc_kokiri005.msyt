      - control:
          kind: reset_colour
      - text: ".\n"
      - control:
          kind: sound
          unknown:
            - 11
            - 18
      - text: "Por si no lo sabes, el "
      - control:
          kind: set_colour
          colour: blue
      - text: "pedestal de la\nespada"
      - control:
          kind: reset_colour
      - text: " constituye el punto central\ndel bosque.\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 1
