      - text: La lluvia me relaja...
  Talk00:
    attributes: Npc_Kakariko002
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 20
      - text: "Ah, Link... "
      - control:
          kind: pause
          length: short
      - text: ¿Qué ocurre?
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 6
            - 0
