          length: longer
      - control:
          kind: text_size
          percent: 100
  Talk05:
    attributes: Npc_Kakariko002
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 1
      - text: "Bueno, no sé por dónde empezar..."
      - control:
          kind: pause
          length: longer
      - text: "\nSupongo que por el nombre, claro."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Me llamo Pay.
  Talk06:
    attributes: Npc_Kakariko002
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 1
      - text: Me pusieron ese nombre
      - control:
          kind: pause
          length: short
      - text: " porque tengo\nuna marca de nacimiento en forma de\nsemilla de papaya."
      - control:
          kind: choice
          choice_labels:
