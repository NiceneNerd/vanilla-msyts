  SavedLv3_00:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 9
      - text: "Y yo que pensaba que los monstruos\npasarían de mí al ser tan anodino..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ya sabes, en plan que soy tan soso\nque ni se dan cuenta de mi presencia."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "¡Pero ya se ve que no! Tal vez no sea\ntan insignificante como pensaba. En fin,\nmuchísimas gracias por ayudarme."
  SavedLv3_01:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 9
      - text: "¡Eres increíble, chico! Nunca había visto\na alguien tan arrojado como tú."
      - control:
