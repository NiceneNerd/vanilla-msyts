    contents:
      - control:
          kind: sound2
          unknown:
            - 2
            - 205
      - text: "Es un "
      - control:
          kind: set_colour
          colour: blue
      - text: trofeo especial
      - control:
          kind: reset_colour
      - text: " que se ha ido\npasando de una campeona a otra\ndesde hace unos diez mil años."
  Talk_0015:
    attributes: Npc_oasis039
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "La vigente campeona es Asyre, aquí\npresente. Entonces, ¿qué? ¿Vas a\nintentar batir su marca?"
      - control:
          kind: choice
          choice_labels:
            - 14
            - 15
