    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 21
      - text: ¿Cómo es posible?
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Hum...
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "En fin. Si no tienes motivación, no voy\na hacer que Asyre compita a lo tonto."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 8
            - 0
      - text: "Medirse con cobardicas es\nuna soberana pérdida de tiempo."
