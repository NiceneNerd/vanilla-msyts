    attributes: Npc_oasis039
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - control:
          kind: sound2
          unknown:
            - 7
            - 205
      - text: "Espero que ahora entiendas lo difícil\nque es batir la marca de "
      - control:
          kind: set_colour
          colour: red
      - control:
          kind: variable
          variable_kind: 14
          name: SunazarashiRace_BestTime_L
