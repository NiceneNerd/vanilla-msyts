  Talk_0013:
    attributes: Npc_oasis039
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "La mejor marca de Asyre es "
      - control:
          kind: set_colour
          colour: red
      - control:
          kind: variable
          variable_kind: 14
          name: SunazarashiRace_BestTime_L
      - text: " minuto"
      - control:
          kind: reset_colour
      - text: "\ny "
      - control:
          kind: set_colour
          colour: red
      - control:
          kind: variable
