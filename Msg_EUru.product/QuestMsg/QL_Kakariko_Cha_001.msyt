group_count: 2
atr1_unknown: 0
entries:
  QL_Kakariko_Cha_001_Desc:
    contents:
      - text: "Сегодня в меню скорое лечо! Коко очень\nхотела его приготовить, но забыла купить\n"
      - control:
          kind: set_colour
          colour: red
      - text: прыть-морковь
      - control:
          kind: reset_colour
      - text: ", а без нее ничего не выйдет.\nКоко расстроена, ведь ее сестренка Коттла\nобожает это блюдо и ждет его на ужин.\nНадо помочь Коко и найти "
      - control:
          kind: set_colour
          colour: red
      - text: прыть-морковь
      - control:
          kind: reset_colour
      - text: "."
  QL_Kakariko_Cha_001_Finish:
    contents:
      - text: "Коко хотела приготовить своей сестренке\nКоттле скорое лечо, но забыла купить\nпрыть-морковь. Я принес прыть-морковь,\nи Коко сразу повеселела. Уверен, Коттла\nтоже обрадуется такому ужину!\n\n"
      - control:
          kind: set_colour
          colour: red
      - text: "Состав блюда:"
      - control:
          kind: reset_colour
      - text: " прыть-морковь,\nкаменная соль, молоко."
  QL_Kakariko_Cha_001_Name:
    contents:
      - text: «Кухня Коко»
