entries:
  QL_GanonQuest_Desc:
    contents:
      - text: "Призрак короля Роама сказал мне, что\nкоролевство в большой опасности. Силы\nпринцессы Зельды, которая сдерживает\n"
      - control:
          kind: set_colour
          colour: red
      - text: Ганона
      - control:
          kind: reset_colour
      - text: " в замке Хайрул, на исходе. Если\n"
      - control:
          kind: set_colour
          colour: red
      - text: "Ганон "
      - control:
          kind: reset_colour
      - text: "примет свой истинный облик, он\nуничтожит все королевство. Пока этого\nне случилось, я должен помочь Зельде."
  QL_GanonQuest_Finished:
    contents:
      - control:
          kind: raw
          zero:
            zero:
              field_1: 10
              field_2: 4
              field_3: 6
      - text: "ふよう不要？\nガノンの"
      - control:
          kind: raw
          zero:
            zero:
              field_1: 12
              field_2: 4
              field_3: 8
      - text: けいたい形態によってステップを
      - control:
          kind: raw
          zero:
            zero:
              field_1: 6
              field_2: 2
              field_3: 2
      - text: わ分けた
      - control:
          kind: raw
          zero:
            zero:
              field_1: 8
              field_2: 2
              field_3: 4
      - text: ほう方がいいかも
  QL_GanonQuest_Name:
    contents:
      - text: «Уничтожить Ганона»
