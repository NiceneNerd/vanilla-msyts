group_count: 2
atr1_unknown: 0
entries:
  QL_RitoMini_Cook_Desc:
    contents:
      - text: "Джуни недовольна тем, что ее медовый\nмесяц проходит в деревне рито, где даже\nсходить некуда. Она обожает "
      - control:
          kind: set_colour
          colour: red
      - text: "печеные\nяблоки"
      - control:
          kind: reset_colour
      - text: ". Возможно, они поднимут ей\nнастроение.\n\nНужно принести ей "
      - control:
          kind: set_colour
          colour: red
      - text: печеное яблоко
      - control:
          kind: reset_colour
      - text: ",\nпока дело не дошло до развода!"
  QL_RitoMini_Cook_Finish:
    contents:
      - text: "Джуни была очень недовольна тем, что ее\nмедовый месяц проходит в деревне рито.\nЧтобы поднять настроение, я принес ей\nпеченое яблоко. Она была очень довольна\nи отблагодарила меня. Она говорит, что\nготова есть "
      - control:
          kind: set_colour
          colour: red
      - text: печеные яблоки
      - control:
          kind: reset_colour
      - text: " бесконечно.\nПожалуй, стоит принести ей сразу целую\nкучу."
  QL_RitoMini_Cook_Name:
    contents:
      - text: «Голодная жена»
