group_count: 3
atr1_unknown: 0
entries:
  QL_Get_MasterSword_Desc:
    contents:
      - text: "В лесу Короков в Затерянных лесах\nпокоится "
      - control:
          kind: set_colour
          colour: red
      - text: Сокрушитель Зла
      - control:
          kind: reset_colour
      - text: " — меч,\nкоторым можно победить Ганона.\n\nКогда я пришел за мечом, Древо Деку\nобъяснило мне, что меч подвергнет меня\nиспытанию. Он был у меня сто лет назад,\nно смогу ли я вынуть его сейчас?"
  QL_Get_MasterSword_Finish:
    contents:
      - text: "Я получил "
      - control:
          kind: set_colour
          colour: red
      - text: Высший Меч
      - control:
          kind: reset_colour
      - text: " — Сокрушитель Зла.\nТакое ощущение, что сам меч рад вернуться\nко мне...\n\nПринцесса Зельда по-прежнему сдерживает\nБедствие в замке Хайрул... и ждет меня.\nСумею ли я помочь ей?"
  QL_Get_MasterSword_Name:
    contents:
      - text: «Меч Героя»
