          name: TreasureHunt_touzoku08_RemainingBox
      - control:
          kind: localisation
          localisation_kind: plural
          options:
            - " элемент"
            - " элементов"
            - " элемента"
      - text: "\nэтого снаряжения"
      - control:
          kind: reset_colour
      - text: "."
