group_count: 2
atr1_unknown: 0
entries:
  QL_RitoUmayadoMini_HotRecipe_Desc:
    contents:
      - text: "В Табантском крае есть конюшня «Рито»,\nв которой я познакомился с Лестером.\nОн хочет приготовить рис с соусом, но\nу него нет "
      - control:
          kind: set_colour
          colour: red
      - text: приправы горонов
      - control:
          kind: reset_colour
      - text: " для соуса.\n\nНадо найти "
      - control:
          kind: set_colour
          colour: red
      - text: приправу горонов
      - control:
          kind: reset_colour
      - text: " и принести\nее Лестеру в конюшню рито."
  QL_RitoUmayadoMini_HotRecipe_Finish:
    contents:
      - text: "Лестер из конюшни «Рито» купил у меня\n"
      - control:
          kind: set_colour
          colour: red
      - text: приправу горонов
      - control:
          kind: reset_colour
      - text: " за 50 рупий. Теперь\nя знаю, что из "
      - control:
          kind: set_colour
          colour: red
      - text: хайрульского риса
      - control:
          kind: reset_colour
      - text: " и этой\nприправы с добавление других продуктов\nможно приготовить разные виды риса\nс соусом.\n\nЛестер готов давать мне "
      - control:
          kind: set_colour
          colour: red
      - text: хайрульский рис
      - control:
          kind: reset_colour
      - text: "\nв обмен на "
      - control:
          kind: set_colour
          colour: red
      - text: приправу горонов
      - control:
          kind: reset_colour
      - text: "."
  QL_RitoUmayadoMini_HotRecipe_Name:
    contents:
      - text: «С соусом теплее!»
