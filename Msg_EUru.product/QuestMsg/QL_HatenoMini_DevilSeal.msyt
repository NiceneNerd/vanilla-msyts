group_count: 3
atr1_unknown: 0
entries:
  QL_HatenoMini_DevilSeal_Desc:
    contents:
      - text: "Рогатый истукан в деревне Хатено сказал\nприйти к нему, когда я получу хотя бы одну\n"
      - control:
          kind: set_colour
          colour: red
      - text: капсулу сердца
      - control:
          kind: reset_colour
      - text: " или "
      - control:
          kind: set_colour
          colour: red
      - text: энергии
      - control:
          kind: reset_colour
      - text: "."
  QL_HatenoMini_DevilSeal_Finish:
    contents:
      - text: "Рогатый истукан вернул мне мою капсулу.\nТеперь я могу обменивать у него капсулы\nв любое время.\n\nРогатый истукан готов покупать у меня\nкапсулы по "
      - control:
          kind: set_colour
          colour: red
      - text: 100 рупий
      - control:
          kind: reset_colour
      - text: " и продавать мне\nих обратно по "
      - control:
          kind: set_colour
          colour: red
      - text: 120 рупий
      - control:
          kind: reset_colour
      - text: "."
  QL_HatenoMini_DevilSeal_GiveUtsuwa:
    contents:
      - text: "Рогатый истукан в деревне Хатено взял\nодну из моих капсул. Надо снова с ним\n"
      - control:
          kind: set_colour
          colour: red
      - text: "поговорить "
      - control:
          kind: reset_colour
      - text: и забрать ее.
  QL_HatenoMini_DevilSeal_Name:
    contents:
      - text: «Меняла капсул»
