group_count: 2
atr1_unknown: 0
entries:
  QL_Zora_FlogMini_Desc:
    contents:
      - text: "Малыш Тумбо, который живет во владениях\nзора, попросил поймать ему "
      - control:
          kind: set_colour
          colour: red
      - text: "пять прыть-\nлягушек"
      - control:
          kind: reset_colour
      - text: ". Он говорит, что этих лягушек\nпроще всего найти в "
      - control:
          kind: set_colour
          colour: red
      - text: дождливую погоду
      - control:
          kind: reset_colour
      - text: "."
  QL_Zora_FlogMini_Finish:
    contents:
      - text: "Я принес Тумбо пять прыть-лягушек.\nОн был очень доволен и подарил мне\nлатноцвет."
  QL_Zora_FlogMini_Name:
    contents:
      - text: «Дождь из лягушек»
