group_count: 2
atr1_unknown: 0
entries:
  QL_Oasis_Drug_Challenge_Desc:
    contents:
      - text: "Рито Гинемер с рынка «Кара-Кара» очень\nстрадает от жары и просит принести ему\n"
      - control:
          kind: set_colour
          colour: red
      - text: ледяное зелье
      - control:
          kind: reset_colour
      - text: ". Чтобы сварить это зелье,\nнужно взять "
      - control:
          kind: set_colour
          colour: red
      - text: орган монстра
      - control:
          kind: reset_colour
      - text: " и "
      - control:
          kind: set_colour
          colour: red
      - text: "насекомое\nс охлаждающим действием"
      - control:
          kind: reset_colour
      - text: ". Такие должны\nводиться "
      - control:
          kind: set_colour
          colour: red
      - text: "в холодных местах на нагорье\nГерудо"
      - control:
          kind: reset_colour
      - text: ". Надо сварить Гинемеру "
      - control:
          kind: set_colour
          colour: red
      - text: "ледяное\nзелье"
      - control:
          kind: reset_colour
      - text: "!"
  QL_Oasis_Drug_Challenge_Finish:
    contents:
      - text: "Я сварил ледяное зелье из органа монстра\nи насекомого с охлаждающим действием\nи вручил его Гинемеру.\n\nНадо запомнить рецепт — наверняка\nмне самому пригодится такое зелье."
  QL_Oasis_Drug_Challenge_Name:
    contents:
      - text: «Зелье от зноя»
