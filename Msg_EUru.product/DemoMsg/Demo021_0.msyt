            - 6
            - 0
      - text: "Видишь вон те "
      - control:
          kind: set_colour
          colour: red
      - text: орудия
      - control:
          kind: reset_colour
      - text: "?"
      - control:
          kind: pause
          length: short
      - text: "\nЯ отвлеку огонь на себя."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
