      - text: "\nчто правила "
      - control:
          kind: set_colour
          colour: blue
      - text: Наборис
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: pause
          frames: 84
      - text: " Во всех врагов"
      - control:
          kind: pause
          length: long
      - text: "\nона вселяла ужас»."
      - control:
          kind: pause
          length: longer
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: auto_advance
          frames: 321
      - control:
          kind: raw
          one:
            one:
              field_1: 4
              field_2: 1050253722
      - text: «Свела судьба ее
      - control:
          kind: pause
          length: long
      - text: " с принцессой\nизбранной,"
      - control:
          kind: pause
          frames: 90
      - text: " что в город к ней"
      - control:
          kind: pause
          length: long
      - text: "\nза помощью пришла...»"
      - control:
          kind: pause
          length: longer
