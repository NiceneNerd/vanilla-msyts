            four:
              field_1: 0
      - control:
          kind: auto_advance
          frames: 384
      - control:
          kind: raw
          one:
            one:
              field_1: 4
              field_2: 1050253722
      - text: «В небе лазурном чудо-птица парит...
      - control:
          kind: pause
          frames: 102
      - text: "\nВоды чистые бурлят"
      - control:
          kind: pause
          frames: 66
      - text: " вкруг владычицы\nозер...»"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: auto_advance
          frames: 447
      - control:
          kind: raw
          one:
            one:
              field_1: 4
              field_2: 1048576000
      - text: "«Но ждет Героя еще одно "
      - control:
          kind: set_colour
          colour: blue
      - text: Чудище
      - control:
          kind: reset_colour
      - text: ":"
      - control:
