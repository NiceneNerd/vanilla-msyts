      - text: Руте
      - control:
          kind: reset_colour
      - text: ", ведь именно этим\nЧудищем управляла "
      - control:
          kind: set_colour
          colour: blue
      - text: Мифа
      - control:
          kind: reset_colour
      - text: "."
