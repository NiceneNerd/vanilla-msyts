  talk00:
    attributes: Npc_Zora001
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 20
      - text: А!
      - control:
          kind: pause
          length: short
      - text: " Ты тот хайлиец с "
      - control:
          kind: set_colour
          colour: blue
      - text: реки Зора
      - control:
          kind: reset_colour
      - text: "!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Значит, ты все-таки принял\nприглашение принца Сидона."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Король Дорефан и принц Сидон\nдолжны быть в тронном зале."
      - control:
          kind: pause
          length: short
      - text: " Ты\nуже поговорил с ними?"
  talk01:
    attributes: Npc_Zora001
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 4
      - text: А!
      - control:
          kind: pause
          length: short
      - text: " Это снова ты — тот самый хайлиец,\nкоторого пригласил принц Сидон, да?"
      - control:
          kind: pause
          length: short
      - text: "\nТы с ним уже поговорил?"
  talk03:
    attributes: Npc_Zora001
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 2
      - text: Доброе утро!
  talk04:
    attributes: Npc_Zora001
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 2
      - text: Здравствуй!
  talk05:
    attributes: Npc_Zora001
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 2
      - text: Я на службе даже ночью!
  talk06:
    attributes: Npc_Zora001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 45
      - text: Приветствую!
  talk07:
    attributes: Npc_Zora001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: Чем могу помочь?
      - control:
          kind: raw
          one:
            eight:
              unknown_1:
                - - 255
                  - 255
                  - 0
                  - 0
              field_1:
                - 3
                - 0
