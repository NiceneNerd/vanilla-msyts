  DiaryFirst:
    attributes: TwnObj_Village_SheikerHeirDiary_A_01
    contents:
      - control:
          kind: raw
          one:
            two:
              field_1: 0
      - text: Это дневник Пайи.
      - control:
          kind: pause
          length: short
      - text: " Возможно,\nмне не стоит его читать..."
      - control:
          kind: choice
          choice_labels:
            - 20
            - 21
            - 22
          selected_index: 0
          cancel_index: 2
          unknown: 8
