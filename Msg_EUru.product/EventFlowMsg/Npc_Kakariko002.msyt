      - control:
          kind: choice
          choice_labels:
            - 26
            - 22
          selected_index: 0
          cancel_index: 1
          unknown: 6
  NakedPlayer:
    attributes: Npc_Kakariko002
    contents:
      - text: "Пожалуйста, оденься..."
  Near01:
    attributes: Npc_Kakariko002
    contents:
      - text: Ох...
  Near02:
    attributes: Npc_Kakariko002
    contents:
      - text: М-м-м...
  Near03:
    attributes: Npc_Kakariko002
    contents:
      - text: Дождь успокаивает...
  Talk00:
    attributes: Npc_Kakariko002
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 20
      - text: Линк!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 6
            - 0
      - text: Что-то случилось?
      - control:
          kind: pause
          length: short
      - text: " Могу\nя тебе чем-нибудь помочь?"
      - control:
          kind: choice
          choice_labels:
            - 1
            - 2
            - 3
            - 4
          selected_index: 0
          cancel_index: 3
          unknown: 10
  Talk01:
    attributes: Npc_Kakariko002
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Хочешь спросить что-нибудь еще?
      - control:
          kind: choice
          choice_labels:
            - 1
            - 2
            - 3
            - 4
          selected_index: 0
          cancel_index: 3
          unknown: 10
  Talk02:
    attributes: Npc_Kakariko002
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: Шар?
      - control:
          kind: pause
          length: short
      - text: " Ты имеешь в виду\n"
      - control:
          kind: set_colour
          colour: blue
      - text: древнюю сферу
      - control:
          kind: reset_colour
      - text: "?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 5
            - 0
      - text: Это наследие шиика.
      - control:
          kind: pause
          length: short
      - text: " Наша задача —\nне допустить, чтобы сфера попала\nне в те руки."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - control:
          kind: set_colour
          colour: red
      - text: "«Избранник древней сферы\nполучит защиту древних»."
      - control:
          kind: reset_colour
      - control:
          kind: pause
          length: long
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Эти слова передаются у нас от матери\nк дочери из поколения в поколение\nвместе с самой сферой."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Не уверена, что понимаю значение\nэтих слов, но, кажется, я знаю, кто\n"
      - control:
          kind: set_colour
          colour: red
      - text: избранник древней сферы
      - control:
          kind: reset_colour
      - text: "..."
  Talk03:
    attributes: Npc_Kakariko002
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 6
      - text: "Народ "
      - control:
          kind: set_colour
          colour: blue
      - text: шиика
      - control:
          kind: reset_colour
      - text: " долгое время тайно\nслужил королевскому дому Хайрула."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 3
            - 0
      - text: "Но сто лет назад "
      - control:
          kind: set_colour
          colour: red
      - text: Великое Бедствие
      - control:
          kind: reset_colour
