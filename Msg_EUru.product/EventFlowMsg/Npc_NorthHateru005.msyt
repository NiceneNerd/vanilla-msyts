entries:
  "0000":
    attributes: Npc_NorthHateru005
    contents:
      - text: Что это за место?
  "0001":
    attributes: Npc_NorthHateru005
    contents:
      - text: Куда смотришь?
  "0002":
    attributes: Npc_NorthHateru005
    contents:
      - text: Ну извини
  "0003":
    attributes: Npc_NorthHateru005
    contents:
      - text: Зора?
  "0004":
    attributes: Npc_NorthHateru005
    contents:
      - text: Пока
  "0005":
    attributes: Npc_NorthHateru005
    contents:
      - text: Саторик?
  "0006":
    attributes: Npc_NorthHateru005
    contents:
      - text: Расскажи
  "0007":
    attributes: Npc_NorthHateru005
    contents:
      - text: Не надо
  "0008":
    attributes: Npc_NorthHateru005
    contents:
      - text: И что потом?
  "0009":
    attributes: Npc_NorthHateru005
    contents:
      - text: Достаточно
  "0010":
    attributes: Npc_NorthHateru005
    contents:
      - text: Ну же! Потом что?
  talk000:
    attributes: Npc_NorthHateru005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Эй...
  talk001:
    attributes: Npc_NorthHateru005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Что?!
      - control:
          kind: pause
          length: short
      - text: " Опять парень!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ну почему в эту конюшню\nдевушки не приходят..."
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
            - 2
          selected_index: 0
          cancel_index: 2
          unknown: 8
  talk002:
    attributes: Npc_NorthHateru005
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 0
      - text: А-а!
      - control:
          kind: pause
          length: longer
      - text: " Нечасто сюда такие\nкрасавицы заходят!"
      - control:
          kind: pause
          length: short
      - text: " Хм..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 3
            - 0
      - text: "А, ты же парень..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Знаешь, такой одеждой можешь\nобмануть кого-нибудь из Герудо,\nно не "
      - control:
          kind: set_colour
          colour: blue
      - text: хайлийца
      - control:
          kind: reset_colour
      - text: "!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Эх...
      - control:
          kind: pause
          length: short
      - text: " Пока я сижу в этой конюшне,\nя всегда буду встречать одних\nтолько парней..."
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
            - 2
          selected_index: 0
          cancel_index: 2
          unknown: 8
  talk005:
    attributes: Npc_NorthHateru005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "А, это ты..."
      - control:
          kind: pause
          length: longer
      - text: " Чего тебе?"
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
            - 2
          selected_index: 0
          cancel_index: 2
          unknown: 8
  talk010:
    attributes: Npc_NorthHateru005
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 12
      - text: "Противная конюшня, в которой\nне встретишь ни одной девушки..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Недавно вижу: зора идет — и тот\nоказался мужчиной."
      - control:
          kind: pause
          length: short
      - text: " Сказал, что\n"
      - control:
          kind: set_colour
          colour: red
      - text: "принц "
      - control:
          kind: reset_colour
      - text: какой-то.
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 9
      - text: "Если бы не Саторик,\nя б со скуки помер..."
      - control:
          kind: choice
          choice_labels:
            - 3
            - 5
            - 4
          selected_index: 0
          cancel_index: 2
          unknown: 8
  talk020:
    attributes: Npc_NorthHateru005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 6
      - text: Взгляни на восток.
      - control:
          kind: pause
          length: longer
      - text: " Странные тучи, да?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Мне всегда от дождя плохо...
      - control:
          kind: pause
          length: longer
      - text: "\nХорошо хоть, Саторик со мной."
      - control:
          kind: choice
          choice_labels:
            - 5
            - 4
          selected_index: 0
          cancel_index: 1
          unknown: 6
  talk030:
    attributes: Npc_NorthHateru005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: Не нужны мне твои извинения.
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "За последние дни из посетителей\nздесь были только ты да какой-то\n"
      - control:
          kind: set_colour
          colour: red
      - text: принц зора
      - control:
          kind: reset_colour
      - text: "..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: И ни одной девушки...
      - control:
          kind: choice
          choice_labels:
            - 3
            - 4
          selected_index: 0
          cancel_index: 1
          unknown: 6
  talk040:
    attributes: Npc_NorthHateru005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 6
      - text: Никогда не слышал про зора?
      - control:
          kind: pause
          length: longer
      - text: "\nРыбный народ такой, скользкие,\nблестящие."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Владения зора находятся к востоку\nотсюда — в верховьях "
      - control:
          kind: set_colour
          colour: blue
      - text: реки Зора
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Принц тот сказал, что ищет сильного\n"
      - control:
          kind: set_colour
          colour: blue
      - text: хайлийца
      - control:
          kind: reset_colour
      - text: ", и позвал меня во владения\nзора."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 5
            - 12
      - text: "Но я отказался — не хочу подвергать\nопасности жизнь Саторика."
      - control:
          kind: choice
          choice_labels:
            - 5
            - 4
          selected_index: 0
          cancel_index: 1
          unknown: 6
  talk050:
    attributes: Npc_NorthHateru005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ага, пока."
  talk060:
    attributes: Npc_NorthHateru005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - control:
          kind: set_colour
          colour: blue
      - text: Саторик
      - control:
          kind: reset_colour
      - text: "..."
      - control:
          kind: pause
          length: short
      - text: " Он меня от смерти спас."
      - control:
          kind: pause
          length: short
      - text: "\nЭто длинная история, но, если хочешь,\nмогу рассказать."
      - control:
          kind: choice
          choice_labels:
            - 6
            - 7
          selected_index: 0
          cancel_index: 1
          unknown: 6
  talk061:
    attributes: Npc_NorthHateru005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: Хорошо.
      - control:
          kind: pause
          length: short
      - text: " Тогда слушай..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ты знаешь про "
      - control:
          kind: set_colour
          colour: red
      - text: загадочную гору
      - control:
          kind: reset_colour
      - text: ",\nна которой животные появляются\nтолько в определенное время?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Когда я о ней узнал, сразу решил\nпойти."
      - control:
          kind: pause
          length: short
      - text: " Хотел подзаработать..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Думаю, сейчас настреляю птичек\nпобольше и продам все мясо."
      - control:
          kind: choice
          choice_labels:
            - 8
            - 9
          selected_index: 0
          cancel_index: 1
          unknown: 6
  talk062:
    attributes: Npc_NorthHateru005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 12
      - text: "Ну, пришел я на гору и стал ждать,\nкогда же появится зверье..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "И вдруг ко мне подбегает какая-то\nпотрепанная собачонка, щенок еще."
      - control:
          kind: pause
          length: short
      - text: "\nДумаю, наверное, потерялась."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "А щенок на меня так жалобно смотрит,\nпоскуливает, есть просит."
      - control:
          kind: pause
          length: short
      - text: " Ну, я ему\nтогда кинул чего-то пожевать..."
      - control:
          kind: choice
          choice_labels:
            - 8
            - 9
          selected_index: 0
          cancel_index: 1
          unknown: 6
  talk063:
    attributes: Npc_NorthHateru005
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "Прошла пара дней. И вот просыпаюсь\nя как-то среди ночи и вижу, что передо\nмной стоит "
      - control:
          kind: set_colour
          colour: blue
      - text: сияющая зверюга
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "А я пошевелиться не могу — "
      - control:
          kind: set_colour
          colour: blue
      - text: зверюга
      - control:
          kind: reset_colour
      - text: "\nменя врасплох застала."
      - control:
          kind: pause
          length: short
      - text: " Я даже лук\nсвой натянуть не успел бы."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Тут она двинулась на меня,\nи я понимаю, что сейчас она\nменя просто раздавит."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Вдруг между нами выбегает тот самый\nщенок и как залает на "
      - control:
          kind: set_colour
          colour: blue
      - text: зверюгу
      - control:
          kind: reset_colour
      - text: "! Хотя\nта больше его раз в сто."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Щенок весь трясется, ему самому\nстрашно, но не уходит — меня\nзащищает..."
      - control:
          kind: choice
          choice_labels:
            - 10
            - 9
          selected_index: 0
          cancel_index: 1
          unknown: 6
  talk064:
    attributes: Npc_NorthHateru005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 12
      - control:
          kind: set_colour
          colour: blue
      - text: "Зверюга "
      - control:
          kind: reset_colour
      - text: немного замешкалась...
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ну я хвать щенка под мышку —\nи побежал, точнее, кубарем\nпокатился с горы."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Такая вот история.
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Та "
      - control:
          kind: set_colour
          colour: red
      - text: загадочная гора
      - control:
          kind: reset_colour
      - text: " находится\nк западу от "
      - control:
          kind: set_colour
          colour: blue
      - text: "Центрального\nХайрула"
      - control:
          kind: reset_colour
      - text: " — "
      - control:
          kind: set_colour
          colour: blue
      - text: "Сатори "
      - control:
          kind: reset_colour
      - text: называется.
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: "А мой "
      - control:
          kind: set_colour
          colour: blue
      - text: "Саторик "
      - control:
          kind: reset_colour
      - text: — тот самый щенок!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Если думаешь, что ничего не боишься,\nсходи на ту гору и найди "
      - control:
          kind: set_colour
          colour: blue
      - text: зверюгу
      - control:
          kind: reset_colour
      - text: "."
  talk065:
    attributes: Npc_NorthHateru005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Что?
      - control:
          kind: pause
          length: short
      - text: " Рассказать тебе еще раз\nтрогательную историю про нас\nс Саториком?"
      - control:
          kind: choice
          choice_labels:
            - 6
            - 7
          selected_index: 0
          cancel_index: 1
          unknown: 6
  talk070:
    attributes: Npc_NorthHateru005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: А-а...
      - control:
          kind: pause
          length: short
      - text: " Ну пока."
  talk100:
    attributes: Npc_NorthHateru005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Так, что же сегодня произойдет?"
  talk101:
    attributes: Npc_NorthHateru005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Дождь идет...
  talk110:
    attributes: Npc_NorthHateru005
    contents:
      - control:
          kind: sound
