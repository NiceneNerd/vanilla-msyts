  DiaryFirst:
    attributes: Obj_DiaryIceVillage011_A_01
    contents:
      - text: "«Всем посетителям этой базы»\nОт Снежной королевы Селми."
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
