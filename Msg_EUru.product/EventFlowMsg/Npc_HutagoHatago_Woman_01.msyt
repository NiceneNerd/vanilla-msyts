  First_Greeting00:
    attributes: Npc_HutagoHatago_Woman_01
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 19
      - text: Здравствуй!
      - control:
          kind: pause
          length: short
      - text: " Нечасто здесь таких, как\nты, встретишь. Тоже путешествуешь?"
  First_Greeting01:
    attributes: Npc_HutagoHatago_Woman_01
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 19
      - text: Добрый вечер!
      - control:
          kind: pause
          length: short
      - text: " Нечасто здесь\nтаких, как ты, встретишь. Тоже\nпутешествуешь?"
  First_Greeting02:
    attributes: Npc_HutagoHatago_Woman_01
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 19
      - text: Доброе утро!
      - control:
          kind: pause
          length: short
      - text: " Нечасто здесь таких, как\nты, встретишь. Тоже путешествуешь?"
  Talk100:
    attributes: Npc_HutagoHatago_Woman_01
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 19
      - text: "Я хотела взять это "
      - control:
          kind: set_colour
          colour: blue
      - text: яблоко
      - control:
          kind: reset_colour
      - text: "..."
      - control:
          kind: pause
          length: short
      - text: "\nЗдесь, очевидно, можно\nбрать все, что хочешь."
  Talk101:
    attributes: Npc_HutagoHatago_Woman_01
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 5
      - text: "Парень в конюшне сказал, что\nмы можем брать эти "
      - control:
          kind: set_colour
          colour: blue
      - text: яблоки
      - control:
          kind: reset_colour
      - text: "."
  Talk102:
    attributes: Npc_HutagoHatago_Woman_01
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 5
      - text: "Я съела одно "
      - control:
          kind: set_colour
          colour: blue
      - text: яблоко
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: pause
          length: short
      - text: "\nОни такие спелые!"
  Talk103:
    attributes: Npc_HutagoHatago_Woman_01
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: "Если "
      - control:
          kind: set_colour
          colour: blue
      - text: "яблоки "
      - control:
          kind: reset_colour
      - text: "закончатся, то дети\nеще принесут."
      - control:
          kind: pause
          length: short
      - text: " Они мне сказали,\nчто я могу брать, сколько захочу."
  near00:
    attributes: Npc_HutagoHatago_Woman_01
    contents:
      - text: Так тихо...
  near01:
    attributes: Npc_HutagoHatago_Woman_01
    contents:
      - text: Так красиво...
  near02:
    attributes: Npc_HutagoHatago_Woman_01
    contents:
      - text: Вот так!
  talk00:
    attributes: Npc_HutagoHatago_Woman_01
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Следы Бедствия видны повсюду,\nно все равно трудно поверить, что\nмиру мог прийти конец."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 17
      - text: "В этих краях по-прежнему много\n"
      - control:
          kind: set_colour
          colour: blue
      - text: монстров
      - control:
          kind: reset_colour
      - text: ", а у самого "
      - control:
          kind: set_colour
          colour: blue
      - text: замка Хайрул
      - control:
          kind: reset_colour
      - text: "\nтворится вообще непонятно что."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Если путешествуешь,\nнаучись варить "
      - control:
          kind: set_colour
          colour: blue
      - text: зелья
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
          selected_index: 0
          cancel_index: 1
          unknown: 6
  talk01:
    attributes: Npc_HutagoHatago_Woman_01
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Это "
      - control:
          kind: set_colour
          colour: blue
      - text: святилище
      - control:
          kind: reset_colour
      - text: " только недавно\nзасветилось, хотя, говорят, ему\nгде-то 10 000 лет."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 17
      - text: "И таких в мире много. Чуть ли не на\nкаждом углу какая-нибудь древность..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Археологи и ученые пытаются\nнайти все "
      - control:
          kind: set_colour
          colour: blue
      - text: святилища
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: pause
          length: short
      - text: " Но я в них\nничего не понимаю."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Хотя мне нравится, как они светятся."
      - control:
          kind: pause
          length: short
      - text: "\nТак романтично — находки, открытия..."
  talk02:
    attributes: Npc_HutagoHatago_Woman_01
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Чтобы получилось зелье, надо взять\nнасекомое, ящерицу или там лягушку\nи добавить орган монстра."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Зелья не восстанавливают здоровье,\nзато обладают другими полезными\nсвойствами.\nОни могут, например, повышать твою\nскорость или защищать от холода или\nзноя."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 17
      - text: "Неужели ты об этом не знал?\nКак ты тогда сюда добрался?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Так и быть, дам тебе\nодно из своих зелий."
  talk03:
    attributes: Npc_HutagoHatago_Woman_01
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 9
      - text: "Наверное, ты уже заметил,\nчто тут много близнецов."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "А меня иногда спрашивают,\nпочему я одна..."
  talk04:
    attributes: Npc_HutagoHatago_Woman_01
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: В этих краях тихо...
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 17
      - text: "А вот у "
      - control:
          kind: set_colour
          colour: blue
      - text: замка Хайрул
      - control:
          kind: reset_colour
      - text: " творится\nнепонятно что!"
      - control:
          kind: pause
          length: short
      - text: " Да и "
      - control:
          kind: set_colour
          colour: blue
      - text: монстров
      - control:
          kind: reset_colour
      - text: " много."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Так что, если путешествуешь,\nвсегда бери с собой "
      - control:
          kind: set_colour
          colour: blue
      - text: зелья
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
          selected_index: 0
          cancel_index: 1
          unknown: 6
  talk05:
    attributes: Npc_HutagoHatago_Woman_01
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Этому "
      - control:
          kind: set_colour
          colour: blue
      - text: святилищу
      - control:
          kind: reset_colour
      - text: " где-то 10 000 лет."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 17
      - text: "И таких в мире много. Чуть ли не на\nкаждом углу какая-нибудь древность..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Археологи и ученые изо всех сил\nстараются найти все "
      - control:
          kind: set_colour
          colour: blue
      - text: святилища
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Но я в них ничего не понимаю.
      - control:
          kind: pause
          length: short
      - text: "\nПусть этим молодежь занимается."
  talk06:
    attributes: Npc_HutagoHatago_Woman_01
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 12
      - text: "Я так проголодалась, что\nсейчас в обморок упаду..."
  talk07:
    attributes: Npc_HutagoHatago_Woman_01
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 13
      - text: "Ох, дождь пошел!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Металл притягивает молнии, так что\nбудь осторожен, если у тебя с собой\nмеч или что-нибудь в этом роде."
  talk08:
    attributes: Npc_HutagoHatago_Woman_01
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 12
      - text: Скорее бы дождь прекратился.
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Вот в деревне Какарико все ходят\nв костюмах, в которых никакой\nдождь не страшен."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 5
            - 17
      - text: "Но их могут носить только шиика,\nпоэтому другим народам их не\nпродают."
      - control:
          kind: choice
          choice_labels:
            - 2
            - 3
          selected_index: 0
          cancel_index: 1
          unknown: 6
  talk09:
    attributes: Npc_HutagoHatago_Woman_01
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Какарико — это деревушка к северу\nотсюда."
      - control:
          kind: pause
          length: short
      - text: " Просто иди по дороге."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Я недавно там была: вокруг горы,\nа все жители одинаково одеты."
      - control:
          kind: pause
          length: short
      - text: "\nСтранное место..."
  talk10:
    attributes: Npc_HutagoHatago_Woman_01
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Металл притягивает молнии, так что\nбудь осторожен, если у тебя с собой\nмеч или что-нибудь в этом роде."
  talk11:
    attributes: Npc_HutagoHatago_Woman_01
    contents:
      - control:
          kind: set_colour
          colour: blue
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: Скорое зелье
      - control:
          kind: reset_colour
      - text: " позволит\nтебе быстрее бегать."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Это удобно, когда куда-то торопишься\nили хочешь оторваться от врагов."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 7
            - 9
      - text: "Для приготовления нужны лишь\n"
      - control:
          kind: set_colour
          colour: blue
      - text: "прыть-ящерицы "
      - control:
          kind: reset_colour
      - text: "да "
      - control:
          kind: set_colour
          colour: blue
      - text: органы монстров
      - control:
          kind: reset_colour
      - text: ",\nтак что у меня всегда его вдоволь."
  talk12:
    attributes: Npc_HutagoHatago_Woman_01
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Чтобы получилось зелье, необходимы\nнасекомые и органы монстра."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Зелья не восстанавливают здоровье,\nзато обладают другими полезными\nсвойствами.\nОни могут, например, повышать твою\nскорость или защищать от холода или\nзноя."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 7
            - 9
      - text: "Я вот люблю "
      - control:
          kind: set_colour
          colour: blue
      - text: скорое зелье
      - control:
          kind: reset_colour
      - text: " —\nоно позволяет быстрее бегать."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Для него нужны лишь "
      - control:
          kind: set_colour
          colour: blue
      - text: прыть-ящерицы
      - control:
          kind: reset_colour
      - text: "\nда "
      - control:
          kind: set_colour
          colour: blue
      - text: органы монстров
      - control:
          kind: reset_colour
      - text: ", так что я всегда\nего с собой ношу."
  talk13:
    attributes: Npc_HutagoHatago_Woman_01
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Чтобы получилось зелье, надо взять\nнасекомое, ящерицу или там лягушку\nи добавить орган монстра."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Зелья не восстанавливают здоровье,\nзато обладают другими полезными\nсвойствами.\nОни могут, например, повышать твою\nскорость или защищать от холода или\nзноя."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 17
      - text: "Неужели ты об этом не знал?\nКак ты тогда сюда добрался?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: set_colour
          colour: grey
      - control:
          kind: text_size
          percent: 80
      - text: Я бы точно не смогла.
      - control:
          kind: text_size
          percent: 100
      - control:
          kind: reset_colour
  talk14:
    attributes: Npc_HutagoHatago_Woman_01
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 6
            - 6
      - text: "Кстати, зелья я тебе больше не дам."
  talk15:
    attributes: Npc_HutagoHatago_Woman_01
    contents:
      - control:
          kind: sound
