  talk35:
    attributes: Npc_HatenoVillage022
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Спасибо, что помог."
      - control:
          kind: pause
          length: short
      - text: " Это тебе."
  talk36:
    attributes: Npc_HatenoVillage022
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 0
      - text: "Она любит "
      - control:
          kind: set_colour
          colour: red
      - text: бодрых сверчков
      - control:
          kind: reset_colour
      - text: "?"
      - control:
          kind: pause
          length: short
      - text: "\nИ хочет, чтобы у нее их было "
      - control:
          kind: set_colour
          colour: red
      - text: сто
      - control:
          kind: reset_colour
      - text: "?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 10
      - text: Надо же!
      - control:
          kind: pause
          length: short
      - text: " Прима такая загадочная,\nи это делает ее еще очаровательнее..."
      - control:
          kind: pause
          length: short
      - text: "\nТак мой друг про нее говорит."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: set_colour
          colour: grey
      - control:
          kind: text_size
          percent: 80
      - text: "Ох, волнуюсь!"
      - control:
          kind: pause
          length: short
      - text: " Не проговориться бы."
      - control:
          kind: text_size
          percent: 100
      - control:
          kind: reset_colour
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 1
      - text: "Хм, наловить целую "
      - control:
          kind: set_colour
          colour: red
      - text: "сотню бодрых\nсверчков "
      - control:
          kind: reset_colour
      - text: будет непросто.
      - control:
          kind: pause
          length: short
      - text: " Ты же\nмне поможешь?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 19
      - text: "Наверняка у тебя большой\nопыт в этом деле, я прав?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Поймай хотя бы "
      - control:
