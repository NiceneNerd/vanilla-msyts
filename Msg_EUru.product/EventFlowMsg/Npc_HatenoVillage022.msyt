      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: Меня друг просил узнать.
      - control:
          kind: pause
          length: short
      - text: "\nНу так что ей нравится?"
  talk31:
    attributes: Npc_HatenoVillage022
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 0
      - text: Правда?!
      - control:
          kind: pause
          length: short
      - text: " Быстро ты!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: text_size
          percent: 80
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - control:
          kind: set_colour
          colour: grey
      - text: А я еще ни одного не поймал.
      - control:
          kind: reset_colour
      - control:
          kind: text_size
          percent: 100
  talk32:
    attributes: Npc_HatenoVillage022
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Раз, два, три..."
  talk33:
    attributes: Npc_HatenoVillage022
    contents:
      - control:
          kind: sound
          unknown:
            - 2
            - 18
      - text: "У тебя ведь нет "
      - control:
