  talk01:
    attributes: Npc_OasisGoddess
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: "Остальных ты обманул,\nно меня не проведешь."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ладно, не бойся."
      - control:
          kind: pause
          length: short
      - text: " Я тебя не выдам."
      - control:
          kind: pause
          length: short
      - text: "\nРаз уж пришел, может, поболтаем\nо чем-нибудь?"
      - control:
          kind: choice
