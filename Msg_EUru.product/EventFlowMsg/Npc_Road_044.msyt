  MSG_Ichika_00_1:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Я хожу между "
      - control:
          kind: set_colour
          colour: blue
      - text: конюшней «Аккала»
      - control:
          kind: reset_colour
      - text: "\nи "
      - control:
