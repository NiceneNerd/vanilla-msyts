  Talk_101:
    attributes: Npc_SmallOasis012
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Под "
      - control:
          kind: set_colour
          colour: blue
      - text: Чудищем
      - control:
          kind: reset_colour
      - text: " я имею в виду громадину,\nкоторая шатается по пустыне."
      - control:
          kind: pause
          length: short
      - text: " Это из-\nза него поднялась песчаная буря."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Говорят, если подойти к "
      - control:
          kind: set_colour
          colour: blue
      - text: Чудищу
      - control:
          kind: reset_colour
      - text: "\nблизко, получишь удар молнией."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "Так страшно, что я боюсь идти в город."
      - control:
          kind: choice
          choice_labels:
            - 1
            - 2
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Talk_102:
    attributes: Npc_SmallOasis012
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: Это город к юго-западу отсюда.
      - control:
          kind: pause
          length: short
      - text: " Если\nты не догадался, там живут герудо."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Город большой: есть рынок, бар..."
      - control:
          kind: pause
          length: short
      - text: "\nЖизнь кипит с утра до поздней ночи."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: Но тебя туда все равно не пустят!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Потому что ты воой.
  Talk_103:
    attributes: Npc_SmallOasis012
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 6
      - text: Ты ведь там уже была?
      - control:
          kind: pause
          length: short
      - text: " Иначе\nоткуда у тебя этот наряд..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Как тебе рынок?
      - control:
          kind: pause
          length: short
      - text: " Понравился?"
      - control:
          kind: pause
          length: short
      - text: "\nА в бар ты заходила?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: set_colour
          colour: blue
      - text: Город герудо
      - control:
          kind: reset_colour
      - text: " — самое крупное\nпоселение в этих краях."
      - control:
          kind: pause
          length: short
      - text: " Жизнь\nтам кипит даже по ночам."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Как же я люблю наш бар!
      - control:
          kind: pause
          length: short
      - text: " Могу\nтам круглые сутки сидеть."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 5
            - 18
      - text: "Но из-за "
      - control:
          kind: set_colour
          colour: blue
      - text: Чудища
      - control:
          kind: reset_colour
      - text: " в город\nсейчас попасть сложно."
  Talk_105:
    attributes: Npc_SmallOasis012
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "Не хочу, чтобы "
      - control:
          kind: set_colour
          colour: blue
      - text: Чудище
      - control:
          kind: reset_colour
      - text: " меня\nподжарило, но и здесь уже\nне могу оставаться."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Неужели никто его не усмирит?..
