---
group_count: 19
atr1_unknown: 4
entries:
  "0000":
    attributes: ""
    contents:
      - text: Я попробую
  "0001":
    attributes: ""
    contents:
      - text: В другой раз
  "0002":
    attributes: ""
    contents:
      - text: Согласен
  "0003":
    attributes: ""
    contents:
      - text: "Нет, спасибо"
  Npc_Kokiri011_R_001:
    attributes: Npc_Kokiri011
    contents:
      - text: Испыта-а-ания!
  Talk_0000:
    attributes: Npc_Kokiri011
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 6
      - text: "Здравствуй, дорогой герой!"
      - control:
          kind: pause
          length: short
      - text: "\nМои сердечные поздравления\nс прохождением "
      - control:
          kind: set_colour
          colour: red
      - text: испытания огнем
      - control:
          kind: reset_colour
      - text: "!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Согласись, ведь это было\nпотрясающе весело?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 19
      - text: "А не хочешь ли попробовать еще более\nпотрясающе-веселящее испытание?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 6
            - 1
      - text: "О, не волнуйся, правила не изменятся."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Но дорогой герой должен\nуспеть к финишу за "
      - control:
          kind: set_colour
          colour: red
      - text: 5 минут
      - control:
          kind: reset_colour
      - text: ",\nиначе не получит приз!"
  Talk_0001:
    attributes: Npc_Kokiri011
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 16
      - text: "О, я понимаю..."
      - control:
          kind: pause
          length: short
      - text: " Ну что ж, приходи еще!"
  Talk_0002:
    attributes: Npc_Kokiri011
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 0
      - text: Превосходно!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Дорогой герой знает правила.
      - control:
          kind: pause
          length: short
      - text: "\nВзяв лесной "
      - control:
          kind: set_colour
          colour: red
      - text: меч
      - control:
          kind: reset_colour
      - text: ", лесной "
      - control:
          kind: set_colour
          colour: red
      - text: лук
      - control:
          kind: reset_colour
      - text: " и лесной\n"
      - control:
          kind: set_colour
          colour: red
      - text: щит
      - control:
          kind: reset_colour
      - text: ", приди с ними в святилище."
  Talk_0002_1:
    attributes: Npc_Kokiri011
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 2
      - text: Сейчас я все подготовлю.
  Talk_0002_2:
    attributes: Npc_Kokiri011
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 0
      - text: "Если успеешь быстрее, чем\nза "
      - control:
          kind: set_colour
          colour: red
      - control:
          kind: variable
          variable_kind: 14
          name: ShieldofKolog_mini_BestTime_L
      - text: " мин "
      - control:
          kind: variable
          variable_kind: 14
          name: ShieldofKolog_mini_BestTime_M
      - text: " с "
      - control:
          kind: variable
          variable_kind: 14
          name: ShieldofKolog_mini_BestTime_S
      - text: 0 мс
      - control:
          kind: reset_colour
      - text: ", тебя\nждет приз!"
  Talk_0004:
    attributes: Npc_Kokiri011
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: "Приветствую на "
      - control:
          kind: set_colour
          colour: red
      - text: испытании огнем
      - control:
          kind: reset_colour
      - text: ",\nдорогой герой!"
      - control:
          kind: pause
          length: short
      - text: " Дерзнешь пройти его\nза "
      - control:
          kind: set_colour
          colour: red
      - text: 5 минут
      - control:
          kind: reset_colour
      - text: ", чтобы получить приз?"
  Talk_0005:
    attributes: Npc_Kokiri011
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: "Приветствую на "
      - control:
          kind: set_colour
          colour: red
      - text: испытании огнем
      - control:
          kind: reset_colour
      - text: "!"
      - control:
          kind: pause
          length: short
      - text: "\nТвой рекорд — "
      - control:
          kind: set_colour
          colour: red
      - control:
          kind: variable
          variable_kind: 14
          name: ShieldofKolog_mini_BestTime_L
      - text: " мин "
      - control:
          kind: variable
          variable_kind: 14
          name: ShieldofKolog_mini_BestTime_M
      - text: " с "
      - control:
          kind: variable
          variable_kind: 14
          name: ShieldofKolog_mini_BestTime_S
      - text: 0 мс
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: pause
          length: short
      - text: "\nДорогой герой попробует его побить?"
  Talk_1000:
    attributes: Npc_Kokiri011
    contents:
      - text: "Ты успел за "
      - control:
          kind: set_colour
          colour: red
      - control:
          kind: variable
          variable_kind: 14
          name: ShieldofKolog_mini_ThisTime_L
      - text: " мин "
      - control:
          kind: variable
          variable_kind: 14
          name: ShieldofKolog_mini_ThisTime_M
      - text: " с "
      - control:
          kind: variable
          variable_kind: 14
          name: ShieldofKolog_mini_ThisTime_S
      - text: 0 мс
      - control:
          kind: reset_colour
      - text: "."
  Talk_1001:
    attributes: Npc_Kokiri011
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 10
      - text: По-здра-вля-ю!
      - control:
          kind: pause
          length: short
      - text: " Это рекорд!"
  Talk_1001_1:
    attributes: Npc_Kokiri011
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 0
      - text: "В "
      - control:
          kind: set_colour
          colour: red
      - text: 5 минут
      - control:
          kind: reset_colour
      - text: " ты уложился,\nтак что вот твой приз!"
  Talk_1001_2:
    attributes: Npc_Kokiri011
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 0
      - text: "Ты успел быстрее, чем в прошлый раз,\nи я рад снова вручить тебе приз!"
  Talk_1002:
    attributes: Npc_Kokiri011
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: Он твой безвозмездно!
  Talk_1003:
    attributes: Npc_Kokiri011
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Скажи, если захочешь побить\nсвой рекорд."
  Talk_1008:
    attributes: Npc_Kokiri011
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 0
      - text: "О, дорогому герою не удалось\nпобить свой рекорд..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Но результат все равно выдающийся,\nтак что вот небольшой подарок!"
  Talk_1009:
    attributes: Npc_Kokiri011
    contents:
      - control:
          kind: sound2
          unknown:
            - 8
            - 205
      - text: "Все, дорогой герой!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ты не уложился в пять минут и не\nполучишь подарок."
      - control:
          kind: pause
          length: short
      - text: " Может, в другой\nраз."
  Talk_1010:
    attributes: Npc_Kokiri011
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "О горе, горе!"
      - control:
          kind: pause
          length: short
      - text: " Ты не установил рекорд,\nдорогой герой!"
      - control:
          kind: pause
          length: short
      - text: " Я не могу расстаться\nс призом, увы."
      - control:
          kind: pause
          length: short
      - text: " В другой раз!"
  Talk_1011:
    attributes: Npc_Kokiri011
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 0
      - text: "За участие с тебя "
      - control:
          kind: set_colour
          colour: red
      - text: 20 рупий
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: pause
          length: short
      - text: "\nТы ведь не против, правда?"
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Talk_1012:
    attributes: Npc_Kokiri011
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 0
      - text: "О! "
      - control:
          kind: pause
          length: short
      - text: "Похоже, у тебя уже есть что-то\nиз "
      - control:
          kind: set_colour
          colour: red
      - text: необходимого снаряжения
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 6
            - 1
      - text: "В таком случае дорогой герой\nполучает скидку на участие!"
      - control:
          kind: pause
          length: short
      - text: "\nС тебя всего "
      - control:
          kind: set_colour
          colour: red
      - text: 10 рупий
      - control:
          kind: reset_colour
      - text: "."
  Talk_1013:
    attributes: Npc_Kokiri011
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 0
      - text: "Я одолжу тебе "
      - control:
          kind: set_colour
          colour: red
      - text: снаряжение
      - control:
          kind: reset_colour
      - text: ",\nкоторого у тебя не хватает."
      - control:
          kind: pause
          length: short
      - text: "\nТолько верни потом, ладно?"
  Talk_1014:
    attributes: Npc_Kokiri011
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 0
      - text: О горе!
      - control:
          kind: pause
          length: short
      - text: " Дорогой герой,\nу тебя не хватает рупий!"
  Talk_1100:
    attributes: Npc_Kokiri011
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Дорогой герой думал, что\nя его брошу?"
      - control:
          kind: pause
          length: short
      - text: " Ни..."
      - control:
          kind: pause
          length: short
      - text: " за..."
      - control:
          kind: pause
          length: short
      - text: " что!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Сначала я возьму снаряжение,\nкоторое тебе одолжил, и мы\nвернемся к старту."
      - control:
          kind: pause
          length: short
      - text: " Ура-а-а!"
  Talk_1101:
    attributes: Npc_Kokiri011
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 0
      - text: О!
      - control:
          kind: pause
          length: short
      - text: " Дорогой герой хочет повторить?"
      - control:
          kind: pause
          length: short
      - text: "\nТогда с тебя снова "
      - control:
          kind: set_colour
          colour: red
      - text: 20 рупий
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: choice
          choice_labels:
            - 2
            - 3
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Talk_1102:
    attributes: Npc_Kokiri011
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 0
      - text: О!
      - control:
          kind: pause
          length: short
      - text: " Дорогой герой хочет повторить?"
      - control:
          kind: pause
          length: short
      - text: "\nС учетом скидки с тебя "
      - control:
          kind: set_colour
          colour: red
      - text: 10 рупий
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: choice
          choice_labels:
            - 2
            - 3
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Talk_1103:
    attributes: Npc_Kokiri011
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 11
      - text: Чудесно!
      - control:
          kind: pause
          length: short
      - text: " Вернемся к старту."
      - control:
          kind: pause
          length: short
      - text: " Ура!"
  Talk_1104:
    attributes: Npc_Kokiri011
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "О... "
      - control:
          kind: pause
          length: short
      - text: Ну ладно.
      - control:
          kind: pause
          length: short
      - text: " Пока!"
  Talk_1105:
    attributes: Npc_Kokiri011
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ты думал, что Дамия бросит\nдорогого героя?"
      - control:
          kind: pause
          length: short
      - text: " Ни."
      - control:
          kind: pause
          length: short
      - text: " За."
      - control:
          kind: pause
          length: short
      - text: " Что!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Давай вернемся к старту.
      - control:
          kind: pause
          length: short
      - text: " Ура-а-а!"
  Talk_1106:
    attributes: Npc_Kokiri011
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 12
      - text: "Не расстраивайся, нам\nвсем иногда не везет."
  Talk_1107:
    attributes: Npc_Kokiri011
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Приходи, когда денежки появятся!"
