      - text: "!"
  talk100:
    attributes: Npc_TabantaBridgeHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Хочешь спросить дорогу?
      - control:
          kind: pause
          length: short
      - text: "\nЯ к твоим услугам!"
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
