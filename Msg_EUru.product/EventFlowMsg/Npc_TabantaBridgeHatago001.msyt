  talk000:
    attributes: Npc_TabantaBridgeHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: Доброе утро!
      - control:
          kind: pause
          length: short
      - text: " Добро пожаловать\nв "
      - control:
          kind: set_colour
          colour: blue
      - text: конюшню «Табантская»
      - control:
          kind: reset_colour
      - text: "!"
      - control:
          kind: pause
          length: short
      - text: " Хорошие\nу меня белые козы, правда?"
  talk001:
    attributes: Npc_TabantaBridgeHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Здравствуй!
      - control:
          kind: pause
          length: short
      - text: " Добро пожаловать\nв "
      - control:
          kind: set_colour
          colour: blue
      - text: конюшню «Табантская»
      - control:
          kind: reset_colour
      - text: "!"
      - control:
          kind: pause
          length: short
      - text: "\nЯ проверяю важный груз."
  talk002:
    attributes: Npc_TabantaBridgeHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: "Да, добро пожаловать в "
      - control:
          kind: set_colour
          colour: blue
      - text: "конюшню\n«Табантская»"
      - control:
          kind: reset_colour
      - text: "!"
      - control:
          kind: pause
          length: short
      - text: " Надо ухаживать\nза лошадьми..."
  talk003:
    attributes: Npc_TabantaBridgeHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 9
      - text: "Добро пожаловать в "
      - control:
          kind: set_colour
          colour: blue
      - text: "конюшню\n«Табантская»"
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: pause
          length: short
      - text: " Спящие детки\nвсегда такие милые..."
  talk004:
    attributes: Npc_TabantaBridgeHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: "Добро пожаловать в "
      - control:
          kind: set_colour
          colour: blue
      - text: "конюшню\n«Табантская»"
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: pause
          length: short
      - text: " Погода сегодня\nне очень, да?"
  talk005:
    attributes: Npc_TabantaBridgeHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Добро пожаловать\nв "
      - control:
          kind: set_colour
          colour: blue
      - text: конюшню «Табантская»
      - control:
