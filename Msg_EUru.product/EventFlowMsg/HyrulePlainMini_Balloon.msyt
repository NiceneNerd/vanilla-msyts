      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Ты принес "
      - control:
          kind: set_colour
          colour: red
      - text: "шарик "
      - control:
          kind: reset_colour
      - text: "даже в такую\nпогоду?"
      - control:
          kind: pause
          length: long
      - text: " Но как же он полетит\nсквозь тучи?"
  Talk_0002:
    attributes: Npc_HyrulePlain004
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 34
      - text: Хорошие дети ложатся спать вовремя!
      - control:
          kind: pause
          length: short
      - text: "\nМне пора баиньки."
      - control:
          kind: pause
          length: short
      - text: " Пока-пока!"
  Talk_0003:
    attributes: Npc_HyrulePlain004
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 2
      - text: Хорошие дети не спят слишком долго!
      - control:
          kind: pause
          length: short
      - text: "\nДядя, у меня к тебе просьба!"
      - control:
          kind: pause
          length: short
      - text: " Пойдем!"
  Talk_0004:
    attributes: Npc_HyrulePlain004
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 2
      - text: "Иногда мне снится, что давным-давно\nя жила в стране высоко над облаками."
      - control:
          kind: pause
          length: short
      - text: "\nА может, когда-то так и было?"
  Talk_0005:
    attributes: Npc_HyrulePlain004
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Я очень-очень хочу увидеть, как\n"
      - control:
          kind: set_colour
          colour: red
      - text: воздушный шарик
      - control:
          kind: reset_colour
      - text: " улетает в небо!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Если ты мне поможешь, я подарю\nтебе сокровище с неба!"
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Talk_0006:
    attributes: Npc_HyrulePlain004
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Погода плохая, и туч много..."
      - control:
          kind: pause
          length: short
      - text: "\nНо я хочу тебе кое-что сказать,\nдядя!"
  Talk_0007:
    attributes: Npc_HyrulePlain004
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 10
      - text: Спасибо!
      - control:
          kind: pause
          length: short
      - text: " Не зря я себя так\nхорошо веду!"
      - control:
          kind: pause
          length: short
      - text: " Привяжи "
      - control:
          kind: set_colour
          colour: red
      - text: шарик
      - control:
          kind: reset_colour
      - text: "\nк бочке и запусти ее в небо!"
  Talk_0009:
    attributes: Npc_HyrulePlain004
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Ты принес "
      - control:
          kind: set_colour
          colour: red
      - text: "шарик "
      - control:
          kind: reset_colour
      - text: "несмотря\nна погоду?"
      - control:
          kind: pause
          length: short
      - text: " Ты очень хороший!"
  Talk_0010:
    attributes: Npc_HyrulePlain004
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 9
      - text: Ура!
      - control:
          kind: pause
          length: short
      - text: " Ты принес "
      - control:
          kind: set_colour
          colour: red
      - text: шарик
      - control:
          kind: reset_colour
      - text: "!"
      - control:
          kind: pause
          length: short
      - text: " Привяжи\nего к бочке и запусти ее в небо!"
  Talk_0012:
    attributes: Npc_HyrulePlain004
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 1
      - text: "Я хотела бы улететь на "
      - control:
          kind: set_colour
          colour: red
      - text: шарике
      - control:
          kind: reset_colour
      - text: "\nв заоблачную страну и летать\nтам на большой птице!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: "Спасибо, дядя!"
      - control:
          kind: pause
          length: short
      - text: " Вот, возьми\nмое "
      - control:
          kind: set_colour
          colour: red
      - text: "сокровище "
      - control:
          kind: reset_colour
      - text: с неба!
  Talk_0013:
    attributes: Npc_HyrulePlain004
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 11
      - text: Ура!
      - control:
          kind: pause
          length: short
      - text: " Лети, "
      - control:
          kind: set_colour
          colour: red
      - text: шарик
      - control:
          kind: reset_colour
      - text: "!"
      - control:
          kind: pause
          length: short
      - text: "\nВ заоблачную страну!"
  Talk_0014:
    attributes: Npc_HyrulePlain004
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "Моя мечта лопнула,\nкак воздушный "
      - control:
          kind: set_colour
          colour: red
      - text: шарик
      - control:
          kind: reset_colour
      - text: "..."
  Talk_0018:
    attributes: Npc_HyrulePlain004
    contents:
      - text: Бочка тяжелая!
      - control:
          kind: pause
          length: short
      - text: " Чтобы она взлетела,\nнужен воздушный "
      - control:
          kind: set_colour
          colour: red
      - text: шарик
      - control:
          kind: reset_colour
      - text: "!"
      - control:
          kind: pause
          length: short
      - text: " А лучше\nнесколько."
