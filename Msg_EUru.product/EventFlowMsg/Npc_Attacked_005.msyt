    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: Я почти победила...
  Failed01:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "Ох, больно-то как!"
      - control:
          kind: pause
          length: short
