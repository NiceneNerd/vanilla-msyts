      - text: "..."
      - control:
          kind: pause
          length: short
      - text: "\nКакая из них какая?"
  SavedLv1_00:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 18
      - text: Ух ты!
      - control:
          kind: pause
          length: short
      - text: " Спасибо, что выручил меня!"
  SavedLv1_01:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 18
      - text: Ну ты силен!
      - control:
          kind: pause
          length: short
