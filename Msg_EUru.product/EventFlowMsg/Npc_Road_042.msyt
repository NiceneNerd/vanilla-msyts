  SavedLv2_00:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 11
      - text: Ого!
      - control:
          kind: pause
          length: short
      - text: " Ты же мне жизнь спас!"
  SavedLv2_01:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 11
      - text: Ну ты силен!
      - control:
          kind: pause
          length: short
      - text: " Большущее спасибо!"
  SavedLv3_00:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 11
      - text: Я тебе жизнью обязан!
  SavedLv3_01:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 11
      - text: Спасибо!
      - control:
          kind: pause
          length: short
      - text: " Ты мне жизнь спас!"
  Thanks00:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 10
      - text: "Возьми, пожалуйста,\nв знак благодарности."
