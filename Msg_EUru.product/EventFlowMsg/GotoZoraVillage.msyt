  Npc_Lanayru003_R_001:
    attributes: Npc_Lanayru003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: Привет!
      - control:
          kind: pause
          length: short
      - text: " Нечасто здесь\nвстретишь хайлийцев."
      - control:
          kind: single_choice
          label: 10
  Npc_Lanayru003_R_002:
    attributes: Npc_Lanayru003
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 16
      - text: "Слушай, ты встречал в этих\nкраях каких-нибудь зора?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Они просили тебя сходить к "
      - control:
          kind: set_colour
          colour: blue
      - text: "мосту\nИного"
      - control:
          kind: reset_colour
      - text: " и встретиться с их "
      - control:
          kind: set_colour
          colour: red
      - text: "принцем\nСидоном"
      - control:
          kind: reset_colour
      - text: "?"
  Npc_Lanayru003_R_003:
    attributes: Npc_Lanayru003
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: "Я другое дело: занимаюсь торговлей,\nпоэтому очень много путешествую."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: Кстати...
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 4
      - text: Меня зовут Магнолия!
  Npc_Lanayru003_R_004:
    attributes: Npc_Lanayru003
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 12
      - text: Ходила я к этому принцу...
      - control:
          kind: pause
          length: short
      - text: "\nОчень странный тип."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Сразу мне такой: «А как тебя\nзовут?"
      - control:
          kind: pause
          length: short
      - text: " Говори же скорее!»"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 18
      - text: Меня оттуда как ветром сдуло.
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 5
            - 1
      - text: Вот сам сходи — убедишься.
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: "Наверняка с парнями он ведет\nсебя не так нахально!"
  Npc_Lanayru003_R_005:
    attributes: Npc_Lanayru003
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 6
      - text: "Слушай, ты говорил вон с тем зора?"
      - control:
          kind: choice
          choice_labels:
            - 11
            - 12
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Npc_Lanayru003_R_006:
    attributes: Npc_Lanayru003
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 1
      - text: "Он сказал тебе встретиться\nс их "
      - control:
          kind: set_colour
          colour: red
      - text: принцем Сидоном
      - control:
          kind: reset_colour
      - text: ", да?"
      - control:
          kind: choice
          choice_labels:
            - 11
            - 12
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Npc_Lanayru003_R_007:
    attributes: Npc_Lanayru003
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 14
      - text: Не ври!
      - control:
          kind: pause
          length: short
      - text: " Я слышала, как\nон что-то там булькал!"
  Npc_Lanayru003_R_008:
    attributes: Npc_Lanayru003
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 14
      - text: Опять ты врешь!
      - control:
          kind: pause
          length: short
      - text: " Я же все слышала!"
  Npc_Lanayru003_R_009:
    attributes: Npc_Lanayru003
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 1
      - text: В общем...
  Npc_Lanayru003_R_010:
    attributes: Npc_Lanayru003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: "Идешь в сторону "
      - control:
          kind: set_colour
          colour: blue
      - text: владений зора
      - control:
          kind: reset_colour
      - text: "?"
      - control:
          kind: choice
          choice_labels:
            - 11
            - 12
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Npc_Lanayru003_R_011:
    attributes: Npc_Lanayru003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: Понятно.
      - control:
          kind: pause
          length: short
      - text: " Тогда остерегайся их\nпринца — он очень странный."
  Npc_Lanayru003_R_012:
    attributes: Npc_Lanayru003
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: Ясно.
      - control:
          kind: pause
          length: short
      - text: " Но все равно будь осторожен:\nу них там очень странный принц."
  Npc_Lanayru003_R_Near:
    attributes: Npc_Lanayru003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 3
      - text: "Эй, сюда!"
