          percent: 100
  Npc_Zora001_R_008:
    attributes: Npc_Zora001
    contents:
      - control:
          kind: text_size
          percent: 125
      - control:
          kind: sound
          unknown:
            - 7
            - 4
      - text: Иди к принцу!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 11
      - text: Немедленно!
      - control:
          kind: text_size
          percent: 100
  Npc_Zora001_R_Near01:
    attributes: Npc_Zora001
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 26
      - text: Пам-пам-парам!♪
  Npc_Zora013_R_001:
    attributes: Npc_Zora013
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: Извини за беспокойство.
      - control:
          kind: pause
          length: short
      - text: "\nЯ Ледо из народа зора."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Я заметил, что ты хайлиец,\nпоэтому окликнул тебя."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 5
            - 6
      - text: Уделишь мне минутку?
      - control:
          kind: choice
          choice_labels:
            - 8
            - 9
          selected_index: 0
          cancel_index: 2
          unknown: 6
  Npc_Zora013_R_002:
    attributes: Npc_Zora013
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: Видишь башню на той горе?
      - control:
          kind: pause
          length: short
      - text: "\nК северу от нее "
      - control:
          kind: set_colour
          colour: blue
      - text: река Зора
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Поднимись, пожалуйста, вверх\nпо течению реки — до моста под\nназванием "
      - control:
          kind: set_colour
          colour: blue
      - text: Иного
      - control:
          kind: reset_colour
      - text: "."
  Npc_Zora013_R_003:
    attributes: Npc_Zora013
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 1
      - text: "..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "Вижу в твоих глазах вопрос:\n«Почему он просит именно\nменя?»"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Что ж, если бы меня так внезапно\nо чем-то попросили, я бы тоже был\nозадачен."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 7
            - 11
      - text: "Но, поверь, дело того стоит!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 1
      - text: "Нашему "
      - control:
          kind: set_colour
          colour: red
      - text: принцу Сидону
      - control:
          kind: reset_colour
      - text: " нужен\nпомощник, причем именно такой,\nкак ты, то есть хайлиец."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 1
      - text: "Когда речь идет о королевских\nделах, можно рассчитывать на\nхорошее вознаграждение."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: "Вот почему я прошу тебя сходить\nк "
      - control:
          kind: set_colour
          colour: blue
      - text: мосту Иного
      - control:
          kind: reset_colour
      - text: "!"
      - control:
          kind: pause
          length: short
      - text: " "
      - control:
          kind: set_colour
          colour: red
      - text: Принц Сидон
      - control:
          kind: reset_colour
      - text: "\nдолжен быть там."
  Npc_Zora013_R_004:
    attributes: Npc_Zora013
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 4
      - text: "Хайлиец, сходи, пожалуйста,\nк "
      - control:
          kind: set_colour
          colour: red
      - text: принцу Сидону
      - control:
          kind: reset_colour
      - text: "."
  Npc_Zora013_R_005:
    attributes: Npc_Zora013
    contents:
      - control:
          kind: sound
