              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 16
      - control:
          kind: text_size
          percent: 80
      - text: "А может, все-таки заставить его?"
      - control:
          kind: pause
          length: short
      - text: " Попробую..."
      - control:
          kind: text_size
          percent: 100
  Npc_Zora001_R_002:
    attributes: Npc_Zora001
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 2
      - control:
          kind: text_size
          percent: 125
      - text: Хайлиец!
      - control:
          kind: text_size
          percent: 100
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 1
      - text: "Я вижу, ты опытный воин."
      - control:
