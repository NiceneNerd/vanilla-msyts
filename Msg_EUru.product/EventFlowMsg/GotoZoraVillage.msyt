  Npc_Zora001_R_007:
    attributes: Npc_Zora001
    contents:
      - control:
          kind: text_size
          percent: 125
      - control:
          kind: sound
          unknown:
            - 4
            - 22
      - text: Ух ты!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 4
            - 1
      - text: Впервые такого вижу!
      - control:
