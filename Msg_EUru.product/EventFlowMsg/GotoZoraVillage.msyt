            - 1
            - 1
      - text: "Он должен быть у "
      - control:
          kind: set_colour
          colour: blue
      - text: "моста\nИного "
      - control:
          kind: reset_colour
      - text: "на "
      - control:
          kind: set_colour
          colour: blue
      - text: реке Зора
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: "Я уверен, он тебя вознаградит!"
  Npc_Zora013_R_007:
    attributes: Npc_Zora013
    contents:
      - control:
          kind: sound
          unknown:
