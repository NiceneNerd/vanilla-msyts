            four:
              field_1: 0
      - text: Просто иди вверх по течению!
  Npc_Zora001_R_004:
    attributes: Npc_Zora001
    contents:
      - text: "Не подведи меня, хайлиец!"
  Npc_Zora001_R_005:
    attributes: Npc_Zora001
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 19
