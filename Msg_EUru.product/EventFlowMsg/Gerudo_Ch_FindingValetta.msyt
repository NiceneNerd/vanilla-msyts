  talk00:
    attributes: Npc_oasis041
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "Нет смысла врать, расскажу\nвсе как есть..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Мне сказали, к юго-западу от нашего\nгорода живет настоящая фея —\nв останках "
      - control:
          kind: set_colour
          colour: blue
      - text: огромного животного
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Я знала, что "
      - control:
          kind: set_colour
          colour: blue
      - text: Барта
      - control:
          kind: reset_colour
      - text: "\nэтим заинтересуется..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Но не могла и представить, что она\nтуда пойдет!"
      - control:
          kind: pause
