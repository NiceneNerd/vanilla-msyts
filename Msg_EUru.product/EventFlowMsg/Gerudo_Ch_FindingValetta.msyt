      - control:
          kind: text_size
          percent: 100
  Oasis042_talk09:
    attributes: Npc_oasis042
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "Ох, вы в своем репертуаре."
      - control:
