          unknown:
            - 4
            - 0
      - control:
          kind: text_size
          percent: 80
      - text: Правда?
      - control:
          kind: text_size
          percent: 100
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Я думала, тебе полагается такое знать."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Ну ладно.
      - control:
          kind: pause
          length: short
      - text: " У нас с сестрой свой\nмагазинчик одежды."
      - control:
          kind: pause
          length: short
      - text: " Заходи, если\nзахочешь приодеться."
  Talk_27:
    attributes: Npc_Kakariko016
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 1
      - control:
          kind: text_size
          percent: 80
      - text: Правда?
      - control:
