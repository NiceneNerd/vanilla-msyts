  Npc_oasis003_DLC2nd_SD_013:
    attributes: Npc_oasis003
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 6
      - text: "Ты имеешь в виду ту "
      - control:
          kind: set_colour
          colour: red
      - text: "древнюю\nсферу"
      - control:
          kind: reset_colour
      - text: ", что хранилась у нас\nмногие поколения?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 4
            - 0
      - text: "Линк, уж не собираешься ли ты\nпройти священное испытание\n"
      - control:
          kind: set_colour
          colour: blue
      - text: госпожи Урбозы
      - control:
          kind: reset_colour
      - text: "?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "В таком случае я обязана\nтебе обо всем рассказать."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 17
      - text: "Дело в том, что "
      - control:
          kind: set_colour
          colour: red
      - text: "древнюю\nсферу"
      - control:
          kind: reset_colour
      - control:
          kind: pause
          length: long
      - text: " украли."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Видимо, происшествие со "
      - control:
          kind: set_colour
          colour: blue
      - text: "Шлемом\nГрома"
      - control:
          kind: reset_colour
      - text: " нас ничему не научило..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "А знаешь, кто украл сферу?"
      - control:
          kind: pause
          length: short
      - text: "\nВсе те же "
      - control:
          kind: set_colour
          colour: red
      - text: бандиты Ига
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "К счастью, первой пропажу заметила\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Белла
      - control:
          kind: reset_colour
      - text: " и рассказала мне."
      - control:
          kind: pause
          length: short
      - text: " Остальным\nмы решили пока не рассказывать."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 6
            - 16
      - text: "Линк, нам снова нужна твоя помощь:\nзабери "
      - control:
          kind: set_colour
          colour: red
      - text: древнюю сферу
      - control:
          kind: reset_colour
      - text: " у бандитов."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Тогда я позволю тебе использовать\nее, чтобы пройти испытание "
      - control:
          kind: set_colour
          colour: blue
      - text: "госпожи\nУрбозы"
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 5
            - 1
      - control:
          kind: animation
          name: TalkPositive_01
      - text: "Все-таки в этом и есть главное\nпредназначение "
      - control:
          kind: set_colour
          colour: red
      - text: древней сферы
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: raw
