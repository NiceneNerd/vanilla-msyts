            - 2
            - 3
            - 4
            - 1
          selected_index: 0
          cancel_index: 3
          unknown: 10
  talk04:
    attributes: Npc_Goron022
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Линком звали "
      - control:
          kind: set_colour
          colour: blue
      - text: хайлийца
      - control:
          kind: reset_colour
      - text: ", который\nсражался плечом к плечу с Даруком,\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Воином
      - control:
          kind: reset_colour
      - text: " горонов, сто лет назад."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Если тебе нужны подробности,\nлучше с нашим "
      - control:
          kind: set_colour
          colour: blue
      - text: шефом
      - control:
          kind: reset_colour
      - text: " поговори."
  talk06:
    attributes: Npc_Goron022
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: Привет!
      - control:
          kind: pause
          length: short
      - text: " Хорошее утро..."
  talk07:
    attributes: Npc_Goron022
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: Привет!
      - control:
          kind: pause
          length: short
      - text: " Еще один жаркий день..."
  talk08:
    attributes: Npc_Goron022
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: Привет!
      - control:
          kind: pause
          length: short
      - text: " Еще одна жаркая ночь..."
  talk12:
    attributes: Npc_Goron022
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: Привет!
  talk13:
    attributes: Npc_Goron022
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: Пока!
  talk14:
    attributes: Npc_Goron022
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 2
      - text: Тогда добро пожаловать.
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 5
            - 0
      - text: "Нечасто к нам "
      - control:
          kind: set_colour
          colour: blue
      - text: хайлийцы
      - control:
          kind: reset_colour
      - text: " заходят!"
      - control:
          kind: pause
          length: short
      - text: "\nУ нас тут бывает жарковато."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Желаю хорошо провести время.
  talk15:
    attributes: Npc_Goron022
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 0
      - text: "По делам, значит?"
      - control:
          kind: pause
          length: short
      - text: " Горонград славится\nпрекрасными "
      - control:
          kind: set_colour
          colour: blue
      - text: драгоценными камнями
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: pause
          length: short
      - text: "\nТолько мы их не едим — невкусные."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Мы их продаем: другие народы\nготовы тратить на них бешеные\nденьги."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: "Поэтому мы в Горонграде не\nбедствуем, живем неплохо."
      - control:
          kind: pause
          length: short
      - text: "\nИ "
      - control:
          kind: set_colour
          colour: blue
      - text: шеф
      - control:
          kind: reset_colour
      - text: " у нас хороший."
  talk17:
    attributes: Npc_Goron022
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: Я про Чучудище ничего не знаю...
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Лучше у нашего "
      - control:
          kind: set_colour
          colour: blue
      - text: шефа
      - control:
          kind: reset_colour
      - text: " спроси —\nон в таких штуках разбирается."
  talk18:
    attributes: Npc_Goron022
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 0
      - text: "На "
      - control:
          kind: set_colour
          colour: blue
      - text: Горе Смерти
      - control:
          kind: reset_colour
      - text: " можно найти кучу\nдрагоценных камней."
      - control:
          kind: pause
          length: short
      - text: " Тем более\nсейчас, когда добычу остановили."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 1
      - text: "Можешь сам туда подняться\nи забрать себе все, что найдешь."
  talk20:
    attributes: Npc_Goron022
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Турист, значит?"
      - control:
          kind: pause
          length: short
      - text: " Жару\nхорошо переносишь?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "У нас для "
      - control:
          kind: set_colour
          colour: blue
      - text: хайлийцев
      - control:
          kind: reset_colour
      - text: " продаются\nспециальные огнестойкие доспехи."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Обязательно загляни в магазинчик\nпод названием "
      - control:
          kind: set_colour
          colour: blue
      - text: «Тверже скалы»
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Или можешь "
      - control:
          kind: set_colour
          colour: blue
      - text: пожарное зелье
      - control:
          kind: reset_colour
      - text: "\nпоискать — оно тоже сойдет."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ну ладно, сам разберешься."
      - control:
          kind: pause
          length: short
      - text: "\nЖелаю хорошо провести время."
  talk21:
    attributes: Npc_Goron022
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 1
      - text: "Ты вовремя к нам пришел, турист!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ящер на "
      - control:
          kind: set_colour
          colour: blue
      - text: Горе Смерти
      - control:
          kind: reset_colour
      - text: " угомонился,\nтеперь у нас снова безопасно."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 6
            - 1
      - text: Но жарковато...
      - control:
          kind: pause
          length: short
      - text: " даже для меня."
  talk22:
    attributes: Npc_Goron022
    contents:
      - control:
          kind: set_colour
          colour: grey
      - control:
          kind: text_size
          percent: 80
      - control:
          kind: sound
          unknown:
            - 5
            - 0
      - text: "Хм, а в прошлый раз сказал,\nчто идет к Чучудищу..."
      - control:
          kind: text_size
          percent: 100
      - control:
          kind: reset_colour
  talk23:
    attributes: Npc_Goron022
    contents:
      - control:
          kind: set_colour
          colour: grey
      - control:
          kind: text_size
          percent: 80
      - control:
          kind: sound
