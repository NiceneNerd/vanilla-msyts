  Talk_25:
    attributes: Npc_Goron_Camp001
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 14
      - control:
          kind: text_size
          percent: 125
      - text: Лава мерзлявая!
      - control:
          kind: raw
          zero:
            four:
