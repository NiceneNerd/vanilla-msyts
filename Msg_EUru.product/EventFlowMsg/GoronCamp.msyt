entries:
  "0000":
    attributes: ""
    contents:
      - text: Кровь отхлынула...
  "0001":
    attributes: ""
    contents:
      - text: Деньги в огонь!
  "0002":
    attributes: ""
    contents:
      - text: Что-то мне боязно...
  "0003":
    attributes: ""
    contents:
      - text: У меня кровь кипит!
  "0004":
    attributes: ""
    contents:
      - text: Какие там правила?
  "0005":
    attributes: ""
    contents:
      - text: Не дождешься
  "0006":
    attributes: ""
    contents:
      - text: Сдулся
  "0007":
    attributes: ""
    contents:
      - text: Дай пройти!
  "0008":
    attributes: ""
    contents:
      - text: Пока!
  Talk_00:
    attributes: Npc_Goron_Camp001
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 14
      - control:
          kind: text_size
          percent: 125
      - text: Кровь кипит в мышцах!
      - control:
          kind: text_size
          percent: 100
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 2
            - 1
      - text: А я люблю крепкие мышцы!
      - control:
          kind: pause
          length: short
      - text: "\nТут у нас, горонов, древнее\nместо силы."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Молодые испытывают здесь свою\nсилу духа и становятся настоящими\nгоронами с каменной волей!"
      - control:
          kind: choice
          choice_labels:
            - 7
            - 8
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Talk_01:
    attributes: Npc_Goron_Camp001
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 0
      - text: "Ну что, кровь кипит?!"
      - control:
          kind: pause
          length: short
      - text: "\nЗаплати "
      - control:
          kind: set_colour
          colour: red
      - text: "20 рупий "
      - control:
          kind: reset_colour
      - text: "и зажги\nпо-настоящему!"
      - control:
          kind: choice
          choice_labels:
            - 1
            - 4
            - 0
          selected_index: 0
          cancel_index: 2
          unknown: 8
  Talk_02:
    attributes: Npc_Goron_Camp001
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 21
      - text: "Более слабовольного ответа\nникогда не слышал!"
      - control:
          kind: pause
          length: short
      - text: " Эх ты,\nпрямо как сосулькой по голове!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 2
            - 1
      - text: "Если все-таки соберешься с духом,\nприходи!"
      - control:
          kind: pause
          length: short
      - text: " Я буду ждать! Слово горона!"
  Talk_03:
    attributes: Npc_Goron_Camp001
    contents:
      - control:
          kind: sound
          unknown:
            - 2
            - 14
      - text: У тебя денег нет!
      - control:
          kind: pause
          length: short
      - text: " А без них\nникаких тебе испытаний!"
  Talk_04:
    attributes: Npc_Goron_Camp001
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 4
      - text: "По пути на вершину собери "
      - control:
          kind: set_colour
          colour: red
      - text: "не менее\n100 рупий"
      - control:
          kind: reset_colour
      - text: "!"
      - control:
          kind: pause
          length: short
      - text: " И сделать это нужно за\nотведенное время."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Для парня с каменной\nволей — это раз плюнуть!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 2
            - 1
      - text: "Но если хоть одно из условий не\nвыполнено — испытание не пройдено!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ладно, пора начинать..."
  Talk_05:
    attributes: Npc_Goron_Camp001
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 1
      - text: "Эй! "
      - control:
          kind: pause
          length: short
      - text: Что ты там копаешься?!
      - control:
          kind: pause
          length: short
      - text: "\nВремя-то вышло!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 2
            - 1
      - text: "Жаль, но в этот раз не повезло!"
      - control:
          kind: pause
          length: short
      - text: "\nЕсли захочешь попытаться еще\nраз, приходи!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ах да, ты не можешь оставить\nсобранные рупии себе, извини."
  Talk_06:
    attributes: Npc_Goron_Camp001
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 10
      - control:
          kind: text_size
          percent: 125
      - text: Магма дорогая!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 7
            - 1
      - control:
          kind: text_size
          percent: 100
      - text: Ты с честью выдержал испытание!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Теперь ты настоящий волевой горон!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound2
          unknown:
            - 2
            - 205
      - text: "Ну почти! Как и обещал,\nя пропущу тебя!"
  Talk_07:
    attributes: Npc_Goron_Camp002
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 14
      - control:
          kind: text_size
          percent: 125
      - text: Супержар!
      - control:
          kind: text_size
          percent: 100
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 2
            - 1
      - text: "Тут у нас, горонов, древнее место\nсилы!"
      - control:
          kind: pause
          length: short
      - text: " Мы должны показать силу\nдуха на пути к вершине!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Если у тебя кровь в жилах не стынет,\nто поднимайся на вершину скалы\nи поговори с нашим "
      - control:
          kind: set_colour
          colour: blue
      - text: старшим братцем
      - control:
          kind: reset_colour
      - text: "!"
  Talk_08:
    attributes: Npc_Goron_Camp002
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 14
      - control:
          kind: text_size
          percent: 125
      - text: Круто!
      - control:
          kind: text_size
          percent: 100
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 2
            - 1
      - text: "Здесь проходит традиционное\n"
      - control:
          kind: set_colour
          colour: red
      - text: испытание
      - control:
          kind: reset_colour
      - text: " горонов: нужно особым\nспособом забраться на скалу Воли!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Но мой "
      - control:
          kind: set_colour
          colour: blue
      - text: старший братец
      - control:
          kind: reset_colour
      - text: " лучше\nобъяснит. Если что, он сидит\nна скале."
  Talk_09:
    attributes: Npc_Goron_Camp003
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 43
      - control:
          kind: text_size
          percent: 80
      - text: Пот ручьем...
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: text_size
          percent: 100
      - text: "Все, никаких сил больше нет..."
      - control:
          kind: pause
          length: short
      - text: "\nНо я не должен..."
      - control:
          kind: pause
          length: short
      - text: " сдаваться..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound2
          unknown:
            - 10
            - 205
      - text: "Эх, мне бы крылья, как у рито!"
      - control:
          kind: pause
          length: short
      - text: "\nПарил бы над вулканом..."
  Talk_10:
    attributes: Npc_Goron_Camp001
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 0
      - text: Боязно?!
      - control:
          kind: pause
          length: short
      - text: " Да не верю!"
      - control:
          kind: pause
          length: short
      - text: " Я же вижу,\nчто ты не из трусливых!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ладно, когда соберешься с духом,\nприходи."
      - control:
          kind: pause
          length: short
      - text: " Буду ждать тебя здесь.\nСлово горона!"
  Talk_11:
    attributes: Npc_Goron_Camp001
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 10
      - text: Да! Такой ответ мне по душе!
      - control:
          kind: pause
          length: short
      - text: " У тебя\nсейчас кровь не то что закипит,\nа в раскаленную лаву превратится!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Давай-ка я объясню тебе правила!
      - control:
          kind: pause
          length: short
      - text: "\nОни проще каменной глыбы!"
  Talk_12:
    attributes: Npc_Goron_Camp002
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 14
      - control:
          kind: text_size
          percent: 125
      - text: Займись делом!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 2
            - 1
      - control:
          kind: text_size
          percent: 100
      - text: "Время-то идет, тик-так!"
      - control:
          kind: pause
          length: short
      - text: "\nТоропись, а то не успеешь!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Или сдулся?
      - control:
          kind: choice
          choice_labels:
            - 5
            - 6
          selected_index: 0
          cancel_index: 2
          unknown: 6
  Talk_13:
    attributes: Npc_Goron_Camp003
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 43
      - control:
          kind: text_size
          percent: 80
      - text: Уважаемый...
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: text_size
          percent: 100
      - text: "Ты что, тоже хочешь пройти\nиспытание?"
      - control:
          kind: pause
          length: short
      - text: " У тебя получится,\nя уверен."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Эх, мне бы крылья, как у рито!"
      - control:
          kind: pause
          length: short
      - text: "\nПарил бы над вулканом..."
  Talk_14:
    attributes: Npc_Goron_Camp003
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 43
      - control:
          kind: text_size
          percent: 80
      - text: Такой мужественный!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: text_size
          percent: 100
      - text: Ты такой...
      - control:
          kind: pause
          length: short
      - text: " такой мужественный!"
      - control:
          kind: pause
          length: short
      - text: "\nПокажи класс..."
      - control:
          kind: pause
          length: short
      - text: " Покажи класс, парень!"
  Talk_15:
    attributes: Npc_Goron_Camp001
    contents:
      - control:
          kind: text_size
          percent: 125
      - control:
          kind: sound
          unknown:
            - 8
            - 7
      - text: "Эй, ты куда?!"
      - control:
          kind: text_size
          percent: 100
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 2
            - 1
      - text: "Испугался, что ли?\nГде твоя сила воли?!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Все, ты провалил испытание!"
      - control:
          kind: pause
          length: short
      - text: "\nВозвращай собранные рупии."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Но если вдруг захочешь\nпопытаться еще раз, приходи!"
  Talk_16:
    attributes: Npc_Goron_Camp001
    contents:
      - control:
          kind: sound
          unknown:
            - 2
            - 0
      - text: Правила простые как три рупии!
      - control:
          kind: pause
          length: short
      - text: "\nДавай объясню еще раз — слушай\nвнимательно!"
  Talk_17:
    attributes: Npc_Goron_Camp001
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 21
      - control:
          kind: text_size
          percent: 125
      - text: "Эх, маловато будет!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: text_size
          percent: 100
      - text: "Я же тебе сказал, сколько\nрупий надо собрать!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ладно, не беда. В следующий\nраз у тебя все получится."
      - control:
          kind: pause
          length: short
      - text: " Когда\nсоберешься с духом, приходи!"
  Talk_18:
    attributes: Npc_Goron_Camp001
    contents:
      - control:
          kind: sound
          unknown:
            - 2
            - 0
      - text: "А да, совсем забыл. Воля — это круто,\nконечно, но деньги..."
      - control:
          kind: pause
          length: long
      - text: " куда уж без них."
      - control:
          kind: pause
          length: long
      - text: "\nКороче, с тебя "
      - control:
          kind: set_colour
          colour: red
      - text: 20 рупий
      - control:
          kind: reset_colour
      - text: "!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Как настрой?!
      - control:
          kind: choice
          choice_labels:
            - 1
            - 4
            - 0
          selected_index: 0
          cancel_index: 2
          unknown: 8
  Talk_19:
    attributes: Npc_Goron_Camp001
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 21
      - control:
          kind: text_size
          percent: 125
      - text: "Эй, ты издеваешься?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: text_size
          percent: 100
      - text: "Ты меня, вообще, слушал?!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Сколько рупий, я сказал, нужно\nсобрать?"
      - control:
          kind: pause
          length: short
      - text: " А у тебя сколько?!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Если хочешь, можешь\nпопытаться еще раз!"
  Talk_20:
    attributes: Npc_Goron_Camp001
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 22
      - control:
          kind: text_size
          percent: 125
      - text: Вот невезуха-то!!!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: text_size
          percent: 100
      - text: Ведь почти собрал 100 рупий!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: В следующий раз все получится!
      - control:
          kind: pause
          length: short
      - text: "\nКогда соберешься с духом, приходи!"
  Talk_21:
    attributes: Npc_Goron_Camp001
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 1
      - control:
          kind: text_size
          percent: 125
      - text: Забрался!!!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 2
            - 10
      - text: Мо-ло-ток!!!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: text_size
          percent: 100
      - text: "Уложился в установленное\nвремя! Посмотрим, сколько\nты собрал рупий..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: set_colour
          colour: red
      - control:
          kind: text_size
          percent: 125
      - control:
          kind: sound
          unknown:
            - 8
            - 0
      - control:
          kind: variable
