  New_09:
    attributes: Npc_Minigame_BirdMan001
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 20
      - text: Что?
      - control:
          kind: pause
          length: short
      - text: " Ты умеешь летать как..."
      - control:
