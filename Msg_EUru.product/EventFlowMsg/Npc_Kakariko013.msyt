  ClerkAsk00:
    attributes: Npc_Kakariko013
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Славная "
      - control:
          kind: set_colour
          colour: blue
      - text: прыть-морковь
      - control:
          kind: reset_colour
      - text: "\nуродилась, правда?"
      - control:
          kind: choice
          choice_labels:
            - 1000
            - 1001
          selected_index: 0
          cancel_index: 1
          unknown: 6
  ClerkAsk01:
    attributes: Npc_Kakariko013
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Не очень?!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 8
            - 0
      - text: "Мог бы хоть из вежливости\nсказать что-нибудь хорошее!"
  ClerkAsk02:
    attributes: Npc_Kakariko013
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 9
      - text: Урожай что надо!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Моя "
      - control:
          kind: set_colour
          colour: blue
      - text: прыть-морковь
      - control:
          kind: reset_colour
      - text: " продается\nв "
      - control:
          kind: set_colour
          colour: blue
      - text: продуктовой лавке
      - control:
          kind: reset_colour
      - text: ", если что."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Захочешь купить — заходи\nтуда в любое время!"
  FirstTalk00:
    attributes: Npc_Kakariko013
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Слушаю?
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Парень, твое лицо мне знакомо."
  FirstTalk02:
    attributes: Npc_Kakariko013
    contents:
      - text: Как гласит легенда...
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 8
            - 0
      - text: "Герой каждый день учился\nфехтовать "
      - control:
          kind: set_colour
          colour: blue
      - text: прыть-морковкой
      - control:
          kind: reset_colour
      - text: "..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "И ел он только "
      - control:
          kind: set_colour
          colour: blue
      - text: прыть-морковь
      - control:
          kind: reset_colour
      - text: "..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "И закалял свою волю,\nнюхая "
      - control:
          kind: set_colour
          colour: blue
      - text: прыть-морковь
      - control:
          kind: reset_colour
      - text: "..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "По крайней мере, так мне\nдедушка рассказывал."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "В общем, заканчивай со своими\nшуточками."
      - control:
          kind: pause
          length: short
      - text: " Лучше будь как тот\nгерой и грызи морковку."
  FirstTalk03:
    attributes: Npc_Kakariko013
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Если хочешь моркови, загляни\nв "
      - control:
          kind: set_colour
          colour: blue
      - text: продуктовую лавку
      - control:
          kind: reset_colour
      - text: "."
  FirstTalk04:
    attributes: Npc_Kakariko013
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Если хочешь моркови, ты\nпришел в нужное место."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "А если хочешь стать сильным, как\nлегендарный герой, иди за мной."
  FirstTalk05:
    attributes: Npc_Kakariko013
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Если хочешь моркови, загляни\nв лавку к моей старушке."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "А если хочешь стать сильным,\nкак легендарный герой, найди\nменя "
      - control:
          kind: set_colour
          colour: red
      - text: вечером
      - control:
          kind: reset_colour
      - text: "."
  FirstTalk06:
    attributes: Npc_Kakariko013
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Если хочешь моркови, загляни\nв лавку к моей старушке."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "А если хочешь стать сильным, как\nлегендарный герой, иди за мной."
  FirstTalk07:
    attributes: Npc_Kakariko013
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Если хочешь моркови, загляни\nв лавку к моей старушке."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "А если хочешь стать сильным, как\nлегендарный герой, я могу дать тебе\nпару советов."
  FirstTalk08:
    attributes: Npc_Kakariko013
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Если хочешь моркови, загляни\nв лавку к моей старушке."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "А если хочешь стать сильным, как тот\nгерой, найди меня "
      - control:
          kind: set_colour
          colour: red
      - text: завтра вечером
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: pause
          length: short
      - text: "\nСегодня я уже подустал."
  FirstTalk11:
    attributes: Npc_Kakariko013
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 0
      - text: Что-что?
      - control:
          kind: pause
          length: short
      - text: " Ты и есть тот герой?"
  FirstTalk12:
    attributes: Npc_Kakariko013
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 9
      - text: Хо-хо!
      - control:
          kind: pause
          length: short
      - text: " Да нет..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Вряд ли это тот самый меч.
  FirstTalk13:
    attributes: Npc_Kakariko013
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 21
      - control:
          kind: text_size
          percent: 125
      - text: Ох ты!
      - control:
          kind: text_size
          percent: 100
  FirstTalk14:
    attributes: Npc_Kakariko013
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 10
      - text: Ха-ха-ха!
  FirstTalk15:
    attributes: Npc_Kakariko013
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Ну ты и шутник.
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "У того-то героя был легендарный\nмеч — "
      - control:
          kind: set_colour
          colour: red
      - text: Сокрушитель Зла
      - control:
          kind: reset_colour
      - text: "."
  FirstTalk16:
    attributes: Npc_Kakariko013
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 1
      - text: "Да у тебя, кажись, меч героя есть —\nлегендарный "
      - control:
          kind: set_colour
          colour: red
      - text: Сокрушитель Зла
      - control:
          kind: reset_colour
      - text: "!"
  FirstTalk18:
    attributes: Npc_Kakariko013
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 19
      - text: А?
  FirstTalk19:
    attributes: Npc_Kakariko013
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 4
      - text: "Да, точно!"
      - control:
          kind: pause
          length: short
      - text: " Как две капли воды!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ты похож на героя, который пытался\nостановить Великое Бедствие много\nлет назад."
      - control:
          kind: single_choice
          label: 10
  Meeting00:
    attributes: Npc_Kakariko013
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 18
      - text: "Трисса, дорогая моя!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Ты не устала?
      - control:
          kind: pause
          length: short
      - text: " Покупатели не надоели?"
      - control:
          kind: pause
          length: short
      - text: "\nТебе не холодно?"
      - control:
          kind: pause
          length: short
      - text: " Может, покушаешь?"
  Meeting01:
    attributes: Npc_Kakariko014
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 9
      - text: Хе-хе!
      - control:
          kind: pause
          length: short
      - text: " Дед, не суетись."
      - control:
          kind: pause
          length: short
      - text: "\nЯ ведь не маленькая!"
  Near00:
    attributes: Npc_Kakariko013
    contents:
      - control:
          kind: sound2
          unknown:
            - 24
            - 205
      - text: Семь морковок...
  Near01:
    attributes: Npc_Kakariko013
    contents:
      - text: Надо идти...
  Near03:
    attributes: Npc_Kakariko013
    contents:
      - text: "Расти, моя хорошая!"
  Near05:
    attributes: Npc_Kakariko013
    contents:
      - text: Скосить бы траву...
  Near06:
    attributes: Npc_Kakariko013
    contents:
      - text: Я не уступлю!
  Near07:
    attributes: Npc_Kakariko013
    contents:
      - text: Хо! Ха!
  Near08:
    attributes: Npc_Kakariko013
    contents:
      - text: Трисса!
  Near10:
    attributes: Npc_Kakariko013
    contents:
      - text: Хр-р-р...
  Near11:
    attributes: Npc_Kakariko013
    contents:
      - text: Дождя я не боюсь...
  Near12:
    attributes: Npc_Kakariko013
    contents:
      - text: Грозы я не боюсь...
  Near13:
    attributes: Npc_Kakariko013
    contents:
      - control:
          kind: sound2
          unknown:
            - 24
            - 205
      - text: Восемь морковок...
  Near14:
    attributes: Npc_Kakariko013
    contents:
      - control:
          kind: sound2
          unknown:
            - 24
            - 205
      - text: Девять морковок...
  Near15:
    attributes: Npc_Kakariko013
    contents:
      - control:
          kind: sound2
          unknown:
            - 24
            - 205
      - text: Шесть морковок...
  SupriseHouse:
    attributes: Npc_Kakariko013
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 0
      - text: Кто здесь?
  SupriseHouse_00:
    attributes: Npc_Kakariko013
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 0
      - text: Ах ты ж!.. Напугал старика!
  SupriseHouse_01:
    attributes: Npc_Kakariko013
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 0
      - text: Зря ты сюда забрался!
      - control:
          kind: pause
          length: short
      - text: "\nСейчас я тебя проучу!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 4
            - 19
      - text: "Слушай, ты похож на героя, который\nпытался остановить Великое Бедствие\nмного лет назад..."
      - control:
          kind: single_choice
          label: 10
  Talk00:
    attributes: Npc_Kakariko013
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Гордость нашей деревни —\nнастоящая "
      - control:
          kind: set_colour
          colour: blue
      - text: прыть-морковь
      - control:
          kind: reset_colour
      - text: "!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Сочная, ароматная и выращенная\nс любовью."
      - control:
          kind: pause
          length: short
      - text: " А уж еда-то из нее\nкакая вкусная получается!"
  Talk01:
    attributes: Npc_Kakariko013
    contents:
      - control:
          kind: sound
          unknown:
            - 2
            - 14
      - text: "Да там у него огород, за ручьем."
      - control:
          kind: pause
          length: short
      - text: "\nВыращивает свои "
      - control:
          kind: set_colour
          colour: blue
      - text: латные тыквы
      - control:
          kind: reset_colour
      - text: "..."
      - control:
          kind: pause
          length: short
      - text: "\nДавно я ему в глаза не смотрел."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Он все твердит, что лучшее\nнападение — это защита."
      - control:
          kind: pause
          length: short
      - text: "\nСовсем спятил."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 8
            - 0
      - text: Мы с ним раньше на охоту ходили.
      - control:
          kind: pause
          length: short
      - text: "\nТак он пока к зверю-то подкрадется,\nуж утро наступит!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ты не его слушай, а меня, понял?"
      - control:
          kind: pause
          length: short
      - text: "\nЛучшая защита — нападение!"
  Talk02:
    attributes: Npc_Kakariko013
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: Это продуктовая лавка моей старушки.
      - control:
          kind: pause
          length: short
      - text: "\nЯ к ней иногда заглядываю."
  Talk04:
    attributes: Npc_Kakariko013
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Нацелься на врага и не выпускай\nего из виду в течение всего боя."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Это основа основ!
  Talk05:
    attributes: Npc_Kakariko013
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Выполни удар в прыжке, и, когда\nприземлишься, возникнет ударная\nволна."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Чем выше прыжок, тем мощнее атака."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Но если упадешь с большой\nвысоты, сам получишь урон."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Освоишь этот прием — сможешь\nнеожиданно нападать сверху сразу\nна нескольких врагов."
  Talk06:
    attributes: Npc_Kakariko013
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 0
      - text: "Надеюсь, ты не думаешь, что нападать\nбез предупреждения некрасиво?"
      - control:
          kind: pause
          length: short
      - text: " Это\nотличная тактика!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 1
      - text: "Тихонько так подкрадываешься\nк врагу, а потом ка-а-ак..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 8
            - 0
      - control:
          kind: text_size
          percent: 125
      - text: Бац!
      - control:
          kind: text_size
          percent: 100
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: И лучше подбираться пригнувшись.
      - control:
          kind: pause
          length: short
      - text: "\nЕсли застанешь врага врасплох,\nнанесешь большой урон."
      - control:
          kind: choice
          choice_labels:
            - 16
            - 17
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Talk07:
    attributes: Npc_Kakariko013
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Вообще-то я хотел пройти испытание\nв святилище над деревней, чтобы\nовладеть новыми приемами..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Но никто не знает, как в то святилище\nпопасть."
      - control:
          kind: pause
          length: short
      - text: " Ну я хотя бы тренируюсь вот\nтак каждый день..."
  Talk08:
    attributes: Npc_Kakariko013
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Когда пригибаешься, тебя сложнее\nзаметить."
      - control:
          kind: pause
          length: short
      - text: " И не только монстрам, но\nи зверям и прочей живности."
  Talk09:
    attributes: Npc_Kakariko013
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 12
      - text: "Когда я был помоложе, тренировался\nс утра до вечера, а сейчас быстро\nустаю."
      - control:
          kind: pause
          length: short
      - text: " Возраст берет свое..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Если хочешь потренироваться,"
      - control:
          kind: pause
          length: short
      - text: "\nприходи "
      - control:
          kind: set_colour
          colour: red
      - text: завтра вечером
      - control:
          kind: reset_colour
      - text: "."
  Talk10:
    attributes: Npc_Kakariko013
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - control:
          kind: set_colour
          colour: blue
      - text: Прыть-морковь
      - control:
          kind: reset_colour
      - text: " укрепляет дух\nи тело."
      - control:
          kind: pause
          length: short
