      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Но сейчас в городе говорят лишь\nпро каких-то бандитов "
      - control:
          kind: set_colour
          colour: red
      - text: клана Ига
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
