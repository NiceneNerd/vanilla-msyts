  MSG_Gerud_00:
    attributes: Npc_Road_012
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: "Я легендарная охотница за\nсокровищами."
      - control:
          kind: pause
          length: short
