entries:
  "0000":
    attributes: ""
    contents:
      - text: Ладно
  "0001":
    attributes: ""
    contents:
      - text: В другой раз
  Npc_kokiri012_Talk000:
    attributes: Npc_kokiri012
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: Ух ты!
      - control:
          kind: pause
          length: short
      - text: " Неужели ты тот самый герой?!"
      - control:
          kind: pause
          length: short
      - text: "\nДобро пожаловать в лес Короков!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Всегда мечтал увидеть настоящего\nгероя!"
      - control:
          kind: pause
          length: short
      - text: " Ты намного героичнее, чем\nя себе представлял!"
  Npc_kokiri012_Talk001:
    attributes: Npc_kokiri012
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Давай же, иди скорее к "
      - control:
          kind: set_colour
          colour: red
      - text: мечу
      - control:
          kind: reset_colour
      - text: "!"
  Npc_kokiri012_Talk002:
    attributes: Npc_kokiri012
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: Красота!
      - control:
          kind: pause
          length: short
      - text: " Загляденье!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ну, как твои геройские дела?\nПродвигаются?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "А ты знал, что кроме ледяных палочек\nесть еще огненные и электрические?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 6
            - 0
      - text: "Дорогой герой, я хочу, чтобы ты стал\nеще сильнее и навел порядок в нашем\nлесу..."
      - control:
          kind: pause
          length: short
      - text: " ну и во всем Хайруле, ладно."
  Npc_kokiri012_Talk003:
    attributes: Npc_kokiri012
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: Красота!
      - control:
          kind: pause
          length: short
      - text: " Загляденье!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Даже "
      - control:
          kind: set_colour
          colour: blue
      - text: Высший Меч
      - control:
          kind: reset_colour
      - text: " вытащил!"
      - control:
          kind: pause
          length: short
      - text: " Геро-о-ой!"
      - control:
          kind: pause
          length: short
      - text: "\nНу а если меч устанет, ты про палочки\nне забывай, да?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 6
            - 0
      - text: "Они не только ледяные бывают, но и\nогненные и — ух-х! — электрические!"
  Npc_kokiri012_Talk004:
    attributes: Npc_kokiri012
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 0
      - text: "Дорогой геро-о-ой, а ты покажешь\nмне Высший Меч?"
      - control:
          kind: pause
          length: short
      - text: " Ну пазязя!"
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Npc_kokiri012_Talk005:
    attributes: Npc_kokiri012
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: Ура-а-а!
      - control:
          kind: pause
          length: short
      - text: " Показывай!"
  Npc_kokiri012_Talk006:
    attributes: Npc_kokiri012
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 10
      - text: У-у-у!
      - control:
          kind: pause
          length: short
      - text: " «Получай, злодей!»"
      - control:
          kind: pause
          length: short
      - text: " БЫДЫЩЬ!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 6
            - 0
      - text: "Спасибо, дорогой герой!"
      - control:
          kind: pause
          length: short
      - text: " Я так рад,\nчто ты мне показал этот меч!"
      - control:
          kind: pause
          length: short
      - text: " Ах!"
      - control:
          kind: pause
          length: short
      - text: "\nА теперь иди, спасай принцессу."
  Npc_kokiri012_Talk007:
    attributes: Npc_kokiri012
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: О-о-о...
      - control:
          kind: pause
          length: short
      - text: " печалька..."
