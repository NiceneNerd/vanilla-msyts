      - text: "«Сундук\nудачи»"
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: pause
          length: short
      - text: " Очень весело..."
      - control:
          kind: pause
          length: short
      - text: " когда\nвыигрываешь."
  talk20:
    attributes: Npc_SouthernVillage010
    contents:
      - control:
