  talk01:
    attributes: Npc_SouthernVillage010
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ты что, не слышал про наш пруд?"
      - control:
          kind: pause
          length: short
      - text: "\nПо легенде, на нем можно найти\nсвою любовь."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Туристы издалека приходят, чтобы\nна него посмотреть."
      - control:
          kind: pause
          length: short
      - text: " Это "
      - control:
          kind: set_colour
          colour: red
      - text: пруд Любви
      - control:
          kind: reset_colour
      - text: "\nна "
      - control:
          kind: set_colour
          colour: blue
      - text: горе Туфт
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Тоже сходи.
      - control:
          kind: pause
          length: short
      - text: " Тебе понравится."
  talk02:
    attributes: Npc_SouthernVillage010
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Понятно.
      - control:
          kind: pause
          length: short
      - text: " Обычно к нам именно\nради этого туристы приходят."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Хотят посмотреть на "
      - control:
          kind: set_colour
          colour: red
      - text: пруд Любви
      - control:
          kind: reset_colour
      - text: "\nна "
      - control:
          kind: set_colour
          colour: blue
      - text: горе Туфт
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: pause
          length: short
      - text: " По легенде, на нем\nможно найти свою любовь."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Но у тебя, наверное, и так\nот девушек отбоя нет..."
  talk03:
    attributes: Npc_SouthernVillage010
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 33
      - text: М-м-н-н...
  talk04:
    attributes: Npc_SouthernVillage010
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Недавно на "
      - control:
          kind: set_colour
          colour: blue
      - text: "гору Туфт "
      - control:
          kind: reset_colour
      - text: "поднялись\nкакие-то туристы."
      - control:
          kind: pause
          length: short
      - text: " Интересно, они\nнашли свою любовь?"
  talk05:
    attributes: Npc_SouthernVillage010
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 34
      - text: Я спать.
      - control:
          kind: pause
          length: short
      - text: " Завтра поболтаем..."
  talk08:
    attributes: Npc_SouthernVillage010
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "Но мои финансы поют романсы:\nв последнее время мне постоянно\nне везет."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 3
            - 0
      - text: "Слушай, одолжи мне "
      - control:
          kind: set_colour
          colour: red
      - text: 100 рупий
      - control:
          kind: reset_colour
      - text: ", а?"
      - control:
          kind: pause
          length: short
      - text: "\nЕсли выиграю, отдам тебе в "
      - control:
          kind: set_colour
          colour: red
      - text: "десять\nраз "
      - control:
          kind: reset_colour
      - text: больше.
      - control:
          kind: choice
          choice_labels:
            - 2
            - 3
          selected_index: 0
          cancel_index: 1
          unknown: 6
  talk09:
    attributes: Npc_SouthernVillage010
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: Жаль.
      - control:
          kind: pause
          length: short
      - text: " Придется у кого-нибудь\nдругого в долг просить..."
  talk10:
    attributes: Npc_SouthernVillage010
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Дружище, ты меня в прошлый\nраз так выручил."
      - control:
          kind: pause
          length: short
      - text: " Спасибо."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 3
            - 0
      - text: Только вот...
      - control:
          kind: pause
          length: short
      - text: " я опять на мели."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ты не мог бы мне еще деньжонок\nподкинуть?"
      - control:
          kind: pause
          length: short
      - text: " Хотя бы "
      - control:
          kind: set_colour
          colour: red
      - text: 100 рупий
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: pause
          length: short
      - text: "\nЯ верну в"
      - control:
          kind: set_colour
          colour: red
      - text: " десять раз "
      - control:
          kind: reset_colour
      - text: больше.
      - control:
          kind: choice
          choice_labels:
            - 2
            - 3
          selected_index: 0
          cancel_index: 1
          unknown: 6
  talk11:
    attributes: Npc_SouthernVillage010
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Дружище, не спеши."
      - control:
          kind: pause
          length: short
      - text: " Я только завтра\nузнаю, выиграл я или нет."
      - control:
          kind: pause
          length: short
      - text: " Приходи\nпотом."
  talk12:
    attributes: Npc_SouthernVillage010
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: "Дружище, я выиграл!"
      - control:
          kind: pause
          length: short
      - text: " Мне повезло!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Спасибо, что одолжил денег!"
      - control:
          kind: pause
          length: short
      - text: " Вот,\nкак обещал, отдаю в "
      - control:
          kind: set_colour
          colour: red
      - text: десять раз
      - control:
          kind: reset_colour
      - text: "\nбольше, чем брал!"
  talk13:
    attributes: Npc_SouthernVillage010
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "Дружище, я проиграл и опять на мели."
  talk14:
    attributes: Npc_SouthernVillage010
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 0
      - text: Надо еще раз сыграть!
      - control:
          kind: pause
          length: short
      - text: "\nИ тебе рекомендую!"
  talk15:
    attributes: Npc_SouthernVillage010
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: Премного благодарен!
  talk16:
    attributes: Npc_SouthernVillage010
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 0
      - text: Вот теперь я разбогатею.
      - control:
          kind: pause
          length: short
      - text: " Ты только\nне переживай!"
      - control:
          kind: pause
          length: short
      - text: " Приходи завтра,\nи я тебе расскажу, как все прошло."
  talk17:
    attributes: Npc_SouthernVillage010
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 12
      - text: "Издеваешься, да?"
      - control:
          kind: pause
          length: short
      - text: " У тебя\nсамого столько нет."
      - control:
          kind: pause
          length: short
      - text: " Эх..."
  talk18:
    attributes: Npc_SouthernVillage010
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 0
      - text: В такую погоду не порыбачишь.
      - control:
          kind: pause
          length: short
      - text: "\nВот бы сейчас поиграть..."
      - control:
          kind: single_choice
          label: 4
  talk19:
    attributes: Npc_SouthernVillage010
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Клойн у нас ведет игру "
      - control:
          kind: set_colour
