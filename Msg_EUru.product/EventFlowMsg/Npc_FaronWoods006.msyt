            one_field:
              - 4
              - field_1: 0
      - text: деревня Какарико...
      - control:
          kind: pause
          length: short
      - text: "\nВон туда топай."
  Kakariko01:
    attributes: Npc_FaronWoods006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "За Близнецами есть конюшня —\nтам спросишь, куда дальше идти."
  Near00:
    attributes: Npc_FaronWoods006
    contents:
      - control:
