  Colony00:
    attributes: Npc_FaronWoods006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 6
      - text: Куда-куда?
      - control:
          kind: pause
          length: short
      - text: " На Великое плато?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Что ты на нем забыл?
      - control:
          kind: pause
          length: short
      - text: " Там ведь,\nнаверное, нет ни души."
      - control:
          kind: pause
          length: short
      - text: " Да и как\nна него забраться — высоко же!"
  Colony01:
    attributes: Npc_FaronWoods006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "В общем, вон твое плато."
      - control:
          kind: pause
          length: short
      - text: " Но будь\nосторожен — монстров по пути\nхватает."
  GoodBye00:
    attributes: Npc_FaronWoods006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: Удачи!
      - control:
          kind: pause
          length: short
      - text: " В такие времена без нее никак..."
  GoodBye01:
    attributes: Npc_FaronWoods006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: Жаль.
      - control:
          kind: pause
          length: short
      - text: " Могли бы поболтать..."
  Hyrule00:
    attributes: Npc_FaronWoods006
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 0
      - text: В замок Хайрул?!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Там же стражей тьма-тьмущая!
      - control:
          kind: pause
          length: short
      - text: "\nПогибнешь ведь!"
  Hyrule01:
    attributes: Npc_FaronWoods006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Вообще-то вон он, вдалеке."
      - control:
          kind: pause
          length: short
      - text: "\nНо правда, не ходи ты туда..."
  Kakariko00:
    attributes: Npc_FaronWoods006
    contents:
      - control:
