      - text: "Да, хочу"
  "0001":
    attributes: ""
    contents:
      - text: "Нет, не надо"
  "0004":
    attributes: ""
    contents:
      - text: Об ингредиентах
  "0005":
    attributes: ""
    contents:
      - text: О зельях
  "0006":
    attributes: ""
    contents:
      - text: О феях
  "0007":
    attributes: ""
    contents:
      - text: Закончить
  "0008":
    attributes: ""
    contents:
      - text: Кулинарный успех
  "0009":
    attributes: ""
    contents:
      - text: Экстракт монстра
  "0010":
