entries:
  "0000":
    attributes: ""
    contents:
      - text: Так точно!
  "0001":
    attributes: ""
    contents:
      - text: "Нет, спасибо"
  Raimei_CK_001:
    attributes: Npc_oasis025
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 19
      - text: Савоотен!
      - control:
          kind: pause
          length: short
      - text: " "
      - control:
          kind: set_colour
          colour: blue
      - text: Шлем Грома
      - control:
          kind: reset_colour
      - text: "!"
      - control:
          kind: pause
          length: short
      - text: " До сих пор\nлишь ваай из рода предводительницы\nмогли его носить!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Хотя да, ты же спасла город..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Кто же ты такая?
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: Ха-ха!
      - control:
          kind: pause
          length: short
      - text: " Я знаю ответ: ты друг народа\nгерудо!"
      - control:
          kind: pause
          length: short
      - text: " Только другу могли доверить\nнашу главную реликвию."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Если у меня в отряде освободится\nместо, я с радостью тебя возьму!"
  Talk_02:
    attributes: Npc_oasis025
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 6
      - text: Что ты тут делаешь?
      - control:
          kind: pause
          length: short
      - text: " Здесь\nтренируются ваай из личной\nохраны "
      - control:
          kind: set_colour
          colour: blue
      - text: предводительницы
      - control:
          kind: reset_colour
      - text: "!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "У нас и так переполох из-за воров.\nЭто не место для экскурсий!"
      - control:
          kind: pause
          length: short
      - text: " Уходи!"
  Talk_04:
    attributes: Npc_oasis025
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 29
      - text: "Сав'сааба."
      - control:
          kind: pause
          length: longer
  Talk_05:
    attributes: Npc_oasis025
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 28
      - text: Саваак.
      - control:
          kind: pause
          length: longer
  Talk_06:
    attributes: Npc_oasis025
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 27
      - text: Савотта.
      - control:
          kind: pause
          length: longer
  Talk_07:
    attributes: Npc_oasis025
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 0
      - text: Опять ты?
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "У нас и так переполох из-за\nворов."
      - control:
          kind: pause
          length: short
      - text: " Не добавляй поводов\nдля беспокойства!"
  Talk_08:
    attributes: Npc_oasis025
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 0
      - text: Снова ты?!
      - control:
          kind: pause
          length: short
      - text: " Сколько раз тебе\nговорить: не путайся под ногами!"
      - control:
          kind: pause
          length: short
      - text: "\nВон отсюда!"
  Talk_09:
    attributes: Npc_oasis025
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 19
      - text: Что?
      - control:
          kind: pause
          length: short
      - text: " Тебя Белла к нам отправила?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: Приношу свои извинения!
      - control:
          kind: pause
          length: short
      - text: "\nЯ Тики, начальница охраны."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Реликвия нашего народа была\nукрадена бандитами из клана\nИга, поэтому мы все на взводе."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Мы знаем, что логово воров\n"
      - control:
          kind: set_colour
          colour: red
      - text: в Шпатной лощине
      - control:
          kind: reset_colour
      - text: ", но не можем\nв него попасть."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Мои подопечные измождены,\nпоэтому я рада любой помощи."
  Talk_10:
    attributes: Npc_oasis025
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 12
      - text: "Я понимаю, что вернуть реликвию\nмы должны своими силами, но мои\nподопечные измождены."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Мы знаем, что логово клана Ига\nв "
      - control:
          kind: set_colour
          colour: red
      - text: Шпатной лощине
      - control:
          kind: reset_colour
      - text: ", но не можем\nв него попасть."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Я рада твоей помощи.
      - control:
          kind: pause
          length: short
      - text: " Поговори\nсо всеми — они ответят на любые\nтвои вопросы."
  Talk_11:
    attributes: Npc_oasis025
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: "Белла мне обо всем рассказала:\nблагодаря тебе реликвия снова\nу нас!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Теперь "
      - control:
          kind: set_colour
          colour: blue
      - text: предводительнице
      - control:
          kind: reset_colour
      - text: "\nбудет немного спокойнее."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Хотя надо ведь еще с "
      - control:
          kind: set_colour
          colour: blue
      - text: "Чудищем "
      - control:
          kind: reset_colour
      - text: "что-то\nделать..."
      - control:
          kind: pause
          length: short
      - text: " Похоже, до полного мира\nи спокойствия еще далеко."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "И все же я от всего сердца благодарю\nтебя за помощь — саак-саак."
      - control:
          kind: pause
          length: short
      - text: " Мы этого\nникогда не забудем."
  Talk_12:
    attributes: Npc_oasis025
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: А!
      - control:
          kind: pause
          length: short
      - text: " Хайлийская ваай!"
      - control:
          kind: pause
          length: short
      - text: " Мне нужны\nтакие, как ты."
      - control:
          kind: pause
          length: short
      - text: " Хочешь вступить\nв мой отряд?"
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Talk_13:
    attributes: Npc_oasis025
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 9
      - text: Ха-ха!
      - control:
          kind: pause
          length: short
      - text: " Какая решимость!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Но "
      - control:
          kind: set_colour
          colour: blue
      - text: "предводительница "
      - control:
          kind: reset_colour
      - text: "сказала,\nчто ты на очень важном задании."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Приходи, когда все сделаешь,\nи мы обсудим этот вопрос."
  Talk_14:
    attributes: Npc_oasis025
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 12
      - text: "Жаль, жаль..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - control:
          kind: set_colour
          colour: blue
      - text: "Предводительница "
      - control:
          kind: reset_colour
      - text: "сказала, что\nты на очень важном задании."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Приходи, когда все сделаешь."
      - control:
          kind: pause
          length: short
      - text: "\nВозможно, я еще сумею тебя\nпереубедить."
  Talk_16:
    attributes: Npc_oasis025
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Подумать только: хайлийская\nваай, которая сражается лучше\nгерудо..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Хочешь вступить в мой отряд?
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Talk_17:
    attributes: Npc_oasis025
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - control:
          kind: text_size
          percent: 125
      - text: Внимание!
      - control:
          kind: text_size
          percent: 100
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Вот эта хайлийская ваай будет нам\nпомогать."
      - control:
          kind: pause
          length: short
      - text: " Расскажите ей все, что\nзнаете."
  Talk_18:
    attributes: Npc_oasis025
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: Теперь ты одна из нас.
      - control:
          kind: pause
          length: short
      - text: " Спрашивай\nмоих подопечных о чем угодно —\nони ответят на любые вопросы."
  TerrorArrow:
