  RetMSG_Orud_04:
    attributes: Npc_Road_027
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 16
      - text: Уф...
      - control:
          kind: pause
          length: short
      - text: " Отправиться ли мне на запад\nиль двинуть на восток — вот в чем\nвопрос."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "О, совсем забыл."
      - control:
          kind: pause
          length: short
      - text: " Я же совершенно\nне умею ориентироваться!"
      - control:
          kind: pause
          length: short
      - text: " Восток\nили запад — какая разница!"
  RetMSG_Orud_04_1:
    attributes: Npc_Road_027
    contents:
      - control:
