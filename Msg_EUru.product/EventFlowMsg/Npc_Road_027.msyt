    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 16
      - text: Найдет ли Дейто ответ на вопрос?..
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Достану ли я "
      - control:
          kind: raw
          two_hundred_one:
            one_field:
