    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 16
      - text: Уф...
      - control:
          kind: pause
          length: short
      - text: " Взглянуть ли страшному врагу\nв лицо бесстрашно иль драпануть\nтрусливо — вот в чем вопрос."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Нашел ответ!
      - control:
          kind: pause
          length: short
      - text: " Я драпану бесстрашно!"
  RetMSG_Orud_01_1:
    attributes: Npc_Road_027
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 16
      - text: Найдет ли Дейто ответ на вопрос?..
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
