  RetMSG_Orud_02:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 16
      - text: Уф...
      - control:
          kind: pause
          length: short
      - text: " Достану ли я "
      - control:
          kind: raw
