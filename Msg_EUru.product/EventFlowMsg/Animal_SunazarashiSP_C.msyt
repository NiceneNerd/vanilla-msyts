              field_1: 0
      - control:
          kind: sound
          unknown:
            - 7
            - 18
      - text: "Спасибо за мудрый совет, Патриция!"
  talk05:
    attributes: Npc_oasis047
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "«С вершин "
      - control:
          kind: set_colour
          colour: blue
      - text: нагорья Герудо
      - control:
          kind: reset_colour
      - text: "\nвиден путь дракона»."
  talk06:
    attributes: Npc_oasis047
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "«Восьмая героиня стоит\nза "
      - control:
          kind: set_colour
          colour: blue
      - text: Хризобовым полем
      - control:
          kind: reset_colour
      - text: ».
  talk07:
    attributes: Npc_oasis047
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
