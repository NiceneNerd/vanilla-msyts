          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Я нес "
      - control:
          kind: set_colour
          colour: red
      - text: ритуальный трезубец
      - control:
          kind: reset_colour
      - text: "\nДенто, чтобы он его осмотрел\nперед церемонией."
      - control:
