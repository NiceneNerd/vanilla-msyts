    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: "Хе-хе! Стыдно не знать.\nНа "
      - control:
          kind: set_colour
          colour: blue
      - text: озере Дарман
      - control:
          kind: reset_colour
      - text: ", конечно!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Он тут однажды "
      - control:
          kind: set_colour
          colour: red
      - text: "здоровенного\nмонстра"
      - control:
          kind: reset_colour
      - text: " раскрошил."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Я не в курсе, что за монстр-то был,\nно то, что "
      - control:
          kind: set_colour
          colour: red
      - text: здоровенный
      - control:
          kind: reset_colour
      - text: ", можешь не\nсомневаться."
