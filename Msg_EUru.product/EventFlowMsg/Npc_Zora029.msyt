                - 0
                - 0
                - 3
  talk12:
    attributes: Npc_Zora029
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: Пока!
      - control:
          kind: pause
          length: short
      - text: " Мне не терпится\nувидеть, как ты ныряешь!"
  talk13:
    attributes: Npc_Zora029
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 10
      - text: "Сейчас мое любимое место для\nныряния — "
      - control:
          kind: set_colour
          colour: blue
      - text: "утес Отваги "
      - control:
          kind: reset_colour
      - text: "на "
      - control:
          kind: set_colour
          colour: blue
      - text: "Пике\nМолний"
      - control:
          kind: reset_colour
      - text: "!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ах, какую красоту видишь, ныряя\nоттуда в воды "
      - control:
          kind: set_colour
          colour: blue
      - text: Восточной Чаши
      - control:
          kind: reset_colour
      - text: "!.."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Будь осторожен: на "
      - control:
          kind: set_colour
          colour: blue
      - text: "Пике\nМолний "
      - control:
          kind: reset_colour
      - text: "обитает "
      - control:
          kind: set_colour
          colour: blue
      - text: левр
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Встретишься с ним — и считай,\nпара-тройка ребер сломаны..."
  talk14:
    attributes: Npc_Zora029
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 1
      - text: "Я по-прежнему ищу высокие места,\nоткуда можно нырять и наслаждаться\nсвоим отражением в воде..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 11
      - text: "Хорошо, когда умеешь плавать вверх\nпо водопадам — с них нырять лучше\nвсего!"
      - control:
          kind: pause
          length: short
      - text: " Обязательно попробуй."
  talk15:
    attributes: Npc_Zora029
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 10
            - 21
      - text: Ого!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 1
      - text: "Ты, видимо, хайлиец, которого\nтак ждал "
      - control:
          kind: set_colour
          colour: red
      - text: принц Сидон
      - control:
          kind: reset_colour
      - text: "!"
  talk16:
    attributes: Npc_Zora029
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 19
      - text: Меня зовут Грув!
      - control:
          kind: pause
          length: short
      - text: " Я помогал "
      - control:
          kind: set_colour
          colour: red
      - text: "принцу\nСидону"
      - control:
          kind: reset_colour
      - text: " искать такого, как ты."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 7
            - 11
      - text: "Хорошо, что ты к нам пришел!"
      - control:
          kind: pause
          length: short
      - text: "\nПоиски завершились успешно!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Если не считать, что..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 9
            - 12
      - text: "Во время поисков меня забыли\nна высокой башне..."
