          cancel_index: 1
          unknown: 6
  Com_Talk_60:
    attributes: Npc_Kakariko010
    contents:
      - text: "Это обойдется тебе в "
      - control:
          kind: set_colour
          colour: red
      - control:
          kind: variable
          variable_kind: 16
          name: Shop_SelectItemPrice
      - control:
          kind: localisation
          localisation_kind: plural
          options:
            - " рупию"
            - " рупий"
            - " рупии"
      - control:
