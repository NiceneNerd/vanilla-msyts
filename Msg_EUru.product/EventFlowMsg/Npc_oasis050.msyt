          unknown: 6
  talk51:
    attributes: Npc_oasis050
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: Удачи нам обеим!
  talk52:
    attributes: Npc_oasis050
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: Понятно.
  talk53:
    attributes: Npc_oasis050
    contents:
      - control:
          kind: set_colour
          colour: grey
      - control:
          kind: text_size
          percent: 80
      - control:
          kind: sound
          unknown:
            - 0
            - 6
      - text: "Хм, а в прошлый раз иначе сказала..."
      - control:
          kind: text_size
