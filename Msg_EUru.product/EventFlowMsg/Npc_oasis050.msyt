          percent: 100
  talk50:
    attributes: Npc_oasis050
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: А ты милая.
      - control:
          kind: pause
          length: short
      - text: " Тоже ищешь свою любовь?"
      - control:
