      - control:
          kind: reset_colour
  talk56:
    attributes: Npc_oasis050
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 0
      - text: Это где?
      - control:
          kind: pause
          length: short
      - text: " Ну-ка, расскажи подробнее."
  talk57:
    attributes: Npc_oasis050
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 16
      - text: "Спустись, пожалуйста."
      - control:
          kind: pause
          length: short
      - text: "\nЧто за глупые шутки?"
  talk58:
    attributes: Npc_oasis050
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 14
      - text: Некрасиво так себя вести.
      - control:
          kind: pause
          length: short
      - text: "\nСпустись, пожалуйста."
  talk59:
    attributes: Npc_oasis050
