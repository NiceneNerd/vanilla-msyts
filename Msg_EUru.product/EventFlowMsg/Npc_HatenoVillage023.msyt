            - 4
            - 5
            - 6
            - 2
          selected_index: 0
          cancel_index: 3
          unknown: 10
  talk21:
    attributes: Npc_HatenoVillage023
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 12
      - control:
          kind: set_colour
          colour: blue
      - text: Великое Бедствие
      - control:
          kind: reset_colour
      - text: "..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Я сама этого не застала, но в детстве\nкругом видела лишь выжженные поля."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Говорят, Хатено еще не сильно\nзадело: тут быстро все отстроили."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "А вот "
      - control:
          kind: set_colour
          colour: blue
      - text: "посад "
      - control:
          kind: reset_colour
      - text: "по-прежнему в руинах,\nда и "
      - control:
          kind: set_colour
          colour: blue
      - text: замок Хайрул
      - control:
          kind: reset_colour
      - text: " во власти Зла."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ты лучше кого постарше о тех\nвременах расспроси — может,\nпобольше узнаешь."
  talk22:
    attributes: Npc_HatenoVillage023
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 16
      - control:
          kind: set_colour
          colour: blue
      - text: Замок Хайрул
      - control:
          kind: reset_colour
      - text: " находится в самом\nсердце королевства."
      - control:
          kind: pause
          length: short
      - text: " Огромный,\nнеприступный..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Там жил "
      - control:
          kind: pause
          length: short
      - text: "Король Роам со своей\nдочерью, прекрасной принцессой."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Все парни, глядя на замок, мечтали\nо подвигах, о том, как они вернутся\nдомой героями."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 12
      - text: "Но замок, гордость королевства,\nпал под натиском Зла."
      - control:
          kind: pause
          length: short
      - text: " Он и ныне\nв плачевном состоянии..."
  talk23:
    attributes: Npc_HatenoVillage023
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Так назывался город при "
      - control:
          kind: set_colour
          colour: blue
      - text: "замке\nХайрул"
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: pause
          length: short
      - text: " Там и торговля вся шла,\nи свои ученые были."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 6
      - text: "У тебя "
      - control:
          kind: set_colour
          colour: blue
      - text: карта
      - control:
          kind: reset_colour
      - text: " есть?"
      - control:
          kind: pause
          length: short
      - text: " Руины перед\nзамком — это и был "
      - control:
          kind: set_colour
          colour: blue
      - text: "Хайрульский\nпосад"
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Сам можешь представить,\nкакой большой он был."
  talk24:
    attributes: Npc_HatenoVillage023
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 3
      - text: "Хм, это ж "
      - control:
