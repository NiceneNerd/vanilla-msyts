entries:
  "0000":
    attributes: ""
    contents:
      - text: Я встретил ее душу
  "0002":
    attributes: ""
    contents:
      - text: Я ее не забуду
  "0003":
    attributes: ""
    contents:
      - text: Мы вместе навсегда
  talk00:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 11
      - text: Линк!
      - control:
          kind: pause
          frames: 10
      - text: " Я очень благодарен тебе за\nто, что ты спас владения зора!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 1
      - text: Это большая радость для всех нас.
  talk01:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 6
      - text: Линк...
      - control:
          kind: pause
          frames: 20
      - text: " я хочу тебя спросить..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Ты побывал внутри "
      - control:
          kind: set_colour
          colour: blue
      - text: Ва-Руты
      - control:
          kind: reset_colour
      - text: ",\nЧудища воды..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 3
            - 1
      - text: И вернулся...
      - control:
          kind: pause
          frames: 20
      - text: " но без Мифы."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 3
            - 16
      - text: Неужели мои опасения верны?
      - control:
          kind: pause
          frames: 20
      - text: "\nСкажи, мы опоздали?"
      - control:
          kind: single_choice
          label: 0
  talk02:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 6
      - text: Ее душу...
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Мифа...
      - control:
          kind: pause
          frames: 20
      - text: " Она что-нибудь сказала?"
  talk04:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 12
      - text: Ясно.
      - control:
          kind: pause
          frames: 20
      - text: " Мифа, доченька моя,\nя не смог тебе помочь..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 5
            - 1
      - text: "Но как Воин она все же\nисполнила свой долг."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Я не могу не поддержать\nее стремление."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "..."
      - control:
          kind: pause
          frames: 22
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: Линк...
      - control:
          kind: pause
          frames: 20
      - text: " она тебя любила.\nИ ты это знаешь."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 1
      - text: "Уверен, она рада тебе помогать."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 6
      - text: "Обещай, что всегда будешь\nпомнить ее. Обещай мне это."
      - control:
          kind: choice
          choice_labels:
            - 2
            - 3
          selected_index: 0
          cancel_index: 2
          unknown: 6
  talk05:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: "Твои слова очень важны для\nменя!"
      - control:
          kind: pause
          frames: 20
      - text: " У тебя доброе сердце."
  talk06:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: Спасибо за такие слова!
      - control:
          kind: pause
          frames: 20
      - text: "\nОни много для меня значат."
  talk07:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 4
      - text: "Линк, если тебе когда-нибудь\nпонадобится помощь, приходи."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 1
      - text: "Мой народ всегда с радостью\nпримет тебя — что бы ни случилось."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Спасибо тебе, Линк."
      - control:
          kind: pause
          frames: 20
      - text: " Спасибо."
  talk08:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 11
      - text: "Благодарю, Линк."
      - control:
          kind: pause
          frames: 20
      - text: " Ты всех нас спас."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Уверен, Мифа счастлива\nбыть рядом с тобой."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Я буду следить за вашим боем.
