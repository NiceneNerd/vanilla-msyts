          choice_labels:
            - 4
            - 3
          selected_index: 0
          cancel_index: 1
          unknown: 6
  talk14:
    attributes: Npc_SmallOasis002
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Это деревушка далеко на\nсевере, в "
      - control:
