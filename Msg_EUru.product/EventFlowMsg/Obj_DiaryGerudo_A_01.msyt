  talk00:
    attributes: Obj_DiaryGerudo_A_01
    contents:
      - text: "В этом дневнике я буду записывать\nвсе, что узнаю об "
      - control:
          kind: set_colour
          colour: red
      - text: Испытании Героя
      - control:
          kind: reset_colour
