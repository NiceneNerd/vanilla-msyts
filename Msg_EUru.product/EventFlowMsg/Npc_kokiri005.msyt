entries:
  "0000":
    attributes: ""
    contents:
      - text: Расскажи про лес
  "0001":
    attributes: ""
    contents:
      - text: Меч...
  "0002":
    attributes: ""
    contents:
      - text: Хочу уйти из леса
  "0003":
    attributes: ""
    contents:
      - text: Пока
  Near01:
    attributes: Npc_kokiri005
    contents:
      - control:
          kind: sound2
          unknown:
            - 1
            - 205
      - text: Мака тебе поможет!
  Npc_kokiri005_After_Demo_Talk000:
    attributes: Npc_kokiri005
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 0
      - text: "Добро пожаловать, дорогой\nгерой!"
      - control:
          kind: pause
          length: short
      - text: " Мака рад тебя видеть."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Я могу рассказать тебе о нашем лесе!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Или ты пришел поговорить с "
      - control:
          kind: set_colour
          colour: blue
      - text: "Древом\nДеку"
      - control:
          kind: reset_colour
      - text: "?"
      - control:
          kind: pause
          length: short
      - text: " Тогда иди по корню дальше!"
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
            - 2
            - 3
          selected_index: 0
          cancel_index: 3
          unknown: 10
  Npc_kokiri005_After_Demo_Talk001:
    attributes: Npc_kokiri005
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 0
      - text: "Мака может тебе еще\nчем-нибудь помочь?"
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
            - 2
            - 3
          selected_index: 0
          cancel_index: 3
          unknown: 10
  Npc_kokiri005_After_Demo_Talk010:
    attributes: Npc_kokiri005
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: "Про "
      - control:
          kind: set_colour
          colour: blue
      - text: Лес Короков
      - control:
          kind: reset_colour
      - text: "?"
      - control:
          kind: pause
          length: short
      - text: " Мака расскажет!"
      - control:
          kind: pause
          length: short
      - text: "\nГлавные местные диковинки —\nДрево Деку и пьедестал с мечом!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 9
            - 1
      - control:
          kind: set_colour
          colour: grey
      - control:
          kind: text_size
          percent: 80
      - text: "Ох, не влетело бы мне за «диковинки»..."
      - control:
          kind: text_size
          percent: 100
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: reset_colour
      - control:
          kind: sound
          unknown:
            - 6
            - 1
      - text: "Внутри "
      - control:
          kind: set_colour
          colour: blue
      - text: Древа Деку
      - control:
          kind: reset_colour
      - text: " тебя ждет постель\nи прочие полезности, дорогой герой!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "А "
      - control:
          kind: set_colour
          colour: blue
      - text: пьедестал меча
      - control:
          kind: reset_colour
      - text: ", о-о-о!"
      - control:
          kind: pause
          length: short
      - text: " В нем торчит\nсам легендарный "
      - control:
          kind: set_colour
          colour: red
      - text: Сокрушитель Зла
      - control:
          kind: reset_colour
      - text: "!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 18
      - text: "А вообще, "
      - control:
          kind: set_colour
          colour: blue
      - text: пьедестал меча
      - control:
          kind: reset_colour
      - text: " отмечает\nсамый что ни на есть центр леса..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Если встать на него и осмотреться,\nпоймешь, как идут дороги в лесу!"
      - control:
          kind: pause
          length: short
      - text: "\nБобовые лампы — хорошие ориентиры."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: "В общем, дорогой герой, если\nзаблудишься — возвращайся\nк пьедесталу!"
  Npc_kokiri005_After_Demo_Talk020:
    attributes: Npc_kokiri005
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: "А, ты про "
      - control:
          kind: set_colour
          colour: red
      - text: Сокрушитель Зла
      - control:
          kind: reset_colour
      - text: "!"
      - control:
          kind: pause
          length: short
      - text: " Только\nты можешь носить этот меч."
      - control:
          kind: pause
          length: short
      - text: " Рад, что\nнаконец нашел его?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "О-о-о, как мы старались, чтобы никто\nдругой до него не добрался!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 1
      - text: Это мы Затерянные леса придумали!
      - control:
          kind: pause
          length: short
      - text: "\nТы там совсем заблудился, наверное?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 10
            - 6
      - text: Хотя стоп!
      - control:
          kind: pause
          length: short
      - text: " Ты же здесь, значит,\nне заблудился."
      - control:
          kind: pause
          length: short
      - text: " Я несу чушь!"
  Npc_kokiri005_After_Demo_Talk030:
    attributes: Npc_kokiri005
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 4
      - text: Дорогой герой хочет уйти?
      - control:
          kind: pause
          length: short
      - text: "\nМы подозревали, что такое\nможет случиться..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 7
            - 1
      - text: "И да, ДА!"
      - control:
          kind: pause
          length: short
      - text: " Даже к этому\nмы подготовились!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 6
            - 1
      - text: "Иди на восток от "
      - control:
          kind: set_colour
          colour: red
      - text: пьедестала меча
      - control:
          kind: reset_colour
      - text: ",\nпока не увидишь указатель."
      - control:
          kind: pause
          length: short
      - text: " Там возьми\nвправо и иди к "
      - control:
          kind: set_colour
          colour: red
      - text: призрачному дереву
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 6
            - 1
      - text: "Мы устроили так, что это дерево,\nесли заберешься в него..."
      - control:
          kind: pause
          length: long
      - text: " ПЫЩЬ!.."
      - control:
          kind: pause
          length: long
      - text: "\nПеренесет тебя за пределы леса!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 7
            - 9
      - text: "Удобней ведь, чем снова\nчерез "
      - control:
          kind: set_colour
          colour: blue
      - text: Затерянные леса
      - control:
          kind: reset_colour
      - text: " топать?"
  Npc_kokiri005_After_Demo_Talk040:
    attributes: Npc_kokiri005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Обращайся, если что непонятно\nбудет в лесу."
      - control:
          kind: pause
          length: short
      - text: " Мака тебе поможет!"
  Npc_kokiri005_GotMSword_Talk000:
    attributes: Npc_kokiri005
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: О!
      - control:
          kind: pause
          length: short
      - text: " О-о-о!"
      - control:
          kind: pause
          length: short
      - text: " Дорогой герой, Мака видел,\nкак ты вытащил  "
      - control:
          kind: set_colour
          colour: red
      - text: Сокрушитель Зла
      - control:
          kind: reset_colour
      - text: "!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 6
            - 0
      - text: Мака может рассказать тебе о нем!
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
            - 2
            - 3
          selected_index: 0
          cancel_index: 3
          unknown: 10
  Npc_kokiri005_GotMSword_Talk010:
    attributes: Npc_kokiri005
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 6
      - text: "Ты про "
      - control:
          kind: set_colour
          colour: blue
      - text: Сокрушитель Зла
      - control:
          kind: reset_colour
      - text: "?"
      - control:
          kind: pause
          length: short
      - text: " Он еще как\n"
      - control:
          kind: set_colour
          colour: blue
      - text: "Высший Меч "
      - control:
          kind: reset_colour
      - text: известен.
      - control:
          kind: pause
          length: short
      - text: " Рассказать?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "О-о-о, "
      - control:
          kind: set_colour
          colour: blue
      - text: Высший Меч
      - control:
          kind: reset_colour
      - text: " очень особенный:\nчем больше его использовать,\nтем больше он будет уставать."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Но даже когда он совсем лишится сил,\nбез паники, дорогой герой!"
      - control:
          kind: pause
          length: short
      - text: " Просто\nподожди, пока он отдохнет."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Возможно, он копит силы для какой-то\nрешающей битвы?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 7
            - 9
      - text: Спроси Древо Деку.
      - control:
          kind: pause
          length: short
      - text: " Поговори\nс ним, дорогой герой!"
  Npc_kokiri005_Talk000:
    attributes: Npc_kokiri005
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 0
      - text: Наконец-то!
      - control:
          kind: pause
          length: short
      - text: " Герой!"
      - control:
          kind: pause
          length: short
      - text: "\nЯ давно тебя жду!"
  Npc_kokiri005_Talk010:
    attributes: Npc_kokiri005
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: "Давай же, бери скорее "
      - control:
          kind: set_colour
          colour: red
      - text: меч
      - control:
          kind: reset_colour
      - text: "!"
