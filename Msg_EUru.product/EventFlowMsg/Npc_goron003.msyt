  talk10:
    attributes: Npc_goron003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "С тех пор как на "
      - control:
          kind: set_colour
          colour: blue
      - text: Горе Смерти
      - control:
          kind: reset_colour
      - text: "\nпоявилось "
      - control:
          kind: set_colour
          colour: blue
      - text: Чудище Ва-Рудания
      - control:
          kind: reset_colour
      - text: ",\nу нас стало намного жарче."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Поэтому мы выставили на продажу\n"
      - control:
          kind: set_colour
          colour: red
      - text: пожарные доспехи
      - control:
          kind: reset_colour
      - text: " — для таких\nтуристов, как ты."
  talk12:
    attributes: Npc_goron003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Еще совсем недавно я был шахтером\nи работал там, на самой "
      - control:
          kind: set_colour
          colour: blue
      - text: Горе Смерти
      - control:
          kind: reset_colour
      - text: "..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Когда появилось "
      - control:
          kind: set_colour
          colour: blue
      - text: Чудище Ва-Рудания
      - control:
          kind: reset_colour
      - text: ",\nвсе работы там остановили, персонал\nсократили..."
  talk13:
    attributes: Npc_goron003
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "Эх, раньше все было намного лучше..."
  talk14:
    attributes: Npc_goron003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Хе-хе, теперь Рудания уже не\nбуянит."
      - control:
          kind: pause
          length: short
      - text: " Добуянился, так сказать."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 0
      - text: "На "
      - control:
          kind: set_colour
          colour: blue
      - text: вершине Горы Смерти
      - control:
          kind: reset_colour
      - text: " снова\nбезопасно!"
      - control:
          kind: pause
          length: short
      - text: " Надо будет еще туда\nкак-нибудь сходить..."
  talk15:
    attributes: Npc_goron003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Из-за него туда теперь не сходишь.
      - control:
          kind: pause
          length: short
      - text: "\nКаждый день "
      - control:
          kind: set_colour
          colour: blue
      - text: "шеф "
      - control:
          kind: reset_colour
      - text: "обстреливает его\nиз "
      - control:
          kind: set_colour
          colour: red
      - text: пушек
      - control:
          kind: reset_colour
      - text: ", чтобы загнать в кратер..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Но ящер всегда возвращается.
  talk16:
    attributes: Npc_goron003
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 0
      - control:
          kind: set_colour
          colour: blue
      - text: Шеф
      - control:
          kind: reset_colour
      - text: " — это мэр Горонграда."
      - control:
          kind: pause
          length: short
      - text: "\nОчень-очень важный горон."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Он живет в доме с очень-очень\nбольшой табличкой."
      - control:
          kind: pause
          length: short
      - text: " Обязательно\nпоздоровайся с ним."
  talk17:
    attributes: Npc_goron003
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 0
      - text: Недавно я туда поднимался.
      - control:
          kind: pause
          length: short
      - text: " Шеф\nс Юнобо отделали "
      - control:
          kind: set_colour
          colour: blue
      - text: "Руданию "
      - control:
          kind: reset_colour
      - text: "как\nследует — больше он не буянит."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 0
      - text: Ему досталось по заслугам!
      - control:
          kind: pause
          length: short
      - text: " Я ведь\nименно из-за него потерял работу!"
  talk18:
    attributes: Npc_goron003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: "Привет, "
      - control:
