  talk20:
    attributes: Npc_HighMountain013
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 2
      - text: Привет.
      - control:
          kind: pause
          length: short
      - text: " Мы как раз говорили\nоб "
      - control:
          kind: set_colour
          colour: blue
      - text: огромной птице
      - control:
          kind: reset_colour
