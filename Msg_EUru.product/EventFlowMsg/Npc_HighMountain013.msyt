      - control:
          kind: reset_colour
      - text: "."
  talk18:
    attributes: Npc_HighMountain013
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 17
      - text: "Даже не знаю, что тебе сказать..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: Из-за
      - control:
          kind: set_colour
          colour: blue
      - text: " огромной птицы"
      - control:
          kind: reset_colour
      - text: " к нам сейчас\nмало кто приходит, поэтому товар\nрасходится плохо."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
