    attributes: Npc_HighMountain013
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 16
      - text: Это в деревне.
      - control:
          kind: pause
          length: short
      - text: " А вот если пойдешь\nв "
      - control:
          kind: set_colour
          colour: blue
      - text: Эберские горы
      - control:
