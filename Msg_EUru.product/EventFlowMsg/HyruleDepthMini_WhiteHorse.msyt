              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Они подходят только\nэтой "
      - control:
          kind: set_colour
          colour: red
      - text: белой лошади
      - control:
          kind: reset_colour
      - text: "!"
  talk22:
    attributes: Npc_HyruleDepthHatago007
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 21
      - text: О!
      - control:
          kind: pause
          length: short
      - text: " Это же "
      - control:
          kind: set_colour
          colour: red
      - text: белая лошадь
      - control:
          kind: reset_colour
      - text: "!"
      - control:
