  talk00:
    attributes: Npc_HyruleDepthHatago007
    contents:
      - text: "Мне мой дед рассказывал, что он\nвидел на "
      - control:
          kind: set_colour
          colour: blue
      - text: "Сарском холме "
      - control:
          kind: set_colour
          colour: red
      - text: "белую\nлошадь "
      - control:
          kind: set_colour
          colour: blue
      - control:
          kind: reset_colour
      - text: королевской семьи.
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Правда, это давно было."
  talk01:
    attributes: Npc_HyruleDepthHatago007
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 0
      - text: О!
      - control:
          kind: pause
          length: short
      - text: " Это же "
      - control:
          kind: set_colour
          colour: red
      - text: белая лошадь
      - control:
          kind: reset_colour
      - text: "!"
  talk02:
    attributes: Npc_HyruleDepthHatago007
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 16
      - text: "А недавно ко мне приходил кое-кто,\nкто тоже видел "
      - control:
          kind: set_colour
          colour: red
      - text: "белую лошадь "
      - control:
          kind: reset_colour
      - text: "на\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Сарском холме
      - control:
          kind: reset_colour
      - text: "..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Может, она родственница той "
      - control:
          kind: set_colour
          colour: red
      - text: "белой\nлошади"
      - control:
          kind: reset_colour
      - text: " королевской семьи, о которой\nмне рассказывал дед?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Если ты найдешь "
      - control:
          kind: set_colour
          colour: red
      - text: белую лошадь
      - control:
          kind: reset_colour
      - text: ",\nможешь привести ее ко мне?"
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
          selected_index: 0
          cancel_index: 1
          unknown: 6
  talk03:
    attributes: Npc_HyruleDepthHatago007
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 20
      - text: "Но обязательно "
      - control:
          kind: set_colour
          colour: blue
      - text: "зарегистрируй "
      - control:
          kind: reset_colour
      - text: "ее\nв конюшне, прежде чем приводить\nко мне!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Говорят, у "
      - control:
          kind: set_colour
          colour: red
      - text: белой лошади
      - control:
          kind: reset_colour
      - text: " на "
      - control:
          kind: set_colour
          colour: blue
      - text: "Сарском\nхолме "
      - control:
          kind: reset_colour
      - text: нет ни единого пятнышка.
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Я слышал, что ее очень\nтрудно поймать."
      - control:
          kind: pause
          length: short
      - text: " Удачи!"
  talk04:
    attributes: Npc_HyruleDepthHatago007
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Путешественник, ты случайно\nне встречал "
      - control:
          kind: set_colour
          colour: red
      - text: белую лошадь
      - control:
          kind: reset_colour
      - text: "?"
  talk05:
    attributes: Npc_HyruleDepthHatago007
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 0
      - text: "Ты все-таки решил\nвыполнить мою просьбу?"
  talk09:
    attributes: Npc_HyruleDepthHatago007
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 12
      - text: Извини за беспокойство.
      - control:
          kind: pause
          length: short
      - text: "\nДай знать, если передумаешь."
  talk10:
    attributes: Npc_HyruleDepthHatago007
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Говорят, что у той"
      - control:
          kind: set_colour
          colour: red
      - text: " белой лошади"
      - control:
          kind: reset_colour
      - text: "\nна "
      - control:
          kind: set_colour
          colour: blue
      - text: Сарском холме
      - control:
          kind: reset_colour
      - text: " нет ни единого\nпятнышка."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "А еще она не пасется\nс другими лошадьми."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Если поймаешь ее, "
      - control:
          kind: set_colour
          colour: blue
      - text: зарегистрируй
      - control:
          kind: reset_colour
      - text: "\nв конюшне, прежде чем приводить\nко мне!"
  talk12:
    attributes: Npc_HyruleDepthHatago007
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 0
      - text: Надо же!
      - control:
          kind: pause
          length: short
      - text: " Ты привел "
      - control:
          kind: set_colour
          colour: red
      - text: "белую\nлошадь "
      - control:
          kind: reset_colour
      - text: королевской семьи!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 1
      - text: "Я так рад, что я дожил\nдо этого момента!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "У меня для тебя сюрприз: королевское\nседло и узда."
      - control:
          kind: pause
          length: short
      - text: " Я получил их от своего\nдеда!"
  talk13:
    attributes: Npc_HyruleDepthHatago007
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 0
      - text: "Ты поймал "
      - control:
          kind: set_colour
          colour: red
      - text: белую лошадь
      - control:
          kind: reset_colour
      - text: "?!"
      - control:
          kind: pause
          length: short
      - text: " Тогда\n"
      - control:
          kind: set_colour
          colour: blue
      - text: "зарегистрируй "
      - control:
          kind: reset_colour
      - text: "ее в конюшне и скорее\nвозвращайся!"
  talk16:
    attributes: Npc_HyruleDepthHatago007
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 1
      - control:
          kind: set_colour
          colour: red
      - text: Белая лошадь
      - control:
          kind: reset_colour
      - text: ", на которой\nездила "
      - control:
          kind: set_colour
          colour: blue
      - text: "принцесса "
      - control:
          kind: reset_colour
      - text: Зельда.
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Дед так часто мне рассказывал об\nэтой "
      - control:
          kind: set_colour
          colour: red
      - text: лошади
      - control:
          kind: reset_colour
      - text: ", когда я был маленький!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 1
      - text: "Говорят, когда люди видели принцессу\nверхом на "
      - control:
          kind: set_colour
          colour: red
      - text: белой лошади
      - control:
          kind: reset_colour
      - text: ", они были\nпоражены ее неземной красотой."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: "Я не сомневаюсь, что эта "
      - control:
          kind: set_colour
          colour: red
      - text: лошадь
      - control:
          kind: reset_colour
      - text: " —\nвнучка той лошади, про которую\nмне рассказывал мой дед."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Какая красавица!
  talk17:
    attributes: Npc_HyruleDepthHatago007
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ты можешь "
      - control:
          kind: set_colour
          colour: blue
      - text: зарегистрировать
      - control:
          kind: reset_colour
      - text: "\nлошадь прямо в этой конюшне!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Приведи "
      - control:
