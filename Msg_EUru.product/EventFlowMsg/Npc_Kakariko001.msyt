  Anataha00:
    attributes: Npc_Kakariko001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 20
      - text: Что?
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Так смотришь, будто впервые\nменя видишь."
      - control:
          kind: pause
          length: short
      - text: " Этот взгляд..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Это я, Импа..."
      - control:
          kind: pause
          length: short
      - text: " Ну хоть\nимя-то ты помнишь?"
      - control:
          kind: pause
          length: longer
  Anataha01:
    attributes: Npc_Kakariko001
    contents:
      - control:
          kind: sound
