  talk14:
    attributes: Npc_Zora032
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 1
      - control:
          kind: text_size
          percent: 80
      - text: "Неужели он и есть хайлиец, которого\nпригласил принц Сидон?"
      - control:
          kind: pause
          length: short
      - text: " Невероятно."
      - control:
          kind: pause
          length: short
      - text: "\nСпрошу его..."
      - control:
