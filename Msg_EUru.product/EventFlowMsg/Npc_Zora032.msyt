  talk00:
    attributes: Npc_Zora032
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 16
      - text: "Здравствуй, путешественник."
      - control:
          kind: pause
          length: short
      - text: " Позволь\nзадать тебе вопрос: как звучит пароль\nотряда Базза?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Белые облака!
      - control:
          kind: pause
          length: longer
      - text: " Лазурное..."
      - control:
          kind: single_choice
          label: 2
  talk01:
    attributes: Npc_Zora032
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 11
      - text: Правильно!
      - control:
          kind: pause
          length: short
      - text: " Значит, это\nдействительно ты, Линк!"
      - control:
          kind: pause
          length: short
      - text: "\nТот самый!"
      - control:
          kind: choice
          choice_labels:
            - 3
            - 4
          selected_index: 0
          cancel_index: 2
          unknown: 6
  talk02:
    attributes: Npc_Zora032
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 9
      - text: "Я сразу тебя узнал, но решил\nпроверить!"
      - control:
          kind: pause
          length: short
      - text: " Только члены моего\nотряда знают пароль!"
  talk03:
    attributes: Npc_Zora032
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 9
      - text: Не заливай!
      - control:
          kind: pause
          length: short
      - text: " Только члены\nмоего отряда знают пароль!"
  talk04:
    attributes: Npc_Zora032
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "Слушай, а ты совсем не изменился..."
      - control:
          kind: pause
          length: longer
      - text: "\nГде же тебя носило целых сто лет?"
      - control:
          kind: choice
          choice_labels:
            - 5
            - 6
          selected_index: 0
          cancel_index: 2
          unknown: 6
  talk05:
    attributes: Npc_Zora032
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 20
      - text: Не может быть!
      - control:
          kind: pause
          length: short
      - text: " Так\nдолго никто не спит!"
  talk06:
    attributes: Npc_Zora032
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 16
      - text: Долгая история?
      - control:
          kind: pause
          length: short
      - text: " Понимаю."
      - control:
          kind: pause
          length: short
      - text: " За это\nвремя много всего произошло..."
  talk07:
    attributes: Npc_Zora032
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: Ну ладно.
      - control:
