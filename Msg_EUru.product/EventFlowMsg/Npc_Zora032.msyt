  talk31:
    attributes: Npc_Zora032
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 11
      - text: "Как же хорошо, что дожди\nнаконец прекратились!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
