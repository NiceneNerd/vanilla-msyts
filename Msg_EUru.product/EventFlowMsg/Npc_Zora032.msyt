  talk30:
    attributes: Npc_Zora032
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 1
      - text: "Благодаря тем тренировкам\nя стал начальником стражи!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
