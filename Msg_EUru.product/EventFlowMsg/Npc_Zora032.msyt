  talk16:
    attributes: Npc_Zora032
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: Кстати!
      - control:
          kind: pause
          length: short
      - text: " Папу моего помнишь?"
      - control:
          kind: pause
          length: short
      - text: "\nНу Сеггина, Генерала Смерть?"
      - control:
          kind: pause
          length: short
      - text: "\nУ него все в порядке!"
  talk17:
    attributes: Npc_Zora032
