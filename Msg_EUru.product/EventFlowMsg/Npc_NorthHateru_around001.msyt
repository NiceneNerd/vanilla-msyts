entries:
  "0000":
    attributes: ""
    contents:
      - text: Ты кто?
  NHateru_around_Near:
    attributes: Npc_NorthHateru_around001
    contents:
      - text: Хм...
  NHateru_around_Talk001:
    attributes: Npc_NorthHateru_around001
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 20
      - text: Ого!
      - control:
          kind: pause
          length: short
      - text: " Нечасто здесь\nкого-нибудь встретишь!"
      - control:
          kind: single_choice
          label: 0
  NHateru_around_Talk003:
    attributes: Npc_NorthHateru_around001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: Кто я?
      - control:
          kind: pause
          length: short
      - text: " Путешественница."
      - control:
          kind: pause
          length: short
      - text: " И в этом\nотношении я ничем не отличаюсь\nот тебя."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 5
            - 1
      - text: "Сюда редко заходят обычные\nлюди: слишком много монстров,\nпоэтому опасно."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: "Зато можно найти какое-нибудь\nсокровище — ради этого можно\nи жизнью рискнуть!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Обычно я забираюсь "
      - control:
          kind: set_colour
          colour: red
      - text: повыше
      - control:
          kind: reset_colour
      - text: "\nи осматриваюсь в поисках\nподозрительных мест."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Ты тоже попробуй.
      - control:
          kind: pause
          length: short
      - text: " Видишь скалу?"
      - control:
          kind: pause
          length: short
      - text: "\nНаверняка с нее можно увидеть\nчто-нибудь интересное."
  NHateru_around_Talk006:
    attributes: Npc_NorthHateru_around001
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 11
      - text: "У меня идея: в честь нашей встречи\nможешь брать все съестное, что\nнайдешь!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: Казан тоже в твоем распоряжении.
      - control:
          kind: pause
          length: short
      - text: "\nЯ не против, если ты приготовишь\nпоесть."
  NHateru_around_Talk007:
    attributes: Npc_NorthHateru_around001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Если ищешь что-нибудь, советую\nзабраться "
      - control:
          kind: set_colour
          colour: red
      - text: "повыше "
      - control:
          kind: reset_colour
      - text: и осмотреться.
