            - 0
            - 1
            - 2
          selected_index: 0
          cancel_index: 2
          unknown: 8
  talk09:
    attributes: Npc_HighMountain025
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: Заходи.
      - control:
          kind: pause
          length: short
      - text: " Чувствуй себя как дома."
  talk13:
    attributes: Npc_HighMountain025
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 9
      - text: У меня перерыв.
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "Из-за этого "
      - control:
          kind: set_colour
          colour: blue
      - text: Чудища над деревней
      - control:
          kind: reset_colour
      - text: "\nпросто крылья опускаются..."
  talk14:
    attributes: Npc_HighMountain025
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 2
      - text: Пора работать.
      - control:
          kind: pause
          length: short
      - text: " Советую заглянуть\nв нашу гостиницу."
      - control:
          kind: pause
          length: short
      - text: " Она называется\n"
      - control:
          kind: set_colour
          colour: blue
      - text: «Гнездо ласточки»
      - control:
          kind: reset_colour
      - text: "."
  talk15:
    attributes: Npc_HighMountain025
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 19
      - text: "Привет, путешественник."
  talk16:
    attributes: Npc_HighMountain025
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Мы как раз говорили о "
      - control:
          kind: set_colour
          colour: blue
      - text: "птице "
      - control:
          kind: reset_colour
