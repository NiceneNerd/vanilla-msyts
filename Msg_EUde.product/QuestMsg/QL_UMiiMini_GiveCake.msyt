entries:
  QL_UMiiMini_GiveCake_Desc:
    contents:
      - text: "Zufällig hörst du einen Streit zwischen einem\nEhepaar, Hagi und Ruli, mit an. Ruli ist mit\nihrem Latein am Ende, da ihre Tochter Hannie\naußer Kuchen nichts isst.\nSie sucht nach einem Kuchen, der ihre Tochter\nheilt, selbst wenn sie dafür ihre eigene Seele\nan ein "
      - control:
          kind: set_colour
          colour: red
      - text: Monster
      - control:
          kind: reset_colour
      - text: " verkaufen muss.\n\nDoch wo kannst du nur einen solchen "
      - control:
          kind: set_colour
          colour: red
      - text: Kuchen
      - control:
          kind: reset_colour
      - text: "\nfinden?"
  QL_UMiiMini_GiveCake_Finish:
    contents:
      - text: "Du hast Ruli einen "
      - control:
          kind: raw
          two_hundred_one:
            dynamic:
              - 0
              - len: 4
                field_2:
                  - 1
                  - 0
                  - 0
                  - 0
      - text: "Monsterkuchen gegeben!\nDafür wurdest du großzügig entlohnt!\n\nRulis Tochter Hannie scheint sich vollständig\nerholt zu haben. Es geht ihr jetzt schon wieder\nso gut, dass sie draußen spielt."
  QL_UMiiMini_GiveCake_Give:
    contents:
      - text: "Du hast Ruli einen "
      - control:
          kind: raw
          two_hundred_one:
            dynamic:
              - 0
              - len: 4
                field_2:
                  - 1
                  - 0
                  - 0
                  - 0
      - text: "Monsterkuchen gegeben!\n\nIhre Tochter Hannie, die außer Kuchen nichts\nisst, hat den "
      - control:
          kind: raw
          two_hundred_one:
            dynamic:
              - 0
              - len: 4
                field_2:
                  - 1
                  - 0
                  - 0
                  - 0
      - text: "Monsterkuchen bereits verputzt.\n\nSprich mit "
      - control:
          kind: set_colour
          colour: red
      - text: Ruli
      - control:
          kind: reset_colour
      - text: ", um herauszufinden, wie es\nHannie geht!"
  QL_UMiiMini_GiveCake_Name:
    contents:
      - text: Das kleine Kuchenmonster
