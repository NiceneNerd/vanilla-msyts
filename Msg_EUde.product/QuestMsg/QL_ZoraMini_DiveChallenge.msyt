group_count: 3
atr1_unknown: 0
entries:
  QL_ZoraMini_DiveChallenge_Desc:
    contents:
      - text: "Rytt hat dich dazu aufgefordert, von hier aus\nin den See unter euch zu springen.\n\nEr erkennt deinen Sprung allerdings nur dann\nan, wenn du "
      - control:
          kind: set_colour
          colour: red
      - text: mit dem Kopf
      - control:
          kind: reset_colour
      - text: ", nicht den Füßen,\nzuerst eintauchst."
  QL_ZoraMini_DiveChallenge_Finish:
    contents:
      - text: "Du bist erfolgreich gesprungen und den\nWasserfall hinaufgeschwommen. Rytt ist\nzutiefst bewegt."
  QL_ZoraMini_DiveChallenge_Name:
    contents:
      - text: Der Kopfsprung
  QL_ZoraMini_DiveChallenge_Step1:
    contents:
      - text: "Du hast einen Kopfsprung in den See gemacht\nund Rytt hat ihn anerkannt.\n\nNun solltest du versuchen, den "
      - control:
          kind: set_colour
          colour: red
      - text: "Wasserfall\nhinaufzuschwimmen"
      - control:
          kind: reset_colour
      - text: "."
  QL_ZoraMini_DiveChallenge_Step2:
    contents:
      - text: "Du bist erfolgreich den Wasserfall aus dem See,\nin den er mündet, hinaufgeschwommen.\n\nFrage "
      - control:
          kind: set_colour
          colour: red
      - text: Rytt
      - control:
          kind: reset_colour
      - text: ", was er davon hält."
