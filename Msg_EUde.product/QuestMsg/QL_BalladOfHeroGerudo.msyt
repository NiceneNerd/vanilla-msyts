entries:
  QL_BalladOfHeroGerudo_Desc:
    contents:
      - text: "Der Zerstörer führt dich zu einem Monument,\ndas dir den Weg zu Prüfungen weist. Zwischen\nKashiwas Lied und diesen Prüfungen scheint\neine Verbindung zu bestehen:\n„Es gilt, "
      - control:
          kind: set_colour
          colour: red
      - text: "die grimme Herrscherin des\nSandes vom Thron zu fegen"
      - control:
          kind: reset_colour
      - text: "...\nEs gilt, "
      - control:
          kind: set_colour
          colour: red
      - text: "sich mit treuem Reittier durch die\nLichtringe zu bewegen"
      - control:
          kind: reset_colour
      - text: "...\nEs gilt, "
      - control:
          kind: set_colour
          colour: red
      - text: "der Wüstenstadt Schatz zum\ngroßen Schlund zu tragen"
      - control:
          kind: reset_colour
      - text: "...“\nNoch zu bestehende Prüfungen: "
      - control:
          kind: set_colour
          colour: red
      - text: "3"
      - control:
          kind: reset_colour
  QL_BalladOfHeroGerudo_Finish:
    contents:
      - text: "Du hast die Illusion von Ganons Donnerfluch\nbesiegt, die aus deinen eigenen Erinnerungen\nentstanden war.\n\nKashiwas Lied, "
      - control:
          kind: set_colour
          colour: red
      - text: Urbosas Ballade
      - control:
          kind: reset_colour
      - text: ", ist so\nbewegend, dass du dich in die Ereignisse von\nvor 100 Jahren zurückversetzt fühlst.\n\nDas Bestehen der Prüfung hat deine\nheilige Kraft und damit auch "
      - control:
          kind: raw
          two_hundred_one:
            dynamic:
              - 0
              - len: 4
                field_2:
                  - 1
                  - 4
                  - 4
                  - 0
      - text: "Urbosas Zorn\ngestärkt."
  QL_BalladOfHeroGerudo_Name:
    contents:
      - text: "EX: Die Ballade von Urbosa"
