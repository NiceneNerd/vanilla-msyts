      - text: antike Materialien
      - control:
          kind: reset_colour
      - text: "?\nUnd wer ist dieser "
      - control:
          kind: set_colour
          colour: red
      - text: Robelo
      - control:
          kind: reset_colour
      - text: "? Frage Purah nach\nden Details."
