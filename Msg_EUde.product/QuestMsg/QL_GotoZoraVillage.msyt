entries:
  QL_GotoZoraVillage_Desc:
    contents:
      - text: "Der Prinz des Stammes der Zoras, Sidon,\n"
      - control:
          kind: set_colour
          colour: red
      - text: erwartet dich im Dorf der Zoras
      - control:
          kind: reset_colour
      - text: ".\n\nDie Felsen auf dem Weg zum Dorf sind\nallerdings durch den starken Regen sehr\nrutschig geworden."
  QL_GotoZoraVillage_Finish:
    contents:
      - text: "Du hast den Thronsaal im Dorf der Zoras\nerreicht.\n\nSidon zufolge befindet sich das Dorf der Zoras\nin Gefahr und braucht die Hilfe eines fähigen\nHylianers."
  QL_GotoZoraVillage_Name:
    contents:
      - text: Das Dorf der Zoras
  QL_GotoZoraVillage_Step1:
    contents:
      - text: "Du bist im Dorf der Zoras angekommen.\n\nKönig Dorephan befindet sich im Thronsaal.\n"
      - control:
          kind: set_colour
          colour: red
      - text: Begib dich zum Thronsaal
      - control:
          kind: reset_colour
      - text: " im oberen Stockwerk."
