group_count: 3
atr1_unknown: 0
entries:
  QL_TreasureHunt02_Desc:
    contents:
      - text: "Es soll einen Gegenstand geben, der einem\nerlaubt, sich an den Ort zu teleportieren, an\ndem man ihn platziert hat.\n\nAm "
      - control:
          kind: set_colour
          colour: red
      - text: Stall von Süd-Akkala
      - control:
          kind: reset_colour
      - text: " soll es eine Extra-\nAusgabe von „Kletis' Gerüchteküche“ mit\nGerüchten zu diesem Gegenstand geben."
  QL_TreasureHunt02_Finish:
    contents:
      - text: "In einem versteckten Raum in den Tiefen von\nIrrland hast du das "
      - control:
          kind: set_colour
          colour: red
      - control:
          kind: raw
          two_hundred_one:
            dynamic:
              - 0
              - len: 4
                field_2:
                  - 3
                  - 2
                  - 2
                  - 0
      - text: Teleport-Medaillon
      - control:
          kind: reset_colour
      - text: "\ngefunden!\n\nDieses Artefakt kann dich tatsächlich an den\nOrt teleportieren, an dem du es ablegst.\n\nManchmal lohnt es sich, auch den\nunglaubwürdigsten Gerüchten nachzugehen!"
  QL_TreasureHunt02_Name:
    contents:
      - text: "EX: Teleportation!?"
  QL_TreasureHunt02_Step01:
    contents:
      - text: "In der Extra-Ausgabe Nr. 2 von „Kletis'\nGerüchteküche“ hast du gelesen, dass es ein\nMedaillon geben soll, das den Besitzer an die\nStelle teleportieren kann, an der es abgelegt\nwurde. Es soll "
      - control:
          kind: set_colour
          colour: red
      - text: "in einem Labyrinth im Nordosten\nvon Akkala unterirdisch"
      - control:
          kind: reset_colour
      - text: " versteckt sein.\n\nWenn du diesem Gerücht Glauben schenkst,\nsolltest du dich auf die Suche nach diesem\nLabyrinth machen!"
