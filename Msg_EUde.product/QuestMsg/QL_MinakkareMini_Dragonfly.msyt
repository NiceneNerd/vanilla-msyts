              - 0
              - len: 4
                field_2:
                  - 2
                  - 1
                  - 1
                  - 0
      - text: Frostlibelle
      - control:
          kind: reset_colour
      - text: ",\nund "
      - control:
          kind: set_colour
          colour: red
      - text: "1 "
      - control:
          kind: raw
          two_hundred_one:
            dynamic:
              - 0
              - len: 4
