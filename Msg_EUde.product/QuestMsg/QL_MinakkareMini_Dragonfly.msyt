  QL_MinakkareMini_Dragonfly_Desc:
    contents:
      - text: "Jana sucht ein Geschenk für ihre kleine\nSchwester Glima, die nichts lieber mag\nals Insekten.\n\nDoch egal, wie oft sie fragt, Glima verrät ihr\nnicht, welche Art Insekt sie am liebsten hat.\n\nGeh zu "
      - control:
          kind: set_colour
          colour: red
      - text: Glima
      - control:
          kind: reset_colour
      - text: " und höre ihr zu."
  QL_MinakkareMini_Dragonfly_Dragonfly:
    contents:
      - text: "Jana findet Libellen furchtbar. Sie ist jedoch\nnoch immer entschlossen, ihrer Schwester\nselbst ein Geburtstagsgeschenk zu überreichen.\n\nFinde für "
      - control:
          kind: set_colour
          colour: red
      - text: Jana
      - control:
          kind: reset_colour
      - text: " je "
      - control:
          kind: set_colour
          colour: red
      - text: "1 "
      - control:
          kind: raw
          two_hundred_one:
            dynamic:
              - 0
              - len: 4
                field_2:
                  - 2
                  - 1
                  - 1
                  - 0
      - text: Glutlibelle
      - control:
          kind: reset_colour
      - text: ", "
      - control:
          kind: set_colour
          colour: red
      - text: "1 "
      - control:
          kind: raw
