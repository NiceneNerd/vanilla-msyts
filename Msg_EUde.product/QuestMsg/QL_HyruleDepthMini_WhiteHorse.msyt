  QL_HyruleDepthMini_WhiteHorse_Desc:
    contents:
      - text: "Todor am Stall der Ebene hat dich gebeten,\nihm einen "
      - control:
          kind: set_colour
          colour: red
      - text: "Schimmel "
      - control:
          kind: reset_colour
      - text: "zu bringen.\n\nDer "
      - control:
          kind: set_colour
          colour: red
      - text: "Schimmel "
      - control:
          kind: reset_colour
      - text: "soll sich am Salphura-Hügel\naufhalten und ein schneeweißes Fell ohne\nMarkierungen haben. Es heißt, er halte\nAbstand zu den anderen Pferden."
  QL_HyruleDepthMini_WhiteHorse_Finish:
    contents:
      - text: "Du hast Todor am Stall der Ebene den\n"
      - control:
          kind: set_colour
          colour: red
      - text: Schimmel
      - control:
          kind: reset_colour
      - text: " gezeigt. Zum Dank hat er dir einen \nköniglichen Sattel und Zaumzeug gegeben.\n\nTodors Großvater sagte immer, dass der\nAnblick der Prinzessin auf diesem "
      - control:
          kind: set_colour
          colour: red
      - text: Schimmel
      - control:
          kind: reset_colour
      - text: "\nder schönste Anblick der Welt gewesen sei."
  QL_HyruleDepthMini_WhiteHorse_Name:
    contents:
      - text: Das Pferd der Könige
