entries:
  QL_TreasureHunt_touzoku02_Desc:
    contents:
      - text: "Die Königsfamilie von Hyrule besaß historisch\nwertvolle Gewandungen, die von einem Räuber\nnamens Ramda gestohlen wurden. Einer dieser\nGegenstände war die Krone der Schatten.\n\nIn den "
      - control:
          kind: set_colour
          colour: red
      - text: Hyrule-Stallruinen
      - control:
          kind: reset_colour
      - text: " sollen sich\nAufzeichnungen befinden, die verraten, wo sich\ndieser Schatz befindet."
  QL_TreasureHunt_touzoku02_Finish:
    contents:
      - text: "„Die Krone der Prinzessin der Schatten\nfindet sich bei einer Tempelruine am\nRegenzia-Fluss.“\n\nDu hast den versteckten Schatz im\nTempel der Weisen aufgespürt und\n"
      - control:
          kind: set_colour
          colour: red
      - control:
          kind: raw
          two_hundred_one:
            dynamic:
              - 0
              - len: 4
                field_2:
                  - 2
                  - 4
                  - 4
                  - 0
      - text: "Midnas Krone "
      - control:
          kind: reset_colour
      - text: erhalten!
  QL_TreasureHunt_touzoku02_Name:
    contents:
      - text: "EX: Krone der Schatten"
  QL_TreasureHunt_touzoku02_Step01:
    contents:
      - text: "„Die Krone der Prinzessin der Schatten\nfindet sich "
      - control:
          kind: set_colour
          colour: red
      - text: "bei einer Tempelruine am\nRegenzia-Fluss"
      - control:
          kind: reset_colour
      - text: ".“\n\nRamdas Extra-Notizen verraten, wo sich die\nKrone befindet. Aber wo ist diese "
      - control:
          kind: set_colour
          colour: red
      - text: Tempelruine
      - control:
          kind: reset_colour
      - text: "?"
