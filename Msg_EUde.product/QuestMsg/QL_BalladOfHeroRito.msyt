entries:
  QL_BalladOfHeroRito_Desc:
    contents:
      - text: "Der Zerstörer führt dich zu einem Monument,\ndas dir den Weg zu Prüfungen weist. Zwischen\nKashiwas Lied und diesen Prüfungen scheint\neine Verbindung zu bestehen:\n„Es gilt, "
      - control:
          kind: set_colour
          colour: red
      - text: "ein Horn des Drachen aus Feuer\nzu durchbohren und zu fassen"
      - control:
          kind: reset_colour
      - text: "...\nEs gilt, "
      - control:
          kind: set_colour
          colour: red
      - text: sich von Lichtringen führen zu lassen
      - control:
          kind: reset_colour
      - text: "...\nEs gilt, "
      - control:
          kind: set_colour
          colour: red
      - text: vier Ziele zu treffen
      - control:
          kind: reset_colour
      - text: " ohne zu\nzagen...“\nNoch zu bestehende Prüfungen: "
      - control:
          kind: set_colour
          colour: red
      - text: "3"
      - control:
          kind: reset_colour
  QL_BalladOfHeroRito_Finish:
    contents:
      - text: "Du hast die Illusion von Ganons Windfluch\nbesiegt, die aus deinen eigenen Erinnerungen\nentstanden war.\n\nKashiwas Lied, "
      - control:
          kind: set_colour
          colour: red
      - text: Revalis Ballade
      - control:
          kind: reset_colour
      - text: ", ist so\nbewegend, dass du dich in die Ereignisse von\nvor 100 Jahren zurückversetzt fühlst.\n\nDas Bestehen der Prüfung hat deine\nheilige Kraft und damit auch "
      - control:
          kind: raw
          two_hundred_one:
            dynamic:
              - 0
              - len: 4
                field_2:
                  - 1
                  - 4
                  - 4
                  - 0
      - text: "Revalis Sturm\ngestärkt."
  QL_BalladOfHeroRito_Name:
    contents:
      - text: "EX: Die Ballade von Revali"
