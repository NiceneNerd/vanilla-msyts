            - 1
      - control:
          kind: sound2
          unknown:
            - 8
            - 205
      - text: Ho ho ho...
      - control:
          kind: pause
          length: long
      - text: "\nWenn du das "
      - control:
          kind: set_colour
          colour: blue
      - text: Parasegel
      - control:
          kind: reset_colour
      - text: " haben möchtest,\nsolltest du dich ins Zeug legen."
  GoodBye02:
    attributes: Npc_King_Vagrant004
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 5
      - text: Grml...
      - control:
          kind: pause
          length: long
      - text: "\nWenn du nichts von mir willst, dann lass\nmich in Ruhe schlafen."
  GoodBye03:
    attributes: Npc_King_Vagrant004
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 9
      - text: Ich verlasse mich auf dich.
