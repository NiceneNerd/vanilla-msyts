  Near01:
    attributes: Npc_King_Vagrant004
    contents:
      - text: Und... zack! Und... zack!
  Near02:
    attributes: Npc_King_Vagrant004
    contents:
      - text: Zzz... Zzz...
  Near03:
    attributes: Npc_King_Vagrant004
