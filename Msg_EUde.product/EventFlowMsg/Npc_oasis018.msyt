entries:
  "0000":
    attributes: ""
    contents:
      - text: Ich sehe sie!
  "0001":
    attributes: ""
    contents:
      - text: Ich sehe sie nicht...
  Talk_01:
    attributes: Npc_oasis018
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 18
      - text: Nachts gefällt es mir am besten.
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ich kann einfach nicht verstehen, warum\nso viele Leute in der Nachmittagshitze\nherumlaufen...\nSo, ich sollte mir wohl ein erfrischendes\nGetränk holen, um wach zu werden..."
  Talk_02:
    attributes: Npc_oasis018
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: Puh!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Das ist wirklich das Einzige,\nwas mich so richtig wach macht."
  Talk_04:
    attributes: Npc_oasis018
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 25
      - text: "Meine Großmutter hat immer gesagt,"
      - control:
          kind: pause
          length: short
      - text: "\niss niemals eine "
      - control:
          kind: raw
          two_hundred_one:
            dynamic:
              - 0
              - len: 4
                field_2:
                  - 2
                  - 1
                  - 1
                  - 0
      - text: "Maxi-Durian, wenn\ndu einen trinken gehst..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ich weiß nicht, warum ihr das so am\nHerzen lag, aber da sie es immer wieder\ngesagt hat, wird wohl etwas dran sein."
  Talk_05:
    attributes: Npc_oasis018
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 25
      - text: Ich brauche noch was zu knabbern...
  Talk_06:
    attributes: Npc_oasis018
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 25
      - text: "Meine Mutter hat immer gesagt,"
      - control:
          kind: pause
          length: short
      - text: "\nGerichte, die Pilze enthalten, tun\ngut, wenn man einen Kater hat..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Wenn's mich also mal erwischt hat,\nmache ich immer eine "
      - control:
          kind: set_colour
          colour: blue
      - text: "Curry-\n"
      - control:
          kind: raw
          two_hundred_one:
            dynamic:
              - 0
              - len: 4
                field_2:
                  - 2
                  - 1
                  - 1
                  - 0
      - text: Duftpilzpfanne
      - control:
          kind: reset_colour
      - text: ".\nDas ist ein ganz einfaches Gericht\naus "
      - control:
          kind: set_colour
          colour: blue
      - control:
          kind: raw
          two_hundred_one:
            dynamic:
              - 0
              - len: 4
                field_2:
                  - 3
                  - 4
                  - 4
                  - 0
      - text: Goronengewürz
      - control:
          kind: reset_colour
      - text: " und "
      - control:
          kind: set_colour
          colour: blue
      - text: Pilzen
      - control:
          kind: reset_colour
      - text: "."
  Talk_07:
    attributes: Npc_oasis018
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 18
      - text: "Jetzt wird es ziemlich schnell heiß\nwerden... "
      - control:
          kind: pause
          length: short
      - text: "Der beste Zeitpunkt, um\nzu Bett zu gehen."
  Talk_08:
    attributes: Npc_oasis018
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 34
      - text: "...He, was willst du denn?\nIch schlafe immer tagsüber."
  Talk_09:
    attributes: Npc_oasis018
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 25
      - text: "Pilze, die nur von gutaussehenden\nLeuten gesehen werden können?"
      - control:
          kind: pause
          length: short
      - text: "\nAls ob ich die dann sehen könnte!\nDu willst mir doch nicht erzählen,\ndass du sie siehst?"
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Talk_10:
    attributes: Npc_oasis018
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 0
      - text: Wirklich?
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Und ich dachte immer, dass Ardin sich\ndas nur ausgedacht hat, damit auch Kun-\nden kommen, wenn sie ausverkauft ist.\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Was kümmert's mich, ob ich sie sehe.\nIn meinen jungen Jahren war ich eine\nSchönheit, das kannst du mir glauben."
  Talk_11:
    attributes: Npc_oasis018
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Na, wenn ein hübsches Ding wie du\nsie nicht sieht..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ardin muss sich wieder irgendwas\nausgedacht haben."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Um Kunden zu halten, erfindet sie\ndie merkwürdigsten Dinge."
  Talk_12:
    attributes: Npc_oasis018
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "Ausverkauft? Das gibt's doch nicht..."
  Talk_13:
    attributes: Npc_oasis018
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: "Oh, eine hylianische Vaai..."
      - control:
          kind: pause
          length: short
      - text: "\nSavotta..."
  Talk_14:
    attributes: Npc_oasis018
    contents:
      - control:
          kind: sound
