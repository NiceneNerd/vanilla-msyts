  Npc_kokiri005_After_Demo_Talk000:
    attributes: Npc_kokiri005
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 0
      - text: "Willkommen, willkommen! "
      - control:
          kind: pause
          length: short
      - text: "Schön, dich zu\nsehen, werter Held!"
      - control:
          kind: pause
          length: short
      - text: " Wenn du Fragen zu\ndiesen Wäldern hast, frag mich, Macus!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Denn Macus macht das schon! Wenn du\nmit dem "
      - control:
          kind: set_colour
          colour: blue
      - text: Deku-Baum
      - control:
          kind: reset_colour
      - text: " sprechen möchtest,\nklettere einfach hier hoch!"
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
            - 2
            - 3
          selected_index: 0
          cancel_index: 3
          unknown: 10
  Npc_kokiri005_After_Demo_Talk001:
    attributes: Npc_kokiri005
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 0
      - text: "Brauchst du noch Hilfe?\nMacus macht alles für dich!"
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
            - 2
            - 3
          selected_index: 0
          cancel_index: 3
          unknown: 10
  Npc_kokiri005_After_Demo_Talk010:
    attributes: Npc_kokiri005
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: "Die größten Kuriositäten im "
      - control:
          kind: set_colour
          colour: blue
      - text: "Wald der\nKrogs"
      - control:
          kind: reset_colour
      - text: " sind natürlich der Deku-Baum und\nder Schwertsockel."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 9
            - 1
      - control:
          kind: set_colour
          colour: grey
      - control:
          kind: text_size
          percent: 80
      - text: "Wenn mir für „Kuriositäten“ mal niemand die\nWurzeln lang zieht..."
      - control:
          kind: text_size
          percent: 100
      - control:
          kind: reset_colour
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 6
            - 1
      - text: "Im "
      - control:
          kind: set_colour
          colour: blue
      - text: "Deku-Baum "
      - control:
          kind: reset_colour
      - text: "haben wir einen Platz zum\nAusruhen und andere nützliche Dinge für\ndich vorbereitet, werter Held!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ja, und der "
      - control:
          kind: set_colour
          colour: blue
      - text: Schwertsockel
      - control:
          kind: reset_colour
      - text: "..."
      - control:
          kind: pause
          length: short
      - text: " Dort ruht\ndas "
      - control:
          kind: set_colour
          colour: red
      - text: heilige Bannschwert
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: pause
          length: short
      - text: " Dazu braucht\nman im Grunde ja nix weiter zu sagen.\n"
      - control:
          kind: sound
          unknown:
            - 11
            - 18
      - text: "Hier ist noch eine recht interessante\nInformation:"
      - control:
          kind: pause
          length: short
      - text: " Der "
      - control:
          kind: set_colour
          colour: blue
      - text: Schwertsockel
      - control:
          kind: reset_colour
      - text: " liegt\nexakt im Zentrum des Waldes."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Vom Sockel aus kann man die Waldwege\nbesser erkennen als von irgendwo sonst!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Und die "
      - control:
          kind: set_colour
          colour: blue
      - text: "Bohnenlampen "
      - control:
          kind: reset_colour
      - text: "am Wegrand\ndienen der Orientierung! "
      - control:
          kind: pause
          length: short
      - text: "Auf die solltest\ndu immer ein Auge haben.\n"
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: "Wenn du dich also doch mal verläufst,\nkehre am besten zum "
      - control:
          kind: set_colour
          colour: blue
      - text: Schwertsockel
      - control:
          kind: reset_colour
      - text: "\nzurück!"
  Npc_kokiri005_After_Demo_Talk020:
    attributes: Npc_kokiri005
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - control:
          kind: set_colour
          colour: red
      - text: Das heilige Bannschwert
      - control:
          kind: reset_colour
      - text: ", das alles\nUnheil versiegelt..."
      - control:
          kind: pause
          length: long
      - text: " Das nur du allein\nführen kannst..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Du hast es endlich gefunden!\nEs war ganz schön gut versteckt, oder?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Wir haben uns ins Zeug gelegt, damit\nnicht einfach irgendwer per Zufall\ndarüber stolpert...\n"
      - control:
          kind: sound
          unknown:
            - 11
            - 1
      - text: "Hoffentlich haben wir es nicht\nübertrieben. "
      - control:
          kind: pause
          length: long
      - text: "Ich meine, hast du dich\nin den Verlorenen Wäldern verlaufen?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 10
            - 6
      - text: "Ach, was rede ich da?"
      - control:
          kind: pause
          length: long
      - text: " Natürlich nicht!"
      - control:
          kind: pause
          length: short
      - text: "\nSonst wärst du jetzt ja nicht hier!\n'tschuldigung!"
  Npc_kokiri005_After_Demo_Talk030:
    attributes: Npc_kokiri005
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 4
      - text: Du willst den Wald verlassen?
      - control:
          kind: pause
          length: short
      - text: " Na ja,\nes war ja irgendwie abzusehen, dass\ndu auch mal wieder gehen willst..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 7
            - 1
      - text: "Dementsprechend sind wir natürlich\nvorbereitet!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 6
            - 1
      - text: "Gehe vom "
      - control:
          kind: set_colour
          colour: red
      - text: Schwertsockel
      - control:
          kind: reset_colour
      - text: " nach Osten,\nbis du rechts einen Wegweiser siehst."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 6
            - 1
      - text: "Wenn du dann dort einfach den Hügel\nhochgehst, kommst du an einen\n"
      - control:
          kind: set_colour
          colour: red
      - text: Geisterbaum
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Dem spazierst du einfach in den Mund\nund wirst so aus dem Wald gebracht!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 7
            - 9
      - text: "Wäre ja auch ein bisschen anstrengend,\nwenn man noch mal ganz durch die\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Verlorenen Wälder
      - control:
          kind: reset_colour
      - text: " müsste, oder?"
  Npc_kokiri005_After_Demo_Talk040:
    attributes: Npc_kokiri005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Wenn ich dir mit irgendwas in diesen\nWäldern helfen kann, Macus macht das\nschon!"
  Npc_kokiri005_GotMSword_Talk000:
    attributes: Npc_kokiri005
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: "Oh! "
      - control:
          kind: pause
          length: short
      - text: Werter Held!
      - control:
          kind: pause
          length: short
      - text: " Ich habe gesehen, wie\ndu das "
      - control:
          kind: set_colour
          colour: red
      - text: heilige Bannschwert
      - control:
          kind: reset_colour
      - text: " aus seinem\nSockel gezogen hast!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 6
            - 0
      - text: "Wenn du mehr über das "
      - control:
          kind: set_colour
          colour: red
      - text: Schwert
      - control:
          kind: reset_colour
      - text: " wissen\nwillst, frag mich, Macus!"
      - control:
          kind: pause
          length: short
      - text: " Denn Macus\nmacht das schon!"
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
            - 2
            - 3
          selected_index: 0
          cancel_index: 3
          unknown: 10
  Npc_kokiri005_GotMSword_Talk010:
    attributes: Npc_kokiri005
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 6
      - text: Das heilige Bannschwert...
      - control:
          kind: pause
          length: long
      - text: "\nOder auch "
      - control:
          kind: set_colour
          colour: blue
      - control:
          kind: raw
          two_hundred_one:
            dynamic:
              - 0
              - len: 4
                field_2:
                  - 3
                  - 2
                  - 2
                  - 0
      - text: Master-Schwert
      - control:
          kind: reset_colour
      - text: "..."
      - control:
          kind: pause
          length: short
      - text: "\nDu willst also mehr darüber wissen?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Das "
      - control:
          kind: set_colour
          colour: blue
      - control:
          kind: raw
          two_hundred_one:
            dynamic:
              - 0
              - len: 4
                field_2:
                  - 3
                  - 2
                  - 2
                  - 0
      - text: Master-Schwert
      - control:
          kind: reset_colour
      - text: " ist ein besonderes\nSchwert, das aber mit jedem Gebrauch\nschwächer wird."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Wenn das passiert, ist das aber kein\nGrund zur Panik."
      - control:
          kind: pause
          length: short
      - text: " Du musst dann nur\nwarten, bis es sich regeneriert hat."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Es spart vielleicht einfach seine Kräfte,\nbis du sie wirklich brauchst."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 7
            - 9
      - text: "Der Deku-Baum kann dir dazu sicher\nmehr erzählen."
  Npc_kokiri005_Talk000:
    attributes: Npc_kokiri005
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 0
      - text: Der Held!
      - control:
          kind: pause
          length: short
      - text: "\nIch habe"
      - control:
          kind: pause
          length: short
      - text: " so lang"
      - control:
          kind: pause
          length: short
      - text: " auf dich gewartet!"
  Npc_kokiri005_Talk010:
    attributes: Npc_kokiri005
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: "Los, hol dir das "
      - control:
          kind: set_colour
          colour: red
      - text: Schwert
      - control:
          kind: reset_colour
      - text: "!"
