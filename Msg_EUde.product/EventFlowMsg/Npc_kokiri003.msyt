    attributes: Npc_kokiri003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Sieh an, sieh an! "
      - control:
          kind: pause
          length: short
      - text: "Herzlich willkommen,\nwerter Held, in meinem kleinen, aber\nfeinen Laden für "
      - control:
          kind: set_colour
          colour: blue
      - text: Waldrat
      - control:
          kind: reset_colour
      - text: "!\nIch verkaufe hier Dinge, die ich im Wald\nfinde, und Dinge, die ich nicht im Wald\nfinde!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Sieh dich um, such dir was aus!\nOder..."
      - control:
          kind: pause
          length: short
      - text: " Möchtest du lieber etwas\nverkaufen?"
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
            - 2
          selected_index: 0
          cancel_index: 2
          unknown: 8
