    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Wenn das so ist, solltest du in den Wipfel\ndes Deku-Baums klettern."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Dort erwartet dich die schwierigste aller\n"
      - control:
