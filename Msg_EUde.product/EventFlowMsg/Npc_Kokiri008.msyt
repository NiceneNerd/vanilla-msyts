          kind: sound
          unknown:
            - 10
            - 19
      - text: Was?
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Du hast das Master-Schwert nicht\nherausgezogen! Bring dich in Form\nund erweise dich als würdig!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Schnell, damit der Deku-Baum und wir\nuns wieder sicher fühlen können!"
  Npc_Kokiri008_Talk504:
    attributes: Npc_Kokiri008
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Klettere in den Wipfel des Deku-Baums\nund stelle dich der schwierigsten aller\nPrüfungen!"
