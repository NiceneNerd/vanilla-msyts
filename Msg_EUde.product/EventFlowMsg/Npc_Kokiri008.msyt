    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 0
      - text: Ist das dein Ernst?
      - control:
          kind: pause
          length: short
      - text: "\nDu hast sie schon gefunden?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Dann ist mir hiermit der Gesprächsstoff\nausgegangen, mit dem ich dich\nbeeindrucken könnte..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Halt, warte! Ich habe ja noch meine\nSpezialfähigkeit!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ich stoße Sporen aus dem Pilz auf\nmeinem Kopf aus und bin dann von\noben bis unten voller Pilze!"
