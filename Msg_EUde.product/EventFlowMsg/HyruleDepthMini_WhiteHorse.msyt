          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Lass mich dir das Geschirr und den\nSattel der Königsfamilie geben!"
      - control:
          kind: pause
          length: short
      - text: " Hat mir\nmein Opa vermacht!"
  talk13:
