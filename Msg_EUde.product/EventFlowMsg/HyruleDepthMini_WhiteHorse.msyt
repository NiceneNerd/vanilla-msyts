    attributes: Npc_HyruleDepthHatago007
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 20
      - text: Sehr gut!
      - control:
          kind: pause
          length: long
      - text: " Es wäre toll, wenn du einen\n"
