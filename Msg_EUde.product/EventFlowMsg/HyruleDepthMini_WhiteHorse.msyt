    attributes: Npc_HyruleDepthHatago007
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 12
      - text: "Tut mir leid, dass ich dich mit meiner\nBitte belästigt habe."
      - control:
          kind: pause
          length: longer
