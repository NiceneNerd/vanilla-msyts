      - control:
          kind: set_colour
          colour: red
      - text: Schimmel
      - control:
          kind: reset_colour
      - text: "\ngesehen?"
  talk05:
    attributes: Npc_HyruleDepthHatago007
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 0
      - text: Willst du mir meine Bitte doch erfüllen?
