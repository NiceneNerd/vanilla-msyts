  talk01:
    attributes: Obj_DiaryGerudo_A_01
    contents:
      - text: Tag Eins.
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ich begab mich in die Berge, um die\n"
      - control:
          kind: set_colour
          colour: red
      - text: "Heldenprüfung "
      - control:
          kind: reset_colour
      - text: "zu erforschen, welche\nin einem alten Dokument erwähnt ist.\n„Am Sockel der verschneiten Gerudo-\nBerge schlummert die "
      - control:
          kind: set_colour
          colour: red
      - text: Heldenprüfung
      - control:
          kind: reset_colour
      - text: ".“"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Den im Dokument erwähnten "
      - control:
          kind: set_colour
          colour: red
      - text: Sockel
      - control:
          kind: reset_colour
      - text: "\nhabe ich bereits erreicht."
      - control:
