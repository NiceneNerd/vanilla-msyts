  talk12:
    attributes: Npc_Zora025
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 4
      - text: "Prinz Sidon ist der Bruder der großen\nMipha und König Dorephans Sohn!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Sidon ist der attraktivste Zora in ganz\nHyrule,"
      - control:
          kind: pause
          length: short
      - text: " aber auch seine Schwester Mipha\nsoll außerordentlich schön gewesen sein.\nZwei so bezaubernde Geschwister..."
      - control:
          kind: pause
          length: short
      - text: "\nDa wird man ganz neidisch!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "Ich wünschte, ich hätte auch so tolle\nGeschwister!"
      - control:
          kind: pause
          length: short
      - text: " Stattdessen habe ich nur\nCleff..."
  talk13:
    attributes: Npc_Zora025
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 4
      - text: "Die große Mipha ist vor 100 Jahren\nspurlos verschwunden."
      - control:
          kind: pause
          length: short
      - text: " Sie war die\nältere Schwester von Prinz Sidon.\nIch war vor 100 Jahren noch nicht auf\nder Welt,"
      - control:
          kind: pause
          length: short
      - text: " deshalb weiß ich das alles nicht\nso genau...\nAber die älteren Zoras sollten sich\nnoch daran erinnern können."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Alle, die über 100 Jahre alt sind!"
      - control:
          kind: pause
          length: short
      - text: "\nDie, die immer so komisch reden!"
  talk14:
