          unknown: 6
  talk25:
    attributes: Npc_HighMountain013
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 9
      - text: "Ich meine... "
      - control:
          kind: pause
          length: short
      - text: Bis dann!
  talk26:
    attributes: Npc_HighMountain013
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Ich habe es zwar nicht selbst gesehen,"
      - control:
          kind: pause
          length: short
      - text: "\naber..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Revali war dabei, als es vor 100 Jahren\ngegen die "
      - control:
