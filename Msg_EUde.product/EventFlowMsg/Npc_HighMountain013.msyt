      - control:
          kind: sound
          unknown:
            - 0
            - 17
      - text: "Hmf... "
      - control:
          kind: pause
          length: short
      - text: Ich kann dazu nur eines sagen...
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "Solange dieses "
      - control:
          kind: set_colour
          colour: blue
      - text: Ungeheuer
      - control:
          kind: reset_colour
      - text: " über uns\nlauert, kommen weniger Leute zu uns,"
      - control:
          kind: pause
          length: short
      - text: "\nund das ist schlecht fürs Geschäft."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Tja,"
      - control:
          kind: pause
          length: short
