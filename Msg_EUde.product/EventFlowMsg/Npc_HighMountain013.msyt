          colour: blue
      - text: Flüge durch die Luft
      - control:
          kind: reset_colour
      - text: " bestens gerüstet!"
  talk17:
    attributes: Npc_HighMountain013
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 13
      - text: "Sag mal,"
      - control:
          kind: pause
          length: short
      - text: " bist du sicher,"
      - control:
          kind: pause
          length: short
      - text: " dass du nicht\nein paar von meinen "
      - control:
          kind: set_colour
          colour: blue
      - text: Klamotten
      - control:
          kind: reset_colour
      - text: " brauchen\nkannst?"
  talk18:
