      - text: " sieh dich mal um!"
  talk03:
    attributes: Npc_HighMountain013
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 11
      - text: Sehr gut!
      - control:
          kind: pause
          length: short
      - text: " Dann sieh dich mal um!"
  talk04:
    attributes: Npc_HighMountain013
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: Bitte beehre mich mal wieder!
  talk05:
    attributes: Npc_HighMountain013
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: Ich habe es schon gehört!
      - control:
          kind: pause
          length: long
      - text: "\nDu hast uns dieses "
      - control:
          kind: set_colour
          colour: blue
      - text: "Ungeheuer am\nHimmel"
      - control:
          kind: reset_colour
      - text: " vom Hals geschafft, was?"
  talk06:
    attributes: Npc_HighMountain013
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Hallo! "
      - control:
          kind: pause
          length: longer
      - text: Setzt du deine Reise fort?
  talk07:
    attributes: Npc_HighMountain013
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Toll,"
      - control:
          kind: pause
          length: short
      - text: " dass du auch so spät noch\nunterwegs bist!"
  talk08:
    attributes: Npc_HighMountain013
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Das hier ist der Rüstungsladen\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Roter Pfau
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: pause
          length: short
      - text: " Tritt ruhig ein!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Wir haben zahlreiche Produkte aus\nunseren berühmten Orni-Federn im\nAngebot."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 10
            - 0
      - text: Aber versteh mich nicht falsch!
      - control:
          kind: pause
          length: short
      - text: " \nWir verwenden keinen alten Federn so\nwie meine"
      - control:
          kind: pause
          length: short
      - text: ", haha."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Wir verwenden ausschließlich beim\nJahreszeitenwechsel ausgefallene, warme\nund weiche Kükenfedern!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: So was findest du sonst nirgendwo!
      - control:
          kind: pause
          length: short
      - text: " Mit\neiner kompletten "
      - control:
          kind: set_colour
          colour: blue
      - text: "Garnitur "
      - control:
          kind: reset_colour
      - text: "kann dir keine\nnoch so große Kälte mehr was anhaben!"
  talk11:
    attributes: Npc_HighMountain013
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Freut mich,"
      - control:
          kind: pause
          length: short
      - text: " dass du trotz des Regens\nhergekommen bist!"
  talk14:
    attributes: Npc_HighMountain013
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 0
      - text: "...Oh!"
      - control:
          kind: pause
          length: long
      - text: " Warst das du,"
      - control:
          kind: pause
          length: short
      - text: " der mit Teba\nunterwegs war?"
  talk15:
    attributes: Npc_HighMountain013
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: Ich wollte dir danken...
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "Dieses "
      - control:
          kind: set_colour
          colour: blue
      - text: Ungeheuer...
      - control:
          kind: pause
          length: short
      - control:
          kind: reset_colour
      - text: " Jedes Mal, wenn es\nüber uns drüber geflogen ist, habe ich\nGänsehaut gekriegt."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 6
            - 1
      - text: "Ich nehme an,"
      - control:
          kind: pause
          length: short
      - text: " du setzt deine Reise fort?"
  talk16:
    attributes: Npc_HighMountain013
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: "Hast du dir die komplette Ausstattung\nbei uns besorgt?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Damit bist du für die "
      - control:
          kind: set_colour
          colour: blue
      - text: Schneeberge
      - control:
          kind: reset_colour
      - text: " und\n"
      - control:
