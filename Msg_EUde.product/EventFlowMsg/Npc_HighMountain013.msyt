  talk02:
    attributes: Npc_HighMountain013
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 16
      - text: "Wenn du etwa in die "
      - control:
          kind: set_colour
          colour: blue
      - text: "nördlichen Schnee-\nberge"
      - control:
          kind: reset_colour
      - text: " oder an einen "
      - control:
          kind: set_colour
          colour: blue
      - text: "Ort willst,"
      - control:
          kind: pause
          length: short
      - text: " der noch\nhöher liegt als hier"
      - control:
          kind: reset_colour
      - text: "..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Dann wird dir die Kälte ganz schön zu\nschaffen machen!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Aber dagegen hilft unsere "
      - control:
