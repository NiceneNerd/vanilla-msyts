      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Dieses Problem überlasse ich dem\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Häuptling
      - control:
          kind: reset_colour
      - text: " und kümmere mich ansonsten\num meinen eigenen Kram."
  talk20:
    attributes: Npc_HighMountain013
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 2
      - text: Hallo!
      - control:
          kind: pause
          length: longer
      - text: "\nWir haben gerade über das "
      - control:
          kind: set_colour
