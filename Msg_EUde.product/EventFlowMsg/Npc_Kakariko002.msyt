          selected_index: 0
          cancel_index: 3
          unknown: 10
  Talk01:
    attributes: Npc_Kakariko002
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Wenn ich Euch irgendwie dienlich\nsein kann,"
      - control:
          kind: pause
          length: short
      - text: " zögert nicht, an mich\nheranzutreten."
      - control:
          kind: choice
          choice_labels:
            - 1
            - 2
            - 3
            - 4
          selected_index: 0
          cancel_index: 3
          unknown: 10
  Talk02:
    attributes: Npc_Kakariko002
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: D...
      - control:
          kind: pause
          length: short
      - text: " Dieses... "
      - control:
          kind: set_colour
          colour: blue
      - text: Juwel
      - control:
          kind: reset_colour
      - text: " hat Eure Neugier\ngeweckt?"
      - control:
          kind: pause
          length: short
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 5
            - 0
      - text: "Es ist die Aufgabe unseres Stammes,\nes zu beschützen."
      - control:
          kind: pause
          length: short
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: „
      - control:
          kind: set_colour
          colour: red
      - text: "Dem vom Juwel Auserwählten wird\nder Segen der Alten gewährt."
      - control:
          kind: reset_colour
      - text: “
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Diesen Leitsatz gibt bei uns jede Mutter\nan ihre Tochter weiter, zusammen mit\ndem Juwel selbst."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Es ist sozusagen unser Familienschatz...
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Wir wissen nicht, was es bedeutet."
      - control:
          kind: pause
          length: short
      - text: "\n„"
      - control:
          kind: set_colour
          colour: red
      - text: Dem vom Juwel Auserwählten
      - control:
          kind: reset_colour
      - text: “...
      - control:
          kind: pause
          length: short
      - text: "\nKönnte das vielleicht...?"
  Talk03:
    attributes: Npc_Kakariko002
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 6
      - text: "Unser Stamm, die "
      - control:
          kind: set_colour
          colour: blue
      - text: Shiekah
      - control:
          kind: reset_colour
      - text: "..."
      - control:
          kind: pause
          length: long
      - text: " Wir sind\nein Geheimbund,"
      - control:
          kind: pause
          length: short
      - text: " der stets im Dienste\ndes Königshauses von Hyrule stand.\n"
      - control:
          kind: sound
          unknown:
            - 3
            - 0
      - text: "Doch vor 100 Jahren wurde die\nKönigsfamilie durch die "
      - control:
          kind: set_colour
          colour: red
      - text: "Große\nVerheerung"
      - control:
          kind: reset_colour
      - text: " ausgelöscht.\nUnser Stamm verstreute sich daraufhin\nin alle Winde.\n\n"
      - control:
          kind: sound
          unknown:
            - 3
            - 1
      - text: "Leider gibt es nicht wenige,"
      - control:
          kind: pause
          length: short
      - text: " die "
      - control:
          kind: set_colour
          colour: red
      - text: Ganon
      - control:
          kind: reset_colour
      - text: "\ndie Treue geschworen haben.\n\nSie nennen sich Yiga und ermorden alle,"
      - control:
          kind: pause
          length: short
      - text: "\ndie es wagen, sich gegen "
      - control:
          kind: set_colour
          colour: red
      - text: "Ganon "
      - control:
          kind: reset_colour
      - text: "zu\nstellen.\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Seit Ihr erwacht seid,"
      - control:
          kind: pause
          length: short
      - text: " Link,"
      - control:
          kind: pause
          length: short
      - text: " seid Ihr\nihnen natürlich ein Dorn im Auge.\n\nSie werden auch vor den schändlichsten\nMethoden nicht zurückschrecken,"
      - control:
          kind: pause
          length: short
      - text: " um\nEuch umzubringen...\n"
      - control:
          kind: sound
          unknown:
            - 5
            - 12
      - text: "Ich wünschte,"
      - control:
          kind: pause
          length: short
      - text: " ich könnte Euch irgendwie\nunterstützen..."
