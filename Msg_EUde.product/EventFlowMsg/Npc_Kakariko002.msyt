              field_1: 0
      - control:
          kind: sound
          unknown:
            - 6
            - 0
      - text: Wie... Wie geht es Euch?
      - control:
          kind: pause
          length: short
      - text: "\nKann ich Euch irgendwie behilflich sein?"
      - control:
          kind: choice
          choice_labels:
            - 1
