  Talk_001:
    attributes: NPC_oasis004
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 0
      - text: "In der "
      - control:
          kind: set_colour
          colour: blue
      - text: "Gerudo-Wüste "
      - control:
          kind: reset_colour
      - text: "gibt es zwischen\nTag und Nacht einen großen Temperatur-\nunterschied.\nWenn du nicht für Hitze als auch Kälte\nausgerüstet bist, leidet deine Gesundheit\nund schnell ist es um dich geschehen.\n"
      - control:
          kind: sound
          unknown:
            - 3
            - 0
      - text: "Ich sag's dir... "
      - control:
          kind: pause
          length: short
      - text: "Noch nicht mal Pferde\ngehen gern in die Wüste."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "Und wenn man dann noch die Monster\nmit einrechnet... "
      - control:
          kind: pause
          length: short
      - text: "Ich weiß, dass ich\nkeine zwei Schritt weit kommen würde...\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Daher wäre es bestimmt nett, wenn\nman sich mit dem "
      - control:
          kind: set_colour
          colour: red
      - text: legendären Schwert
      - control:
          kind: reset_colour
      - text: "\nden Weg erleichtern könnte."
