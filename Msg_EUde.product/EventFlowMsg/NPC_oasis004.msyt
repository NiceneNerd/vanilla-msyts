          cancel_index: 1
          unknown: 6
  Talk_002:
    attributes: NPC_oasis004
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 0
      - text: "Ich rede von dem "
      - control:
          kind: set_colour
          colour: red
      - text: legendären Schwert
      - control:
          kind: reset_colour
      - text: ",\ndas im Wald von Hyrule ruhen und auf\nseinen Herrn warten soll.\nDie Geschichte kennt eigentlich jeder,\ndachte ich..."
      - control:
          kind: pause
          length: longer
      - text: " Du hast nie davon gehört?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Meiner Meinung nach ist damit ja der\n"
      - control:
          kind: set_colour
          colour: blue
      - text: "Wald von Hyrule "
      - control:
          kind: reset_colour
      - text: "in der Eldin-Region\ngemeint.\nDort leben Feen und es heißt, dass sie\neinem den Verstand vernebeln, wenn\nman den Wald betritt.\nWenn es das "
      - control:
          kind: set_colour
          colour: red
      - text: legendäre Schwert
      - control:
          kind: reset_colour
      - text: " wirklich\ngibt, würde ich es gern einmal selbst, mit\nmeinen eigenen Augen, sehen.\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Aber, weißt du, in dem Wald würde ich\nmich wirklich nur ungern verirren..."
