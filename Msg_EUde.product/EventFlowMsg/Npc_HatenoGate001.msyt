            - 0
            - 4
      - text: Es wird immer rätselhafter...
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Dieses neue Mysterium will jedenfalls\ngründlich erforscht werden! "
      - control:
          kind: pause
