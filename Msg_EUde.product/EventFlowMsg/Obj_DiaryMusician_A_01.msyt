  talk01:
    attributes: Obj_DiaryMusician_A_01
    contents:
      - text: "Das Lied des "
      - control:
          kind: set_colour
          colour: blue
      - text: Zuguland-Waldes
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: reset_colour
      - text: "Dieses Lied ist im "
      - control:
          kind: set_colour
          colour: blue
      - text: "Zuguland-Wald "
      - control:
          kind: reset_colour
      - text: "von\nPhirone ein Klassiker."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Es scheint unter anderem um Drachen\nund Riesenschlangen zu gehen."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ich frage mich, ob es solche Wesen\nheutzutage überhaupt noch gibt..."
      - control:
          kind: choice
          choice_labels:
            - 3
            - 4
            - 5
          selected_index: 0
          cancel_index: 2
          unknown: 8
  talk02:
    attributes: Obj_DiaryMusician_A_01
    contents:
      - text: "Das Lied der "
      - control:
          kind: set_colour
          colour: blue
      - text: Westlichen Hyrule-Ebene
      - control:
          kind: reset_colour
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Im südlichen Bereich der Westlichen\nHyrule-Ebene liegt die "
      - control:
          kind: set_colour
          colour: blue
      - text: Jaad-Brücke
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "In der Nähe befinden sich zahllose\nFelsen mit einem Loch in der Mitte."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Und über genau diese Felsen soll es ein\nLied geben. Worum es darin wohl geht..."
      - control:
          kind: choice
          choice_labels:
            - 3
            - 4
            - 5
          selected_index: 0
          cancel_index: 2
          unknown: 8
  talk03:
    attributes: Obj_DiaryMusician_A_01
    contents:
      - text: "Das Lied der "
      - control:
          kind: set_colour
          colour: blue
      - text: Folbluth-Ebene
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: reset_colour
      - text: "In der "
      - control:
          kind: set_colour
          colour: blue
      - text: "Folbluth-Ebene "
      - control:
          kind: reset_colour
      - text: "im südlichen Teil\nRanelles ist ein Lied überliefert, das\nvon einem Tier handelt.\nDie Folbluth-Ebene befindet sich\nnordöstlich von "
      - control:
          kind: set_colour
          colour: blue
      - text: Kakariko
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Vielleicht sollte ich dort eine Pause\neinlegen und der Sache nachgehen."
      - control:
          kind: choice
          choice_labels:
            - 3
            - 4
            - 5
          selected_index: 0
          cancel_index: 2
          unknown: 8
  talk04:
    attributes: Obj_DiaryMusician_A_01
    contents:
      - text: "Das Lied des "
      - control:
          kind: set_colour
          colour: blue
      - text: Gerudo-Turms
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: reset_colour
      - text: "Ich erfuhr, dass es ein altes Lied über\nein Bauwerk namens "
      - control:
          kind: set_colour
          colour: blue
      - text: Gerudo-Turm
      - control:
          kind: reset_colour
      - text: "\ngeben soll.\nWo sich dieser "
      - control:
          kind: set_colour
          colour: blue
      - text: "Gerudo-Turm "
      - control:
          kind: reset_colour
      - text: "befinden\nsoll, weiß ich zwar nicht, aber in jüngster\nZeit erscheinen überall neue Türme.\nVielleicht handelt es sich also beim\n"
      - control:
          kind: set_colour
          colour: blue
      - text: "Gerudo-Turm "
      - control:
          kind: reset_colour
      - text: "um einen Turm, der in\nder Gerudo-Region erschien."
      - control:
          kind: choice
          choice_labels:
            - 3
            - 4
            - 5
          selected_index: 0
          cancel_index: 2
          unknown: 8
  talk05:
    attributes: Obj_DiaryMusician_A_01
    contents:
      - text: "Das Lied des "
      - control:
          kind: set_colour
          colour: blue
      - text: Calora-Sees
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: reset_colour
      - text: "Seit geraumer Zeit wird ein Lied über\nden gigantischen Hügel im "
      - control:
          kind: set_colour
          colour: blue
      - text: Calora-See
      - control:
          kind: reset_colour
      - text: "\nvon Phirone überliefert.\nIn der Region soll es des Öfteren zu\nheftigen Gewittern kommen. Ich sollte\ndort nur gut vorbereitet forschen..."
      - control:
          kind: choice
          choice_labels:
            - 3
            - 4
            - 5
          selected_index: 0
          cancel_index: 2
          unknown: 8
  talk06:
    attributes: Obj_DiaryMusician_A_01
    contents:
      - text: "Das Lied der "
      - control:
          kind: set_colour
          colour: blue
      - text: Horon-Bucht
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: reset_colour
      - text: "Es soll ein altes Lied über die\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Horon-Bucht
      - control:
          kind: reset_colour
      - text: " in Ranelle geben."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "In dieser Bucht weht ohne Unterlass ein\nstarker Wind. Darauf sollte ich mich vor-\nbereiten, bevor ich dorthin aufbreche..."
      - control:
          kind: choice
          choice_labels:
            - 3
            - 4
            - 5
          selected_index: 0
          cancel_index: 2
          unknown: 8
  talk07:
    attributes: Obj_DiaryMusician_A_01
    contents:
      - text: "Das Lied der "
      - control:
          kind: set_colour
          colour: blue
      - text: Nordbucht
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: reset_colour
      - text: "Es gibt wohl ein altes Lied über die\nseltsam geformte Steinsäule im Wasser\nder "
      - control:
          kind: set_colour
          colour: blue
      - text: Nordbucht
      - control:
          kind: reset_colour
      - text: ".\nEs heißt, dass sie dort platziert wurde,\num dem Helden, der einst wiederkehrt,\ngegen die Verheerung beizustehen...\nWelche Schätze und Reichtümer können\ndort nur verborgen sein?"
      - control:
          kind: choice
          choice_labels:
            - 3
            - 4
            - 5
          selected_index: 0
          cancel_index: 2
          unknown: 8
  talk08:
    attributes: Obj_DiaryMusician_A_01
    contents:
      - text: "Das Lied des "
      - control:
          kind: set_colour
          colour: blue
      - text: Washa-Plateaus
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: reset_colour
      - text: "Am "
      - control:
          kind: set_colour
          colour: blue
      - text: Washa-Plateau
      - control:
          kind: reset_colour
      - text: " scheint es ein\nLied über die Nacht des Blutmondes\nzu geben.\nIch frage mich, wann die Nacht des\nBlutmondes stattfindet, und was in\ndieser Nacht wohl geschieht..."
      - control:
          kind: choice
          choice_labels:
            - 3
            - 4
            - 5
          selected_index: 0
          cancel_index: 2
          unknown: 8
  talk09:
    attributes: Obj_DiaryMusician_A_01
    contents:
      - text: "Dies sind alle alten Lieder, von denen\nich erfuhr."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ich werde meine Reise fortsetzen, in der\nHoffnung, diese Lieder eines Tages an\nden Helden unserer Zeit weiterzugeben."
      - control:
          kind: choice
          choice_labels:
            - 4
            - 5
          selected_index: 0
          cancel_index: 1
          unknown: 6
  talk10:
    attributes: Obj_DiaryMusician_A_01
    contents:
      - text: "Die alten Lieder, die ich sammelte...\nEndlich habe ich sie alle an den Helden\nunserer Zeit weitergeben können.\nUnd so ist schließlich das Versprechen\nan meinen alten Meister erfüllt..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ich stelle mir gern vor, dass er sich für\nmich freuen würde. Doch ich habe meine\nFamilie schon zu lange vernachlässigt.\nIch werde nun zum "
      - control:
          kind: set_colour
          colour: blue
      - text: Dorf der Orni
      - control:
          kind: reset_colour
      - text: "\nzurückkehren."
      - control:
          kind: choice
          choice_labels:
            - 4
            - 5
          selected_index: 0
          cancel_index: 1
          unknown: 6
