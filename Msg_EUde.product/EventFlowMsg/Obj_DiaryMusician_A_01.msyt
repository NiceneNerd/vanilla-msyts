  talk00:
    attributes: Obj_DiaryMusician_A_01
    contents:
      - text: "Ich lege hier eine Aufzeichnung meiner\nReisen auf der Suche nach alten Liedern\nnieder. Gezeichnet, "
      - control:
          kind: set_colour
          colour: blue
      - text: Kashiwa
      - control:
          kind: reset_colour
