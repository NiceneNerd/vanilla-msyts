    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 11
      - text: Du leihst mir dein Ohr?
      - control:
          kind: pause
          length: short
      - text: "\nVielen Dank!"
      - control:
          kind: raw
          zero:
            four:
