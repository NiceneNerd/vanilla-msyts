entries:
  Atacked00:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 42
      - text: Ich habe Angst...
  Atacked01:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 42
      - text: "Jetzt... "
      - control:
          kind: pause
          length: short
      - text: Jetzt geht es zu Ende.
      - control:
          kind: pause
          length: short
      - text: "\nIch weiß es..."
  Failed00:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 18
      - text: "Bin ich... "
      - control:
          kind: pause
          length: short
      - text: Bin ich etwa...
      - control:
          kind: pause
          length: long
      - text: "\nAch nein, alles klar..."
      - control:
          kind: pause
          length: short
      - text: " Ich lebe noch."
  Failed01:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 18
      - text: Womit habe ich das verdient?
  Guardian00:
    attributes: Npc_Road_008
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 22
      - text: Lauf weg!
      - control:
          kind: pause
          length: short
      - text: " Bevor der Wächter hier ist!"
  MSG_Futa_00:
    attributes: Npc_Road_008
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 4
      - text: "Ich finde ja, dass man sich nicht nur dem\nWetter, sondern auch seiner Stimmung\nnach anzieht."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Gerade in einer Welt wie dieser ist Mode\nwichtig!"
  MSG_Futa_00_1:
    attributes: Npc_Road_008
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Gerade in einer Welt wie dieser ist Mode\nwichtig!"
      - control:
          kind: pause
          length: longer
      - text: " Bring etwas Farbe in dein Leben\nund das der anderen!"
  MSG_Futa_01:
    attributes: Npc_Road_008
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 6
      - text: "He, du..."
      - control:
          kind: pause
          length: short
      - text: " Kommst du mit Hitze klar?"
      - control:
          kind: pause
          length: short
      - text: "\nUnd wie sieht's mit Kälte aus?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Wenn man es immer angenehm haben\nwill, dann muss man je nach Wetter seine\nKleidung wechseln."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Du hast doch sicher Kleidung zum\nWechseln dabei, oder?"
  MSG_Futa_01_1:
    attributes: Npc_Road_008
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Man muss seine Kleidung anpassen,\nwenn man bei Hitze oder Kälte unterwegs\nist."
      - control:
          kind: pause
          length: short
      - text: " Dann hat man es immer angenehm!"
  MSG_Futa_02:
    attributes: Npc_Road_008
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 6
      - text: "Na, was ist?"
      - control:
          kind: pause
          length: short
      - text: " Soll ich raten?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Du möchtest gern Klamotten kaufen,\nalso willst du wissen, wo "
      - control:
          kind: set_colour
          colour: blue
      - text: Kakariko
      - control:
          kind: reset_colour
      - text: " ist."
      - control:
          kind: pause
          length: short
      - text: "\nHab ich recht?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 1
      - text: Geh einfach von hier aus nach Norden.
      - control:
          kind: pause
          length: short
      - text: "\nNicht ganz billig da, aber die Klamotten\nsind auch wirklich gut!"
  MSG_Futa_02_1:
    attributes: Npc_Road_008
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Wenn du Kleidung brauchst, geh nach\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Kakariko
      - control:
          kind: reset_colour
      - text: " im Norden. "
      - control:
          kind: pause
          length: longer
      - text: "Die haben gute\nSachen dort."
      - control:
          kind: pause
          length: short
      - text: " Gut, aber teuer..."
  MSG_Futa_03:
    attributes: Npc_Road_008
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 4
      - text: He!
      - control:
          kind: pause
          length: short
      - text: " Die Sachen, die du da trägst, sind ja\nmal schick!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Oh, ich?"
      - control:
          kind: pause
          length: short
      - text: "\nIch trage eine Spezialanfertigung!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Warum machen wir uns nicht gegenseitig\nnoch ein paar Komplimente über unsere\nKleidung?"
  MSG_Futa_03_1:
    attributes: Npc_Road_008
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: Ich trage eine Spezialanfertigung!
      - control:
          kind: pause
          length: longer
      - text: "\nAber niemand merkt es..."
  MSG_Futa_04:
    attributes: Npc_Road_008
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 26
      - text: Ki hi hi hi!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Wusstest du, dass es in "
      - control:
          kind: set_colour
          colour: blue
      - text: Hateno
      - control:
          kind: reset_colour
      - text: " eine\nFärberei gibt?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Der Besitzer lacht so schräg wie ich\ngerade..."
      - control:
          kind: pause
          length: short
      - text: " Schau dir das mal an, wenn\ndu etwas färben willst!"
  MSG_Futa_04_1:
    attributes: Npc_Road_008
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 26
      - text: Ki hi hi hi! ♪
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Der Besitzer der Färberei in "
      - control:
          kind: set_colour
          colour: blue
      - text: "Hateno "
      - control:
          kind: reset_colour
      - text: "hat\nso ein melodisches Lachen, man könnte\nglatt ein Lied daraus machen!"
  Near00:
    attributes: Npc_Road_008
    contents:
      - text: Schon wieder Morgen...
  Near01:
    attributes: Npc_Road_008
    contents:
      - text: Ich will neue Klamotten!
  Near02:
    attributes: Npc_Road_008
    contents:
      - text: Was ess ich denn?
  Near03:
    attributes: Npc_Road_008
    contents:
      - text: Meine Augen...
  Near04:
    attributes: Npc_Road_008
    contents:
      - text: Ach nö...
  Rest_00:
    attributes: Npc_Road_008
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Wenn du deinem Pferd schon Sattel und\nZaumzeug anlegst, können die Sachen\nruhig ein bisschen schick sein, oder?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "In Ställen kannst du deine Pferde-\nausrüstung aufpeppen."
      - control:
          kind: pause
          length: short
      - text: " Auch Pferde\nwollen fesch sein!"
  Rest_01:
    attributes: Npc_Road_008
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: "Ich hab da einen Trick, um bei Regen\nnicht nass zu werden..."
      - control:
          kind: pause
          length: long
      - text: " Ich geh einfach\nnicht raus!"
      - control:
          kind: pause
          length: short
      - text: " Hehehe!"
  Rest_02:
    attributes: Npc_Road_008
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 6
      - text: Magst du Regen?
      - control:
          kind: pause
          length: long
      - text: " Niemand mag Regen..."
  RetMSG_Futa_00:
    attributes: Npc_Road_008
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: Du bist anders.
      - control:
          kind: pause
          length: short
      - text: " Was du da trägst, hat\neinfach das gewisse Etwas..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Das bleibt unter uns, aber die Leute am\nStall der Zwillingsberge denken, „Stil“\nist das Ende vom Besen..."
  RetMSG_Futa_00_1:
    attributes: Npc_Road_008
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Es wäre schon toll, wenn die Leute am\nStall der Zwillingsberge mal ein \nModebewusstsein entwickeln könnten..."
  RetMSG_Futa_01:
    attributes: Npc_Road_008
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Deine Accessoires sind ja wirklich der\nWahnsinn, hat dir das schon mal jemand\ngesagt?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Nicht?
      - control:
          kind: pause
          length: short
      - text: " Die Leute haben einfach alle\nweder Ahnung noch Geschmack!"
  RetMSG_Futa_01_1:
    attributes: Npc_Road_008
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Alles, was du trägst, ist aufeinander\nabgestimmt."
      - control:
          kind: pause
          length: longer
      - text: " Andere Reisende scheint\nso etwas überhaupt nicht zu kümmern."
  RetMSG_Futa_02:
    attributes: Npc_Road_008
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "Ach! "
      - control:
          kind: pause
          length: short
      - text: Ich will was Neues!
      - control:
          kind: pause
          length: short
      - text: " Ich brauche\nVeränderung!"
      - control:
          kind: pause
          length: short
      - text: " Ich muss mich umziehen!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Wenn du dieser Straße nach Norden\nfolgst, kommst du nach "
      - control:
          kind: set_colour
          colour: blue
      - text: Kakariko
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Da gibt's Spitzenmode, aber ich hab\nschon so viel Geld ausgegeben..."
  RetMSG_Futa_02_1:
    attributes: Npc_Road_008
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "Vielleicht kaufe ich mir in "
      - control:
          kind: set_colour
          colour: blue
      - text: Kakariko
      - control:
          kind: reset_colour
      - text: " was\nSchickes?"
      - control:
          kind: pause
          length: longer
      - text: " Aber ich sollte wohl besser\nnicht..."
  RetMSG_Futa_03:
    attributes: Npc_Road_008
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 6
      - text: Soll ich dir Modetipps geben?
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Nein?
      - control:
          kind: pause
          length: short
      - text: " Na gut."
  RetMSG_Futa_03_1:
    attributes: Npc_Road_008
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 6
      - text: Du willst Modetipps von mir?
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Da bin ich jetzt tatsächlich überfragt.
  RetMSG_Futa_04:
    attributes: Npc_Road_008
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "Ach, diese Farbe..."
      - control:
          kind: pause
          length: short
      - text: " Langsam kann ich\nsie nicht mehr sehen."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Vielleicht gehe ich mal in die Färberei in\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Hateno
      - control:
          kind: reset_colour
      - text: "..."
  RetMSG_Futa_04_1:
    attributes: Npc_Road_008
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "Ich glaube, ich verpasse dieser Kleidung\nin der Färberei in "
      - control:
          kind: set_colour
          colour: blue
      - text: Hateno
      - control:
          kind: reset_colour
      - text: " mal eine neue\nFarbe!"
  SavedLv1_00:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 11
      - text: Du bist ja ganz schön auf Zack!
      - control:
          kind: pause
          length: long
      - text: " Danke!"
  SavedLv1_01:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 11
      - text: Das hätte sonstwie ausgehen können!
      - control:
          kind: pause
          length: long
      - text: "\nDanke dir!"
  SavedLv2_00:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 11
      - text: "Bin ich froh, dass du vorbeigekommen\nbist!"
      - control:
          kind: pause
          length: short
      - text: " Vielen Dank!"
  SavedLv2_01:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 18
      - text: Ohne dich...
      - control:
          kind: pause
          length: long
      - text: " Nein, ich will gar nicht\ndarüber nachdenken."
  SavedLv3_00:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 11
      - text: Du hast mir das Leben gerettet!
      - control:
          kind: pause
          length: long
      - text: "\nUnd das ist keine Übertreibung!"
  SavedLv3_01:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 18
      - text: "Wenn du nicht gewesen wärst, dann..."
  Thanks00:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 11
      - text: "Hier, ein kleines Dankeschön für meine\nRettung!"
