    attributes: Npc_FarthestIsland002
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ich will frei sein, ohne Einschränkungen."
      - control:
          kind: pause
          length: long
      - text: "\nFrei und sorglos wie der Wind."
      - control:
          kind: pause
          length: short
      - text: " Deswegen\nlausche ich hier der Stimme des Windes."
      - control:
          kind: raw
