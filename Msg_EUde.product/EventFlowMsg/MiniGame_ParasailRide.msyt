    attributes: Npc_FarthestIsland002
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 0
      - text: Das war sicher nicht einfach.
      - control:
          kind: pause
          length: long
      - text: " Du scheinst\nhoch gelegene Orte zu mögen."
      - control:
          kind: pause
          length: short
      - text: " Wie ich."
      - control:
          kind: pause
          length: short
      - text: "\nMan kann den Wind so schön hören."
  Initial_01_00:
    attributes: Npc_FarthestIsland002
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 0
      - text: Da wirst du hier kein Glück haben.
      - control:
          kind: pause
          length: long
      - text: "\nHier gibt's nur dieses seltsame Bauwerk."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Was ich hier will?
      - control:
          kind: pause
          length: short
      - text: " Nun..."
      - control:
          kind: pause
          length: long
      - text: " Ich bin auf der\nSuche nach Freiheit?"
      - control:
          kind: pause
          length: short
      - text: " Ja, so könnte man\ndas sagen."
