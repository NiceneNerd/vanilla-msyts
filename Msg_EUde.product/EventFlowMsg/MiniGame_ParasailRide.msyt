  Explain_00:
    attributes: Npc_FarthestIsland002
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 16
      - text: Der Wind
      - control:
          kind: pause
          length: short
      - text: " weht mal schnell und mal\nlangsam, aber er bewegt sich immer frei."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Und ich denke, der beste Weg, eins mit\nihm zu werden, ist, seine Bewegung\nnachzuahmen."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Dafür habe ich mir eine Übung einfallen\nlassen, bei der man "
      - control:
          kind: set_colour
          colour: red
      - text: Ringe
      - control:
          kind: reset_colour
      - text: " in der Luft\ndurchfliegt."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Es geht darum, so viele "
      - control:
          kind: set_colour
          colour: red
      - text: Ringe
      - control:
          kind: reset_colour
      - text: " wie nur\nmöglich zu durchfliegen."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
