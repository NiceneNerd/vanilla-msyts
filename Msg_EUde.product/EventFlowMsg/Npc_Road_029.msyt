      - control:
          kind: sound
          unknown:
            - 1
            - 4
      - text: Ich bin Karsha vom Stamm der Gerudo!
      - control:
          kind: pause
          length: short
      - text: "\nMerke dir meinen Namen!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Zu zweit wäre die Reise zwischen dem\n"
      - control:
          kind: set_colour
          colour: blue
      - text: "Stall des Waldes "
      - control:
          kind: reset_colour
      - text: "und dem "
      - control:
          kind: set_colour
          colour: blue
      - text: Stall am Berge
      - control:
          kind: reset_colour
      - text: "\nungleich sicherer.\n"
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: "Vielleicht begegne ich ja einem netten\nVooi..."
  RetMSG_Orud_00_1:
    attributes: Npc_Road_029
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: "Zu zweit wäre die Reise zwischen dem\n"
      - control:
          kind: set_colour
          colour: blue
      - text: "Stall des Waldes "
      - control:
