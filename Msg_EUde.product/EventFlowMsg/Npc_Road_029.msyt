    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: "Meine Heimat ist die "
      - control:
          kind: set_colour
          colour: blue
      - text: Gerudo-Wüste
      - control:
          kind: reset_colour
      - text: ",\nweit im Südwesten."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Allerdings beabsichtige ich nicht, dorthin\nzurückzukehren, bevor ich nicht mich\nselbst gefunden habe!"
  Rest_01:
    attributes: Npc_Road_029
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 12
      - text: "Die Hitze an diesem Ort ist selbst mir zu\nstark, andererseits trocknet nasse\nKleidung auch unglaublich schnell."
  Rest_02:
    attributes: Npc_Road_029
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 12
