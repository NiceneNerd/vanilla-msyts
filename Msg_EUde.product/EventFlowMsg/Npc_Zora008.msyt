      - text: " Linlin!"
  talk02:
    attributes: Npc_Zora008
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 2
      - text: "'n Abend,"
      - control:
          kind: pause
          length: short
      - text: " Linlin!"
  talk03:
    attributes: Npc_Zora008
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 11
      - text: "Ja, ohne Zweifel!"
      - control:
          kind: pause
          length: short
      - text: "\nDas ist Linlin, wie er leibt und lebt!"
      - control:
          kind: pause
          length: longer
      - text: "\nLange nicht gesehen, altes Haus!\nHat dich also damals nicht die\n"
      - control:
          kind: set_colour
          colour: red
      - text: Verheerung Ganon
      - control:
          kind: reset_colour
      - text: " erwischt, so\nwie Mipha?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Wo hast du denn dann die ganze Zeit\ngesteckt?"
      - control:
          kind: pause
          length: longer
      - text: " Was hast du getrieben?"
      - control:
          kind: pause
          length: short
      - text: "\n..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 9
            - 19
      - text: "Oh, Entschuldigung..."
      - control:
          kind: pause
          length: short
      - text: "\nIch tobe hier rum wie ein Kind."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "100 Jahre ist das alles her, was?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 4
      - text: "Ich glaube, wir haben uns beide viel zu\nerzählen! "
      - control:
          kind: pause
          length: short
      - text: "Komm doch noch mal vorbei,\nwenn du Zeit hast! "
      - control:
          kind: pause
          length: short
      - text: Ja?
  talk04:
    attributes: Npc_Zora008
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 20
      - text: Was?
      - control:
          kind: pause
          length: short
      - text: " Aber... Linlin!"
      - control:
          kind: pause
          length: longer
      - text: "\nHast du etwa Mipha vergessen?"
  talk05:
    attributes: Npc_Zora008
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 2
      - text: Hallo!
  talk06:
    attributes: Npc_Zora008
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 2
      - text: "Morgen,"
      - control:
          kind: pause
          length: short
      - text: " Linlin!"
  talk07:
    attributes: Npc_Zora008
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 6
      - text: Kann ich dir helfen?
      - control:
          kind: raw
          one:
            eight:
              unknown_1: []
              field_1:
                - 6
                - 0
                - 7
                - 0
                - 8
