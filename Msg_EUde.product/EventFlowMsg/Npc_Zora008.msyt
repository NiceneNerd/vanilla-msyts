  talk13:
    attributes: Npc_Zora008
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 18
      - text: Kein Regen mehr...
      - control:
          kind: pause
          length: longer
      - text: "\nIst "
      - control:
          kind: set_colour
          colour: blue
      - text: Ruta
      - control:
          kind: reset_colour
      - text: " endlich zur Ruhe gekommen?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Oh, entschuldige..."
      - control:
          kind: pause
          length: longer
      - text: " Linlin!"
  talk14:
    attributes: Npc_Zora008
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 20
      - text: "Ah, Linlin!"
      - control:
          kind: pause
          length: short
      - text: "\nHast du den Titanen besänftigt?!"
  talk15:
    attributes: Npc_Zora008
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 11
      - text: Linlin...
      - control:
          kind: pause
          length: longer
      - text: "\nVielen Dank, dass du den Titanen\nbesänftigt hast!"
      - control:
          kind: raw
          one:
            eight:
              unknown_1: []
              field_1:
                - 12
                - 0
                - 7
                - 0
                - 8
                - 0
                - 9
              field_2:
                - 0
                - 0
                - 0
                - 3
  talk16:
    attributes: Npc_Zora008
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 16
      - text: Aber ganz was anderes...
      - control:
          kind: pause
          length: short
      - text: "\nIch vermisse seit einiger Zeit meine\nTochter Fine.\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Sie redete davon, dass sie einen Freund\nfürs Leben finden wolle..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 5
            - 6
      - text: "Ob sie wohl wieder zum "
      - control:
          kind: set_colour
          colour: blue
      - text: Ufer der Träume
      - control:
          kind: reset_colour
      - text: "\nlosgezogen ist?"
  talk17:
    attributes: Npc_Zora008
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 16
      - text: "Ach ja, und..."
      - control:
          kind: pause
          length: short
      - text: " dieser „todesmutige\nAbenteurer“, mit dem meine Tochter\nsich da angefreundet hat..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Er ist ein Hylianer wie du, aber\nansonsten scheint er ein ziemlicher\nWaschlappen zu sein."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 3
            - 12
      - text: "Von wem hat meine Tochter nur diese\nBegeisterung für hylianische Abenteurer\ngeerbt...?"
  talk18:
    attributes: Npc_Zora008
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 11
      - text: "Du hast im Wasserbett geschlafen, oder?"
      - control:
          kind: pause
          length: longer
      - text: "\nUnd, wie hat es sich da gelegen?"
      - control:
          kind: choice
          choice_labels:
            - 10
            - 11
          selected_index: 0
          cancel_index: 2
          unknown: 6
  talk19:
    attributes: Npc_Zora008
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 9
      - text: Das freut mich!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Dann war es ja doch nicht umsonst,"
      - control:
          kind: pause
          length: short
      - text: "\ndass wir extra ein Bett für Hylianer\nangeschafft haben!"
  talk20:
    attributes: Npc_Zora008
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 12
      - text: Oje.
      - control:
          kind: pause
          length: longer
      - text: " Es war vielleicht ein wenig zu kalt\nfür dich..."
  talk21:
    attributes: Npc_Zora008
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 1
      - text: Das hier ist unsere Küche.
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Wir Zoras essen unsere Fische roh, aber\ndie meisten Hylianer sind ja froh, wenn\nsie ihr Essen kochen können..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 4
      - text: "Also bitte schön! "
      - control:
          kind: pause
          length: short
      - text: "Du kannst dir hier nach\nHerzenslust deine Gerichte zubereiten!"
  talk22:
    attributes: Npc_Zora008
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 1
      - text: "Weniger hätte ich vom Recken der\nHylianer auch nicht erwartet!"
  talk23:
    attributes: Npc_Zora008
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 1
      - text: "Da taucht mein Linlin nach 100 Jahren\nwieder auf und besänftigt auch noch\ngleich "
      - control:
          kind: set_colour
          colour: blue
      - text: Ruta
      - control:
          kind: reset_colour
      - text: "..."
      - control:
          kind: pause
          length: short
      - text: " Unglaublich!"
  talk24:
    attributes: Npc_Zora008
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 12
      - text: "Das mit Mipha hat mich damals sehr\nmitgenommen..."
      - control:
          kind: pause
          length: short
      - text: " Aber dich hat es sicher\nam meisten von allen getroffen..."
  talk25:
    attributes: Npc_Zora008
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 1
      - text: "...Da war ich wohl etwas in Gedanken\nversunken."
  talk26:
    attributes: Npc_Zora008
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "Ich kann mich nie so recht entscheiden,\nwas ich kochen soll..."
      - control:
          kind: pause
          length: longer
      - text: " Mal überlegen."
