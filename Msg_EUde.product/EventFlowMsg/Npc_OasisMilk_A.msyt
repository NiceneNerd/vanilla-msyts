          kind: sound
          unknown:
            - 8
            - 0
      - text: "He, du!"
      - control:
          kind: pause
          length: short
      - text: " Siehst du etwa auf uns herab?!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
