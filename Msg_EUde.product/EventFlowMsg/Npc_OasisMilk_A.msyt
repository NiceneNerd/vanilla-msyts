  Talk_02:
    attributes: Npc_OasisMilk_A
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: "Oh, stimmt ja, das Passwort!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Das lautet "
      - control:
          kind: set_colour
