    attributes: Npc_OasisMilk_B
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 11
      - text: Was soll das denn werden?
      - control:
          kind: pause
          length: short
      - text: "\nWillst du für uns tanzen?!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 8
            - 0
      - text: "He, du versperrst uns die ganze Sicht,\nalso Beeilung und runter vom Tisch!"
