            - 0
            - 4
      - text: "Dies hier ist das werte Gemach der\nehrwürdigen "
      - control:
          kind: set_colour
          colour: blue
      - text: Sandrobbe
      - control:
          kind: reset_colour
      - text: " unserer\nverehrten "
      - control:
