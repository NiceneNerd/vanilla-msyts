              field_1: 0
      - text: "Die "
      - control:
          kind: set_colour
          colour: blue
      - text: ehrwürdige Patrizia
      - control:
          kind: reset_colour
      - text: " ist eine ganz\nbesondere "
      - control:
          kind: set_colour
          colour: blue
      - text: Sandrobbe
      - control:
          kind: reset_colour
      - text: ", die auch als\nachtbares Orakel dient."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Im Moment ist sie allerdings mit unserer\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Königin
      - control:
          kind: reset_colour
      - text: " unterwegs."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Wenn du also für eine Weissagung hier\nbist, muss ich dich auf später vertrösten."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound2
          unknown:
            - 2
            - 205
      - text: "Und vergiss nicht, Obst mitzubringen,\nwenn du eine Weissagung hören\nmöchtest."
