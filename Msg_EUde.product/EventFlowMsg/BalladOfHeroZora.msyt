  Npc_ZoraB001_SD_010:
    attributes: Npc_ZoraB001
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 20
      - text: "Prüfung? Oh, ich verstehe, Link!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 1
      - text: "In letzter Zeit ist vieles so, wie es war,\nals meine Schwester "
      - control:
          kind: set_colour
          colour: blue
      - text: "Mipha "
      - control:
          kind: reset_colour
      - text: "sich der\nPrüfung gestellt hat, um "
      - control:
          kind: set_colour
          colour: blue
      - text: Ruta
      - control:
          kind: reset_colour
      - text: " zu steuern."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ich habe "
      - control:
          kind: set_colour
          colour: blue
      - text: Muzu
      - control:
          kind: reset_colour
      - text: " und "
      - control:
          kind: set_colour
          colour: blue
      - text: Tottika
      - control:
          kind: reset_colour
      - text: " in das Gebiet\nöstlich der "
      - control:
          kind: set_colour
          colour: blue
      - text: "Ulra-Schlucht "
      - control:
          kind: reset_colour
      - text: "ausgeschickt,\nin dem ein Teil der Prüfung stattfand."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 7
            - 10
      - text: "Oh, jetzt verstehe ich! Dieses Mal bist\ndu es, der sich der Prüfung stellt."
  Npc_ZoraB001_SD_012:
    attributes: Npc_ZoraB001
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 1
      - text: "Du willst dich der Prüfung stellen,\nstimmt's, Link? Ich weiß, dass du\nes schaffen kannst!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 7
            - 10
      - text: "Ich drück dir die ganze Zeit die Flossen!\nDu bist schließlich der beste Freund,\nden ich jemals hatte!"
  Npc_ZoraB001_SD_013:
    attributes: Npc_ZoraB001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 16
      - text: "Du möchtest etwas über meine\nSchwester "
      - control:
          kind: set_colour
          colour: blue
      - text: "Mipha "
      - control:
          kind: reset_colour
      - text: "erfahren?\nIch musste da neulich an etwas denken..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Vor 100 Jahren gab es einen Schwert-\nkämpfer, der dann und wann Zeit mit\nmeiner Schwester "
      - control:
          kind: set_colour
          colour: blue
      - text: "Mipha "
      - control:
          kind: reset_colour
      - text: verbracht hat.
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 1
      - text: "Ich hatte immer Angst, dass er mir\nmeine Schwester wegnimmt..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 7
            - 10
      - text: "Dieser Schwertkämpfer, das warst\ndu, oder?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Verzeih mir meine Direktheit...
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 1
      - text: "Aber du hättest der Mann meiner\nSchwester sein können..."
      - control:
          kind: pause
          length: long
      - text: "\nMein Schwager..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 7
            - 10
      - text: Oh! Was rede ich denn da?!
      - control:
          kind: pause
          length: long
      - text: " Link,\nbitte vergiss alles, was ich gerade\ngesagt habe!"
  Npc_ZoraB001_SD_020:
    attributes: Npc_ZoraB001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: Gut...
      - control:
          kind: pause
          length: long
      - text: " Du kannst gern jederzeit wieder\nvorbeikommen!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 7
            - 10
      - text: Ich drück dir die ganze Zeit die Flossen!
  Npc_ZoraB001_SD_100:
    attributes: Npc_ZoraB001
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 10
      - text: "Oh, Link! Wie schön, dich zu sehen!"
      - control:
          kind: choice
          choice_labels:
            - 2
            - 0
            - 1
          selected_index: 0
          cancel_index: 2
          unknown: 8
  Npc_ZoraB001_SD_101:
    attributes: Npc_ZoraB001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 11
      - text: "Link! Wenn ich dir noch mit etwas helfen\nkann, dann immer heraus damit!"
      - control:
          kind: choice
          choice_labels:
            - 2
            - 0
            - 1
          selected_index: 0
          cancel_index: 2
          unknown: 8
  Npc_ZoraB001_SD_102:
    attributes: Npc_ZoraB001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Brauchst du noch etwas?
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Npc_ZoraB001_SD_103:
    attributes: Npc_ZoraB001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 11
      - text: "Ah, Link, ich kann dir doch bestimmt\nmit etwas helfen?"
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Npc_ZoraB001_SD_104:
    attributes: Npc_ZoraB001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Möchtest du noch etwas wissen, Link? "
      - control:
          kind: choice
          choice_labels:
            - 2
            - 0
            - 1
          selected_index: 0
          cancel_index: 2
          unknown: 8
