  talk01:
    attributes: Npc_OasisGoron_001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ich war die ganze Zeit ziemlich nervös,\nso ganz allein unter Frauen."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Aber... "
      - control:
          kind: pause
          length: longer
      - text: "selbst mit Stradon hier bin ich\nimmer noch nervös..."
  talk02:
    attributes: Npc_OasisGoron_001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Sawa-Sawa... "
      - control:
          kind: pause
          length: short
      - text: Sawo...
      - control:
          kind: pause
          length: short
      - text: " Äh..."
      - control:
          kind: pause
          length: long
      - text: "\n...Was ich meine, ist: Guten Abend!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "Gerudo-Wörter kann man sich wirklich\nschlecht merken, mit all ihren „wa“s und\n„wo“s..."
  talk03:
    attributes: Npc_OasisGoron_001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Ähm... Sabotta?
      - control:
          kind: pause
          length: short
      - text: "\nNein... Savotta!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "Eine kleine Gerudo hat mir beigebracht,\ndass man beim „v“ fest in die Unterlippe\nbeißen muss, damit es schön vibriert.\nDie war ganz schön streng!"
      - control:
          kind: pause
          length: longer
      - text: "\nIch hör da ehrlich gesagt kaum einen\nUnterschied..."
  talk04:
    attributes: Npc_OasisGoron_001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Äh... "
      - control:
          kind: pause
          length: short
      - text: Savaaq!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Mit diesen ganzen „wa“s und „wo“s der\nGerudo komme ich immer durchein-\nander, aber das hab ich mir gemerkt!"
  talk05:
    attributes: Npc_OasisGoron_001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Savaaq!
      - control:
          kind: pause
          length: short
      - text: "\nSo langsam hab ich's drauf mit den\nBegrüßungen."
  talk06:
    attributes: Npc_OasisGoron_001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Savotta!
      - control:
          kind: pause
          length: short
      - text: "\nEndlich hab ich's mir gemerkt."
  talk07:
    attributes: Npc_OasisGoron_001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Sav'saawa!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Äh, nein...\n„Sav'saaba“ hieß das, mit „ba“ am Ende."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Die spinnen, die Gerudo..."
