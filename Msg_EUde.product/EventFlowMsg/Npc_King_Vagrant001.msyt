    attributes: Npc_King_Vagrant001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 5
      - text: Du bist mir ein Rabauke...
      - control:
          kind: pause
          length: long
      - text: "\nFackeln sind zum Anzünden gedacht."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
