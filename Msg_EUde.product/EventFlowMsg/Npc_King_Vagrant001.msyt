      - control:
          kind: pause
          length: short
      - text: "Hier tummelt sich\nnämlich das ein oder andere Monster!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Vielleicht kannst du sie dir damit vom\nLeib halten."
  Torch05:
    attributes: Npc_King_Vagrant001
