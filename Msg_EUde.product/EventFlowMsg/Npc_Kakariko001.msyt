      - control:
          kind: set_colour
          colour: blue
      - text: Revali
      - control:
          kind: reset_colour
      - text: " von den Orni kontrollierte\n"
      - control:
          kind: set_colour
          colour: red
      - text: Vah Medoh
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: pause
          length: short
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Der Titan von "
      - control:
          kind: set_colour
          colour: blue
      - text: Mipha
      - control:
          kind: reset_colour
      - text: ", der Zora, nannte\nsich "
      - control:
          kind: set_colour
          colour: red
      - text: Vah Ruta
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: pause
          length: short
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Und "
      - control:
          kind: set_colour
          colour: blue
      - text: Urbosa
      - control:
          kind: reset_colour
      - text: " von den Gerudo steuerte\n"
      - control:
