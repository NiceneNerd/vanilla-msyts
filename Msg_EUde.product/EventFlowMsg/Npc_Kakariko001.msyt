    attributes: ""
    contents:
      - text: Jetzt zieh dir etwas an!
  Sayonara00:
    attributes: Npc_Kakariko001
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 24
      - text: "Hm... "
      - control:
          kind: pause
          length: long
      - text: "Der Link, den ich kannte, hätte\nohne zu zögern alles für sie gegeben."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Aber gut... "
      - control:
          kind: pause
          length: short
      - text: "Denke nach, und wenn du\ndiese Bereitschaft in deinem Herzen\ngefunden hast, komm wieder zu mir."
  Sayonara01:
    attributes: Npc_Kakariko001
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 9
      - text: Ich habe 100 Jahre auf dich gewartet.
      - control:
          kind: pause
          length: short
      - text: "\nDa machen ein paar Tage mehr oder\nweniger nichts aus."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Es ist eine gewichtige Entscheidung.
      - control:
          kind: pause
          length: short
      - text: "\nDenk in aller Ruhe darüber nach."
  SecondTalk01:
    attributes: Npc_Kakariko001
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 16
      - text: Du bist immer noch hier?
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Der "
      - control:
          kind: set_colour
          colour: blue
      - text: Shiekah-Stein
      - control:
          kind: reset_colour
      - text: " ist der einzige Hinweis\nauf "
      - control:
          kind: set_colour
          colour: red
      - text: Ganon
      - control:
          kind: reset_colour
      - text: ","
      - control:
          kind: pause
          length: short
      - text: " den Zelda dir hinterlassen\nhat.\nAber du scheinst seine Kraft noch nicht\nvollständig nutzen zu können..."
  SecondTalk01_ch3:
    attributes: Npc_Kakariko001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Du solltest schleunigst nach "
      - control:
          kind: set_colour
          colour: red
      - text: Hateno
      - control:
          kind: reset_colour
      - text: "\neilen."
      - control:
          kind: choice
          choice_labels:
            - 5
