    attributes: Npc_Kakariko001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Wie dem auch sei...
  FirstTalk00:
    attributes: NPC_CaptionVoice
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Du bist also endlich erwacht, Link..."
      - control:
          kind: pause
