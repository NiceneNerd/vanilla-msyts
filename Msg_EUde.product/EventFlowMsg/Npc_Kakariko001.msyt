  FirstTalk01:
    attributes: NPC_CaptionVoice
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 1
      - control:
          kind: auto_advance
          frames: 120
      - text: Es ist lange her... sehr lang!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: auto_advance
          frames: 222
      - text: "Ein paar Jährchen mehr hab ich \nwohl auf dem Buckel. Ich hoffe,\ndu erkennst mich noch?"
  FirstTalk02:
    attributes: Npc_Kakariko001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Die Prinzessin hat alles riskiert, um dir\ndiese Botschaft zu hinterlassen."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Wenn du nicht fest entschlossen bist,\nkann ich sie dir nicht übermitteln."
      - control:
          kind: pause
          length: short
      - text: "\nBist du bereit, für sie zu sterben?"
      - control:
          kind: choice
          choice_labels:
            - 0
            - 2
          selected_index: 0
          cancel_index: 1
          unknown: 6
  FirstTalk05:
    attributes: Npc_Kakariko001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: Gut...
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Da du dein Gedächtnis verloren hast,"
      - control:
          kind: pause
          length: short
      - text: "\nsollte ich dir wohl alles von Anfang an\nerzählen..."
  FirstTalk06:
    attributes: Npc_Kakariko001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Gut...
      - control:
          kind: pause
          length: longer
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Da du dein Gedächtnis verloren hast,"
      - control:
          kind: pause
          length: short
      - text: "\nwerde ich es dir so oft erzählen, wie du\nmöchtest..."
      - control:
          kind: pause
          length: longer
  FirstTalk07:
    attributes: Npc_Kakariko001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: 100 Jahre ist es nun her...
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Um uns auf die prophezeite Wiederkehr\n"
      - control:
          kind: set_colour
          colour: red
      - text: "Ganons "
      - control:
          kind: reset_colour
      - text: "vorzubereiten, folgten wir dem\nVorbild der 10 000 Jahre alten Legende."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "Doch das Ergebnis war nicht so, wie wir\nes uns erhofft hatten..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Wir hatten seine Macht unterschätzt.
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Um diesen Fehler nicht zu wiederholen,\n"
      - control:
          kind: pause
          length: short
      - text: "hinterließ die Prinzessin dir die\nfolgenden Worte..."
  FirstTalk08:
    attributes: Npc_Kakariko001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 16
      - text: 100 Jahre ist es nun her...
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Um uns auf die prophezeite Wiederkehr\n"
      - control:
          kind: set_colour
          colour: red
      - text: Ganons
      - control:
          kind: reset_colour
      - text: " vorzubereiten, folgten wir dem\nVorbild der 10 000 Jahre alten Legende.\n"
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "Doch das Ergebnis war nicht so, wie wir\nes uns erhofft hatten..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Wir hatten seine Macht unterschätzt.
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Um diesen Fehler nicht zu wiederholen,\n"
      - control:
          kind: pause
          length: short
      - text: "legte die "
      - control:
          kind: set_colour
          colour: blue
      - text: "Prinzessin "
      - control:
          kind: reset_colour
      - text: "die Zukunft von\nHyrule in deine Hände."
  FirstTalk_consecutive:
    attributes: Npc_Kakariko001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 20
      - text: "...Was ist?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 10
            - 1
      - text: "Hat der lange Schlaf dich deiner\nEntschlossenheit beraubt?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 1
      - text: "Du bist gerade erst aufgewacht...\nIch verlange nicht von dir, dass du gleich\njetzt den Kampf mit "
      - control:
          kind: set_colour
          colour: red
      - text: Ganon
      - control:
          kind: reset_colour
      - text: " aufnimmst.\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Zuerst solltest du deinen beschädigten\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Shiekah-Stein
      - control:
          kind: reset_colour
      - text: " reparieren."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Eile sobald wie möglich zum "
      - control:
          kind: set_colour
          colour: red
      - text: "Institut\nfür Antikenforschung in Hateno"
      - control:
          kind: reset_colour
      - text: "!"
      - control:
          kind: choice
          choice_labels:
            - 5
            - 8
            - 13
            - 6
          selected_index: 0
          cancel_index: 3
          unknown: 10
  Give_ChampionShirt_00:
    attributes: Npc_Kakariko001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Dadurch, dass du die Erinnerung der\nPrinzessin miterlebt hast, "
      - control:
          kind: pause
          length: short
      - text: "ist auch ein\nStück deiner Vergangenheit erwacht.\nBevor ich es vergesse, sollte ich dir\ndas hier geben."
  Give_ChampionShirt_10:
    attributes: Npc_Kakariko001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Das ist dein Gewand. "
      - control:
          kind: pause
          length: short
      - text: "Die Prinzessin\nhatte es meiner Obhut anvertraut."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Du hast es einst erhalten, als du ein\nRecke geworden bist."
      - control:
          kind: pause
          length: short
      - text: " Geh sorgsam\ndamit um!"
  Kotoba00:
    attributes: Npc_Kakariko001
    contents:
      - control:
          kind: sound
