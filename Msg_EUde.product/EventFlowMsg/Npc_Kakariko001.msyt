    attributes: Npc_Kakariko001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Die "
      - control:
          kind: set_colour
          colour: red
      - text: vier Titanen
      - control:
          kind: reset_colour
      - text: "... So nennen wir die\nantiken Kriegsmaschinen der Shiekah."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Sie wurden von den vier Recken\ngesteuert, bevor sie Ganon zum Opfer\nfielen.\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Daruk
      - control:
