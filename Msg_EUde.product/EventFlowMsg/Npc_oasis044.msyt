          unknown:
            - 0
            - 29
      - text: "Sav'saaba!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Das hier ist die Stadt, die niemals\nschläft. "
      - control:
          kind: pause
          length: short
      - text: "Unser Geschäft hat rund um\ndie Uhr geöffnet!\nDeshalb komme ich leider kaum nach\nHause zu meinem Vooi..."
      - control:
          kind: choice
          choice_labels:
            - 1
            - 5
            - 2
          selected_index: 0
          cancel_index: 2
          unknown: 8
  Talk_11:
    attributes: Npc_oasis044
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Savaaq!
      - control:
          kind: pause
          length: short
      - text: "\nWenn man gutes Essen kochen will,\nbraucht man die richtigen Zutaten.\nBei uns kümmert sich zum Glück mein\nVooi darum!"
      - control:
          kind: choice
          choice_labels:
            - 1
            - 5
            - 2
          selected_index: 0
          cancel_index: 2
          unknown: 8
  Talk_12:
    attributes: Npc_oasis044
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Hast du es etwa auf Pröbchen\nabgesehen?"
      - control:
          kind: pause
          length: longer
      - text: " Tut mir leid, so was\nmachen wir hier nicht.\nAber keine Angst, hier schmeckt alles\ngut!"
      - control:
          kind: choice
          choice_labels:
            - 1
            - 5
            - 2
          selected_index: 0
          cancel_index: 2
          unknown: 8
  Talk_13:
    attributes: Npc_oasis044
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Ich bin frisch verheiratet, ob du's\nglaubst oder nicht!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Die meisten Vaai, die so einen Stand\nhaben, sind unter der Haube."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Verheiratete Vaai leben außerhalb der\nStadt und kommen ab und zu zurück,\num so etwas Geld zu verdienen.\n"
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Ich sehe zwar meinen Vooi wenig, aber\nGeld ist nun mal leider zum Leben nötig."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Du kannst mir gerne helfen, indem du\nviel bei mir kaufst oder verkaufst!"
      - control:
          kind: choice
          choice_labels:
            - 1
            - 5
            - 2
          selected_index: 0
          cancel_index: 2
          unknown: 8
  Talk_14:
    attributes: Npc_oasis044
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: Sarq-sarq!
      - control:
          kind: pause
          length: short
      - text: "\nMein Vooi hat die geerntet!"
      - control:
          kind: pause
          length: short
      - text: "\nSehr lecker!"
  Talk_15:
    attributes: Npc_oasis044
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: Sarq-sarq!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Wenn du kochen willst, kannst du gern\nden Gemeinschaftstopf da benutzen. Das\nFeuer musst du aber selbst entzünden!"
  Talk_16:
    attributes: Npc_oasis044
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: Sarq-sarq!
      - control:
          kind: pause
          length: short
      - text: "\nIch muss meinem Vooi sagen, dass\ner für Nachschub sorgen muss!"
  Talk_17:
    attributes: Npc_oasis044
    contents:
      - control:
          kind: sound
          unknown:
            - 0
