  Talk_01:
    attributes: Npc_oasis044
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 27
      - text: "Savotta!\nHast du schon gefrühstückt?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Die anderen Völker sind ganz\nkleinwüchsig, egal ob Vooi oder Vaai."
      - control:
          kind: pause
          length: short
      - text: "\nAuch auf meinen Vooi trifft das zu."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ich mache mir immer Sorgen, dass\ner nicht genug isst!"
      - control:
          kind: choice
          choice_labels:
            - 1
            - 5
            - 2
          selected_index: 0
          cancel_index: 2
          unknown: 8
  Talk_02:
    attributes: Npc_oasis044
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: "Ich bin hier, wenn du mich brauchst!"
  Talk_03:
    attributes: Npc_oasis044
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Wir brauchen keinen Schlaf, aber die\nTemperaturschwankungen in der Wüste\nsind extrem...\nWenn dich das mitnimmt, dann solltest\ndu dich gut ausruhen!"
  Talk_05:
    attributes: Npc_oasis044
    contents:
