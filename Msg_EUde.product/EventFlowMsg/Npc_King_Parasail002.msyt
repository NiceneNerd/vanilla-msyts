    contents:
      - text: "..."
  "0002":
    attributes: ""
    contents:
      - text: Ich weiß es nicht.
  "0003":
    attributes: ""
    contents:
      - text: "..."
  "0004":
    attributes: ""
    contents:
      - text: Ja!
  "0005":
    attributes: ""
    contents:
      - text: "..."
  "0006":
    attributes: ""
    contents:
      - text: Gib es mir!
  "0007":
    attributes: ""
    contents:
      - text: Parasegel?
  "1000":
    attributes: ""
    contents:
      - text: Wer bist du?
  "1001":
    attributes: ""
    contents:
      - text: Wiedersehen.
  Anataha00:
    attributes: Npc_King_Parasail002
    contents:
      - control:
          kind: sound2
          unknown:
            - 4
            - 205
      - text: "Ach, jetzt plötzlich interessiert dich,\nwer ich bin?\n\nIch bin..."
      - control:
          kind: pause
          length: long
      - text: " ein alter Sonderling, der schon\nseit langer Zeit allein hier in der\nAbgeschiedenheit lebt."
  Appear01:
    attributes: Npc_King_Parasail002
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Junge, Junge..."
      - control:
          kind: pause
          length: long
      - text: "\nDas hätte ich jetzt nicht erwartet."
  Appear02:
    attributes: Npc_King_Parasail002
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 13
      - text: "Ich habe gesehen, wie überall diese\nTürme aus dem Boden geschossen sind.\n\nDas muss heißen..."
      - control:
          kind: pause
          length: long
      - text: " Die Kraft, die unter\nHyrule schlief, wurde erweckt."
  Appear03:
    attributes: Npc_King_Parasail002
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Du warst gerade auf dem Turm, oder?"
      - control:
          kind: pause
          length: long
      - text: "\nWas gab's denn dort oben?"
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Appear04:
    attributes: Npc_King_Parasail002
    contents:
      - control:
          kind: sound2
          unknown:
            - 9
            - 205
      - text: "...Wirklich?"
      - control:
          kind: pause
          length: long
      - text: " Eine Stimme aus Richtung\ndes Schlosses?"
  Appear05:
    attributes: Npc_King_Parasail002
    contents:
      - control:
          kind: sound2
          unknown:
            - 2
            - 205
      - text: "Du brauchst es nicht zu verheimlichen...\n\n\nIch habe alles ganz genau beobachtet,\nwas sich dort oben abgespielt hat."
  Appear06:
    attributes: Npc_King_Parasail002
    contents:
      - text: "Ich bin mir recht sicher, "
      - control:
          kind: pause
          length: long
      - text: "dass du eine\nStimme aus Richtung des Schlosses\nvernommen hast. Nicht wahr?"
  Appear07:
    attributes: Npc_King_Parasail002
    contents:
      - control:
          kind: sound2
          unknown:
            - 7
            - 205
      - text: "Volltreffer, was?"
  Appear08:
    attributes: Npc_King_Parasail002
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Hast du nicht eine Vermutung, wessen\nStimme das gewesen sein könnte?"
      - control:
          kind: choice
          choice_labels:
            - 2
            - 3
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Appear09:
    attributes: Npc_King_Parasail002
    contents:
      - control:
          kind: sound2
          unknown:
            - 11
            - 205
      - text: Ich verstehe schon...
      - control:
          kind: pause
          length: long
      - text: "\nDas ist bedauerlich."
  Appear10:
    attributes: Npc_King_Parasail002
    contents:
      - control:
          kind: sound2
          unknown:
            - 11
            - 205
      - text: "Mit einem Wildfremden wie mir möchtest\ndu nicht darüber sprechen, was?"
      - control:
          kind: pause
          length: long
      - text: "\nNun gut..."
  Appear11:
    attributes: Npc_King_Parasail002
    contents:
      - text: "Aber dieses unheilvolle Wabern rund\num das Schloss ist dir sicher nicht\nentgangen, oder?"
  Appear12:
    attributes: Npc_King_Parasail002
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 5
      - text: Wir nennen es...
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: text_size
          percent: 125
      - text: "Die "
      - control:
          kind: set_colour
          colour: red
      - text: Verheerung Ganon
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: text_size
          percent: 100
      - text: Es ist nun 100 Jahre her...
      - control:
          kind: pause
          length: long
      - text: " Da hat dieses\nUnheil das Reich "
      - control:
          kind: set_colour
          colour: blue
      - text: Hyrule
      - control:
          kind: reset_colour
      - text: " zerstört, das\nsich einst hier befand.\nEs tauchte wie aus dem Nichts auf,\nzerstörte die Städte und kostete einen\nGroßteil des Volkes das Leben.\nUnd seit 100 Jahren verweilt es nun\ndort...\n\nEs hängt wie ein Geschwür an "
      - control:
          kind: set_colour
          colour: blue
      - text: "Schloss\nHyrule"
      - control:
          kind: reset_colour
      - text: ", einst stolzes Wahrzeichen des\nKönigreiches."
  Appear13:
    attributes: Npc_King_Parasail002
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: Und du...
      - control:
          kind: pause
          length: long
      - text: " Du verspürst den Drang,\nzum Schloss zu gehen. "
      - control:
          kind: pause
          length: long
      - text: Habe ich recht?
      - control:
          kind: choice
          choice_labels:
            - 4
            - 5
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Appear14:
    attributes: Npc_King_Parasail002
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 1
      - control:
          kind: animation
          name: CustomLaugh
      - text: Das dachte ich mir.
  Appear15:
    attributes: Npc_King_Parasail002
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 1
      - control:
          kind: animation
          name: CustomLaugh
      - text: "...Du willst zum Schloss."
      - control:
          kind: pause
          length: long
      - text: " Auch wenn du\nschweigst, deine Augen verraten es mir."
  Appear16:
    attributes: Npc_King_Parasail002
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: Leider wird da so schnell nichts draus.
      - control:
          kind: pause
          length: long
      - text: "\nDas Plateau, auf dem wir uns befinden,\nist von jähen Abgründen umgeben."
  Appear17:
    attributes: Npc_King_Parasail002
    contents:
      - text: "Jeder, der dort hinunterfällt,"
      - control:
          kind: pause
          length: long
      - text: " ist verloren."
  Appear18:
    attributes: Npc_King_Parasail002
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Wenn man allerdings wie ich ein\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Parasegel
      - control:
          kind: reset_colour
      - text: " besitzt, sieht die Sache\nschon anders aus..."
      - control:
          kind: choice
          choice_labels:
            - 6
            - 7
          selected_index: 0
          cancel_index: 2
          unknown: 6
  Appear19:
    attributes: Npc_King_Parasail002
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 10
      - text: Ho ho ho...
      - control:
          kind: pause
          length: long
      - text: "\nNa gut.\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: Aber...
      - control:
          kind: pause
          length: long
      - text: " Nur unter einer Bedingung."
  Appear20:
    attributes: Npc_King_Parasail002
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 10
      - text: Ho ho ho...
      - control:
          kind: pause
          length: long
      - text: "\nHast du es eben nicht gesehen, als ich\nhier gelandet bin?\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: Ich könnte es dir geben...
      - control:
          kind: pause
          length: long
      - text: "\nAber nur unter einer Bedingung."
  Appear21:
    attributes: Npc_King_Parasail002
    contents:
      - control:
          kind: sound2
          unknown:
            - 14
            - 205
      - text: "Ich will, dass du einen gewissen\nSchatz für mich besorgst..."
  Appear22:
    attributes: Npc_King_Parasail002
    contents:
      - control:
          kind: sound2
          unknown:
            - 0
            - 205
      - text: Folge mir.
  ClearTerrorEnemy:
    attributes: ""
    contents:
      - text: Puh...
  GoodBye00:
    attributes: Npc_King_Parasail002
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Du willst mein "
      - control:
          kind: set_colour
          colour: blue
      - text: Parasegel
      - control:
          kind: reset_colour
      - text: " haben, oder\nnicht?"
      - control:
          kind: pause
          length: long
      - text: " Dann bin ich mal gespannt, was\nfür einen Schatz du mir bringst."
  GoodBye01:
    attributes: Npc_King_Parasail002
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Ich kann es kaum erwarten, ihn in\nmeinen Händen zu halten..."
  NakedPlayer:
    attributes: ""
