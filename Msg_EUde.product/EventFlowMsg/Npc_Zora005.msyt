  talk00:
    attributes: Npc_Zora005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Zozo, ein Besucher."
      - control:
          kind: pause
          length: longer
      - text: " Wie schön, dass\ndich dein Weg in das "
      - control:
          kind: set_colour
          colour: blue
      - text: Dorf der Zoras
      - control:
          kind: reset_colour
      - text: "\ngeführt hat.\n..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 10
            - 0
      - text: "Du... Du bist doch... "
      - control:
          kind: pause
          length: longer
      - text: Link!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 6
            - 1
      - text: "Der Hylianer, der vor 100 Jahren Mipha\nzwang, ihm in die Schlacht zu folgen, wo\nsie der "
      - control:
          kind: set_colour
          colour: red
      - text: Verheerung Ganon
      - control:
          kind: reset_colour
      - text: " anheim fiel.\nDein Gesicht..."
      - control:
          kind: pause
          length: short
      - text: " Als könnte ich es jemals\nvergessen..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Du zolltest wissen: "
      - control:
          kind: pause
          length: short
      - text: "Die Alten, die Mipha\nverehren, sprechen deinen Namen mit\nAbscheu aus.\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 16
      - text: "...Doch ich bin der Meinung, man zoll die\nVergangenheit auf sich beruhen lassen."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "...Link..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ich bin nicht länger Priester und lebe\nmein Leben nun in geruhsamer\nZurückgezogenheit."
  talk01:
    attributes: Npc_Zora005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Stattfinden zoll das Ganze...
      - control:
          kind: pause
          length: short
      - text: " Hmm..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Im Dorf "
      - control:
          kind: set_colour
          colour: blue
      - text: Taburasa
      - control:
          kind: reset_colour
      - text: " in "
      - control:
          kind: set_colour
          colour: blue
      - text: Akkala
      - control:
          kind: reset_colour
      - text: "..."
  talk02:
    attributes: Npc_Zora005
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 0
      - text: Das wollte ich hören.
      - control:
          kind: pause
          length: short
      - text: "\nUnd jetzt verrat mir die Details..."
  talk03:
    attributes: Npc_Zora005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Bis zum Dorf "
      - control:
          kind: set_colour
          colour: blue
      - text: Taburasa
      - control:
          kind: reset_colour
      - text: " in "
      - control:
          kind: set_colour
          colour: blue
      - text: Akkala
      - control:
          kind: reset_colour
      - text: " ist\nes nicht weit."
      - control:
          kind: pause
          length: short
      - text: " Wenn ich mich alzo\nbeeile..."
      - control:
          kind: choice
          choice_labels:
            - 2
            - 3
          selected_index: 0
          cancel_index: 2
          unknown: 6
  talk04:
    attributes: Npc_Zora005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 9
      - text: Mach dir keine Zorgen um mich.
      - control:
          kind: pause
          length: longer
      - text: " Ich\nhabe Vertrauen in meine Füße und\nFlossen."
  talk05:
    attributes: Npc_Zora005
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 1
      - text: "Es ist schon eine Weile her, dass ich\netwas zu tun hatte."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Du zolltest auch kommen! "
      - control:
          kind: pause
          length: short
      - text: "Ich treffe in\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Taburasa
      - control:
          kind: reset_colour
      - text: " schon mal alle Vorbereitungen."
  talk06:
    attributes: Npc_Zora005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ich bin entschlossen, ein letztes Mal\nmeine Berufung als Priester zu erfüllen."
  talk07:
    attributes: Npc_Zora005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ich bin entschlossen, ein letztes Mal\nmeine Berufung als Priester zu erfüllen."
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
          selected_index: 0
          cancel_index: 1
          unknown: 6
  talk15:
    attributes: Npc_Zora005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Aber seitdem ich aufgehört habe, ist\njeder Tag langweilig..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ich sitze Tag für Tag tatenlos herum,\nbis ich diese Welt dereinst verlasse..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Link, sag mir..."
      - control:
          kind: pause
          length: short
      - text: "\nWas ist der Sinn des Ganzen?"
  talk16:
    attributes: Npc_Zora005
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 10
      - text: Pah ha ha...
      - control:
          kind: pause
          length: short
      - text: " Ich kümmere mich darum."
  talk17:
    attributes: Npc_Zora005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Möge die Göttin Hylia deinen Weg mit\nihrem Wohlwollen begleiten..."
  talk18:
    attributes: Npc_Zora005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Wenn du auf deinen Reisen ein "
      - control:
          kind: set_colour
          colour: red
      - text: "verlobtes\nPaar"
      - control:
          kind: reset_colour
      - text: " triffst, lasse es mich wissen."
  talk20:
    attributes: Npc_Zora005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Link...
      - control:
          kind: pause
          length: longer
      - text: "\nWie wäre es mit einer Übernachtung\nund einer Mahlzeit?"
      - control:
          kind: choice
          choice_labels:
            - 6
            - 1
          selected_index: 0
          cancel_index: 1
          unknown: 6
  talk21:
    attributes: Npc_Zora005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Dank der Gnade der Göttin Hylia kann\nich dir eine Übernachtung anbieten."
      - control:
          kind: pause
          length: longer
      - text: "\nWann zoll ich dich wecken?"
      - control:
          kind: choice
          choice_labels:
            - 11
            - 12
            - 13
            - 14
          selected_index: 0
          cancel_index: 3
          unknown: 10
  talk22:
    attributes: Npc_Zora005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: In Ordnung.
      - control:
          kind: pause
          length: short
      - text: " Dann ruh dich jetzt aus."
  talk23:
    attributes: Npc_Zora005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Gut geschlafen?
  talk24:
    attributes: Npc_Zora005
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "Oooh... "
      - control:
          kind: pause
          length: short
      - text: Göttin Hylia...
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Sprich dein göttliches Urteil über dein\nSchäfchen auf dem Tisch..."
  talk25:
    attributes: Npc_Zora005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Ich habe etwas gehört...
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Dumsda und Pauda planen eine kleine\nZeremonie?"
  talk26:
    attributes: Npc_Zora005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ich werde die Zeremonie von Dumsda\nund Pauda leiten."
  talk27:
    attributes: Npc_Zora005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Aber ein wichtiger Gast scheint noch zu\nfehlen."
      - control:
          kind: pause
          length: longer
      - text: " Vielleicht zolltest du einmal mit\nDumsda sprechen."
  talk28:
    attributes: Npc_Zora005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Was für eine schöne Zeremonie!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ich war jetzt länger nicht an zo etwas\nbeteiligt, alzo war meine Stimme vor\nAufregung vielleicht etwas schrill..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Aber Dumsda und Pauda sind füreinander\nbestimmt."
      - control:
          kind: pause
          length: short
      - text: " Die Göttin Hylia selbst muss\nsie zusammengeführt haben."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Mögest auch du irgendwann deinen\nSeelenverwandten finden..."
  talk29:
    attributes: Npc_Zora005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ach ja, Dumsda wollte mit dir reden."
      - control:
          kind: pause
          length: short
      - text: "\nSei doch bitte zo gut, ja?"
  talk30:
    attributes: Npc_Zora005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Hmm. Link...
  talk31:
    attributes: Npc_Zora005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ich kann zogleich mit der Zeremonie\nbeginnen, wenn du willst. Sprich mit\nDumsda, wenn du zo weit bist.\nWenn du dich allerdings hier ausruhen\nmöchtest, sprich am Schalter mit mir."
  talk33:
    attributes: Npc_Zora005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Die Vorbereitungen für die Zeremonie\nsind abgeschlossen. Sprich mit Dumsda,\nzobald es losgehen kann."
  talk34:
    attributes: Npc_Zora005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Oh Schäfchen, das vom Schicksal\nhergeführt wurde..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ich kann dir eine Übernachtung und eine\nMahlzeit anbieten."
      - control:
          kind: pause
          length: short
      - text: " Sprich mich an der\nRezeption an."
  talk35:
    attributes: Npc_Zora005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Möchtest du dich hier ausruhen?
      - control:
          kind: pause
          length: longer
      - text: "\nDann sprich mich an der Rezeption an."
