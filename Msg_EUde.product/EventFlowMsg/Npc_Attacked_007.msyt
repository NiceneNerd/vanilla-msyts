entries:
  After00:
    attributes: Npc_Attacked_007
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 35
      - text: "Alles klar, dann auf nach Gerudo-Stadt!\nDa erwartet mich meine zukünftige Frau!\nKeine Sorge, Liebes, bin auf dem Weg!"
  Atacked00:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 13
      - text: "Was suchen diese Monster hier\nüberhaupt?!"
      - control:
          kind: pause
          length: short
      - text: " Ob das an den komischen\nVorfällen in letzter Zeit liegt?"
  Atacked01:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 13
      - text: Uff... Das war haarscharf.
  Failed00:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 13
      - text: "Dabei haben doch alle gesagt, dass in\ndieser Wüste nicht mal Monster über-\nleben können..."
      - control:
          kind: pause
          length: short
      - text: " Aber die sind überall!\nMuss an diesen komischen Vorfällen\nin letzter Zeit liegen..."
  Failed01:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: Aaah... Aua... Was für ein Schlamassel.
      - control:
          kind: pause
          length: short
      - text: "\nDiese Wüste ist ja gar nicht verlassen!\nMonster überall!"
  GoodBye00:
    attributes: Npc_Attacked_007
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: "Wir befinden uns auf dem Weg nach\nGerudo-Stadt."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Sieh uns nur an! Wir sind zwei echt heiße\nTypen, oder? Die Damen werden uns\nunwiderstehlich finden.\n"
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: "Tut mir ja leid für dich, Kumpel, aber mit\ndiesem Mondgesicht wirst du nicht\nbesonders gut ankommen..."
  Guardian00:
    attributes: Npc_Attacked_007
    contents:
      - control:
          kind: sound
          unknown:
            - 2
            - 32
      - text: Schnell! Renn! Da kommt ein Wächter!
  Meeting00:
    attributes: Npc_Attacked_007
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 0
      - text: "Was?! Ich dachte, DU kennst den Weg!"
  Meeting02:
    attributes: Npc_Attacked_007
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: Wir reisen in die Stadt der Gerudo.
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 3
            - 1
      - text: "Na gut, wenn wir uns hier in der Wüste\nverlaufen, wird daraus wohl nichts..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 1
      - text: "Wenn wir es bis dahin schaffen, wird sich\nunser Leben auf einen Schlag ändern!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Zum Glück kennt Dillie den Weg,\nalso kann da gar nichts schiefgehen!"
  Meeting03:
    attributes: Npc_Attacked_002
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 19
      - text: "Ähm... Keine Ahnung, wo das sein soll.\nDu bist doch derjenige, der den Weg\nkennt?!"
  Near00:
    attributes: Npc_Attacked_007
    contents:
      - control:
          kind: sound2
          unknown:
            - 15
            - 205
      - text: Wo ist nur Gerudo-Stadt...
  PairAtacked00:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 12
      - text: "Mein Kumpel kämpft noch...\ngegen die Monster..."
  PairAtacked01:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 2
            - 14
      - text: "Mir geht's gut, hilf Dillie!"
  PairFailed00:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 12
      - text: Dillie... Lass mich nicht im Stich...
  PairFailed01:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 12
      - text: "Nein, nicht Dillie...\nWie konnte das nur geschehen..."
  SavedLv1_00:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 4
      - text: "Mein lieber Mann, du hast ja einen\nordentlichen Schlag drauf!"
      - control:
          kind: pause
          length: short
      - text: " Hier, ein\nkleines Dankeschön."
  SavedLv1_01:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 4
      - text: "Gut gemacht, Kumpel."
      - control:
          kind: pause
          length: short
      - text: "\nWir schulden dir was."
  SavedLv2_00:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 11
      - text: "He Kumpel, dir macht keiner was vor."
      - control:
          kind: pause
          length: short
      - text: "\nDu hast uns die Haut gerettet."
  SavedLv2_01:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 11
      - text: "Du hast uns gerettet, Kumpel!\nVielen Dank dafür."
  SavedLv3_00:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: "Ich verdanke dir mein Leben!\nLass mich dir danken!"
  SavedLv3_01:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 23
      - text: "Das war der Wahnsinn!\nWo hast du das nur gelernt?!\nHier, nimm, das hast du dir verdient."
