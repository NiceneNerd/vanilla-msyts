            - 11
            - 12
      - text: "Wir haben's irgendwie überstanden..."
      - control:
          kind: pause
          length: short
      - text: "\nHeute muss unser Glückstag sein..."
  Atacked00:
    attributes: ""
    contents:
      - control:
          kind: sound
