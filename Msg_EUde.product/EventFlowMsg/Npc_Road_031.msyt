    attributes: Npc_Road_031
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 17
      - text: "Im "
      - control:
          kind: set_colour
          colour: blue
      - text: "Stall der Schlucht "
      - control:
          kind: reset_colour
