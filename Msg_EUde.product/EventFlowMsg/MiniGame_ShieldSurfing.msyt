    contents:
      - text: Ja!
  "0001":
    attributes: ""
    contents:
      - text: Nein.
  "0002":
    attributes: ""
    contents:
      - text: Jepp!
  "0003":
    attributes: ""
    contents:
      - text: "Nein, danke."
  "0004":
    attributes: ""
    contents:
      - text: Lieber doch nicht.
  "0005":
    attributes: ""
    contents:
      - text: Passt schon!
  "0006":
    attributes: ""
    contents:
      - text: Ich möchte aufhören.
  "0007":
    attributes: ""
    contents:
      - text: Öhm... nichts!
  "0008":
    attributes: ""
    contents:
      - text: Wirklich aufhören.
  "0009":
    attributes: ""
    contents:
      - text: Doch lieber weiter.
  "0010":
    attributes: ""
    contents:
      - text: Nächster Parcours!
  "0011":
    attributes: ""
    contents:
      - text: Ich passe.
  "0012":
    attributes: ""
    contents:
      - text: Es ist das Leben!
  "0013":
    attributes: ""
    contents:
      - text: Keine Ahnung!
  "0014":
    attributes: ""
    contents:
      - text: Fortgeschrittene!
  "0015":
    attributes: ""
    contents:
      - text: Anfänger!
  "0016":
    attributes: ""
    contents:
      - text: "Halt, stopp!"
  "0018":
    attributes: ""
    contents:
      - text: Tut mir leid...
  "0019":
    attributes: ""
    contents:
      - text: Ach so?
  AboutCourse_Beginner:
    attributes: Npc_IceVillage011
    contents:
      - control:
          kind: sound2
          unknown:
            - 7
            - 205
      - text: "Von hier aus immer den Berg runter\nliegt eine große Brücke."
      - control:
          kind: pause
          length: long
      - text: " Da schlitterst\ndu für den Anfang einmal hin.\nWirklich einfach, nicht wahr? Der ideale\nParcours, um an deiner Technik zu feilen."
  AboutGame_00:
    attributes: Npc_IceVillage011
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 19
      - text: "Sieh mal einer an, du bist wohl eher ein\ngewissenhafter Typ. "
      - control:
          kind: pause
          length: short
      - text: Wie unerwartet.
      - control:
          kind: pause
          length: short
      - text: "\nDann lass mich erklären.\nIm Wesentlichen geht es darum, wie\nschnell du zum Ziel schlitterst."
      - control:
          kind: pause
          length: short
      - text: " Ein\nWettrennen gegen die Zeit!\nWie du schildschlitterst, weißt du aber,\noder?"
      - control:
          kind: pause
          length: short
      - text: " Halte deinen Schild vor dich und\nhüpfe aus dem Sprung heraus drauf!\nDu musst auch nicht die ganze Zeit auf\ndeinem Schild bleiben..."
      - control:
          kind: pause
          length: short
      - text: " Von mir aus\nkannst du sogar fliegen!"
  AskAboutShieldSurfing_Beginner:
    attributes: Npc_IceVillage011
    contents:
      - text: "Gut... "
      - control:
          kind: pause
          length: short
      - text: "Anstatt dich einer Prüfung zu\nunterziehen, werde ich deine Motivation\neinfach mit einer kurzen Frage testen!\n"
      - control:
          kind: raw
          one:
            one:
              field_1: 4
              field_2: 1056964608
      - text: "Was bedeutet dir "
      - control:
          kind: text_size
          percent: 125
      - text: Schildschlittern
      - control:
          kind: text_size
          percent: 100
      - text: "?"
      - control:
          kind: choice
          choice_labels:
            - 12
            - 13
          selected_index: 0
          cancel_index: 2
          unknown: 6
  AskCourse_Expert:
    attributes: Npc_IceVillage011
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: "Eine andere Antwort hätte ich von\nmeinem Vorzeigeschüler auch nicht\nerwartet!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "An welchem Parcours möchtest du\ndich denn versuchen?"
      - control:
          kind: choice
          choice_labels:
            - 15
            - 14
            - 16
          selected_index: 0
          cancel_index: 2
          unknown: 8
  AskPurpose_Beginner:
    attributes: Npc_IceVillage011
    contents:
      - text: "Du musst extra hergekommen sein, um\nmich, den ehemaligen Schildschlitter-\nProfi, zu treffen, oder?"
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
          selected_index: 0
          cancel_index: 1
          unknown: 6
  AskPurpose_Expert:
    attributes: Npc_IceVillage011
    contents:
      - text: "Also, was gibt's?"
      - control:
          kind: pause
          length: short
      - text: " Bist du gekommen, um\nmir zu zeigen, wie gut du geworden bist?"
      - control:
          kind: choice
          choice_labels:
            - 2
            - 1
          selected_index: 0
          cancel_index: 1
          unknown: 6
  AskRetry_Beginner:
    attributes: Npc_IceVillage011
    contents:
      - text: "Also. "
      - control:
          kind: pause
          length: short
      - text: "Willst du noch einmal schlittern?\nDas kostet wie immer "
      - control:
          kind: set_colour
          colour: red
      - text: 20 Rubine
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: choice
          choice_labels:
            - 2
            - 6
          selected_index: 0
          cancel_index: 1
          unknown: 6
  AskRetry_Expert:
    attributes: Npc_IceVillage011
    contents:
      - text: "Also, willst du mir noch einmal zeigen,\nwas du draufhast?"
      - control:
          kind: pause
          length: short
      - text: "\nWähle einen Parcours!"
      - control:
          kind: choice
          choice_labels:
            - 15
            - 14
            - 6
          selected_index: 0
          cancel_index: 2
          unknown: 8
  AskRupee_Beginner:
    attributes: Npc_IceVillage011
    contents:
      - text: "Mal sehen... Wie klingen "
      - control:
          kind: set_colour
          colour: red
      - text: 20 Rubine
      - control:
          kind: reset_colour
      - text: " pro\nUnterrichtsstunde? Bist du dabei?"
      - control:
          kind: choice
          choice_labels:
            - 2
            - 4
          selected_index: 0
          cancel_index: 1
          unknown: 6
  AskRupee_Expert:
    attributes: Npc_IceVillage011
    contents:
      - text: Alles wie immer.
      - control:
          kind: pause
          length: short
      - text: " "
      - control:
          kind: set_colour
          colour: red
      - text: 20 Rubine
      - control:
          kind: reset_colour
      - text: " Lehrgeld.\nIn Ordnung?"
      - control:
          kind: choice
          choice_labels:
            - 2
            - 3
          selected_index: 0
          cancel_index: 1
          unknown: 6
  AskTraining_Beginner:
    attributes: Npc_IceVillage011
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 4
      - text: "Wusste ich es doch! "
      - control:
          kind: pause
          length: short
      - text: "Aber ich habe mich\naus dem Sport zurückgezogen..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Es ist mittlerweile so lange her, dass\nsie mich die „Königin der Hügel“ genannt\nhaben...\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Aber wenn dich das nicht stört, kann\nich mir ja mal ansehen, ob ich dir noch\nden ein oder anderen Tipp geben kann."
      - control:
          kind: choice
          choice_labels:
            - 2
            - 3
          selected_index: 0
          cancel_index: 1
          unknown: 6
  CautionBigDoor_Expert:
    attributes: Npc_IceVillage011
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 12
      - text: "Eines noch! "
      - control:
          kind: pause
          length: short
      - control:
          kind: set_colour
          colour: red
      - text: Verlasse nicht den Parcours
      - control:
          kind: reset_colour
      - text: ",\nin Ordnung?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Es gibt da ein "
      - control:
          kind: set_colour
          colour: red
      - text: großes Steintor
      - control:
          kind: reset_colour
      - text: ". Wenn du\nvom Parcours abkommst, könntest du da-\nmit zusammenstoßen und dich verletzen."
  ChangeBeginnerToExpert_Expert:
    attributes: Npc_IceVillage011
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 25
      - text: Der Fortgeschrittenen-Parcours...
      - control:
          kind: pause
          length: short
      - text: "\nDer ist zwar länger, aber auch\ninteressanter!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Und wenn du eine gute Zeit schaffst,\ngebe ich dir einen Schild als Belohnung!"
  ChangeBeginnerToExpert_Expert1:
    attributes: Npc_IceVillage011
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 25
      - text: "Gut, dann sehe ich mir jetzt einmal an,\nwie du dich auf dem Fortgeschrittenen-\nParcours schlägst.\nDieser Parcours ist länger als der für\nAnfänger, also gibt gut Acht!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Da ich dir bereits alle Schilde gegeben\nhabe, erhältst du von nun an Rubine –\numso mehr, je schneller du am Ziel bist!"
  ChangeExpertToBeginner_Expert:
    attributes: Npc_IceVillage011
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 25
      - text: "Dann lass mal sehen, wie du dich auf\ndem Anfänger-Parcours schlägst."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ich werde ein Auge darauf haben, wie\ndu die Grundlagen meisterst."
  Check_Money00:
    attributes: Npc_IceVillage011
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Du kannst natürlich jederzeit zum\nAnfänger-Parcours zurückwechseln."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Der Fortgeschrittenen-Parcours kostet\nauch "
      - control:
          kind: set_colour
          colour: red
      - text: 20 Rubine
      - control:
          kind: reset_colour
      - text: ". Wollen wir loslegen?"
      - control:
          kind: choice
          choice_labels:
            - 2
            - 15
            - 16
          selected_index: 0
          cancel_index: 2
          unknown: 8
  Check_Money02:
    attributes: Npc_IceVillage011
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Das kostet natürlich immer noch\n"
      - control:
          kind: set_colour
          colour: red
      - text: 20 Rubine
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: choice
          choice_labels:
            - 2
            - 6
          selected_index: 0
          cancel_index: 1
          unknown: 6
  EntranceExplain_Beginner:
    attributes: Npc_IceVillage011
    contents:
      - text: "Fangen wir erst mal mit einem ganz\neinfachen Parcours an, damit ich deine\nTechnik besser beurteilen kann."
  EntranceTalk_Beginner:
    attributes: Npc_IceVillage011
    contents:
      - text: "Oh, was machst du denn hier draußen\nin den Bergen?"
  EntranceTalk_Beginner_Default:
    attributes: Npc_IceVillage011
    contents:
      - text: Ach!
      - control:
          kind: pause
          length: short
      - text: " Sag bloß..."
  EntranceTalk_Beginner_Default_00:
    attributes: Npc_IceVillage011
    contents:
      - text: "Ich sehe dir beim Schlittern zu, wenn's\ndich nicht stört, dass dir ein ehemaliger\nProfi über die Schulter schaut!"
  EntranceTalk_Beginner_Default_01:
    attributes: Npc_IceVillage011
    contents:
      - text: "Der Spaß kostet dich allerdings jedes Mal\n"
      - control:
          kind: set_colour
          colour: red
      - text: 20 Rubine
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: choice
          choice_labels:
            - 2
            - 4
          selected_index: 0
          cancel_index: 1
          unknown: 6
  EntranceTalk_Beginner_Default_02:
    attributes: Npc_IceVillage011
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Dann bekäme ich jetzt "
      - control:
          kind: set_colour
          colour: red
      - text: 20 Rubine
      - control:
          kind: reset_colour
      - text: "\nUnterrichtsgebühr."
      - control:
          kind: choice
          choice_labels:
            - 2
            - 4
          selected_index: 0
          cancel_index: 1
          unknown: 6
  EntranceTalk_Beginner_SP:
    attributes: Npc_IceVillage011
    contents:
      - text: "Einen solchen Schild sein Eigen zu\nnennen... Das kann nicht jeder von\nsich behaupten!"
  EntranceTalk_Expert_Default:
    attributes: Npc_IceVillage011
    contents:
      - text: "Oh, du bist's. "
      - control:
          kind: pause
          length: long
      - text: Lange nicht gesehen!
      - control:
          kind: pause
          length: long
      - text: "\nGeht es dir gut?"
      - control:
          kind: pause
          length: long
      - text: "\nMir geht's prächtig, wie immer!"
  EntranceTalk_Expert_SP:
    attributes: Npc_IceVillage011
    contents:
      - text: "Als Schildschlitterer hast du dich wirklich\ngemacht..."
      - control:
          kind: pause
          length: long
      - text: " Ich bin wirklich stolz auf dich!"
  EntranceTalk_No_Shield:
    attributes: Npc_IceVillage011
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 1
      - control:
          kind: animation
          name: Notice_00
      - text: "...Was denn?"
      - control:
          kind: pause
          length: long
      - text: "\nDu hast keinen einzigen Schild?!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Beim Schildschlittern sollte man für sich\nselbst sorgen können..."
      - control:
          kind: pause
          length: long
      - text: " Da musst du\nschon allein einen Schild finden."
  Exit_01:
    attributes: Npc_IceVillage011
    contents:
      - text: Das reicht für heute.
      - control:
          kind: pause
          length: short
      - text: " Profi wird man beim\nSchildschlittern nicht auf einen Schlag."
  Exit_02:
    attributes: Npc_IceVillage011
    contents:
      - text: Wir sollten zur Hütte zurückkehren.
      - control:
          kind: pause
          length: short
      - text: "\nDa kannst du dich etwas aufwärmen,\nbevor du wieder losziehst."
  Exit_03:
    attributes: Npc_IceVillage011
    contents:
      - text: "Dann mal bis bald, ja?"
      - control:
          kind: pause
          length: short
      - text: " Ich freue mich\nschon drauf, deine Fortschritte beim\nnächsten Mal zu sehen!"
  ExplainExpertCourseToBeginner_Expert_00:
    attributes: Npc_IceVillage011
    contents:
      - text: "Wenn du auf einem langen Parcours\ndie Orientierung verlierst, ist das gar\nnicht gut.\nBehalte daher immer die Position des\nZiels im Hinterkopf, ja?"
  ExplainExpertCourseToBeginner_Expert_01:
    attributes: Npc_IceVillage011
    contents:
      - control:
          kind: sound2
          unknown:
            - 4
            - 205
      - text: "Zuerst solltest du "
      - control:
          kind: set_colour
          colour: red
      - text: in gerader Linie
      - control:
          kind: reset_colour
      - text: " unter\nder Brücke hindurchschlittern, die das\nZiel beim Anfänger-Parcours war."
  ExplainExpertCourseToBeginner_Expert_02:
    attributes: Npc_IceVillage011
    contents:
      - text: "Wenn du das geschafft hast, legst du\neine "
      - control:
          kind: set_colour
          colour: red
      - text: "großzügige Kurve "
      - control:
          kind: reset_colour
      - text: "ein und schlitterst\n"
      - control:
          kind: set_colour
          colour: red
      - text: längere Zeit geradeaus
      - control:
          kind: reset_colour
      - text: ". Ganz einfach!\nDas Ziel des Fortgeschrittenen-Parcours\nwird durch zwei "
      - control:
          kind: set_colour
          colour: red
      - text: große Flaggen
      - control:
          kind: reset_colour
      - text: " markiert.\nHalte einfach darauf zu.\nSolltest du dich zu weit vom Parcours\nentfernen, sammle ich dich einfach\nwieder ein."
  Failed_00_00:
    attributes: Npc_IceVillage011
    contents:
      - control:
          kind: sound2
          unknown:
            - 10
            - 205
      - text: "Meine Güte, das Ziel erreichst du heute\nwohl nicht mehr, was?"
      - control:
          kind: pause
          length: short
      - text: " Brechen wir ab!"
      - control:
          kind: pause
          length: short
      - text: "\nKomm erst mal wieder zurück!"
  Failed_00_01:
    attributes: Npc_IceVillage011
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Mach dir nichts draus, ja? "
      - control:
          kind: pause
          length: short
      - text: "Das kannst du\nsicher besser!"
      - control:
          kind: pause
          length: short
      - text: " Streng dich an, dann wirst\ndu immer besser!"
  Failed_00_02:
    attributes: Npc_IceVillage011
    contents:
      - text: "Nimm dir doch vor dem Start kurz Zeit,\num zu überlegen, wohin du schlitterst."
  Failed_01_00:
    attributes: Npc_IceVillage011
    contents:
      - control:
          kind: sound2
          unknown:
            - 4
            - 205
      - text: Hm? Wo ist er denn hin?!
      - control:
          kind: pause
          length: short
      - text: "\nIch suche besser mal nach ihm!"
  Failed_01_01:
    attributes: Npc_IceVillage011
    contents:
      - text: Sag mal... Wo wolltest du denn hin?
      - control:
          kind: pause
          length: short
      - text: "\nDu hättest dich hier draußen total\nverirren können!"
      - control:
          kind: choice
          choice_labels:
            - 18
            - 19
          selected_index: 0
          cancel_index: 2
          unknown: 6
  Failed_01_02_00:
    attributes: Npc_IceVillage011
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 18
      - text: "Lass so was einfach gleich bleiben, dann\nmusst du dich auch nicht entschuldigen."
      - control:
          kind: pause
          length: short
      - text: "\nImmerhin wurdest du nicht verletzt."
  Failed_01_02_01:
    attributes: Npc_IceVillage011
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 18
      - control:
          kind: pause
          length: short
      - text: "Wenn du noch länger gebraucht hättest,\nwäre ich hier erfroren!"
      - control:
          kind: pause
          length: short
      - text: " Solche Späße\nsolltest du lieber lassen."
  Failed_02:
    attributes: Npc_IceVillage011
    contents:
      - text: Du scheinst heute müde zu sein...
      - control:
          kind: pause
          length: long
      - text: "\nVielleicht wär's besser, wenn du\nerst mal eine Pause einlegst."
  GoalBeginnerCourse_BadTime_Expert:
    attributes: Npc_IceVillage011
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 6
      - text: "Deine Zeit war "
      - control:
          kind: set_colour
          colour: red
      - control:
          kind: variable
          variable_kind: 14
          name: MiniGame_ShieldSurfing_ResultMinute
      - text: " "
      - control:
          kind: localisation
          localisation_kind: plural
          options:
            - Minute
            - Minuten
            - Minute
      - control:
          kind: reset_colour
      - text: ", "
      - control:
          kind: set_colour
          colour: red
      - control:
          kind: variable
          variable_kind: 14
          name: MiniGame_ShieldSurfing_ResultSecond
      - text: " "
      - control:
          kind: localisation
          localisation_kind: plural
          options:
            - Sekunde
            - Sekunden
            - Sekunde
      - control:
          kind: reset_colour
      - text: "\nund "
      - control:
          kind: set_colour
          colour: red
      - control:
          kind: variable
          variable_kind: 14
          name: MiniGame_ShieldSurfing_ResultMilliSecond
      - text: " Hundertstel"
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ganz ehrlich, ich denke, das bekommst\ndu auch besser hin."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Vielleicht verbessert man sich nur\nlangsam, aber über die Zeit sieht\nman den Fortschritt. "
      - control:
          kind: pause
          length: short
      - text: Nicht aufgeben!
  GoalBeginnerCourse_GoodTime_Expert:
    attributes: Npc_IceVillage011
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: Deine Zeit war also...
      - control:
          kind: pause
          length: short
      - text: " "
      - control:
          kind: set_colour
          colour: red
      - control:
          kind: variable
          variable_kind: 14
          name: MiniGame_ShieldSurfing_ResultMinute
      - text: " "
      - control:
          kind: localisation
          localisation_kind: plural
          options:
            - Minute
            - Minuten
            - Minute
      - control:
          kind: reset_colour
      - text: ",\n"
      - control:
          kind: set_colour
          colour: red
      - control:
          kind: variable
          variable_kind: 14
          name: MiniGame_ShieldSurfing_ResultSecond
      - text: " "
      - control:
          kind: localisation
          localisation_kind: plural
          options:
            - Sekunde
            - Sekunden
            - Sekunde
      - control:
          kind: reset_colour
      - text: " und "
      - control:
          kind: set_colour
          colour: red
      - control:
          kind: variable
          variable_kind: 14
          name: MiniGame_ShieldSurfing_ResultMilliSecond
      - text: " Hundertstel"
      - control:
          kind: reset_colour
      - text: "!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Na siehst du, das war richtig gut!"
      - control:
          kind: pause
          length: short
      - text: "\nGenau deshalb hatte ich dich für den\nFortgeschrittenen-Parcours zugelassen.\nKlar, dass du auf dem Anfänger-Parcours\ndiese Zeiten hinlegst!"
  Goal_00:
    attributes: Npc_IceVillage011
    contents:
      - control:
          kind: sound2
          unknown:
            - 1
