          two_hundred_one:
            dynamic:
              - 0
              - len: 4
                field_2:
                  - 1
                  - 0
                  - 0
                  - 0
      - text: Schatzschild!
      - control:
          kind: pause
          length: short
      - text: " Sehe ich recht?!\nIch bin so heiß auf diesen Schild, dass\ndavon der Schnee schmelzen könnte...!"
  LetsStart_01:
    attributes: Npc_IceVillage011
    contents:
      - text: "Dann gehen wir am besten gleich zum\nParcours."
      - control:
          kind: pause
          length: short
      - text: " Folge mir."
  LetsStart_02:
    attributes: Npc_IceVillage011
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 6
      - text: Gut!
      - control:
          kind: pause
          length: short
      - text: " Brauchst du eine kleine Einführung\nins Schlittern?"
      - control:
          kind: choice
          choice_labels:
            - 2
            - 5
          selected_index: 0
          cancel_index: 1
          unknown: 6
  LetsStart_03:
    attributes: Npc_IceVillage011
    contents:
      - text: "Oh, dann kennst du dich ja schon aus!"
      - control:
          kind: pause
          length: short
      - text: "\nAusgezeichnet. Weniger Arbeit für mich."
  LetsStart_04:
    attributes: Npc_IceVillage011
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Gut, dann zeig mir doch mal, was du\ndraufhast."
      - control:
          kind: pause
          length: long
      - text: " Kann's losgehen?"
      - control:
          kind: pause
          length: long
      - text: " Dann mach\ndich bereit zum Start!"
  NG_00:
    attributes: Npc_IceVillage011
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 6
      - text: Was denn? Nicht? Wie enttäuschend...
      - control:
          kind: pause
          length: short
      - text: "\nI-ich meine..."
      - control:
          kind: pause
          length: short
      - text: " Nicht, dass ich hier rum-\nsitze und auf Herausforderer warte..."
  NoPlay_00:
    attributes: Npc_IceVillage011
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 16
      - text: "Na gut... "
      - control:
          kind: pause
          length: short
      - text: "Alle Welt hat sich seinerzeit für\nmeine Profi-Karriere interessiert, aber die\nZeiten ändern sich wohl..."
  NotHadEnoughRupee:
    attributes: Npc_IceVillage011
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 19
      - text: Du kannst das Lehrgeld nicht zahlen...
      - control:
          kind: pause
          length: short
      - text: "\nKomm später wieder, wenn du "
      - control:
          kind: set_colour
          colour: red
      - text: 20 Rubine
      - control:
          kind: reset_colour
      - text: "\nhast."
  Pouch_Check00:
    attributes: Npc_IceVillage011
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Jetzt hast du ja wieder Platz in deiner\nTasche!"
      - control:
          kind: pause
          length: short
      - text: " Sehr schön! Hier ist also dein\nSchild, wie versprochen!"
  Pouch_Check01:
    attributes: Npc_IceVillage011
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 6
      - text: "Dafür, dass du den Fortgeschrittenen-\nParcours gemeistert hast, hatte ich dir\neinen Schild geben wollen..."
  Pouch_Check02:
    attributes: Npc_IceVillage011
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 0
      - text: "Aber du hast ja immer noch keinen Platz\nin deiner Tasche."
      - control:
          kind: pause
          length: long
      - text: " Ich bewahre ihn weiter\nauf und gebe ihn dir dann nächstes Mal!"
  Pouch_Check03:
    attributes: Npc_IceVillage011
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Also, zurück zur Hütte."
  Pouch_Check04:
    attributes: Npc_IceVillage011
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Sprich mich noch einmal an, wenn du\nPlatz in deiner Tasche hast!"
  Pouch_Check05:
    attributes: Npc_IceVillage011
    contents:
      - text: "Ich würde zu gerne sehen, wie du dich\nauf dem Schild machst, den ich dir\ngegeben habe..."
      - control:
          kind: pause
          length: long
      - text: " Zeig mir, was du kannst!"
      - control:
          kind: choice
          choice_labels:
            - 15
            - 14
            - 16
          selected_index: 0
          cancel_index: 2
          unknown: 8
  RefusePay_00:
    attributes: Npc_IceVillage011
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 18
      - text: "Oh, schade!"
      - control:
          kind: pause
          length: short
      - text: " Wie oft hat man schon die\nGelegenheit, von einem ehemaligen\nProfi zu lernen?"
  Stop_00:
    attributes: Npc_IceVillage011
    contents:
      - text: "Oh, was ist denn?"
      - control:
          kind: choice
          choice_labels:
            - 6
            - 7
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Stop_01:
    attributes: Npc_IceVillage011
    contents:
      - text: "Oh... "
      - control:
          kind: pause
          length: short
      - text: "Aber du weißt, dass du deine\nRubine nicht zurückbekommst, wenn\ndu jetzt aufhörst?"
      - control:
          kind: choice
          choice_labels:
            - 8
            - 9
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Stop_02:
    attributes: Npc_IceVillage011
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 12
      - text: "Kommt mir immer noch wie Verschwen-\ndung vor..."
      - control:
          kind: pause
          length: short
      - text: " Dann hoffe ich, dass du\nnächstes Mal wieder mit dabei bist!"
  Stop_03:
    attributes: Npc_IceVillage011
    contents:
      - text: "Na gut, dann zeig mir mal, was du\nkannst!"
  TalkForPay_Beginner:
    attributes: Npc_IceVillage011
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 25
      - text: "Glückwunsch,"
      - control:
          kind: pause
          length: short
      - text: " du hast gerade deine\nerste Hürde als Athlet genommen:"
      - control:
          kind: pause
          length: short
      - text: "\nIn die eigene Zukunft investieren!"
  TellRupee_Beginner:
    attributes: Npc_IceVillage011
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: "Hehehe... "
      - control:
          kind: pause
          length: short
      - text: So viel Einsatz!
      - control:
          kind: pause
          length: short
      - text: " Das erinnert\nmich an mich selbst, vor all der Zeit!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Aber! "
      - control:
          kind: pause
          length: short
      - text: "Ich bin ein ehemaliger Profi, also\nweißt du sicher, dass ich das nicht\numsonst tun kann?"
  suremi_fire_shield1:
    attributes: Npc_IceVillage011
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 0
      - text: "He! "
      - control:
          kind: pause
          length: short
      - text: "Was machst du denn?! "
      - control:
          kind: pause
          length: short
      - text: Aufhören!
