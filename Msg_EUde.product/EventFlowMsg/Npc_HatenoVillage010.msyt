          unknown:
            - 4
            - 11
      - text: Ah!
  talk20:
    attributes: Npc_HatenoVillage010
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: Du verwechselst da was!
      - control:
          kind: pause
          length: short
      - text: " Ich hatte schon\ngehört, dass wohl Purahs Enkeltochter\nzu Besuch sei.\nDas erklärt auch, warum manche in der\nNähe des "
      - control:
          kind: set_colour
