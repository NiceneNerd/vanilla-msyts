entries:
  "0000":
    attributes: Npc_NorthHateru005
    contents:
      - text: Was ist das hier?
  "0001":
    attributes: Npc_NorthHateru005
    contents:
      - text: Was siehst du dir an?
  "0002":
    attributes: Npc_NorthHateru005
    contents:
      - text: Tut mir leid.
  "0003":
    attributes: Npc_NorthHateru005
    contents:
      - text: Zora?
  "0004":
    attributes: Npc_NorthHateru005
    contents:
      - text: Bis dann.
  "0005":
    attributes: Npc_NorthHateru005
    contents:
      - text: Satori?
  "0006":
    attributes: Npc_NorthHateru005
    contents:
      - text: Lass hören.
  "0007":
    attributes: Npc_NorthHateru005
    contents:
      - text: Eher nicht.
  "0008":
    attributes: Npc_NorthHateru005
    contents:
      - text: "Wie geht's weiter?"
  "0009":
    attributes: Npc_NorthHateru005
    contents:
      - text: Reicht.
  "0010":
    attributes: Npc_NorthHateru005
    contents:
      - text: Und dann?
  talk000:
    attributes: Npc_NorthHateru005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Ho...
  talk001:
    attributes: Npc_NorthHateru005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Och nö... "
      - control:
          kind: pause
          length: short
      - text: Noch ein Mann!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "An diesem Stall trifft man immer nur\nMänner..."
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
            - 2
          selected_index: 0
          cancel_index: 2
          unknown: 8
  talk002:
    attributes: Npc_NorthHateru005
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 0
      - text: Oho!
      - control:
          kind: pause
          length: longer
      - text: " Ist 'n Weilchen her, dass wir 'ne\nSchönheit wie dich hier..."
      - control:
          kind: pause
          length: short
      - text: " Hä?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 3
            - 0
      - text: "Was? "
      - control:
          kind: pause
          length: short
      - text: Du bist ein Kerl?
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Weißt du, auf diese Klamotten fallen\nvielleicht die Gerudo rein, aber einen\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Hylianer
      - control:
          kind: reset_colour
      - text: " täuschst du damit nicht!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Tja...
      - control:
          kind: pause
          length: short
      - text: " An diesem Stall werde ich wohl\nimmer nur Männer treffen."
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
            - 2
          selected_index: 0
          cancel_index: 2
          unknown: 8
  talk005:
    attributes: Npc_NorthHateru005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Hm? "
      - control:
          kind: pause
          length: short
      - text: "Ach, du wieder..."
      - control:
          kind: pause
          length: longer
      - text: "\nWas denn?"
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
            - 2
          selected_index: 0
          cancel_index: 2
          unknown: 8
  talk010:
    attributes: Npc_NorthHateru005
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 12
      - text: Ein doofer Stall voller Männer...
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Letztens kam eine Zora...
      - control:
          kind: pause
          length: longer
      - text: " dachte ich!"
      - control:
          kind: pause
          length: long
      - text: "\nAber es war irgend so ein "
      - control:
          kind: set_colour
          colour: red
      - text: Prinz
      - control:
          kind: reset_colour
      - text: "..."
      - control:
          kind: pause
          length: short
      - text: "\nAlso natürlich wieder ein Mann!\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 9
      - text: Nur Satori gibt mir Trost...
      - control:
          kind: choice
          choice_labels:
            - 3
            - 5
            - 4
          selected_index: 0
          cancel_index: 2
          unknown: 8
  talk020:
    attributes: Npc_NorthHateru005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 6
      - text: Schau mal nach Osten.
      - control:
          kind: pause
          length: longer
      - text: "\nDas sind ziemlich ungewöhnliche\nWolken, findest du nicht?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Wenn es regnet, fühle ich mich schlecht."
      - control:
          kind: pause
          length: longer
      - text: "\nDann muss ich bei Satori Trost suchen..."
      - control:
          kind: choice
          choice_labels:
            - 5
            - 4
          selected_index: 0
          cancel_index: 1
          unknown: 6
  talk030:
    attributes: Npc_NorthHateru005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: Ich will keine Entschuldigungen...
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Die einzigen Besucher hier in letzter\nZeit waren du und dieser "
      - control:
          kind: set_colour
          colour: red
      - text: Zora-Prinz
      - control:
          kind: reset_colour
      - text: "..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Hier sind nur Männer, und das zieht\nanscheinend noch mehr Männer an."
      - control:
          kind: choice
          choice_labels:
            - 3
            - 4
          selected_index: 0
          cancel_index: 1
          unknown: 6
  talk040:
    attributes: Npc_NorthHateru005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 6
      - text: Kennst du die Zoras nicht?
      - control:
          kind: pause
          length: longer
      - text: " Das sind so\nglitschige, fischähnliche Typen."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Sie leben östlich von hier... "
      - control:
          kind: pause
          length: short
      - text: "Im Dorf der\nZoras, flussaufwärts am "
      - control:
          kind: set_colour
          colour: blue
      - text: Zora-Fluss
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Sie haben mich in ihr Dorf eingeladen,\nweil ich wie ein kampffähiger "
      - control:
          kind: set_colour
          colour: blue
      - text: Hylianer
      - control:
          kind: reset_colour
      - text: "\naussehe, aber..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 5
            - 12
      - text: "Ich will Satori nicht in Gefahr bringen,\nalso habe ich abgelehnt."
      - control:
          kind: choice
          choice_labels:
            - 5
            - 4
          selected_index: 0
          cancel_index: 1
          unknown: 6
  talk050:
    attributes: Npc_NorthHateru005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Hm, ja... "
      - control:
          kind: pause
          length: short
      - text: Bis dann.
  talk060:
    attributes: Npc_NorthHateru005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Tja, "
      - control:
          kind: set_colour
          colour: blue
      - text: Satori
      - control:
          kind: reset_colour
      - text: "... "
      - control:
          kind: pause
          length: short
      - text: "Ich schulde ihm mein\nLeben."
      - control:
          kind: pause
          length: short
      - text: " Ist 'ne lange Geschichte, willst\ndu sie hören?"
      - control:
          kind: choice
          choice_labels:
            - 6
            - 7
          selected_index: 0
          cancel_index: 1
          unknown: 6
  talk061:
    attributes: Npc_NorthHateru005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: Dann fang ich mal an...
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Hast du von dem "
      - control:
          kind: set_colour
          colour: red
      - text: seltsamen Berg
      - control:
          kind: reset_colour
      - text: " gehört,\nan dem Tiere nur zu einer bestimmten\nZeit erscheinen?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ich dachte, dass man dort sicher\nordentlich was verdienen kann und hab\nmich auf den Weg gemacht..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ich wollte jagen und mir so eine goldene\nNase verdienen."
      - control:
          kind: choice
          choice_labels:
            - 8
            - 9
          selected_index: 0
          cancel_index: 1
          unknown: 6
  talk062:
    attributes: Npc_NorthHateru005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 12
      - text: "Also geh ich zum Berg und warte darauf,\ndass die ganzen Tiere erscheinen..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Und dann kommt da dieser schmutzige\nKöter, eigentlich noch ein Welpe..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Vielleicht wurde er von seinem Rudel\ngetrennt oder von seinem Besitzer\nausgesetzt, keine Ahnung..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Jedenfalls bettelt er mich um Futter an.
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Also hab ich ihm halt was gegeben...
      - control:
          kind: choice
          choice_labels:
            - 8
            - 9
          selected_index: 0
          cancel_index: 1
          unknown: 6
  talk063:
    attributes: Npc_NorthHateru005
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: Eines Tages bin ich nachts eingenickt...
      - control:
          kind: pause
          length: short
      - text: "\nUnd als ich aufgewacht bin, steht dieses\nleuchtende "
      - control:
          kind: set_colour
          colour: blue
      - text: Ding
      - control:
          kind: reset_colour
      - text: " vor mir!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Hat mich kalt erwischt.
      - control:
          kind: pause
          length: short
      - text: " Ich konnte nicht\nmal meinen Bogen ziehen."
      - control:
          kind: pause
          length: short
      - text: " Ich hab es\neinfach nur angeglotzt."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Das "
      - control:
          kind: set_colour
          colour: blue
      - text: Ding
      - control:
          kind: reset_colour
      - text: " kam näher..."
      - control:
          kind: pause
          length: short
      - text: " Als wollte es mich\neinfach zertrampeln."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Da springt plötzlich der Welpe, den ich\ngefüttert hatte, zwischen uns."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Er bellt dieses "
      - control:
          kind: set_colour
          colour: blue
      - text: Ding
      - control:
          kind: reset_colour
      - text: " an..."
      - control:
          kind: pause
          length: short
      - text: " Das sicher\nzehnmal so groß war wie er..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Er wollte mich beschützen...
      - control:
          kind: pause
          length: longer
      - text: "\nObwohl er selber gezittert hat wie\nEspenlaub..."
      - control:
          kind: choice
          choice_labels:
            - 10
            - 9
          selected_index: 0
          cancel_index: 1
          unknown: 6
  talk064:
    attributes: Npc_NorthHateru005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 12
      - text: "Das "
      - control:
          kind: set_colour
          colour: blue
      - text: Ding
      - control:
          kind: reset_colour
      - text: " zuckt überrascht zurück..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Und ich greif mir den Welpen und renne\nHals über Kopf den Berg runter."
      - control:
          kind: pause
          length: short
      - text: " Klingt\neleganter, als es war."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Und das ist meine Geschichte.
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Der "
      - control:
          kind: set_colour
          colour: red
      - text: seltsame Berg
      - control:
          kind: reset_colour
      - text: " ist der "
      - control:
          kind: set_colour
          colour: blue
      - text: Satori-Berg
      - control:
          kind: reset_colour
      - text: " im\nWesten des "
      - control:
          kind: set_colour
          colour: blue
      - text: inneren Hyrule
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: "Und der Welpe von damals ist mein\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Satori
      - control:
          kind: reset_colour
      - text: " hier."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ich sag dir, wenn du deinen Mut wirklich\nauf die Probe stellen willst, dann geh\nund such dieses "
      - control:
          kind: set_colour
          colour: blue
      - text: Ding
      - control:
          kind: reset_colour
      - text: "..."
  talk065:
    attributes: Npc_NorthHateru005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Wie? "
      - control:
          kind: pause
          length: short
      - text: "Willst du die Geschichte von\nSatori und mir etwa noch mal hören?"
      - control:
          kind: choice
          choice_labels:
            - 6
            - 7
          selected_index: 0
          cancel_index: 1
          unknown: 6
  talk070:
    attributes: Npc_NorthHateru005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Oh... "
      - control:
          kind: pause
          length: short
      - text: Bis dann...
  talk100:
    attributes: Npc_NorthHateru005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Tja... "
      - control:
          kind: pause
          length: short
      - text: "Mal sehen, was heute so passiert."
  talk101:
    attributes: Npc_NorthHateru005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Regen...
  talk110:
    attributes: Npc_NorthHateru005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Ich hab solchen Hunger...
  talk120:
    attributes: Npc_NorthHateru005
    contents:
      - control:
          kind: text_size
          percent: 80
      - control:
          kind: sound2
          unknown:
            - 15
            - 205
      - text: "Es kommt...\nLauf weg, Satori..."
      - control:
          kind: text_size
