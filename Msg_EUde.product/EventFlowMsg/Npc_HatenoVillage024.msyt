    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Wir dürfen nicht zulassen, dass die\nWindmühlen anhalten, also passen mein\nMann und ich abwechselnd auf sie auf.\nDabei fühlt es sich an... "
      - control:
          kind: pause
          length: short
      - text: "als wäre diese\nWelt ein sehr friedfertiger Ort..."
  talk23:
    attributes: Npc_HatenoVillage024
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: "Das ist ein kleines Fischerdorf in\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Phirone
      - control:
          kind: reset_colour
      - text: ", weit im Süden von Hateno."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ich heiße Ralera und bin aus Angelstedt\nhierhergezogen, als ich geheiratet habe."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ich bin mit all meinen Habseligkeiten\nüber die "
      - control:
          kind: set_colour
          colour: blue
      - text: Hylia-Brücke
      - control:
          kind: reset_colour
      - text: " hierhergekommen."
      - control:
          kind: pause
          length: short
      - text: "\nDas war vielleicht ein Gewaltmarsch!"
  talk24:
    attributes: Npc_HatenoVillage024
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 11
      - text: "Angelstedt ist ein kleines Fischerdorf\njenseits der "
      - control:
          kind: set_colour
          colour: blue
      - text: Hylia-Brücke
      - control:
          kind: reset_colour
