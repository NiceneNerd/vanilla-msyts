    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Solange es hell ist, passe ich anstelle\nmeines Mannes auf die Windmühlen auf."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 8
            - 0
      - text: "Die Mühlen müssen sich immer mit der\ngleichen Geschwindigkeit drehen, sonst\nwird das Mehl ruiniert!\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: Da versteht mein Mann keinen Spaß!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 7
            - 9
      - text: "Zum Glück ist er sonst nicht so, hihi..."
  talk11:
    attributes: Npc_HatenoVillage024
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 16
      - text: Ich brauche Schlaf...
      - control:
          kind: pause
          length: short
      - text: "\nLass uns doch ein andermal sprechen."
  talk12:
    attributes: Npc_HatenoVillage024
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 19
      - text: "Oh, Entschuldigung!"
      - control:
          kind: pause
          length: longer
      - text: "\nWir kennen uns ja noch gar nicht!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 6
            - 1
      - text: "Ich dachte nur gerade an mein Heimat-\ndorf, Angelstedt."
      - control:
          kind: pause
          length: short
      - text: " Hach, das Meer... "
      - control:
          kind: pause
          length: longer
      - text: "In\nHateno fühlt sich alles anders an."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Aber immerhin hat diese Stadt meinen\nMann hervorgebracht, "
      - control:
          kind: pause
          length: short
      - text: "so schlecht kann\nsie also nicht sein!\nIch bin so glücklich, an seiner Seite\nzu arbeiten!"
      - control:
          kind: single_choice
          label: 6
