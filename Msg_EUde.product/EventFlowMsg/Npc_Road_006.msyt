---
group_count: 53
atr1_unknown: 4
entries:
  Atacked00:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 22
      - text: Ich bin Rätselforscher.
      - control:
          kind: pause
          length: long
      - text: " Wenn dir der\nBegriff rätselhaft scheint, bist du bei mir\ngenau richtig..."
  Atacked01:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 22
      - text: Ich bin doch nur ein Rätselforscher.
      - control:
          kind: pause
          length: long
      - text: "\nDas Bekämpfen von Monstern fällt nicht\nin meinen Zuständigkeitsbereich!"
  Failed00:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 12
      - text: "Das... "
      - control:
          kind: pause
          length: short
      - text: "Das wird mir alles zu viel.\nIch bin doch kein Monsterfressen!"
  Failed01:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 12
      - text: Körperliche Stärke ist nicht...
      - control:
          kind: pause
          length: long
      - text: " meine\nStärke. "
      - control:
          kind: pause
          length: short
      - text: "Ich bin eher der Typ für\nintellektuelle Herausforderungen..."
  Guardian00:
    attributes: Npc_Road_006
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 22
      - text: Lauf einfach! LAUF!
      - control:
          kind: pause
          length: short
      - text: " Ein "
      - control:
          kind: set_colour
          colour: blue
      - text: "Wächter "
      - control:
          kind: reset_colour
      - text: kommt!
  MSG_Orud_00:
    attributes: Npc_Road_006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 35
      - text: "Ich heiße Zorona und bin der forscheste\naller Rätselforscher!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ob es in "
      - control:
          kind: set_colour
          colour: blue
      - text: Eldin
      - control:
          kind: reset_colour
      - text: " Rätsel gibt?"
      - control:
          kind: pause
          length: short
      - text: " Ha, genau das\nist das Rätsel, das es zu lösen gilt!"
  MSG_Orud_00_1:
    attributes: Npc_Road_006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 16
      - text: "Rätsel in "
      - control:
          kind: set_colour
          colour: blue
      - text: Eldin
      - control:
          kind: reset_colour
      - text: "?"
      - control:
          kind: pause
          length: long
      - text: " Das ist ja das Rätsel!"
  MSG_Orud_01:
    attributes: Npc_Road_006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 35
      - text: "Ich bin ein Rätselforscher und heiße\nZorona!"
      - control:
          kind: pause
          length: short
      - text: " Zurzeit bin ich dem rätselhaften\n"
      - control:
          kind: set_colour
          colour: red
      - text: "Monsterladen "
      - control:
          kind: reset_colour
      - text: "auf der Spur...\nEr erscheint ausschließlich bei Nacht und\nzwar in der Nähe eines Teiches, der die\nForm eines Totenkopfes haben soll!\nSoweit die Gerüchte,"
      - control:
          kind: pause
          length: short
      - text: " aber niemand hat\nauch nur die geringste Ahnung, wer\nhinter diesem "
      - control:
          kind: set_colour
          colour: red
      - text: Monsterladen
      - control:
          kind: reset_colour
      - text: " steckt!"
  MSG_Orud_0100:
    attributes: Npc_Road_006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 35
      - text: "Ich bin ein forscher Rätselforscher und "
      - control:
          kind: pause
          length: short
      - text: "\nwerde Zorona genannt!"
      - control:
          kind: pause
          length: short
      - text: " Sag mal, kennst\ndu vielleicht den "
      - control:
          kind: set_colour
          colour: red
      - text: Monsterladen
      - control:
          kind: reset_colour
      - text: "?\nDer taucht jetzt nicht mehr nur am\nTotenkopf-Teich beim "
      - control:
          kind: set_colour
          colour: blue
      - text: Todesberg
      - control:
          kind: reset_colour
      - text: " im\nNorden,"
      - control:
          kind: pause
          length: short
      - text: " sondern auch anderswo auf!\nDabei ist das größte Rätsel weiterhin,\nwas so ein "
      - control:
          kind: set_colour
          colour: red
      - text: Monsterladen
      - control:
          kind: reset_colour
      - text: " überhaupt\nfür Waren feilbietet!"
  MSG_Orud_0100_1:
    attributes: Npc_Road_006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 16
      - text: Ist der Totenkopf-Teich-
      - control:
          kind: set_colour
          colour: red
      - text: Monsterladen
      - control:
          kind: reset_colour
      - text: " am\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Todesberg
      - control:
          kind: reset_colour
      - text: " im Norden etwa nur der\nAnfang einer ganzen Ladenkette?\nWas gibt es in so einem "
      - control:
          kind: set_colour
          colour: red
      - text: Monsterladen
      - control:
          kind: reset_colour
      - text: "\nüberhaupt zu kaufen...?"
  MSG_Orud_01_1:
    attributes: Npc_Road_006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 16
      - text: "Wer betreibt diesen "
      - control:
          kind: set_colour
          colour: red
      - text: Monsterladen
      - control:
          kind: reset_colour
      - text: "...?"
      - control:
          kind: pause
          length: long
      - text: "\nWieso nur zu nächtlicher Stunde...?"
      - control:
          kind: pause
          length: short
      - text: "\nWarum ein Teich in Totenkopf-Form...?"
  MSG_Orud_02:
    attributes: Npc_Road_006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 35
      - text: "Ich bin ein forscher Rätselforscher, den\nman frank und frei Zorona nennt!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Interessierst du dich ebenfalls für Rätsel,\ndann versuch doch einmal, das legendäre\nSchwert im "
      - control:
          kind: set_colour
          colour: blue
      - text: "Wald der Krogs "
      - control:
          kind: reset_colour
      - text: "zu finden!\nDas größte Rätsel dieser Welt,"
      - control:
          kind: pause
          length: short
      - text: " den\nmysteriösen "
      - control:
          kind: set_colour
          colour: red
      - text: Monsterladen
      - control:
          kind: reset_colour
      - text: ", überlässt du\nbitte dem Experten, "
      - control:
          kind: pause
          length: long
      - text: also mir!
  MSG_Orud_02_1:
    attributes: Npc_Road_006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 9
      - text: "Geh du doch dem Geheimnis des\nlegendären Schwertes auf den Grund..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Der Legende nach gibt's irgendwo einen\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Wald der Krogs
      - control:
          kind: reset_colour
      - text: ", in dessen Tiefen besagte\nWunderwaffe schlummern soll!"
  MSG_Orud_03:
    attributes: Npc_Road_006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 35
      - text: "Ich bin der forsche Rätselforscher,\nZorona!"
      - control:
          kind: pause
          length: short
      - text: " Du fragst, welch Rätsel mich\nzurzeit in seinem Banne hält?"
      - control:
          kind: pause
          length: short
      - text: "\nDer "
      - control:
          kind: set_colour
          colour: red
      - text: Monsterladen
      - control:
          kind: reset_colour
      - text: "!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Des Nachts öffnet er am Totenkopf-Teich\nseine Pforten, doch niemand weiß, wer\nalsdann hinter seiner Theke steht!"
  MSG_Orud_0300:
    attributes: Npc_Road_006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 35
      - text: "Ich bin der forsche Rätselforscher,\nZorona!"
      - control:
          kind: pause
          length: short
      - text: " Du fragst, welch Rätsel mich\nzurzeit in seinem Banne hält? \nDer "
      - control:
          kind: set_colour
          colour: red
      - text: Monsterladen
      - control:
          kind: reset_colour
      - text: "!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Einst nur des Nachts am\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Totenkopf-Teich
      - control:
          kind: reset_colour
      - text: ","
      - control:
          kind: pause
          length: short
      - text: " erscheint er jetzt \nauch anderswo im Königreich..."
  MSG_Orud_0300_1:
    attributes: Npc_Road_006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 16
      - text: "Sein Rätsel mich im Banne hält... "
      - control:
          kind: pause
          length: short
      - text: "\nDer "
      - control:
          kind: set_colour
          colour: red
      - text: Monsterladen
      - control:
          kind: reset_colour
      - text: "!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Bislang ist er nur am "
      - control:
          kind: set_colour
          colour: blue
      - text: Totenkopf-Teich
      - control:
          kind: reset_colour
      - text: "\naufgetaucht, jetzt soll er auch in anderen\nRegionen gesichtet worden sein..."
  MSG_Orud_03_1:
    attributes: Npc_Road_006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 16
      - text: "Sein Rätsel mich im Banne hält... "
      - control:
          kind: pause
          length: short
      - text: "\nDer "
      - control:
          kind: set_colour
          colour: red
      - text: Monsterladen
      - control:
          kind: reset_colour
      - text: "!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Er steht des Nachts am Totenkopf-Teich,"
      - control:
          kind: pause
          length: short
      - text: "\ndas ist bekannt, doch sein Betreiber,"
      - control:
          kind: pause
          length: short
      - text: "\nder ist noch gänzlich unbenannt..."
  MSG_Orud_04:
    attributes: Npc_Road_006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 35
      - text: "Ich bin ein forscher Rätselforscher, den\nman frank und frei Zorona nennt!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Um die Schätze in "
      - control:
          kind: set_colour
          colour: blue
      - text: Schloss Hyrule
      - control:
          kind: reset_colour
      - text: " ranken\nsich viele Legenden..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Mich interessiert jedoch ausschließlich\ndas Rätsel des "
      - control:
          kind: set_colour
          colour: red
      - text: Monsterladens
      - control:
          kind: reset_colour
      - text: "!"
      - control:
          kind: pause
          length: long
      - text: " Sieh du\nruhig einmal nach den Schätzen..."
  MSG_Orud_04_1:
    attributes: Npc_Road_006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 9
      - text: "Die Schätze von "
      - control:
          kind: set_colour
          colour: blue
      - text: Schloss Hyrule
      - control:
          kind: reset_colour
      - text: " überlass\nich dir..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ich will nur endlich wissen, was hinter\ndiesem rätselhaften "
      - control:
          kind: set_colour
          colour: red
      - text: Monsterladen
      - control:
          kind: reset_colour
      - text: " steckt!"
  Near00:
    attributes: Npc_Road_006
    contents:
      - text: Des Rätsels Lösung...
  Near01:
    attributes: Npc_Road_006
    contents:
      - text: Ein Teich in Schädelform...
  Near02:
    attributes: Npc_Road_006
    contents:
      - text: Schätze am Schloss...
  Near03:
    attributes: Npc_Road_006
    contents:
      - text: Hm... Monsterladen...
  Near04:
    attributes: Npc_Road_006
    contents:
      - text: Meine Karte wird nass!
  Rest_00:
    attributes: Npc_Road_006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 35
      - text: "Ich bin der Rätselforscher, den man\nZorona nennt!"
      - control:
          kind: pause
          length: short
      - text: " Alles, was ich brauche,\nsind mein Verstand und eine Landkarte!"
  Rest_01:
    attributes: Npc_Road_006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 35
      - text: Ich bin der Rätselforscher Zorona!
      - control:
          kind: pause
          length: short
      - text: " Und\nmeine Karte weicht gerade im Regen auf!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Moment mal.
      - control:
          kind: pause
          length: short
      - text: " Wieso ist deine Landkarte\nnicht aus Papier?"
  Rest_02:
    attributes: Npc_Road_006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 35
      - text: "Ich bin der forscheste aller\nRätselforscher, Zorona!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Wie steht's?"
      - control:
          kind: pause
          length: long
      - text: " Strebst du auch nach einem\nerleuchtenden Leben wie dem meinen?"
  RetMSG_Orud_00:
    attributes: Npc_Road_006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 35
      - text: "Ich heiße Zorona und bin der forscheste\naller Rätselforscher!"
      - control:
          kind: pause
          length: short
      - text: " Die Welt ist voller\nRätsel. "
      - control:
          kind: pause
          length: short
      - text: "Möchtest auch du eines lösen?\nDann könntest du dich in "
      - control:
          kind: set_colour
          colour: blue
      - text: Schloss Hyrule
      - control:
          kind: reset_colour
      - text: "\noder im "
      - control:
          kind: set_colour
          colour: blue
      - text: "Wald der Krogs "
      - control:
          kind: reset_colour
      - text: "nach legendären\nWaffen umsehen..."
  RetMSG_Orud_00_1:
    attributes: Npc_Road_006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 9
      - text: Diese Welt ist voller Rätsel!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: set_colour
          colour: blue
      - text: Schloss Hyrule
      - control:
          kind: reset_colour
      - text: " und der "
      - control:
          kind: set_colour
          colour: blue
      - text: Wald der Krogs
      - control:
          kind: reset_colour
      - text: ",\nbergen sie tatsächlich legendäre Waffen\nund andere unermessliche Schätze?"
  RetMSG_Orud_01:
    attributes: Npc_Road_006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 35
      - text: "Ich bin der forsche Rätselforscher,\nZorona!"
      - control:
          kind: pause
          length: short
      - text: " Rätselst du schon, welches\nRätsel ich zuletzt zu lösen vermochte?\nGras, das sich auch ohne Wind im Tanze\nwiegt..."
      - control:
          kind: pause
          length: long
      - text: " Das sind versteckte Krogs!"
      - control:
          kind: pause
          length: long
      - text: "\nSchlicht und einfach:"
      - control:
          kind: pause
          length: short
      - text: " Krogs!"
  RetMSG_Orud_01_1:
    attributes: Npc_Road_006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 9
      - text: "Eines der letzten großen Rätsel dieser\nWelt, welches ich allein zu lösen\nimstande war..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Gras, das sich auch ohne Wind im Tanze\nwiegt..."
      - control:
          kind: pause
          length: long
      - text: " Das waren Krogs,"
      - control:
          kind: pause
          length: short
      - text: " versteckte\nKrogs!"
  RetMSG_Orud_02:
    attributes: Npc_Road_006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 35
      - text: "Ich bin der forsche Rätselforscher,\nZorona!"
      - control:
          kind: pause
          length: short
      - text: " Du bist gespannt, welch\nGeheimnis ich zuletzt aufdecken konnte?\nLange rätselte ich, welche Waffe einem\n"
      - control:
          kind: raw
          two_hundred_one:
            dynamic:
              - 0
              - len: 4
                field_2:
                  - 1
                  - 0
                  - 0
                  - 0
      - text: Bumerang gleich zurückkommt!
      - control:
          kind: pause
          length: short
      - text: " Nun habe\nich die Antwort:"
      - control:
          kind: pause
          length: short
      - text: " ein Echsalfos-Knochen!"
  RetMSG_Orud_02_1:
    attributes: Npc_Road_006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 9
      - text: "Bist nicht auch du es leid, Pfeile, die du\nverschießt, wieder aufsammeln zu\nmüssen?\nMerke dir: Ein Echsalfos-Knochen kommt\neinem "
      - control:
          kind: raw
          two_hundred_one:
            dynamic:
              - 0
              - len: 4
                field_2:
                  - 1
                  - 0
                  - 0
                  - 0
      - text: "Bumerang gleich nach dem Werfen\nzu dir zurück!"
  RetMSG_Orud_03:
    attributes: Npc_Road_006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 35
      - text: "Ich bin der forscheste Rätselforscher,\nZorona!"
      - control:
          kind: pause
          length: short
      - text: " Auch das profanste Rätsel ist\nmir Herausforderung genug!\nSchmerzt auch dir der Fuß, wenn\ndu einer Schatztruhe den herzhaftesten\naller Tritte versetzt?\n"
      - control:
          kind: sound
          unknown:
            - 1
            - 10
      - text: "Nun,"
      - control:
          kind: pause
          length: short
      - text: " kein Wunder,"
      - control:
          kind: pause
          length: short
      - text: " wenn du dabei kein\nSchuhwerk trägst!"
  RetMSG_Orud_03_1:
    attributes: Npc_Road_006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 9
      - text: "Eines dieser Rätsel, die nur dank meiner\nGenialität gelöst werden konnten..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Tritte gegen Schatztruhen führen zu\nSchmerzen,"
      - control:
          kind: pause
          length: short
      - text: " weil der Treter keine\nSchuhe trägt!"
  RetMSG_Orud_04:
    attributes: Npc_Road_006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 35
      - text: Ich heiße Zorona und bin Rätselforscher!
      - control:
          kind: pause
          length: short
      - text: "\nUnter den mannigfaltigen Rätseln dieser\nWelt hält mich nur eines in seinem Bann!\nEines Tages werde ich den "
      - control:
          kind: set_colour
          colour: red
      - text: Monsterladen
      - control:
          kind: reset_colour
      - text: "\nbetreten und seinen Betreiber\nzweifelsfrei entlarven!"
  RetMSG_Orud_04_1:
    attributes: Npc_Road_006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 9
      - text: Die Welt steckt voller Geheimnisse!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Nimm nur einmal den "
      - control:
          kind: set_colour
          colour: red
      - text: Monsterladen
      - control:
          kind: reset_colour
      - text: "!"
      - control:
          kind: pause
          length: short
      - text: "\nSchon bald werde ich wissen, wer ihn\nbetreibt!"
  SavedLv1_00:
    attributes: Npc_Road_006
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 17
      - text: "Warum greifen Monster überhaupt Leute\nan, hm?"
      - control:
          kind: pause
          length: short
      - text: " Einfach nur, weil wir ihnen nicht\nin den Kram passen? "
      - control:
          kind: pause
          length: long
      - text: Rätselhaft.
  SavedLv1_01:
    attributes: Npc_Road_006
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 17
      - text: "Warum wird ein unschuldiger Hylianer\nwie ich angegriffen, hm?"
      - control:
          kind: pause
          length: long
      - text: " Irgendwie\nmysteriös..."
  SavedLv2_00:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 2
            - 14
      - text: "Ich... "
      - control:
          kind: pause
          length: short
      - text: Ich bin ein forscher Rätselforscher!
      - control:
          kind: pause
          length: long
      - text: "\nKleine Gefahrensituationen..."
      - control:
          kind: pause
          length: short
      - text: " machen..."
      - control:
          kind: pause
          length: short
      - text: "\nmachen mir..."
      - control:
          kind: pause
          length: short
      - text: " gar nichts aus!"
  SavedLv2_01:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 2
            - 14
      - text: "Ich... "
      - control:
          kind: pause
          length: short
      - text: Ich bin ein forscher Rätselforscher!
      - control:
          kind: pause
          length: long
      - control:
