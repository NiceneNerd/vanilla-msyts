          unknown: 8
  talk16:
    attributes: Npc_HatenoVillage030
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: "Danke, dass du das Haus gekauft hast!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 4
            - 1
      - text: "Echt nicht übel! Wo hast du nur das\nGeld her?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Jedenfalls habe ich dank dir 'nen netten\nBonus kassiert! Damit führe ich meine\nFreundin schön zum Essen aus!"
  talk17:
    attributes: Npc_HatenoVillage030
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 12
      - text: Endlich ein bisschen Freizeit...
      - control:
          kind: choice
          choice_labels:
            - 5
            - 1
            - 2
          selected_index: 0
          cancel_index: 2
          unknown: 8
  talk18:
    attributes: Npc_HatenoVillage030
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 12
      - text: "Tja... "
      - control:
          kind: pause
          length: short
      - text: Abreißen ist echt Knochenarbeit.
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Aber wenn ich mir jemanden wie Dumsda\nansehe, der das in einem Tag schafft...\nSo gut will ich auch mal sein!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Oh, und ich habe mitbekommen, dass du\nmit dem Chef geredet hast..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 1
      - text: Willst du dieses Haus wirklich kaufen?
      - control:
          kind: choice
          choice_labels:
            - 6
            - 7
          selected_index: 0
          cancel_index: 2
          unknown: 6
  talk19:
    attributes: Npc_HatenoVillage030
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ich hau mich aufs Ohr.\nWir sehen uns morgen."
  talk20:
    attributes: Npc_HatenoVillage030
    contents:
      - control:
