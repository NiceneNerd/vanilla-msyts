            - 0
            - 0
      - text: "Oh, hallo!"
      - control:
          kind: pause
          length: short
      - text: "\nWir kennen uns noch nicht, oder?"
  talk01:
    attributes: Npc_SouthernVillage004
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Bist du schon mal auf einem "
      - control:
          kind: set_colour
          colour: red
      - text: Floß
      - control:
          kind: reset_colour
      - text: " aufs\nMeer rausgefahren?"
      - control:
