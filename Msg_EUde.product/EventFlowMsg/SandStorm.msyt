  Npc_SmallOasis007_F_003:
    attributes: Npc_SmallOasis007
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 6
      - text: Was sagst du?
      - control:
          kind: pause
          length: short
      - text: " Ein seltsamer Kaufmann?"
      - control:
          kind: pause
          length: short
      - text: "\nDa kann ich dir nicht weiterhelfen."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ich konzentriere mich voll und ganz auf\ndiesen Sandsturm... "
      - control:
          kind: pause
          length: short
      - text: Und die Schätze...
  Npc_SmallOasis007_F_004:
    attributes: Npc_SmallOasis007
    contents:
      - control:
