  Npc_SmallOasis007_F_005:
    attributes: Npc_SmallOasis007
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Ich habe ständig ein Auge auf den Sturm,\naußer natürlich, wenn ich schlafe..."
      - control:
          kind: pause
          length: longer
  Npc_SmallOasis007_F_006:
    attributes: Npc_SmallOasis007
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Und übrigens...\n"
      - control:
          kind: pause
          length: short
      - text: Wer bist du überhaupt?
  Npc_SmallOasis007_F_007:
    attributes: Npc_SmallOasis007
    contents:
      - text: "Ach, und... "
      - control:
          kind: pause
          length: longer
      - text: "Ich weiß, ich habe das schon\nein paar Mal gefragt, aber..."
      - control:
          kind: pause
          length: short
      - text: "\nWer bist du denn jetzt?"
  Npc_SmallOasis007_F_008:
    attributes: Npc_SmallOasis007
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ach, und..."
      - control:
          kind: pause
          length: short
      - text: "\nWer bist du eigentlich?"
  Npc_SmallOasis007_F_010:
    attributes: Npc_SmallOasis007
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Abenteuer sind was für Männer!
      - control:
          kind: pause
          length: short
      - text: "\nFrauen wie du können das nicht\nverstehen..."
  Npc_SmallOasis007_F_011:
    attributes: Npc_SmallOasis007
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Bitte stör mich nicht.
      - control:
          kind: pause
          length: short
      - text: "\nDu lenkst mich nur ab."
  Npc_SmallOasis007_F_012:
    attributes: Npc_SmallOasis007
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 19
      - text: "Hm... "
      - control:
          kind: pause
          length: long
      - text: huch!?
      - control:
          kind: pause
          length: short
      - text: "\nIst der Sandsturm etwa abgeflaut?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Ich hab ihm so lange zugesehen...
      - control:
          kind: pause
          length: long
      - text: "\nWarum ist er denn jetzt weg...? "
      - control:
          kind: pause
          length: short
      - text: "Ist mein\ngroßes Abenteuer jetzt zu Ende?"
  Npc_SmallOasis007_R_001:
    attributes: Npc_SmallOasis007
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Dieser Sandsturm soll angeblich jeden\nTag mehrere Stunden lang abflauen..."
      - control:
          kind: pause
          length: short
      - text: "\nAber bisher merke ich davon nichts."
  Npc_SmallOasis007_R_002:
    attributes: Npc_SmallOasis007
    contents:
      - control:
          kind: text_size
          percent: 80
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Bestimmt wird der Sturm morgen aufhören...
      - control:
          kind: text_size
          percent: 100
  Npc_SmallOasis007_R_005:
    attributes: Npc_SmallOasis007
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "In den Tiefen des Sturms sollen die\n"
      - control:
          kind: set_colour
          colour: red
      - text: Schätze einer vergangenen Zivilisation
      - control:
          kind: reset_colour
      - text: "\nversteckt sein."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Deswegen stehe ich hier und warte, dass\nder Sandsturm nachlässt, damit ich mich\nauf die Suche machen kann..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Vielleicht bin ich einer Fehlinformation\naufgesessen..."
