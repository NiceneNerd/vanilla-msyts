  Npc_SmallOasis007_F_001:
    attributes: Npc_SmallOasis007
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Die "
      - control:
          kind: set_colour
          colour: red
      - text: "Schätze einer vergangenen\nZivilisation"
      - control:
          kind: reset_colour
      - text: ", versteckt in einem\nSandsturm..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Was für ein Abenteuer...
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Ein ganz großes Abenteuer!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Aber bisher hat der Sturm noch nie\nnachgelassen, obwohl er jeden Tag\nmehrere Stunden abflauen soll."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Dabei habe ich ständig ein Auge auf ihn,\naußer natürlich, wenn ich schlafe."
