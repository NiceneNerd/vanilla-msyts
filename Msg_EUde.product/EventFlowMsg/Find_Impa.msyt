          cancel_index: 1
          unknown: 6
  Talk17:
    attributes: Npc_Kakariko007
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 11
      - text: "Hahaha, keine Bange!\nDas sieht nur so aus!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Vor langer Zeit war das Wissen der\nShiekah so groß, dass einige dachten,\nwir seien so mächtig wie Götter."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Und diese Bekleidung ist\nein erstklassiges Beispiel dafür."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Sie ist aus einem Material, das jedem\nWetter widersteht."
      - control:
          kind: pause
          length: short
      - text: " Ich muss mich nicht\nmal vor Blitzen fürchten."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Und auch wenn sie nass sehr schwer\naussieht –"
      - control:
          kind: pause
          length: short
      - text: " sie ist erstaunlich leicht."
