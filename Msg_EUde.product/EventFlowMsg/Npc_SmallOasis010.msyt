          kind: raw
          zero:
            four:
              field_1: 0
      - text: Aber mal überlegen...
      - control:
          kind: pause
          length: short
      - text: "\nHm..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "In letzter Zeit treibt sich hier eine\nHylianer-Vaai in Gerudo-Kleidung herum,\ndie mir irgendwie komisch vorkommt.\n"
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Da steckt doch was dahinter, sage\nich dir..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ich hab öfter gesehen, wie sie in den\n"
      - control:
          kind: set_colour
          colour: red
      - text: Krämerladen
      - control:
          kind: reset_colour
      - text: " gegangen ist."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
