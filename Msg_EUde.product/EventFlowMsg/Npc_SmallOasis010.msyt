    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Ob ich einen verdächtigen Kaufmann\ngesehen habe?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Also für mich sind alle Kaufleute, die\nkeine Gerudo sind, verdächtig..."
