  Talk_11:
    attributes: Npc_oasis005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Dies ist der "
      - control:
          kind: set_colour
          colour: blue
      - text: Gerudo-Geheim-Club
      - control:
          kind: reset_colour
      - text: ",\nkurz "
      - control:
          kind: set_colour
          colour: blue
      - text: GGC
      - control:
          kind: reset_colour
      - text: "..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Wir sind der einzige Ort in dieser Stadt,\nan dem Ihr Kleidung für Vooi kaufen\nkönnt.\nNun, das mag hier nicht ganz legal sein,\naber... "
      - control:
          kind: pause
          length: short
      - text: "es gibt eine unerwartet hohe\nNachfrage...\n"
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: "Ich muss schon sagen, mein Herr..."
      - control:
          kind: pause
          length: short
      - text: "\nNiemand hat sich bisher in eine Vaai\nverwandelt, die Euch gleichen würde..."
  Talk_12:
    attributes: Npc_oasis005
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: Ha-haha...
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Nun, nehmt Euch Zeit..."
  Talk_13:
    attributes: Npc_oasis005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Oho. "
      - control:
          kind: pause
          length: short
      - text: Das ist etwas ganz Besonderes.
      - control:
          kind: pause
          length: short
      - text: "\nMan braucht "
      - control:
          kind: set_colour
          colour: blue
      - control:
          kind: raw
          two_hundred_one:
            dynamic:
              - 0
              - len: 4
                field_2:
                  - 1
                  - 0
                  - 0
                  - 0
      - text: Leuchtsteine
      - control:
          kind: reset_colour
      - text: ", um es\nherzustellen.\nWenn Euch danach verlangt, fertige ich\nes Euch an – "
      - control:
          kind: set_colour
          colour: red
      - text: "wenn Ihr mir die benötigten\nMaterialien bringt"
      - control:
          kind: reset_colour
      - text: ".\nEine kleine Gebühr wird natürlich auch\nfällig."
  Talk_14:
    attributes: Npc_oasis005
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 11
      - text: Sarq-sarq.
      - control:
          kind: pause
          length: short
      - text: " Ein wertvolles Stück, das Ihr\nnur hier findet. "
      - control:
          kind: pause
          length: short
      - text: Gebt gut darauf Acht...
  Talk_15:
    attributes: Npc_oasis005
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 11
      - text: "Sarq-sarq. "
      - control:
          kind: pause
          length: short
      - text: "Dieses Stück solltet Ihr\nzusammen mit anderen aus der gleichen\nSerie tragen."
