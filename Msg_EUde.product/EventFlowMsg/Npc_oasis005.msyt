      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Nun, mehr könnt Ihr ja nun auch nicht\nmehr machen. "
      - control:
          kind: pause
          length: short
      - text: "Schließlich habt Ihr schon\nmein gesamtes Inventar aufgekauft."
  Talk_05:
    attributes: Npc_oasis005
    contents:
      - control:
