  Rest_01:
    attributes: Npc_Road_007
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Die Körper von Goronen sind hart und\nrobust wie Stein."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Hitzebeständig sind sie und gewinnen\ngegen Scheren, aber verlieren gegen\nPapier.\nIrgendwie hat sich da bei mir Schere,\nStein, Papier eingeschlichen..."
  RetMSG_Orud_00:
    attributes: Npc_Road_007
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 12
      - text: "Brütende Hitzewellen, herabstürzende\nFelsen, Monster überall. "
      - control:
          kind: pause
          length: short
      - text: "Das alles ist\nnatürlich ganz schön Furcht einflößend!\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Ich habe "
      - control:
          kind: set_colour
          colour: blue
      - text: Goronia
      - control:
          kind: reset_colour
      - text: " aber nicht deswegen\nverlassen. "
      - control:
