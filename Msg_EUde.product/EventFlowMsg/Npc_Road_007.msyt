  RetMSG_Orud_02:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ich weiß nicht, ob es normal ist, dass so\nviele Lavabrocken herabstürzen. "
      - control:
          kind: pause
          length: short
      - text: "Was das\nwohl soll?"
  RetMSG_Orud_02_1:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Von so einem "
      - control:
          kind: set_colour
          colour: blue
      - text: "Lavabrocken "
      - control:
          kind: reset_colour
      - text: "getroffen zu\nwerden ist nicht schön. "
      - control:
          kind: pause
          length: longer
      - text: "Das tut sogar\nuns Goronen weh.\nPass also gut auf dich auf, wenn du den\n"
      - control:
          kind: set_colour
          colour: blue
      - text: "Todesberg "
      - control:
          kind: reset_colour
      - text: erklimmst.
  RetMSG_Orud_03:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Die "
      - control:
          kind: set_colour
          colour: blue
      - text: "Straße zum Vulkan "
      - control:
          kind: reset_colour
      - text: ist heiß!
      - control:
          kind: pause
          length: long
      - text: "\nGefährlich heiß für alle Nicht-Goronen!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Reisenden nach "
      - control:
          kind: set_colour
          colour: blue
      - text: "Goronia "
      - control:
          kind: reset_colour
      - text: "wird\ndringend empfohlen, für Brandschutz\nzu sorgen! "
      - control:
