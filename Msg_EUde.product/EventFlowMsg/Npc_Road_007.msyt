      - control:
          kind: reset_colour
      - text: "aus "
      - control:
          kind: set_colour
          colour: blue
      - text: Goronia
      - control:
          kind: reset_colour
      - text: "\ngut geht..."
      - control:
          kind: pause
          length: long
      - text: " Ich hab immer gerne mit ihm\ngequatscht, weil er so ein netter Kerl ist."
  RetMSG_Orud_01_1:
    attributes: Npc_Road_007
