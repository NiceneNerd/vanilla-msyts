entries:
  "0000":
    attributes: ""
    contents:
      - text: Du bist durchnässt...
  "0001":
    attributes: ""
    contents:
      - text: Klingt gut.
  "0004":
    attributes: ""
    contents:
      - text: Was erzählst du?
  "0005":
    attributes: ""
    contents:
      - text: Dann viel Spaß!
  Near00:
    attributes: Npc_Kakariko007
    contents:
      - text: Eine Frau schluchzt...
  SupriseHouse:
    attributes: Npc_Kakariko007
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 21
      - text: Wer... Wer ist da!?
      - control:
          kind: pause
          length: longer
      - text: "\nWas machst du da?"
      - control:
          kind: pause
          length: longer
      - text: "\n... "
      - control:
          kind: pause
          length: short
      - text: "..."
      - control:
          kind: pause
          length: longer
  SupriseHouse_00:
    attributes: Npc_Kakariko007
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 18
      - text: "Ach... Ihr seid es?\nBitte erschreckt mich doch nicht so."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 1
      - text: "Ich dachte schon, SIE sind es..."
  Talk00:
    attributes: Npc_Kakariko007
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Manchmal segnet sie Leute mit\nbestimmten Kräften..."
      - control:
          kind: pause
          length: short
      - text: " Ihr solltet sie\neinmal besuchen."
  Talk01:
    attributes: Npc_Kakariko007
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "In diesem Dorf gibt es auch ein\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Gasthaus
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: pause
          length: short
      - text: " Warum verbringt Ihr nicht\ndort die Nacht?"
  Talk02:
    attributes: Npc_Kakariko007
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "Dies sind meine Kinder, Priko und Koko."
      - control:
          kind: pause
          length: short
      - text: "\nIhre Mutter wurde ein Opfer der"
      - control:
          kind: set_colour
          colour: blue
      - text: " Yiga"
      - control:
          kind: reset_colour
      - text: "..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Seitdem kümmert sich Koko rührend um\ndie kleine Priko."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Und ich...
      - control:
          kind: pause
          length: short
      - text: "\nWäre ich nur nicht so feige...!"
  Talk03:
    attributes: Npc_Kakariko007
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Es ist schon sehr spät...
  Talk04:
    attributes: Npc_Kakariko007
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Oh! Seid gegrüßt, Herr Link!"
      - control:
          kind: pause
          length: short
      - text: "\nIch gehe gerade zurück auf meinen\nPosten vor dem Anwesen.\nDie "
      - control:
          kind: set_colour
          colour: blue
      - text: "Yiga "
      - control:
          kind: reset_colour
      - text: "werden immer frecher in letzter\nZeit."
      - control:
          kind: pause
          length: short
      - text: " Ihr solltet sehr wachsam sein!"
  Talk05:
    attributes: Npc_Kakariko007
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Dann sollte ich langsam auch mal\nins Bett gehen..."
  Talk06:
    attributes: Npc_Kakariko007
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 12
      - text: Herr Link...
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Mir schwirren in letzter Zeit so viele\nGedanken im Kopf herum, dass ich\nkaum Schlaf finde.\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Wenn Ihr erschöpft seid, solltet Ihr\nEuch im Gasthaus erholen."
  Talk07:
    attributes: Npc_Kakariko007
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: Meine Töchter erwarten mich.
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ich werde ihnen unter dem\nLieblingsbaum meiner Frau Geschichten\nüber unser Dorf erzählen.\nNa ja, eigentlich erzähle ich ihnen jedes\nMal das Gleiche..."
  Talk08:
    attributes: Npc_Kakariko007
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Unter diesem Baum hat meine Frau\nunseren Töchtern immer alle möglichen\nGeschichten erzählt.\nJetzt muss ich das übernehmen. "
      - control:
          kind: pause
          length: short
      - text: "Leider\nbin ich kein so guter Erzähler wie meine\nFrau es war..."
  Talk09:
    attributes: Npc_Kakariko007
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: Macht Ihr auch gerade Pause?
      - control:
          kind: pause
          length: short
      - text: "\nDas hier sind meine Töchter Koko\nund Priko.\nDiese Momente, in denen ich in aller\nRuhe mit meinen Kindern sprechen kann,\nsind unschätzbar wertvoll für mich.\nObwohl ich ihnen eigentlich immer das\nGleiche erzähle..."
      - control:
          kind: choice
          choice_labels:
            - 4
            - 5
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Talk10:
    attributes: Npc_Kakariko007
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Auf diesem Hügel ist ein "
      - control:
          kind: set_colour
          colour: red
      - text: Schrein
      - control:
          kind: reset_colour
      - text: ", über\nden die Shiekah seit ewigen Zeiten\nwachen.\nIm "
      - control:
          kind: set_colour
          colour: red
      - text: Walde hinter dem Schrein
      - control:
          kind: reset_colour
      - text: " jedoch kann\nein aufmerksamer Zuhörer ab und zu\netwas vernehmen..."
      - control:
          kind: pause
          length: longer
      - text: "\n"
      - control:
          kind: text_size
          percent: 125
      - control:
          kind: sound
          unknown:
            - 4
            - 1
      - text: Das Schluchzen einer Frau!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: text_size
          percent: 100
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Tja, davon handelt die Geschichte..."
      - control:
          kind: pause
          length: longer
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: text_size
          percent: 80
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Eigentlich wohnt dort die "
      - control:
          kind: set_colour
          colour: blue
      - text: Schutzgöttin
      - control:
          kind: reset_colour
      - text: " unseres\nDorfes. Aber in letzter Zeit machen Monster die\nGegend unsicher.\nIch erzähle ihnen also diese Gruselgeschichte,\ndamit sie sich nicht dorthin trauen..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: text_size
          percent: 100
      - control:
          kind: sound
          unknown:
            - 5
            - 0
      - text: "Koko macht die Geschichte Angst, aber\nPriko hört sie aus irgendeinem Grund\ngern..."
  Talk13:
    attributes: Npc_Kakariko007
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "In diesem Dorf gibt es auch ein\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Gasthaus
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: pause
          length: short
      - text: " Warum verbringt Ihr nicht\ndort die Nacht?"
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Talk14:
    attributes: Npc_Kakariko007
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: Hahaha! Danke für das Mitgefühl!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Aber die Shiekah besitzen schon seit\nAlters her eine hochentwickelte\nTechnologie.\nIn unserer Kleidung steckt das Wissen\nvon Jahrtausenden!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Regen perlt einfach ab, und wir sind\nsogar vor Blitzen gefeit."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Also, auch wenn ich komplett durchnässt\naussehe..."
      - control:
          kind: pause
          length: short
      - text: " Glaub mir, da geht kein\nTropfen durch!"
  Talk15:
    attributes: Npc_Kakariko007
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Manchmal segnet sie Leute mit\nbestimmten Kräften..."
      - control:
          kind: pause
          length: short
      - text: " Ihr solltet sie\neinmal besuchen."
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Talk16:
    attributes: Npc_Kakariko007
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "In diesem Dorf findet Ihr alles, was das\nHerz begehrt..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Eine Kleiderboutique, einen\nZutatenladen, einen Krämerladen,\nselbst ein Gasthaus!\nUnd auf dem Hügel über dem Dorf gibt\nes sogar einen "
      - control:
          kind: set_colour
          colour: red
      - text: Schrein
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Etwas weiter hinter diesem Schrein\nwohnt die "
      - control:
          kind: set_colour
          colour: blue
      - text: "Schutzgöttin "
      - control:
          kind: reset_colour
      - text: unseres Dorfes.
      - control:
          kind: pause
          length: short
      - text: "\nSie soll wunderschön sein..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 1
      - text: "Aber ich habe eine solche Ehrfurcht vor\nihr, dass ich mich noch nie dorthin\ngetraut habe..."
  Talk17:
    attributes: Npc_Kakariko007
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Was führt Euch mitten in der Nacht\nhierher? "
      - control:
          kind: pause
          length: short
      - text: "Nachts ist es gefährlich wegen\nder ganzen Monster."
  Talk18:
    attributes: Npc_Kakariko007
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Ah, Herr Link..."
  Talk19:
    attributes: Npc_Kakariko007
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: Es ist schon sehr spät...
  Talk20:
    attributes: Npc_Kakariko007
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: Macht Ihr auch gerade Pause?
      - control:
          kind: pause
          length: short
      - text: "\nDas hier sind meine Töchter Koko\nund Priko.\nDiese Momente, in denen ich in aller\nRuhe mit meinen Kindern sprechen kann,\nsind unschätzbar wertvoll für mich.\nSelbst bei einem so bescheidenen\nWetter wie heute..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ich versuche, jeden Tag so viel Zeit wie\nmöglich mit ihnen zu verbringen."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Obwohl ich ihnen eigentlich immer das\nGleiche erzähle..."
      - control:
          kind: choice
          choice_labels:
            - 4
            - 5
          selected_index: 0
          cancel_index: 1
          unknown: 6
