      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Wie dem auch sei...
      - control:
          kind: pause
          length: short
      - text: " Du musst\nweiterziehen."
      - control:
          kind: pause
          length: short
      - text: " Das weißt du selbst\nam allerbesten.\n"
      - control:
          kind: sound
          unknown:
            - 11
            - 1
      - text: "... "
      - control:
          kind: pause
          length: short
      - text: "..."
      - control:
          kind: pause
          length: short
      - text: " Aber..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Wenn du einmal hierher zurückkehrst,\nsobald das alles vorbei ist..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Dann würden sich alle freuen,\ndenke ich."
  Quest_finish_02:
    attributes: Npc_oasis003
