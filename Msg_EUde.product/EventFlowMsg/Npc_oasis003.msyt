      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Aber dank dir habe ich jetzt endlich\nwieder Zeit für mich."
      - control:
          kind: pause
          length: short
      - text: " Dafür bin ich\ndir wirklich dankbar!"
  Quest_finish_07:
    attributes: Npc_oasis003
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 21
      - text: Huch!
      - control:
          kind: pause
          length: short
      - text: "\nNein, ich spiele nicht mit meiner\nSandrobbenpuppe!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ich bin schließlich kein kleines Kind\nmehr!"
      - control:
          kind: pause
          length: short
