      - text: Stall der Tabanta-Brücke
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: pause
          length: short
      - text: "\nDer Regen will gar nicht aufhören, was?"
  talk005:
    attributes: Npc_TabantaBridgeHatago001
    contents:
      - control:
