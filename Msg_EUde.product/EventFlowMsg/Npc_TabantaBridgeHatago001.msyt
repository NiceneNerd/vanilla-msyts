          unknown:
            - 4
            - 19
      - text: "Ach, der Norden..."
      - control:
          kind: pause
          length: long
      - text: " Den hätte ich ja\nfast vergessen..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Hinter dem Rhoam-Berg lebt ein\ngewisser "
      - control:
          kind: set_colour
          colour: blue
      - text: Kordt
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: pause
