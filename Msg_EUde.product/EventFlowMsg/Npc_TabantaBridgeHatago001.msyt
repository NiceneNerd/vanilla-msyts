  talk100:
    attributes: Npc_TabantaBridgeHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Wenn du etwas über die Gegend\nerfahren möchtest, frag mich einfach!"
      - control:
          kind: pause
          length: short
      - text: "\nWofür interessierst du dich denn?"
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
            - 2
            - 3
          selected_index: 0
          cancel_index: 3
          unknown: 10
  talk101:
    attributes: Npc_TabantaBridgeHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 6
      - text: Der Osten?
      - control:
          kind: pause
          length: short
      - text: " Hm, der Osten..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Mein Tantchen sagt immer, dass man\nin jungen Jahren Herausforderungen\nnicht scheuen sollte."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Mag sein, aber Orte wie die "
      - control:
          kind: set_colour
          colour: blue
      - text: "Harzblut-\nSümpfe"
      - control:
          kind: reset_colour
      - text: " sind den Aufwand nicht wert."
      - control:
          kind: pause
          length: short
      - text: "\nVon denen sollte man sich fernhalten."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Schon bevor mein Tantchen auf der\nWelt war, waren diese Sümpfe von\nschwarzen Wolken bedeckt."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 5
            - 0
      - text: "Für mich ist das ein Zeichen der Göttin\nHylia, dass jeder, der diesen Ort betritt,\nihren heiligen Zorn auf sich zieht."
  talk102:
    attributes: Npc_TabantaBridgeHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 6
      - text: "Im Osten sind die "
      - control:
          kind: set_colour
          colour: blue
      - text: Harzblut-Sümpfe
      - control:
          kind: reset_colour
      - text: ". Dort\nist etwas Seltsames passiert..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Die Sümpfe waren schon von dunklen\nWolken bedeckt, da war mein Tantchen\nnoch gar nicht auf der Welt."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: "Aber dann, ganz plötzlich, sind die\nWolken verschwunden!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Da muss Hylia ihre göttlichen Finger im\nSpiel gehabt haben!"
      - control:
          kind: pause
          length: short
      - text: " Vielleicht bringe ich\ndie Pferde mal dorthin..."
  talk111:
    attributes: Npc_TabantaBridgeHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 6
      - text: Der Süden also...
      - control:
          kind: pause
          length: short
      - text: " Mein Tantchen hat uns\nfrüher immer Geschichten erzählt..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Über den "
      - control:
          kind: set_colour
          colour: red
      - text: Herren des Waldes
      - control:
          kind: reset_colour
      - text: ", der über\nalles Leben am "
      - control:
          kind: set_colour
          colour: blue
      - text: "Satori-Berg "
      - control:
          kind: reset_colour
      - text: "beim\nTamiond-Fluss gebietet..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 9
      - text: "Diese alte Legende erzählt sie sogar\nheute noch den Kindern vor dem\nEinschlafen."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Es soll ja sogar einen Gott geben, der\nStälle beschützt."
      - control:
          kind: pause
          length: short
      - text: " Ich frage mich, wie der\nwohl aussieht."
  talk121:
    attributes: Npc_TabantaBridgeHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Was gibt's denn im Westen..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Im Westen liegt die "
      - control:
          kind: set_colour
          colour: blue
      - text: Tabanta-Brücke
      - control:
          kind: reset_colour
      - text: ", von\nder unser Stall seinen Namen hat."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Wenn du die Brücke überquerst und der\nStraße ein gutes Stück folgst, also ein\nwirklich gutes Stück..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Dann kommst du zum "
      - control:
          kind: set_colour
          colour: blue
      - text: Dorf der Orni
      - control:
          kind: reset_colour
      - text: "."
  talk122:
    attributes: Npc_TabantaBridgeHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Aber hast du diesen unheimlichen Vogel\ngesehen?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 5
            - 1
      - text: "Er kreist seit einiger Zeit über dem\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Dorf der Orni
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Falls du dorthin unterwegs bist, solltest\ndu auf der Hut sein."
  talk131:
    attributes: Npc_TabantaBridgeHatago001
    contents:
      - text: Bist du sicher?
      - control:
          kind: pause
          length: short
      - text: "\nSei auf jeden Fall vorsichtig."
      - control:
