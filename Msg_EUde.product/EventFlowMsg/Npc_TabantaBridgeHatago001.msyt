  talk002:
    attributes: Npc_TabantaBridgeHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: "Ja, willkommen im "
      - control:
          kind: set_colour
          colour: blue
      - text: "Stall der Tabanta-\nBrücke"
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: pause
          length: short
      - text: " Die Bedürfnisse der Pferde sind\nmir am wichtigsten!"
  talk003:
    attributes: Npc_TabantaBridgeHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 9
      - text: "Willkommen im "
      - control:
          kind: set_colour
          colour: blue
      - text: Stall der Tabanta-Brücke
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: pause
          length: short
      - text: "\nDie Gesichter von schlafenden Kindern\nmachen mich glücklich..."
  talk004:
    attributes: Npc_TabantaBridgeHatago001
    contents:
      - control:
          kind: sound
          unknown:
