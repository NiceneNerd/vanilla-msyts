    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Wegen des Monsters kommen wir noch\nnicht einmal in die Nähe des "
      - control:
          kind: set_colour
          colour: blue
      - text: Todesbergs
      - control:
          kind: reset_colour
