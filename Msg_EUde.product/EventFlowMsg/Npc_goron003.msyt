          kind: sound
          unknown:
            - 0
            - 0
      - text: "Bis vor Kurzem habe ich in einem\nBergwerk am "
      - control:
          kind: set_colour
          colour: blue
      - text: "Todesberg "
      - control:
          kind: reset_colour
      - text: "gearbeitet...\n\nAber seit "
      - control:
          kind: set_colour
          colour: blue
      - text: "Vah Rudania "
      - control:
          kind: reset_colour
      - text: "da ist, sind die\nMinen unzugänglich, und ich wurde\nentlassen!"
