      - control:
          kind: set_colour
          colour: blue
      - text: "Chef "
      - control:
          kind: reset_colour
      - text: "es mit seiner\n"
      - control:
          kind: set_colour
          colour: red
      - text: "Kanone "
      - control:
          kind: reset_colour
      - text: "zurück in den Krater, aber es\nkommt immer wieder heraus... "
      - control:
          kind: pause
          length: short
