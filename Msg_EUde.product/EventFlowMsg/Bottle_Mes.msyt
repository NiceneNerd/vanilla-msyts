  talk00:
    attributes: Npc_Zora026
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 11
      - text: Trage meinen Brief sicher zu ihm...
  talk01:
    attributes: Npc_Zora026
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: Oh...
      - control:
          kind: pause
          length: longer
      - text: "\nHast du auch einen Wunsch an die\nWasserfee?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Nein, oder?"
      - control:
          kind: pause
          length: short
      - text: " Du hast ja nicht mal einen\nBehälter oder so was dabei."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 6
      - text: Oder...
      - control:
          kind: pause
          length: short
      - text: "\nBist du etwa ein Bote der Wasserfee?"
      - control:
          kind: choice
          choice_labels:
            - 11
            - 12
          selected_index: 0
          cancel_index: 2
          unknown: 6
  talk02:
    attributes: Npc_Zora026
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: Dann habe ich eine Bitte an dich!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Folge dem "
      - control:
          kind: set_colour
          colour: red
      - text: Brief
      - control:
          kind: reset_colour
      - text: ", den ich schwimmen\nlasse, sprich mit der Person, die ihn\nfindet, und komme dann zu mir zurück!\n"
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "Ich möchte gerne wissen, was für eine\nPerson das ist..."
  talk03:
    attributes: Npc_Zora026
    contents:
      - control:
          kind: sound
          unknown:
            - 2
            - 14
      - text: "Du denkst, ich soll einfach selber gehen?"
      - control:
          kind: pause
          length: short
      - text: "\nJa, ich sehe es dir an der Nasenspitze\nan.\nAber meine Mutter möchte nicht, dass\nich weiter als hier von zu Hause\nweggehe...\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: Deshalb erfülle mir doch diese Bitte!
      - control:
          kind: pause
          length: short
      - text: "\nFinde denjenigen, der den Brief\naufhebt... "
      - control:
          kind: pause
          length: short
      - text: Ja?
      - control:
          kind: single_choice
          label: 20
      - text: "\n"
      - control:
          kind: sound
          unknown:
            - 7
            - 9
      - text: "Hihi-blubb... "
      - control:
          kind: pause
          length: short
      - text: Vielen Dank!
  talk04:
    attributes: Npc_Zora026
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 6
      - text: "Gut, dann werfe ich jetzt den Brief\nins Wasser!"
      - control:
          kind: pause
          length: longer
      - text: " Bist du bereit?"
      - control:
          kind: choice
          choice_labels:
            - 2
            - 3
          selected_index: 0
          cancel_index: 1
          unknown: 6
  talk05:
    attributes: Npc_Zora026
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Gut, ich werde noch etwas warten..."
      - control:
          kind: pause
          length: longer
      - text: "\nAber beeil dich bitte!"
  talk06:
    attributes: Npc_Zora026
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Du musst gut aufpassen, denn der\nBehälter ist sehr "
      - control:
          kind: set_colour
          colour: red
      - text: zerbrechlich
      - control:
          kind: reset_colour
      - text: "."
  talk07:
    attributes: Npc_Zora026
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Oh, du bist doch..."
  talk08:
    attributes: Npc_Zora026
    contents:
      - control:
          kind: sound
          unknown:
            - 2
            - 14
      - text: Das kann doch nicht sein...
      - control:
          kind: pause
          length: longer
      - text: "\nDu denkst wohl, nur weil ich ein Kind\nbin, würde ich alles glauben!"
  talk09:
    attributes: Npc_Zora026
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 9
      - text: Ein Reisender also...
      - control:
          kind: pause
          length: short
      - text: " Das trifft sich gut!"
  talk10:
    attributes: Npc_Zora026
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 6
      - text: "Hm, was ist?"
      - control:
