  talk100:
    attributes: Npc_Bottle_Mes001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: Guten Morgen!
  talk101:
    attributes: Npc_Bottle_Mes001
    contents:
      - control:
