      - text: Hallo!
  talk102:
    attributes: Npc_Bottle_Mes001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: Guten Abend!
  talk103:
    attributes: Npc_Bottle_Mes001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: Hallo!
  talk104:
    attributes: Npc_Bottle_Mes001
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 6
      - text: "Das kommt jetzt etwas plötzlich, aber\nwarst du schon mal im "
      - control:
          kind: set_colour
          colour: blue
      - text: Dorf der Zoras
      - control:
          kind: reset_colour
      - text: "?"
      - control:
          kind: choice
          choice_labels:
            - 6
            - 7
          selected_index: 0
          cancel_index: 1
          unknown: 6
  talk105:
    attributes: Npc_Bottle_Mes001
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "Verstehe... "
      - control:
          kind: pause
          length: longer
      - text: Ist das weit von hier?
      - control:
          kind: choice
          choice_labels:
            - 17
            - 18
          selected_index: 0
          cancel_index: 1
          unknown: 6
  talk106:
    attributes: Npc_Bottle_Mes001
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: Verstehe...
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Ah! "
      - control:
          kind: pause
          length: short
      - text: "'tschuldigung,"
      - control:
          kind: pause
          length: longer
      - text: " hab ganz vergessen,\nmich vorzustellen..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: Sasan mein Name.
      - control:
          kind: pause
          length: longer
      - text: " Ich bin ein fahrender\nHändler."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Ich war eigentlich auf dem Weg zum\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Dorf der Zoras
      - control:
          kind: reset_colour
      - text: ", da hab ich in der Bucht\nein Fass mit einem "
      - control:
          kind: set_colour
          colour: red
      - text: Brief
      - control:
          kind: reset_colour
      - text: " darin entdeckt.\nDer Absender war eine Zora namens\nFine."
      - control:
          kind: pause
          length: short
      - text: " Seitdem bin ich hier, und wir\nschicken uns Briefe hin und her.\nMeine Briefe werfe ich natürlich nicht ins\nWasser!"
      - control:
          kind: pause
          length: short
      - text: " Ich gebe sie Reisenden mit, die\nauf dem Weg zum "
      - control:
          kind: set_colour
          colour: blue
      - text: Dorf der Zoras
      - control:
          kind: reset_colour
      - text: " sind."
  talk107:
    attributes: Npc_Bottle_Mes001
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 1
      - text: Post von einer Zora...
      - control:
          kind: pause
          length: short
      - text: "\nDas macht neugierig, oder?"
      - control:
          kind: choice
          choice_labels:
            - 6
            - 7
          selected_index: 0
          cancel_index: 1
          unknown: 6
  talk108:
    attributes: Npc_Bottle_Mes001
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "Bedaure, aber den Inhalt der Briefe kann\nich dir nicht verraten."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Es wäre eine Taktlosigkeit, ihr Vertrauen\nauf diese Weise zu hintergehen."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 11
      - text: "Das Einzige, was ich dir sagen kann:"
      - control:
          kind: pause
          length: short
      - text: "\nIhre Sprache zeugt von einer eleganten,\nkultivierten Dame."
  talk109:
    attributes: Npc_Bottle_Mes001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 9
      - text: "Verstehe... "
      - control:
          kind: pause
          length: short
      - text: Dann ist es schon gut.
  talk11:
    attributes: Npc_Zora026
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: Gut...
      - control:
          kind: pause
          length: longer
      - text: " Wie gesagt, bitte lass den Brief\nnicht aus den Augen und pass auf, dass\nder Behälter nicht zerbricht."
  talk111:
    attributes: Npc_Bottle_Mes001
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 11
      - text: "Also... "
      - control:
          kind: pause
          length: short
      - text: "Als Nächstes müsste ich von ihr\nwieder einen Brief erhalten."
      - control:
          kind: pause
          length: longer
      - text: " Deshalb\nwerde ich noch eine Weile hier warten."
  talk113:
    attributes: Npc_Bottle_Mes001
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 19
      - text: "Hast du... "
      - control:
          kind: pause
          length: short
      - text: diesen Brief verfolgt?
      - control:
          kind: single_choice
          label: 6
  talk114:
    attributes: Npc_Bottle_Mes001
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 16
      - text: "Wahrscheinlich hat sie dich gebeten,\nherauszufinden, was für eine Art Typ\nich bin, oder?\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "In einem meiner Briefe habe ich mich als\n„todesmutigen Abenteurer“ beschrieben."
      - control:
          kind: pause
          length: short
      - text: "\nDanach wollte sie mich treffen."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Leider bin ich ein ziemlicher Angsthase!
      - control:
          kind: pause
          length: short
      - text: "\nUnd weit rumgekommen bin ich auch\nnicht..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 9
            - 12
      - text: So kann ich wohl kaum zu ihr gehen.
      - control:
          kind: pause
          length: long
      - text: "\nTja, dumm gelaufen."
      - control:
          kind: pause
          length: short
      - text: " Ich hätte wohl nicht\nlügen sollen..."
      - control:
          kind: choice
          choice_labels:
            - 5
            - 19
          selected_index: 0
          cancel_index: 1
          unknown: 6
  talk115:
    attributes: Npc_Bottle_Mes001
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "Innere Werte? "
      - control:
          kind: pause
          length: short
      - text: Hm...
      - control:
          kind: pause
          length: longer
      - text: "\nDanke, dass du das sagst."
  talk116:
    attributes: Npc_Bottle_Mes001
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: Du hast recht...
      - control:
          kind: pause
          length: longer
      - text: " Was nicht ist, kann ja\nnoch werden, oder?"
  talk119:
    attributes: Npc_Zora026
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 21
      - control:
          kind: text_size
          percent: 125
      - text: Aaah!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: text_size
          percent: 100
      - control:
          kind: sound
          unknown:
            - 3
            - 1
      - text: "Dabei hatte ich mir so eine Mühe\nmit diesem Brief gegeben..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 11
      - text: Aber gut.
      - control:
          kind: pause
          length: long
      - text: " Du hast das natürlich nicht\nmit Absicht gemacht."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Ich schreibe ihn einfach neu.
      - control:
          kind: pause
          length: short
      - text: "\nMir sind auch noch ein paar schöne\nFormulierungen eingefallen..."
  talk12:
    attributes: Npc_Zora026
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Unter den Mädchen erzählt man sich,\ndass man hier am "
      - control:
          kind: set_colour
          colour: blue
      - text: Ufer der Träume
      - control:
          kind: reset_colour
      - text: " einen\nFreund fürs Leben finden kann.\nMan muss nur einen "
      - control:
          kind: set_colour
          colour: red
      - text: "Brief "
      - control:
          kind: reset_colour
      - text: "im Fluss auf\ndie Reise schicken."
      - control:
          kind: pause
          length: longer
      - text: " Wenn jemand\nantwortet, hält die Freundschaft ewig!\n"
      - control:
          kind: sound
          unknown:
            - 7
            - 9
      - text: "Und endlich, endlich ist für mich ein\nBrief gekommen!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 7
            - 9
      - text: "Ich wüsste zu gern, wer der Absender\nist!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 9
            - 12
      - text: "Dummerweise hat meine Mutter mir\nverboten, weit weg zu gehen, deshalb\nkann ich es schlecht herausfinden...\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: Einen „todesmutigen Abenteurer“
      - control:
          kind: pause
          length: short
      - text: " hat\ner sich selbst in seinem Brief genannt."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 11
      - text: "Aufregend, oder?"
      - control:
          kind: pause
          length: short
      - text: " Bei seiner zarten\nHandschrift würde man das gar nicht\ndenken!"
  talk121:
    attributes: Npc_Zora126
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 21
      - control:
          kind: text_size
          percent: 125
      - text: Aaah!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: text_size
          percent: 100
      - text: "Mein Brief! Ich hatte ihn mit so viel\nLiebe geschrieben!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ich muss ihn wohl noch einmal neu\nschreiben..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 1
      - text: Aber gut.
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: "Einen Brieffreund zu haben ist gar nicht\nso einfach, aber es ist die Mühe wert!\n"
      - control:
          kind: pause
          length: short
      - text: Hihi-blubb...
  talk122:
    attributes: Npc_Bottle_Mes001
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 14
      - text: "Gut... "
      - control:
          kind: pause
          length: short
      - text: "Ich sollte wohl einfach mal meinen\nMut zusammennehmen und zu ihr\ngehen...\nHm..."
      - control:
          kind: pause
          length: short
      - text: " Die Gefahr einer Flut in der\nGegend scheint wohl auch abgewendet\nworden zu sein."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "In ihrem Brief hat sie geschrieben, sie\nwarte im "
      - control:
          kind: set_colour
          colour: blue
      - text: Dorf der Zoras
      - control:
          kind: reset_colour
      - text: " auf mich..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 11
      - text: Ich werde auf der Stelle dorthin laufen!
      - control:
          kind: pause
          length: long
      - text: "\nAuf dem Weg erlebe ich ja vielleicht\nschon ein paar Abenteuer...\nBis dann!"
      - control:
          kind: pause
          length: longer
      - text: "\nWir sehen uns im "
      - control:
          kind: set_colour
          colour: blue
      - text: Dorf der Zoras
      - control:
          kind: reset_colour
      - text: "!"
  talk123:
    attributes: Npc_Zora126
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 11
      - text: "Oh! "
      - control:
          kind: pause
          length: short
      - text: Du bist es!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Sasan!
      - control:
          kind: pause
          length: short
      - text: " Hör mal!"
      - control:
          kind: pause
          length: longer
      - text: " Das hier ist der nette\nHerr, der dafür gesorgt hat, dass mein\nBrief heil bei dir ankommt!"
  talk124:
    attributes: Npc_Bottle_Mes001
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 21
      - text: "Oh,"
      - control:
          kind: pause
          length: short
      - text: " hallo!"
      - control:
          kind: pause
          length: long
      - text: " Wie du siehst, habe ich es\ntatsächlich bis hier ins "
      - control:
          kind: set_colour
          colour: blue
      - text: Dorf der Zoras
      - control:
          kind: reset_colour
      - text: "\ngeschafft.\n"
      - control:
          kind: sound
          unknown:
            - 1
            - 11
      - text: "Dank dir hab ich den Mut aufgebracht,\nmich mit Fine zu treffen... "
      - control:
          kind: pause
          length: longer
      - text: "Zumindest\ndenke ich, dass du mir geholfen hast."
  talk125:
    attributes: Npc_Zora126
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 9
      - text: "Vielen Dank, dass du unserer\nFreundschaft auf die Sprünge\ngeholfen hast!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Sasan und ich waren uns einig, dass wir\ndir irgendeine kleine Aufmerksamkeit\nzukommen lassen sollten."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Hier, das ist von uns beiden!"
  talk126:
    attributes: Npc_Zora126
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: Sasan!
      - control:
          kind: pause
          length: long
