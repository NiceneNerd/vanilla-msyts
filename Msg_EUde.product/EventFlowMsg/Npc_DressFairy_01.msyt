  Revival00_1:
    attributes: Npc_DressFairy_01
    contents:
      - text: Dank dir habe ich meine Kraft zurück.
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Im Gegenzug kann ich deine "
      - control:
