          unknown:
            - 2
            - 205
      - text: Oh...
      - control:
          kind: pause
          length: longer
      - text: " Nicht alle meiner Schwestern sind\nganz bei Kräften."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
