      - text: Link!
      - control:
          kind: pause
          length: short
      - text: " Wir sind dir zu größtem Dank\nverpflichtet! "
      - control:
          kind: pause
          length: short
      - text: "Dank dir ist unser Dorf\ngerettet!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 1
      - text: "Auch die übrigen Zoras des Dorfes\nkönnen ihr Glück kaum glauben!"
  talk01:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 6
      - text: "Jedoch, Link..."
      - control:
          kind: pause
          frames: 20
      - text: " Eine Frage brennt uns \nauf der Zunge."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Du bist aus "
      - control:
          kind: set_colour
          colour: blue
      - text: "Vah Ruta "
      - control:
          kind: reset_colour
      - text: zurückgekehrt...
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 3
            - 1
      - text: Mipha jedoch nicht.
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 3
            - 16
      - text: "Ist es, wie wir fürchten?"
      - control:
          kind: pause
          frames: 20
      - text: "\nWar es schon zu spät?"
      - control:
          kind: single_choice
          label: 0
  talk02:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 6
      - text: Ihren Geist?
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Das heißt, Mipha ist..."
      - control:
          kind: pause
          frames: 20
      - text: "\nHat sie etwas gesagt?"
  talk04:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 12
      - text: "Oh, Mipha..."
      - control:
          kind: pause
          frames: 20
      - text: "\nUnd wir konnten nichts tun..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 5
            - 1
      - text: "Doch letztendlich... "
      - control:
          kind: pause
          frames: 20
      - text: "hat Mipha ihre\nPflicht als Recke erfüllt."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Als ihr Vater
      - control:
          kind: pause
          frames: 20
      - text: " ist es unsere Aufgabe,\nüber sie zu wachen."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "..."
      - control:
          kind: pause
          frames: 22
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: Link...
      - control:
          kind: pause
          frames: 20
      - text: "\nSie hat dich wirklich geliebt."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 1
      - text: "Dir erneut zur Seite stehen zu können\nerfüllt sie sicherlich mit Freude."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 6
      - text: "Bitte versprich uns, dass du Mipha\nniemals vergisst."
      - control:
          kind: choice
          choice_labels:
            - 2
            - 3
          selected_index: 0
          cancel_index: 2
          unknown: 6
  talk05:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: Wir wissen das zu schätzen.
      - control:
          kind: pause
          frames: 20
      - text: "\nDu hast ein gutes Herz."
  talk06:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: Wir danken dir für deine Worte.
      - control:
          kind: pause
          frames: 20
      - text: "\nSie bedeuten uns viel."
  talk07:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 4
      - text: "Wenn du jemals in Schwierigkeiten sein\nsolltest, dann wende dich an uns, Link."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 1
      - text: "Unser ganzes Volk wird dich jederzeit\nmit offenen Armen empfangen."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Wir danken dir, Link..."
      - control:
          kind: pause
          frames: 20
      - text: "\nVon ganzem Herzen."
  talk08:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 11
      - text: "Wir danken dir, Link."
      - control:
          kind: pause
          frames: 20
      - text: "\nDu hast uns alle gerettet."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Sicherlich ist Mipha glücklich, dir\nerneut beistehen zu können."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Wir verfolgen deinen und Miphas Kampf\nvon hier aus mit."
