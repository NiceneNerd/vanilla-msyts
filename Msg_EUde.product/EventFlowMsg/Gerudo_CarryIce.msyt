    attributes: Npc_OasisMilk_E
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 6
      - text: "Nanu? "
      - control:
          kind: pause
          length: long
      - text: Wer bist denn du...?
      - control:
          kind: pause
          length: long
      - text: " Na, was\nsoll's..."
      - control:
          kind: pause
          length: short
      - text: " Mir egal, wer du bist, solange\nich mein Eis kriege."
  Npc_oasis025_S02_201:
    attributes: Npc_OasisMilk_E
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Hast du das Eis immer noch nicht\nbesorgt?"
      - control:
          kind: pause
          length: short
      - text: " Frag Ante, sie müsste dir\netwas Eis geben."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Würdest du das bitte übernehmen?
