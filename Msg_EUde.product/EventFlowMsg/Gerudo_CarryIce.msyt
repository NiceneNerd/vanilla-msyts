  Npc_GerudoDesert002_F_001:
    attributes: Npc_GerudoDesert002
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 2
      - text: Das ist mein Ende...
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ich werde einfach hier austrocknen\nwie ein Frosch in der Wüste..."
      - control:
          kind: single_choice
          label: 17
  Npc_GerudoDesert002_F_002:
    attributes: Npc_GerudoDesert002
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Hm?
  Npc_GerudoDesert002_F_003:
    attributes: Npc_GerudoDesert002
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 20
      - text: "Ein "
      - control:
          kind: set_colour
          colour: blue
      - text: Vaai will Vooi
      - control:
          kind: reset_colour
      - text: "? "
      - control:
          kind: pause
          length: short
      - text: "Der beste\nDrink meines Lebens? "
      - control:
          kind: pause
          length: short
      - text: "Was mache\nich dann noch hier!?"
  Npc_GerudoDesert002_S02_001:
    attributes: Npc_GerudoDesert003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: Pass gut auf!
  Npc_GerudoDesert003_F_001:
    attributes: Npc_GerudoDesert003
    contents:
      - text: "Du hast Furosa erfolgreich das Eis\ngebracht, oder?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ich muss auch mal endlich wieder einen\n"
      - control:
          kind: set_colour
          colour: blue
      - text: "Vaai will Vooi "
      - control:
          kind: reset_colour
      - text: bei Furosa trinken.
  Npc_GerudoDesert003_R_001:
    attributes: Npc_GerudoDesert003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Das hier ist der "
      - control:
          kind: set_colour
          colour: blue
      - text: Eiskeller
      - control:
          kind: reset_colour
      - text: ", wo wir Eis von\nden Schneebergen im Norden lagern."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Dieses Eis ist für uns sehr wertvoll,\ndeshalb passe ich hier rund um die\nUhr darauf auf."
  Npc_GerudoDesert003_S01_001:
    attributes: Npc_GerudoDesert003
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 18
      - text: Verstehe...
      - control:
          kind: pause
          length: short
      - text: "\nWenn Furosa darum bittet, dann kann\nich nicht Nein sagen.\nGut, dann nimm das Eis mit!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Aber denk dran, wie heiß es hier ist."
      - control:
          kind: pause
          length: short
      - text: "\nWenn du unterwegs herumtrödelst, wird\ndas Eis schmelzen, bevor du da bist.\nAlso gib auf dich acht und bring das Eis\nschnell zu Furosa!"
  Npc_GerudoDesert003_S01_002:
    attributes: Npc_GerudoDesert003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Das hier ist der "
      - control:
          kind: set_colour
          colour: blue
      - text: Eiskeller
      - control:
          kind: reset_colour
      - text: ", wo wir Eis von\nden Schneebergen im Norden lagern."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Dieses Eis ist für uns sehr wertvoll,\ndeshalb passe ich hier auf, damit\nniemand es klauen kann."
      - control:
          kind: single_choice
          label: 17
  Npc_GerudoDesert003_S01_101:
    attributes: Npc_GerudoDesert003
    contents:
      - control:
          kind: animation
          name: Think_01
      - control:
          kind: text_size
          percent: 80
      - text: Wer da...?
      - control:
          kind: pause
          length: short
      - text: "\nFür heute... ist Feierabend..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Wenn's... was gibt...\nkomm morgen wieder..."
      - control:
          kind: pause
          length: short
      - text: " Zzz..."
      - control:
          kind: text_size
          percent: 100
  Npc_GerudoDesert003_S01_102:
    attributes: Npc_GerudoDesert003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Für heute habe ich Feierabend.
      - control:
          kind: single_choice
          label: 17
  Npc_GerudoDesert003_S01_103:
    attributes: Npc_GerudoDesert003
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 18
      - text: "Tut mir leid, aber für heute habe ich\nFeierabend."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ich helfe Furosa immer gerne, aber der\nBetriebsrat achtet streng auf unsere\nArbeitszeiten!"
  Npc_GerudoDesert003_S01_104:
    attributes: Npc_GerudoDesert003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 18
      - text: "Tut mir leid, aber ich bin müde."
      - control:
          kind: pause
          length: short
      - text: "\nAlso lass mich schlafen, ja?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Wenn du etwas von mir willst, dann\nkomm doch morgen wieder."
  Npc_GerudoDesert003_S02_001:
    attributes: Npc_GerudoDesert003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 6
      - text: Sag bloß...
  Npc_GerudoDesert003_S02_002:
    attributes: Npc_GerudoDesert003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Kein Wunder eigentlich...
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Selbst wir schaffen es manchmal nicht\nschnell genug, das Eis zu transportieren."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Mach dir keine Gedanken. "
      - control:
          kind: pause
          length: short
      - text: "Nimm einfach\nnoch eines!"
  Npc_GerudoDesert003_S02_101:
    attributes: Npc_GerudoDesert003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Du weißt ja, wie heiß es in der Wüste ist."
      - control:
          kind: pause
          length: short
      - text: "\nWenn du unterwegs herumtrödelst, wird\ndas Eis schmelzen, bevor du da bist."
      - control:
          kind: choice
          choice_labels:
            - 7
            - 8
          selected_index: 0
          cancel_index: 2
          unknown: 6
  Npc_GerudoDesert003_S02_102:
    attributes: Npc_GerudoDesert003
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: Also wirklich...
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Eine Bitte von Furosa werde ich nicht\nausschlagen, aber denk dran, dass das\nEis hier sehr wertvoll ist."
  Npc_GerudoMilkflost_002:
    attributes: Npc_OasisMilk_E
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Vasaaq...
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Bist du eine Hylianerin?
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ich weiß nicht, wie alt du bist, aber ich\nbin ziemlich sicher, dass es noch nicht\nfür unseren Laden reicht.\nWir verwenden ein ganz besonderes Eis."
      - control:
          kind: pause
          length: short
      - text: "\nAm besten kommst du wieder, wenn du\nden Unterschied zu schätzen weißt."
      - control:
          kind: choice
          choice_labels:
            - 14
            - 16
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Npc_GerudoMilkflost_004:
    attributes: Npc_OasisMilk_E
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 25
      - text: "Willst du unsere Spezialität probieren,\nden "
      - control:
          kind: set_colour
          colour: blue
      - text: Vaai will Vooi
      - control:
          kind: reset_colour
      - text: "?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Tut mir leid, aber um den zu mixen,\nbrauchen wir eine Menge Eis."
      - control:
          kind: pause
          length: short
      - text: " Und\ngenau das ist leider gerade aus."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Das Zeug ist aber auch wirklich stark...
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Einem zarten Pflänzchen wie dir würde\nich den eh nicht einschenken!"
      - control:
          kind: single_choice
          label: 17
      - text: " "
  Npc_GerudoMilkflost_005:
    attributes: Npc_OasisMilk_E
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: Da fällt mir auf...
      - control:
          kind: pause
          length: short
      - text: "\nPokki macht sich in letzter Zeit rar..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Früher kam sie fast jeden Abend hierher,\num einen "
      - control:
          kind: set_colour
          colour: blue
      - text: "Vaai will Vooi "
      - control:
          kind: reset_colour
      - text: zu trinken...
      - control:
          kind: pause
          length: longer
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Oh, Entschuldigung."
      - control:
          kind: pause
          length: short
      - text: "\nMit dir hat das alles natürlich\nnichts zu tun."
  Npc_GerudoMilkflost_006:
    attributes: Npc_OasisMilk_E
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 13
      - text: "Man hat's wirklich nicht leicht mit\ndiesem Mädchen..."
  Npc_GerudoMilkflost_007:
    attributes: Npc_OasisMilk_E
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Der Eiskeller ist nördlich der Stadt.
      - control:
          kind: pause
          length: short
      - text: " Wenn\ndu Ante dort alles erklärst, gibt sie dir\nsicher etwas Eis.\n"
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: Sie geht allerdings sehr früh ins Bett...
      - control:
          kind: pause
          length: short
      - text: "\nWenn du gehen willst, dann solange es\nnoch hell ist."
  Npc_GerudoMilkflost_008:
    attributes: Npc_GerudoDesert002
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "Wo ich den Schrein doch grade\ngefunden hatte..."
      - control:
          kind: pause
          length: longer
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Hmgh...
      - control:
          kind: pause
          length: longer
      - text: "\nIch hab so einen Durst..."
      - control:
          kind: pause
          length: longer
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Kann...
      - control:
          kind: pause
          length: longer
      - text: " mich nicht bewegen..."
      - control:
          kind: pause
          length: longer
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Ich will nur noch einen...
      - control:
          kind: pause
          length: longer
      - text: "\nNur noch einen allerletzten..."
      - control:
          kind: pause
          length: longer
      - text: "\nSchluck...\nEin schöner eiskalter "
      - control:
          kind: set_colour
          colour: red
      - text: Vaai will Vooi
      - control:
          kind: reset_colour
      - text: "..."
  Npc_GerudoMilkflost_016:
    attributes: Npc_OasisMilk_E
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Mit deiner Hilfe konnte ich einen\nhammermäßigen "
      - control:
          kind: set_colour
          colour: blue
      - text: "Vaai will Vooi "
      - control:
          kind: reset_colour
      - text: mixen.
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Würdest du das "
      - control:
          kind: set_colour
          colour: blue
      - text: Pokki
      - control:
          kind: reset_colour
      - text: " sagen? Dass hier\nder beste Drink ihres Lebens auf sie\nwartet?\nWenn ich sie richtig einschätze, ist\nsie dann schneller hier, als man\n„Prost Mahlzeit“ sagen kann..."
  Npc_GerudoMilkflost_017:
    attributes: Npc_OasisMilk_E
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 0
      - text: Wie bitte?!
      - control:
          kind: pause
          length: short
      - text: " Pokki hat das gesagt?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Aber ohne Eis kann ich keinen machen,\nauch wenn ich natürlich gerne helfen\nwürde.\nIm "
      - control:
          kind: set_colour
          colour: red
      - text: Eiskeller
      - control:
          kind: reset_colour
      - text: " bekäme man Eis. "
      - control:
          kind: pause
          length: short
      - text: "Wenn ich\nnur nicht so schlecht zu Fuß wäre..."
      - control:
          kind: single_choice
          label: 22
  Npc_GerudoMilkflost_018:
    attributes: Npc_OasisMilk_E
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Ich hab mich schon gewundert, wo Pokki\nabgeblieben ist, aber DAS hätte ich nie\ngedacht...\n"
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "Warum muss mir gerade jetzt das Eis\nausgegangen sein..."
      - control:
          kind: choice
          choice_labels:
            - 20
            - 21
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Npc_GerudoMilkflost_019:
    attributes: Npc_OasisMilk_E
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 11
      - text: Das würdest du tun?
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Das wäre mir eine große Hilfe!
      - control:
          kind: pause
          length: short
      - text: " Pokki ist\neine unserer besten Kundinnen, musst\ndu wissen.\nDer "
      - control:
          kind: set_colour
          colour: red
      - text: Eiskeller
      - control:
          kind: reset_colour
      - text: " ist nördlich der Stadt."
      - control:
          kind: pause
          length: short
      - text: " Wenn\ndu Ante dort alles erklärst, gibt sie dir\nsicher etwas Eis.\n"
