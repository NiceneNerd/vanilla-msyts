      - control:
          kind: reset_colour
      - text: mixen!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Würdest du "
      - control:
          kind: set_colour
          colour: blue
      - text: Pokki
      - control:
