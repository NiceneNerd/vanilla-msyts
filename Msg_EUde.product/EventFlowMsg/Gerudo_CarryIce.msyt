      - control:
          kind: pause
          length: short
      - text: "\nWenn du gehen willst, dann solange es\nnoch hell ist."
  Npc_GerudoMilkflost_020:
    attributes: Npc_OasisMilk_E
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Oder,"
      - control:
          kind: pause
          length: long
      - text: " warte mal..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Dich das Eis durch die komplette Wüste\nund bis in die Stadt schleppen zu lassen\nist vielleicht etwas viel verlangt.\nZwischen der Stadt und dem Eiskeller\ngibt es Ruinen. "
      - control:
          kind: pause
          length: short
      - text: "Bis zum "
      - control:
          kind: set_colour
          colour: red
      - text: "Eingang dieser\nRuinen"
      - control:
          kind: reset_colour
      - text: " kann ich dir entgegenkommen.\nBitte bring das Eis einfach dorthin!"
  Npc_oasis025_S02_001:
    attributes: Npc_OasisMilk_E
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Könntest du das Eis bitte hierhin\nstellen?"
  Npc_oasis025_S02_002:
    attributes: Npc_OasisMilk_E
    contents:
      - control:
          kind: sound2
          unknown:
            - 20
            - 205
      - text: "HEEEE! "
      - control:
          kind: pause
          length: long
      - text: Hier bin ich!
      - control:
          kind: pause
          length: short
      - text: "\nBitte bring das Eis hierher!"
  Npc_oasis025_S02_101:
    attributes: Npc_OasisMilk_E
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 25
      - text: "Ja... "
      - control:
          kind: pause
          length: short
      - text: "Das ist zweifellos "
      - control:
          kind: set_colour
          colour: red
      - text: "Eis aus dem\nEiskeller"
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Damit kann ich einen hammermäßigen\n"
      - control:
          kind: set_colour
          colour: blue
      - text: "Vaai will Vooi "
      - control:
          kind: reset_colour
      - text: mixen!
      - control:
          kind: raw
