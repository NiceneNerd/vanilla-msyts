  Npc_oasis025_S02_104:
    attributes: Npc_OasisMilk_E
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 25
      - text: Das Eis sieht jedenfalls gut aus.
      - control:
          kind: pause
          length: short
      - text: "\nSarq-sarq!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Damit kann ich einen hammermäßigen\n"
      - control:
          kind: set_colour
          colour: blue
