          kind: reset_colour
      - text: ", \nzusammen mit "
      - control:
          kind: set_colour
          colour: blue
      - text: Stalbokblins
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Andere Quellen besagen, es befinde sich\nsogar jenseits der Ebene ganz drüben\nim Osten, vor diesen seltsamen "
      - control:
          kind: set_colour
          colour: blue
      - text: Ruinen
      - control:
          kind: reset_colour
      - text: ".\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 9
      - text: Du machst das schon!
