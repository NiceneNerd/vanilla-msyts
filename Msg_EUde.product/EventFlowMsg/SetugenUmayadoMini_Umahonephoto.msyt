    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 0
      - text: Oh?
      - control:
          kind: pause
          length: short
      - text: " Du magst mir also doch helfen?"
      - control:
          kind: pause
          length: short
      - text: "\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Wie gesagt, es geht um das\n"
