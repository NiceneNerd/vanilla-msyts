    attributes: Npc_Hunters_Lost
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 13
      - text: Was soll das denn?
      - control:
          kind: pause
          length: short
      - text: " \nDu hast ja gar kein Bild..."
      - control:
          kind: pause
          length: long
      - text: "\nTja, ist nicht so einfach zu finden, was?"
  NPC_Umahonephoto001_R_005:
    attributes: Npc_Hunters_Lost
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 9
      - text: Danke!
      - control:
          kind: pause
          length: short
      - text: " Ein tolles Bild, damit kann ich\neinen großartigen Artikel schreiben!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Hier, wie abgemacht!"
  NPC_Umahonephoto001_R_006:
    attributes: Npc_Hunters_Lost
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Meine Nachforschungen haben mich\nnach Ost und West geführt, und jetzt bin\nich im Norden gelandet..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Hier soll es ein "
      - control:
          kind: set_colour
          colour: red
      - text: Knochenpferd
      - control:
          kind: reset_colour
      - text: " geben,\naber es ist zu kalt, um weiterzusuchen."
      - control:
          kind: pause
          length: short
      - text: "\nDas ist es nicht wert..."
      - control:
          kind: choice
          choice_labels:
            - 2
            - 3
          selected_index: 0
          cancel_index: 1
          unknown: 6
  NPC_Umahonephoto001_R_007:
    attributes: Npc_Hunters_Lost
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "Danke, ich werde wohl einfach die Zähne\nzusammenbeißen müssen. "
      - control:
          kind: pause
          length: short
      - text: "Als älteste\nSchwester muss ich ein Vorbild sein."
  NPC_Umahonephoto001_R_008:
    attributes: Npc_Hunters_Lost
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - control:
          kind: text_size
          percent: 125
      - text: Ah!
      - control:
          kind: text_size
          percent: 100
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Meine Intuition in Verbindung mit deiner\nFrage sagt mir, dass du dich für das\n"
      - control:
          kind: set_colour
          colour: red
      - text: Knochenpferd
      - control:
          kind: reset_colour
      - text: " interessierst!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ich will dieses "
      - control:
          kind: set_colour
          colour: red
      - text: Monster
      - control:
          kind: reset_colour
      - text: " mit eigenen\nAugen sehen! "
      - control:
          kind: pause
          length: short
      - text: "Es wäre unglaublich,\nwenn es wirklich existieren würde!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 3
            - 12
      - text: "Tja... "
      - control:
          kind: pause
          length: short
      - text: "Nur leider komme ich zwar mit\nallen möglichen Gefahren klar, aber bei\ndieser Kälte hört der Spaß auf."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Würdest du mir helfen? "
      - control:
          kind: pause
          length: short
      - text: "Natürlich gegen\nBelohnung!"
      - control:
          kind: pause
          length: short
      - text: " Ein Bild oder ein paar\nNotizen oder so würden mir reichen!"
      - control:
          kind: choice
          choice_labels:
            - 4
            - 5
          selected_index: 0
          cancel_index: 1
          unknown: 6
