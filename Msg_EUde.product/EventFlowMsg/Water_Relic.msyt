---
group_count: 101
atr1_unknown: 4
entries:
  "0000":
    attributes: ""
    contents:
      - text: Ich habe geschlafen.
  "0001":
    attributes: ""
    contents:
      - text: Erinnere mich nicht.
  "0002":
    attributes: ""
    contents:
      - text: Mipha?
  "0003":
    attributes: ""
    contents:
      - text: Ich weiß nicht...
  "0004":
    attributes: ""
    contents:
      - text: Ja!
  "0005":
    attributes: ""
    contents:
      - text: Noch nicht.
  "0008":
    attributes: ""
    contents:
      - text: "Ja, hab ich!"
  "0009":
    attributes: ""
    contents:
      - text: Nur ein bisschen...
  "0010":
    attributes: ""
    contents:
      - text: Es tut mir leid...
  "0011":
    attributes: ""
    contents:
      - text: Wer weiß...
  "0012":
    attributes: ""
    contents:
      - text: Alles klar.
  "0013":
    attributes: ""
    contents:
      - text: Darf ich wirklich?
  "0014":
    attributes: ""
    contents:
      - text: Das Bannschwert?
  "0015":
    attributes: ""
    contents:
      - text: Sieht so aus...
  "0016":
    attributes: ""
    contents:
      - text: Überlass das mir.
  "0017":
    attributes: ""
    contents:
      - text: "Sag's noch mal."
  "0018":
    attributes: ""
    contents:
      - text: Ja!
  "0019":
    attributes: ""
    contents:
      - text: Hatte ich eh vor...
  "0020":
    attributes: ""
    contents:
      - text: Ich... erinnere mich!
  "0022":
    attributes: ""
    contents:
      - control:
          kind: animation
          name: CustomPose
      - text: Natürlich!
  "0023":
    attributes: ""
    contents:
      - text: "Ja, zurückziehen!"
  "0024":
    attributes: ""
    contents:
      - text: "Nein, weitermachen!"
  "0025":
    attributes: ""
    contents:
      - text: Zeldas Stimme...
  "0026":
    attributes: ""
    contents:
      - text: Sie ist im Schloss...
  Npc_Zora003_F_001:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 11
      - text: "Oh, Link! "
      - control:
          kind: pause
          length: short
      - text: "Es ist gut, dich gesund und\nmunter wiederzusehen! "
      - control:
          kind: pause
          frames: 20
      - text: "Wir haben\nungeduldig auf dich gewartet!"
  Npc_Zora003_F_002:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Der Regen hat aufgehört, als wäre er\nnur ein böser Traum gewesen. "
      - control:
          kind: pause
          length: short
      - text: "Somit ist\nunser Dorf nicht länger in Gefahr!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 1
      - text: "Und auch der Rest von "
      - control:
          kind: set_colour
          colour: blue
      - text: Hyrule
      - control:
          kind: reset_colour
      - text: " ist jetzt\nnicht mehr von einer Überflutung\nbedroht!"
  Npc_Zora003_F_003:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 11
      - text: "Der Dauerregen hat ein Ende, und der\nTitan ist zurückgekehrt."
      - control:
          kind: pause
          length: short
      - text: " Es gibt einiges\nzu feiern!"
  Npc_Zora003_F_004:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 16
      - text: Übrigens...
      - control:
          kind: pause
          frames: 20
      - text: " Link, wo ist denn\ndas "
      - control:
          kind: set_colour
          colour: blue
      - text: heilige Bannschwert
      - control:
          kind: reset_colour
      - text: "?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 4
            - 6
      - text: "Als du dein Gedächtnis verloren hast,"
      - control:
          kind: pause
          length: short
      - text: "\nhast du da auch das Bannschwert\nverloren?"
      - control:
          kind: choice
          choice_labels:
            - 14
            - 15
          selected_index: 0
          cancel_index: 2
          unknown: 6
  Npc_Zora003_F_005:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Wir sind uns sicher, es wartet irgendwo\nin Hyrule darauf, dass dein Weg dich zu\nihm führt."
  Npc_Zora003_F_006:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Sicher hat es deinen langen Schlaf\nüberdauert und wartet irgendwo auf\ndie Rückkehr seines Besitzers."
  Npc_Zora003_F_007:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 1
      - text: "Das "
      - control:
          kind: set_colour
          colour: blue
      - text: heilige Bannschwert
      - control:
          kind: reset_colour
      - text: "... "
      - control:
          kind: pause
          length: short
      - text: "Das erinnert\nuns an etwas."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "In den "
      - control:
          kind: set_colour
          colour: blue
      - text: Verlorenen Wäldern
      - control:
          kind: reset_colour
      - text: " soll ein\nberühmtes Schwert ruhen..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ob es das "
      - control:
          kind: set_colour
          colour: blue
      - text: "heilige Bannschwert "
      - control:
          kind: reset_colour
      - text: "ist, das\ndu vor 100 Jahren führtest, vermögen\nwir nicht zu sagen. "
      - control:
          kind: pause
          length: short
      - text: "Möglich wäre es!\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Die "
      - control:
          kind: set_colour
          colour: blue
      - text: "Verlorenen Wälder "
      - control:
          kind: reset_colour
      - text: "liegen westlich\nvon hier, im Norden von "
      - control:
          kind: set_colour
          colour: blue
      - text: Schloss Hyrule
      - control:
          kind: reset_colour
      - text: "."
  Npc_Zora003_F_008:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: Link...
      - control:
          kind: pause
          length: short
      - text: " Im Namen aller Zoras danken\nwir dir für das, was du hier verrichtet\nhast!"
  Npc_Zora003_F_009:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: "Als dein Vater sind wir von großem Stolz\nerfüllt,"
      - control:
          kind: pause
          frames: 20
      - text: " dass du an Links Seite mit dem\n"
      - control:
          kind: set_colour
          colour: blue
      - text: "Titanen "
      - control:
          kind: reset_colour
      - text: "gerungen hast.\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Du hast dich zu einem guten Zora\nentwickelt. "
      - control:
          kind: pause
          frames: 20
      - text: "Eines Tages wirst du ein\nwürdiger Nachfolger für uns werden!"
  Npc_Zora003_F_010:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 16
      - text: "Obwohl... Da du das heilige Bannschwert\nbesitzt, hast du wahrscheinlich wenig\nGelegenheiten, sie zu verwenden.\n"
      - control:
          kind: sound
          unknown:
            - 5
            - 1
      - text: "Dieses Schwert hat die Macht, das Böse\nzu bannen. Nur du kannst es führen."
      - control:
          kind: pause
          frames: 20
      - text: "\nDu solltest es wie einen Schatz hüten!"
  Npc_Zora003_F_011:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 16
      - text: "Du kannst dich wirklich nicht\ndaran erinnern?!"
  Npc_Zora003_F_012:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 12
      - text: Du hast es einfach verloren?!
  Npc_Zora003_F_013:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 1
      - text: "Du warst der Einzige der Recken, der\ndieses Schwert führen konnte."
  Npc_Zora003_F_014:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Sicher hat es deinen langen Schlaf\nüberdauert und wartet in irgendeinem\nWald auf die Rückkehr seines Besitzers."
  Npc_Zora003_F_015:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 2
      - text: Link!
      - control:
          kind: pause
          frames: 20
      - text: "\nErlaube uns, dir zum Dank eine\nBelohnung zu überreichen!"
  Npc_Zora003_F_016:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 4
      - text: "Und du, Sidon..."
  Npc_Zora003_F_018:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 1
      - text: "Weil du den "
      - control:
          kind: set_colour
          colour: blue
      - text: "Wassertitanen "
      - control:
          kind: reset_colour
      - text: "besänftigt\nhast, ist das "
      - control:
          kind: set_colour
          colour: blue
      - text: Dorf der Zoras
      - control:
          kind: reset_colour
      - text: " gerettet!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: "Du hast mehr vollbracht, als wir zu\nhoffen gewagt hatten!"
      - control:
          kind: pause
          length: short
      - text: " Im Namen unseres\nVolkes möchten wir dir danken!"
  Npc_Zora003_F_019:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Bitte nimm den Inhalt dieser\nSchatztruhe an dich."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 1
      - text: "Darin befindet sich ein Andenken an\nMipha."
      - control:
          kind: pause
          length: short
      - text: " Ein Gegenstand, der ihr lieb\nund teuer war...\nAls ein Zeichen unserer Freundschaft\nmöchten wir, dass du ihn von nun an\nverwendest."
      - control:
          kind: pause
          frames: 20
      - text: " Halte ihn in Ehren!"
  Npc_Zora003_F_020:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 10
      - text: Bwahuohuohuo!
  Npc_Zora003_R_001:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 6
      - text: "Hm? "
      - control:
          kind: pause
          frames: 10
      - text: Wie siehst du denn aus!
      - control:
          kind: pause
          frames: 20
      - text: "\nBist du..."
      - control:
          kind: pause
          frames: 20
      - text: " ein Hylianer?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Hat unser Sohn Sidon dich etwa\ngebeten, zu uns zu kommen? "
      - control:
          kind: pause
          frames: 20
      - text: "Es freut\nuns jedenfalls, dass du hier bist!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 5
            - 4
      - text: "Aber... "
      - control:
          kind: pause
          frames: 20
      - text: "Wo ist denn unser Sohn? "
      - control:
          kind: pause
          frames: 20
      - text: "Seid ihr\nnicht zusammen ins Dorf gekommen?"
  Npc_Zora003_R_002:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 6
      - text: "Oh, du bist ein Hylianer, oder nicht?"
  Npc_Zora003_R_003:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 6
      - text: Oooh!
      - control:
          kind: pause
          frames: 20
      - text: "\nDu bist also der Hylianer, den Sidon\ngefunden hat?"
  Npc_Zora003_R_004:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 1
      - text: "Hm? "
      - control:
          kind: pause
          frames: 20
      - text: Was da an deinem Gürtel hängt...
      - control:
          kind: pause
          frames: 20
      - text: "\nIst das etwa ein "
      - control:
          kind: set_colour
          colour: blue
      - text: Shiekah-Stein
      - control:
          kind: reset_colour
      - text: "?!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - control:
          kind: text_size
          percent: 125
      - text: Nanu?
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: text_size
          percent: 100
      - text: Lass dich noch einmal genau ansehen...
  Npc_Zora003_R_005:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 16
      - text: "Was! "
      - control:
          kind: pause
          frames: 10
      - text: Geschlafen?
      - control:
          kind: pause
          frames: 20
      - text: "\nDeshalb also erinnerst du dich nicht\nan uns?"
  Npc_Zora003_R_006:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 16
      - text: "Was! "
      - control:
          kind: pause
          frames: 20
      - text: "Du hast also dein Gedächtnis\nverloren?"
  Npc_Zora003_R_007:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 6
      - text: "Aber an unsere Tochter Mipha "
      - control:
          kind: pause
          frames: 10
      - text: "erinnerst\ndu dich?"
      - control:
          kind: choice
          choice_labels:
            - 2
            - 3
          selected_index: 0
          cancel_index: 2
          unknown: 6
  Npc_Zora003_R_008:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 12
      - text: "Jetzt sage aber nicht, du hast selbst\nMipha vergessen?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 3
            - 1
      - text: Ihr wart doch immer so enge Freunde!
      - control:
          kind: pause
          frames: 10
      - text: "\nUnd sie hast du vergessen?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 5
            - 1
      - text: Sieh dich einmal in unserem Dorf um...
  Npc_Zora003_R_010:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 1
      - text: "Wirklich eine eigentümliche Fügung, "
      - control:
          kind: pause
          frames: 20
      - text: "dass\ndu, ohne es zu wissen, den Recken zu\nuns geführt hast..."
  Npc_Zora003_R_011:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "Du musst wissen, "
      - control:
          kind: pause
          frames: 10
      - text: "dass unser Dorf sich\ndurch das Wüten des Wassertitanen\n"
      - control:
          kind: set_colour
          colour: blue
      - text: "Vah Ruta "
      - control:
          kind: reset_colour
      - text: "in großer Gefahr befindet.\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: Lass uns direkt auf den Punkt kommen...
      - control:
          kind: pause
          frames: 20
      - text: "\nOhne fremde Hilfe kommen wir nicht\ngegen ihn an!\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 6
      - text: "Würdest du uns in unserer Not\nbeistehen?"
  Npc_Zora003_R_012:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 12
      - text: Muzu...
      - control:
          kind: pause
          frames: 20
      - text: "\nFängst du schon wieder damit an..."
  Npc_Zora003_R_013:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 12
      - text: Link...
      - control:
          kind: set_colour
          colour: blue
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: Ruta
      - control:
          kind: reset_colour
      - text: " ist der Titan des Wassers..."
      - control:
          kind: pause
          frames: 10
      - text: "\nEr kann unendliche Mengen Wasser\nhervorbringen."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 3
            - 1
      - text: "Vor Kurzem begann er plötzlich damit,\nWasser in die Luft zu spritzen, "
      - control:
          kind: pause
          frames: 20
      - text: "das dann\nhier als Regen wieder herunterkommt.\n"
      - control:
          kind: sound
          unknown:
            - 5
            - 1
      - text: "Wasser ist unser Element, "
      - control:
          kind: pause
          frames: 20
      - text: "also wäre das\neigentlich noch keine Bedrohung für uns."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 16
      - text: "Doch durch den Dauerregen füllt\nsich der "
      - control:
          kind: set_colour
          colour: blue
      - text: Stausee
      - control:
          kind: reset_colour
      - text: " im Osten von hier\nin bedrohlichem Maße.\n"
      - control:
          kind: sound
          unknown:
            - 3
            - 1
      - text: "Wenn der Damm bricht, "
      - control:
          kind: pause
          frames: 10
      - text: "wird nicht nur\nunser Dorf zerstört..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Auch die Hylianer, die flussabwärts\nwohnen, sind bedroht!"
  Npc_Zora003_R_014:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 11
      - text: "Vielen Dank, Link!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 5
            - 0
      - text: "Es muss Schicksal sein, dass deine\nZiele so genau mit den unseren\nübereinstimmen."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 0
      - text: "Erlaube, dass wir dir ohne weitere\nUmschweife das hier überreichen!"
  Npc_Zora003_R_015:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 16
      - text: Hmm...
      - control:
          kind: pause
          frames: 20
      - text: "\nDer Ruf des Titanen..."
  Npc_Zora003_R_016:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "Der Wassertitan "
      - control:
          kind: set_colour
          colour: blue
      - text: Vah Ruta
      - control:
          kind: reset_colour
      - text: "..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: In der Zeit vor der Verheerung
      - control:
          kind: pause
          frames: 10
      - text: " hat\neure Prinzessin Zelda die Titanen\nerforscht.\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "In ihren Schriften "
      - control:
          kind: pause
          frames: 10
      - text: "erwähnte sie auch die\nVorrichtungen auf "
      - control:
          kind: set_colour
          colour: blue
      - text: Rutas
      - control:
          kind: reset_colour
      - text: " Schultern..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Laut ihrer Erkenntnisse handelt es sich\ndabei um Maschinen, "
      - control:
          kind: pause
          frames: 10
      - text: "die mithilfe von\nElektrizität das "
      - control:
          kind: set_colour
          colour: red
      - text: Wasser kontrollieren
      - control:
          kind: reset_colour
      - text: ".\n"
      - control:
          kind: sound
          unknown:
            - 5
            - 1
      - text: "Doch seit der Titan wütet, fließt kein\nStrom mehr durch sie."
  Npc_Zora003_R_017:
