entries:
  Atacked00:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 18
      - text: "Hui... "
      - control:
          kind: pause
          length: short
      - text: Das war echt nicht ungefährlich...
  Atacked01:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 32
      - text: "Leise... "
      - control:
          kind: pause
          length: short
      - text: "Ich kann noch immer die\nGegenwart eines Monsters spüren..."
  Failed00:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 44
      - text: "Oje... "
      - control:
          kind: pause
          length: short
      - text: das war nicht ohne.
  Failed01:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 44
      - text: "Verflucht! "
      - control:
          kind: pause
          length: short
      - text: Diese Monster...!
  Guardian00:
    attributes: Npc_Road_023
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 32
      - control:
          kind: set_colour
          colour: blue
      - text: "Wächter "
      - control:
          kind: reset_colour
      - text: im Anmarsch!
      - control:
          kind: pause
          length: short
      - text: " Fort von hier, so\nschnell wie möglich!"
  MSG_Rane_00:
    attributes: Npc_Road_023
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 0
      - text: "Heja, tapferer Abenteurer!"
      - control:
          kind: pause
          length: short
      - text: " Schon mal\nwas von der Region Akkala, nordöstlich\nvon hier, gehört?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Such auf der Karte einfach nach 'nem\nGewässer in Form eines Totenkopfs!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Passt übrigens perfekt zu Akkala...
  MSG_Rane_00_1:
    attributes: Npc_Road_023
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Akkala liegt nordöstlich von hier. "
      - control:
          kind: pause
          length: long
      - text: "\nSuch auf der Karte einfach nach 'nem\nGewässer in Form eines Totenkopfs!"
  MSG_Rane_01:
    attributes: Npc_Road_023
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 0
      - text: "Heja, junger Abenteurer!"
      - control:
          kind: pause
          length: short
      - text: " Schon mal\ndran gedacht, beim Totenkopf-Teich\nvorbeizuschauen?\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Falls ja, dann darfst du dir auf keinen Fall\nden Monsterladen da entgehen lassen!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Hab ich zumindest mal gehört...
  MSG_Rane_01_1:
    attributes: Npc_Road_023
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Na, hast du geplant, mal beim\nTotenkopf-Teich vorbeizuschauen?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Falls ja, dann suche den Monsterladen!\n"
      - control:
          kind: pause
          length: short
      - text: "Ich würde dort gar nichts kaufen, aber\nmich interessiert, wer der Besitzer ist..."
  MSG_Rane_02:
    attributes: Npc_Road_023
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 0
      - text: "Heja, tapferer Abenteurer! "
      - control:
          kind: pause
          length: long
      - text: "Dort im\nSüdwesten, wo ich hinreite, da liegt\nder Stall am Fluss!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Zieht's dich aber weiter nach Südosten,\ndann kommst du irgendwann nach\nKakariko."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Sieh doch mal auf deine Karte!
      - control:
          kind: pause
          length: short
      - text: " Ich wette,\ndie beiden Orte sind genau gleich weit\nvon hier entfernt..."
  MSG_Rane_02_1:
    attributes: Npc_Road_023
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Der Stall am Fluss liegt im Südwesten\nund Kakariko im Südosten."
      - control:
          kind: pause
          length: long
      - text: " Ist beides in\netwa gleich weit –"
      - control:
          kind: pause
          length: short
      - text: " sieh mal auf die Karte!"
  MSG_Rane_03:
    attributes: Npc_Road_023
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 0
      - text: "Heja, Abenteurer!"
      - control:
          kind: pause
          length: long
      - text: " Der Weg zum Stall am\nFluss ist kein Spaziergang, oder?"
      - control:
          kind: pause
          length: short
      - text: " Zu\nFuß tut man sich da keinen Gefallen!\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Wenn man größere Entfernungen gehen\nwill, legt man sich am besten ein Pferd\nzu. "
      - control:
          kind: pause
          length: long
      - text: "Und eine Karte auch, unbedingt!"
  MSG_Rane_03_1:
    attributes: Npc_Road_023
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Bis zum Stall am Fluss ist's noch ein\nganzes Stück."
      - control:
          kind: pause
          length: short
      - text: " Da lohnt es sich, eine\nKarte und ein Pferd anzuschaffen!"
  MSG_Rane_04:
    attributes: Npc_Road_023
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Heja, junger Abenteurer!"
      - control:
          kind: pause
          length: long
      - text: " Schon gewusst,\ndass dir für Erz mehr als nur ein paar\nPferdeäpfel gezahlt werden?\nBevor dir das Erz aus der Satteltasche\npurzelt, verwandle es lieber in einen\nordentlichen Batzen Rubine!"
  MSG_Rane_04_1:
    attributes: Npc_Road_023
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Bevor dir das Erz aus der Satteltasche\npurzelt, verwandle es lieber in einen\nordentlichen Batzen Rubine!"
  Near00:
    attributes: Npc_Road_023
    contents:
      - text: Welch frische Luft!
  Near01:
    attributes: Npc_Road_023
    contents:
      - text: Hüha!
  Near02:
    attributes: Npc_Road_023
    contents:
      - text: Ganz ruhig!
  Near03:
    attributes: Npc_Road_023
    contents:
      - text: Brrr...
  Near04:
    attributes: Npc_Road_023
    contents:
      - text: Ist ja gut...
  Rest_00:
    attributes: Npc_Road_023
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 19
      - text: "Das ist so gar nicht meine Art, aber ich\nwill dir mal ganz in Ruhe was erklären."
      - control:
          kind: pause
          length: short
      - text: "\nAlso pass auf,"
      - control:
          kind: pause
          length: short
      - text: " Abenteurer..."
      - control:
          kind: pause
          length: short
      - text: "\nEs gibt da einen Monsterladen, der nur\nnachts aufhat, aber dafür ein paar ganz\nbesondere Waren anbietet..."
  Rest_01:
    attributes: Npc_Road_023
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "Das ist nicht dein Tag, Abenteurer..."
      - control:
          kind: pause
          length: short
      - text: "\nHeute bleibt meine große Klappe\nnämlich verschlossen!"
  Rest_02:
    attributes: Npc_Road_023
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ich weiß, du bist ein Abenteurer, darum\nmüsste ich dir das gar nicht erklären,"
      - control:
          kind: pause
          length: short
      - text: "\naber ich mach's trotzdem!\nAlso, wenn ein Abenteurer im Regen an\nFelswänden herumturnt, kann er ganz\nschnell in sein Unglück schlittern!\nSag ich doch, das wusstest du schon!"
  RetMSG_Rane_00:
    attributes: Npc_Road_023
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 0
      - text: "Heja, furchtloser Abenteurer!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Wenn du nach Akkala gehst, brauchst\ndu auf jeden Fall eine ordentliche\nGrundausrüstung!\nAm besten besorgst du dir die unten am\nTodesberg, nördlich von hier!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Warum der so heißt?
      - control:
          kind: pause
          length: long
      - text: " Ha ha, dreimal\ndarfst du raten!"
  RetMSG_Rane_00_1:
    attributes: Npc_Road_023
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ausrüstung für deinen Ausflug nach\nAkkala besorgst du dir am besten unten\nam Todesberg, nördlich von hier."
  RetMSG_Rane_01:
    attributes: Npc_Road_023
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 0
      - text: "Heja, fescher Abenteurer!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 5
            - 0
      - text: "Wenn du auf dem Weg nach\nSchloss Hyrule bist, dann lass dir gesagt\nsein, dass es dort von Gefahren wimmelt!\nFalls du außerdem glaubst, den Schatz\ndeines Lebens zu finden, mach dich\ndarauf gefasst, ebenjenes zu verlieren!"
  RetMSG_Rane_01_1:
    attributes: Npc_Road_023
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "Wer in Schloss Hyrule nach Schätzen\nsucht, setzt sein Leben aufs Spiel."
      - control:
          kind: pause
          length: short
      - text: "\nIch hoffe, du weißt, was du tust!"
  RetMSG_Rane_02:
    attributes: Npc_Road_023
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 0
      - text: "Heja, unerschrockener Abenteurer!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Es geht das Gerücht um, dass es an der\nWestseite vom Schloss Hyrule 'ne\nziemlich geheime Höhle gibt!\nEs sollen sich sogar schon welche auf\nder Suche nach Schätzen schnurstracks\nin die Höhle reinbegeben haben.\nTja, das war dann aber auch das Letzte,\nwas man von denen gehört hat!"
  RetMSG_Rane_02_1:
    attributes: Npc_Road_023
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "An der Westseite von Schloss Hyrule gibt\nes wohl eine geheime Schatzhöhle."
      - control:
          kind: pause
          length: long
      - text: "\nAber niemand traut sich hin..."
  RetMSG_Rane_03:
    attributes: Npc_Road_023
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 0
      - text: "Heja, junger Abenteurer!"
      - control:
          kind: pause
          length: long
      - text: " Mir ist da so ein\nGerücht zu Ohren gekommen..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ist aber mit Vorsicht zu genießen,"
      - control:
          kind: pause
          length: short
      - text: "\nvielleicht will jemand damit auch nur\nein paar Langfinger in die Falle locken...\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Also,"
      - control:
          kind: pause
          length: long
      - text: " das Gerücht besagt, dass man\nSchloss Hyrule auch durch die Bucht\nhinter Hyrule-Stadt erreichen kann...!\nVon mir hast du das nicht!"
      - control:
          kind: pause
          length: short
      - text: " Verstanden?"
  RetMSG_Rane_03_1:
    attributes: Npc_Road_023
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ein Gerücht besagt, dass man\nSchloss Hyrule von Hyrule-Stadt aus\nauch übers Wasser erreichen kann...\nKann aber auch sein, dass es nur eine\nabsichtlich gestreute Fehlinformation ist,\num Langfinger in die Falle zu locken..."
  RetMSG_Rane_04:
    attributes: Npc_Road_023
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Hör mal, Abenteurer! "
      - control:
          kind: pause
          length: long
      - text: "Wenn du auf\ndeinen Reisen über Erz stolperst,\nnimm's mit! \nErz kann man immer zu 'nem guten Preis\nverkaufen... "
      - control:
          kind: pause
          length: long
      - text: "Da lohnt sich's richtig, seine\nSatteltaschen bis obenhin vollzupacken!"
  RetMSG_Rane_04_1:
    attributes: Npc_Road_023
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Erz kann man immer zu 'nem guten Preis\nverkaufen... "
      - control:
          kind: pause
          length: long
      - text: "Da lohnt sich's richtig, seine\nSatteltaschen bis obenhin vollzupacken!"
  SavedLv1_00:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 11
      - text: "Mein Retter! "
      - control:
          kind: pause
          length: short
      - text: "So stellt man sich einen\nerstklassigen Abenteurer vor! "
      - control:
          kind: pause
          length: short
      - text: "Stets zur\nStelle, wenn andere in Not sind!"
  SavedLv1_01:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 11
      - text: "Hab Dank! "
      - control:
          kind: pause
          length: short
      - text: "Es ist eine Freude, dir bei der\nArbeit zuzusehen!"
  SavedLv2_00:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: Das war eine knappe Angelegenheit!
  SavedLv2_01:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 18
      - text: "Gefahr ist das Salz in der Suppe eines\nAbenteurers?"
      - control:
          kind: pause
          length: long
      - text: " Dann fand ich persönlich\ndiese Suppe etwas versalzen..."
  SavedLv3_00:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 18
      - text: "Als ich diesem Monster ins Gesicht sah,\nbin ich fast aus dem Sattel gefallen!"
  SavedLv3_01:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 32
      - text: "Ich werde mich wohl besser von hier\nverkrümeln, solange es einigermaßen\nsicher ist!"
  Thanks00:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 0
      - text: Ich bin dir zu Dank verpflichtet!
