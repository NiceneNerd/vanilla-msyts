    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Mipha war die Prinzessin der Zoras und\ndie ältere Schwester von Prinz Sidon."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
