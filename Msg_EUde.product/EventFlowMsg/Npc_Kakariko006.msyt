          kind: set_colour
          colour: blue
      - control:
          kind: pause
          length: short
      - text: " "
      - control:
          kind: reset_colour
      - text: "Diese zwei Arten solltest\ndu dir zumindest einprägen."
      - control:
          kind: choice
          choice_labels:
            - 9
            - 10
          selected_index: 0
          cancel_index: 2
          unknown: 6
  Talk32:
