          colour: blue
      - text: Yiga
      - control:
          kind: reset_colour
      - text: ".\n"
      - control:
          kind: sound
          unknown:
            - 11
            - 1
      - control:
          kind: sound2
          unknown:
            - 15
            - 205
      - text: "Sie greifen alle an, die sich "
      - control:
