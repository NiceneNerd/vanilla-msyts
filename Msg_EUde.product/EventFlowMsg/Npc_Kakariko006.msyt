    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Diejenigen, die der "
      - control:
          kind: set_colour
          colour: red
      - text: Verheerung Ganon
      - control:
          kind: reset_colour
      - text: "\nihre Treue geschworen haben, nennen\nsich "
      - control:
