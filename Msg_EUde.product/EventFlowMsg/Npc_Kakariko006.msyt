    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Visiere deinen Gegner an und springe\nnach hinten."
      - control:
          kind: set_colour
          colour: blue
      - control:
          kind: pause
          length: short
      - text: " "
      - control:
          kind: reset_colour
      - text: "Unter Fachleuten nennt\nman das einen Rückwärtssalto!\n"
      - control:
          kind: sound2
          unknown:
            - 7
            - 205
      - text: Hohoho!
      - control:
          kind: pause
          length: short
      - text: " Für so einen alten Knacker\nkenne ich tolle Worte, was?"
