          cancel_index: 1
          unknown: 6
  Talk27:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 13
      - text: "Du... Du hast alles beobachtet?\n\n\nIch liege seit Jahren mit Gustin im\nClinch...\n\nWir waren früher bisweilen zusammen auf\nder Jagd."
      - control:
          kind: pause
          length: short
      - text: " Ich näherte mich der Beute\nimmer geduckt und ganz, ganz langsam...\n"
      - control:
          kind: sound
          unknown:
            - 8
            - 7
      - text: "Er dagegen schrie „Attacke!“ und stürmte\nblindlings voraus!\n\n"
      - control:
          kind: sound
          unknown:
            - 9
            - 1
      - text: "Natürlich ist uns die Beute jedes Mal\nentkommen.\n\nAußerdem wehrt er sich dickköpfig\ndagegen, dass sie im "
      - control:
          kind: set_colour
          colour: blue
      - text: Zutatenladen
      - control:
          kind: reset_colour
      - text: "\nmeine Kürbisse verkaufen.\n"
      - control:
          kind: sound2
          unknown:
            - 10
            - 205
      - text: Aber was ich getan habe...
      - control:
          kind: pause
          length: longer
      - text: "\nDas ist wirklich unverzeihlich."
  Talk28:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 18
      - text: "Hohoho! Na, dann ist ja gut!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Aber... Ich bin gerade etwas beschäftigt.
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Wenn du mit mir sprechen willst, kannst\ndu später noch mal kommen?"
  Talk29:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 22
      - text: "Du bist aber nicht etwa einer von den\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Yiga
      - control:
          kind: reset_colour
      - text: ", oder"
      - control:
          kind: pause
          length: short
      - text: "."
      - control:
          kind: pause
          length: short
      - text: "."
      - control:
          kind: pause
          length: short
      - text: "."
      - control:
          kind: pause
          length: short
      - text: "?"
  Talk30:
