entries:
  "0000":
    attributes: Npc_HighMountain033
    contents:
      - text: Mach ich.
  "0001":
    attributes: Npc_HighMountain033
    contents:
      - text: Weiß ja nicht...
  Talk_000:
    attributes: Npc_HighMountain033
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "...Wer bist denn du?"
      - control:
          kind: pause
          length: long
      - text: "\nEin Reisender?"
      - control:
          kind: pause
          length: long
      - text: " Na gut..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Dann sorgst du dafür, dass der große\nVogel am Himmel uns in Ruhe lässt?"
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Talk_001:
    attributes: Npc_HighMountain033
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 0
      - control:
          kind: text_size
          percent: 125
      - text: Auf keinen Fall!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: text_size
          percent: 100
      - text: "Kihl sagt, dass ich dann wieder singen\nüben muss!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - control:
          kind: text_size
          percent: 80
      - text: "...Es tut mir wirklich leid, dass Mollis Papa Hertis\nverletzt wurde, aber..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: text_size
          percent: 100
      - control:
          kind: sound
          unknown:
            - 8
            - 0
      - text: Aber ich mag einfach nicht singen üben!
  Talk_002:
    attributes: Npc_HighMountain033
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 18
      - text: "Na ja...\nSo stark wie Teba siehst du sowieso\nnicht aus."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Wenn du versuchen würdest, gegen\nden "
      - control:
          kind: set_colour
          colour: blue
      - text: "Geistervogel "
      - control:
          kind: reset_colour
      - text: "zu kämpfen, würde es\nbestimmt nicht gut für dich ausgehen!\n"
      - control:
          kind: sound
          unknown:
            - 10
            - 1
      - text: "Vielleicht fliegt der "
      - control:
          kind: set_colour
          colour: blue
      - text: "Geistervogel "
      - control:
          kind: reset_colour
      - text: "ja weg,\nwenn alle hier im Dorf sich möglichst\nwenig bewegen.\n"
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - control:
          kind: text_size
          percent: 80
      - text: "...Aber wenn er wegfliegt, muss ich nur wieder\nsingen üben..."
      - control:
          kind: pause
          length: long
      - text: " Pah..."
      - control:
          kind: text_size
          percent: 100
  Talk_003:
    attributes: Npc_HighMountain033
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - control:
          kind: text_size
          percent: 80
      - text: "Solange der Vogel da ist, brauchen wir nicht zu\nüben."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 10
            - 1
      - control:
          kind: text_size
          percent: 100
      - text: "Oh! "
      - control:
          kind: pause
          length: short
      - text: "Du darfst Mama nicht erzählen, dass\nich das gesagt habe!"
  Talk_030:
    attributes: Npc_HighMountain033
    contents:
      - control:
          kind: sound2
          unknown:
            - 31
            - 205
      - text: Püüüh...
  Talk_100:
    attributes: Npc_HighMountain033
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 2
      - text: "Oh, hallo!"
      - control:
          kind: pause
          length: short
      - text: " Sieh mal, ich übe!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 3
            - 0
      - text: "Wenn du nicht mit Kihl gesprochen\nhättest, wäre ich vielleicht immer noch\nso selbstsüchtig wie vorher...\n"
      - control:
          kind: sound
          unknown:
            - 7
            - 9
      - text: "Aber dank dir weiß ich jetzt, dass man\nmanchmal auch an andere denken muss!"
  Talk_101:
    attributes: Npc_HighMountain033
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 0
      - text: "Wenn ich hier übe, hören nur die Berge\nzu, also ist es mir egal!"
  Talk_110:
    attributes: Npc_HighMountain033
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 2
      - text: Ich gehe beim Schrein Singen üben!
  Talk_111:
    attributes: Npc_HighMountain033
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 9
      - text: "Der Häuptling hat mich gelobt und\ngesagt, dass ich schon richtig gut singe!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Erst esse ich einen "
      - control:
          kind: set_colour
          colour: blue
      - control:
          kind: raw
          two_hundred_one:
            dynamic:
              - 0
              - len: 4
                field_2:
                  - 1
                  - 0
                  - 0
                  - 0
      - text: "Knusperlachs "
      - control:
          kind: reset_colour
      - text: "und\ndann übe ich morgen gleich weiter!"
  Talk_200:
    attributes: Npc_HighMountain033
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 2
      - text: "Oh, hallo! "
      - control:
          kind: pause
          length: short
      - text: "Ich habe mit meinem Papa\ngeübt!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Als ich den Schrein an den "
      - control:
          kind: set_colour
          colour: red
      - text: "Brüder-\nFelsen "
      - control:
          kind: reset_colour
      - text: "erwähnt habe, hat er mir von dir\nerzählt!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Es gibt bestimmt jede Menge Schreine!
      - control:
          kind: pause
          length: short
      - text: "\nDas ist wie eine große Familie mit vielen\nGeschwistern!\n"
      - control:
          kind: sound2
          unknown:
            - 7
            - 205
      - text: "Wenn du es schaffst, alle zu finden,\ngratulier ich dir!"
  Talk_201:
    attributes: Npc_HighMountain033
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 4
      - text: Es gibt bestimmt ganz viele Schreine!
      - control:
          kind: pause
          length: short
      - text: "\nWenn du es schaffst, alle zu finden,\ngratulier ich dir!"
  Talk_210:
    attributes: Npc_HighMountain033
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 0
      - text: "Ich gehe zum Schrein, Singen üben!"
  Talk_220:
    attributes: Npc_HighMountain033
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 0
      - text: "Der Häuptling hat mich gelobt und\ngesagt, dass ich schon richtig gut singe!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Erst esse ich einen "
      - control:
          kind: set_colour
          colour: blue
      - control:
