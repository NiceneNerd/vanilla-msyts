    contents:
      - text: Bin ich.
  "0001":
    attributes: ""
    contents:
      - text: Geht so.
  "0002":
    attributes: ""
    contents:
      - text: In Ordnung.
  "0003":
    attributes: ""
    contents:
      - text: Warte mal kurz...
  "0004":
    attributes: ""
    contents:
      - text: Ich bin ein Hylianer!
  "0005":
    attributes: ""
    contents:
      - text: "Nein, bin ich nicht."
  "0006":
    attributes: ""
    contents:
      - text: Das bin ich.
  "0007":
    attributes: ""
    contents:
      - text: Nein.
  "0008":
    attributes: ""
    contents:
      - text: Klar.
  "0009":
    attributes: ""
    contents:
      - text: Ich bin in Eile...
  "0010":
    attributes: ""
    contents:
      - text: Bist doch auch eine.
  "0011":
    attributes: ""
    contents:
      - text: Ja!
  "0012":
    attributes: ""
    contents:
      - text: Nein.
  "0016":
    attributes: ""
    contents:
      - text: "Ja, bitte."
  "0017":
    attributes: ""
    contents:
      - text: Nicht nötig.
  "0018":
    attributes: ""
    contents:
      - text: Was machst DU hier?
  Npc_Lanayru003_R_001:
    attributes: Npc_Lanayru003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: Hallo!
      - control:
          kind: pause
          length: short
      - text: " Das ist ja eine Überraschung."
      - control:
          kind: pause
          length: short
      - text: "\nIn dieser Gegend trifft man nicht viele\nHylianer."
      - control:
          kind: single_choice
          label: 10
  Npc_Lanayru003_R_002:
    attributes: Npc_Lanayru003
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 16
      - text: "Hier am "
      - control:
          kind: set_colour
          colour: blue
      - text: Zora-Fluss
      - control:
          kind: reset_colour
      - text: " sind gerade viele\nZoras unterwegs, die alle wollen, dass\nman zu "
      - control:
          kind: set_colour
          colour: red
      - text: "Sidon "
      - control:
          kind: reset_colour
      - text: "geht, ihrem "
      - control:
          kind: set_colour
          colour: red
      - text: Prinzen
      - control:
          kind: reset_colour
      - text: "!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Tja.
      - control:
          kind: pause
          length: short
      - text: " Ich hab mich breitschlagen lassen\nund bin zur "
      - control:
          kind: set_colour
          colour: blue
      - text: Ingogo-Brücke
      - control:
          kind: reset_colour
      - text: " gegangen..."
  Npc_Lanayru003_R_003:
    attributes: Npc_Lanayru003
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: Ich bin eine fahrende Händlerin.
      - control:
          kind: pause
          length: longer
      - text: "\nKein Ort ist zu abgelegen für mich!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Ah, vielleicht sollte ich mich erst einmal\nvorstellen..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 4
      - text: "Ich heiße Kleo! "
      - control:
          kind: pause
          length: longer
      - text: "Freut mich, deine\nBekanntschaft zu machen!"
  Npc_Lanayru003_R_004:
    attributes: Npc_Lanayru003
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 12
      - text: "Ich habe mit diesem "
      - control:
          kind: set_colour
          colour: red
      - text: Prinzen
      - control:
          kind: reset_colour
      - text: " gesprochen,\naber"
      - control:
          kind: pause
          length: short
      - text: " na ja... "
      - control:
          kind: pause
          length: longer
      - text: "Das ist ein ganz komischer\nTyp!\nEr meinte „Ich habe dich beobachtet!“"
      - control:
          kind: pause
          length: short
      - text: "\nund hat mich ganz unverhohlen gefragt,\nob ich mit ihm nach Hause komme!\n"
      - control:
          kind: sound
          unknown:
            - 11
            - 18
      - text: "Deshalb hab ich mich ganz schnell\nverabschiedet und mich dünne gemacht."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 5
            - 1
      - text: "Na, neugierig geworden? "
      - control:
          kind: pause
          length: short
      - text: "Sieh dir "
      - control:
          kind: set_colour
          colour: red
      - text: "Prinz\nSidon"
      - control:
          kind: reset_colour
      - text: " ruhig einmal mit eigenen Augen\nan!"
      - control:
          kind: pause
          length: longer
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: "Als Mann wird er dich sicher nicht so\ndreist anmachen. "
      - control:
          kind: pause
          length: short
      - text: Aber wer weiß...
  Npc_Lanayru003_R_005:
    attributes: Npc_Lanayru003
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 6
      - text: Aber sag mal...
      - control:
          kind: pause
          length: short
      - text: "\nHast du eben mit diesem Zora\ngesprochen?"
      - control:
          kind: choice
          choice_labels:
            - 11
            - 12
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Npc_Lanayru003_R_006:
    attributes: Npc_Lanayru003
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 1
      - text: "Dieser Zora, der die ganze Zeit darauf\ndrängt,"
      - control:
          kind: pause
          length: short
      - text: " man solle zum Dorf der Zoras\ngehen"
      - control:
          kind: pause
          length: short
      - text: " und mit "
      - control:
          kind: set_colour
          colour: red
      - text: Prinz Sidon
      - control:
          kind: reset_colour
      - text: " sprechen."
      - control:
          kind: choice
          choice_labels:
            - 11
            - 12
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Npc_Lanayru003_R_007:
    attributes: Npc_Lanayru003
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 14
      - text: Warum lügst du mich denn an?
      - control:
          kind: pause
          length: longer
      - text: "\nIch konnte seine Stimme bis hier hören!"
  Npc_Lanayru003_R_008:
    attributes: Npc_Lanayru003
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 14
      - text: "Was hast du denn davon, das vor mir zu\nverbergen?"
      - control:
          kind: pause
          length: longer
      - text: " Ich hab alles mitgehört!"
  Npc_Lanayru003_R_009:
    attributes: Npc_Lanayru003
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 1
      - text: "Aber wie dem auch sei...\n"
      - control:
          kind: pause
          length: short
      - text: Wo war ich?
  Npc_Lanayru003_R_010:
    attributes: Npc_Lanayru003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: Hallo...
      - control:
          kind: pause
          length: short
      - text: "\nGehst du zum "
      - control:
          kind: set_colour
          colour: blue
      - text: Dorf der Zoras
      - control:
          kind: reset_colour
      - text: "?"
      - control:
          kind: choice
          choice_labels:
            - 11
            - 12
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Npc_Lanayru003_R_011:
    attributes: Npc_Lanayru003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Pass bloß auf mit diesem "
      - control:
          kind: set_colour
          colour: red
      - text: Prinzen
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: pause
          length: short
      - text: "\nMir war der zu stürmisch."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Aber du bist ein Kerl... "
      - control:
          kind: pause
          length: short
      - text: "Vielleicht hast du\nGlück und er hält sich bei Männern ein\nbisschen zurück."
  Npc_Lanayru003_R_012:
    attributes: Npc_Lanayru003
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "Ja, das ist wohl besser... "
      - control:
          kind: pause
          length: short
      - text: "Als er meinte,\ner hätte mich beobachtet, gingen bei mir\ndie Alarmglocken los."
  Npc_Lanayru003_R_Near:
    attributes: Npc_Lanayru003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 3
      - text: Hier! Hier!
  Npc_Zora001_R_001:
    attributes: Npc_Zora001
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 4
      - text: "Mein Name ist Tacca, und ich bin ein\nZora!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 1
      - text: "Du bist ein Hylianer, oder?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 5
            - 6
      - control:
          kind: text_size
          percent: 125
      - text: "Bist du doch, oder?!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: text_size
          percent: 100
      - control:
          kind: sound
          unknown:
            - 7
            - 10
      - control:
          kind: text_size
          percent: 125
      - text: "Ja, das bist du!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: text_size
          percent: 100
      - control:
          kind: sound
          unknown:
            - 4
            - 1
      - text: "Du bist unsere Rettung! "
      - control:
          kind: set_colour
          colour: red
      - control:
          kind: pause
          length: short
      - text: Sidon
      - control:
          kind: reset_colour
      - text: ", der Prinz\nder Zoras, sucht schon seit Tagen\nhänderingend nach einem Hylianer!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Er müsste gerade irgendwo bei der\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Ingogo-Brücke
      - control:
          kind: reset_colour
      - text: " sein."
      - control:
          kind: pause
          length: longer
      - text: " Du musst sofort\nzu ihm!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 1
      - text: "...Halt, halt. "
      - control:
          kind: pause
          length: short
      - text: "Verzeih. "
      - control:
          kind: pause
          length: short
      - text: "Ich kann dich\nnatürlich nicht zwingen..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 16
      - control:
          kind: text_size
          percent: 80
      - text: "Dann wiederum:"
      - control:
          kind: pause
          length: short
      - text: " Vielleicht sollte ich dich\nzwingen...?"
      - control:
          kind: text_size
          percent: 100
  Npc_Zora001_R_002:
    attributes: Npc_Zora001
    contents:
      - control:
          kind: text_size
          percent: 125
      - control:
          kind: sound
          unknown:
            - 1
            - 2
      - text: "Hallo! "
      - control:
          kind: set_colour
          colour: blue
      - control:
          kind: pause
          length: longer
      - text: Hylianer
      - control:
          kind: reset_colour
      - text: "!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: text_size
          percent: 100
      - control:
          kind: sound
          unknown:
            - 1
            - 1
      - text: "Du siehst wie ein starker Krieger aus...\n"
      - control:
          kind: pause
          length: short
      - text: Ich habe eine Bitte an dich!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Unser Prinz, "
      - control:
          kind: set_colour
          colour: red
      - text: Sidon
      - control:
          kind: reset_colour
      - text: ", ist bei der\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Ingogo-Brücke
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: pause
          length: short
      - text: " Bitte sprich mit ihm!"
  Npc_Zora001_R_003:
    attributes: Npc_Zora001
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 4
      - control:
          kind: set_colour
          colour: red
      - text: Prinz Sidon
      - control:
          kind: reset_colour
      - text: " befindet sich da hinten,\nbei der "
      - control:
          kind: set_colour
          colour: blue
      - text: Ingogo-Brücke
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Folge dem "
      - control:
          kind: set_colour
          colour: blue
      - text: Zora-Fluss
      - control:
          kind: reset_colour
      - text: " stromaufwärts,\ndann solltest du ihn finden."
  Npc_Zora001_R_004:
    attributes: Npc_Zora001
    contents:
      - text: Hylianer!
      - control:
          kind: pause
          length: longer
      - text: " Ich verlasse mich auf dich!"
  Npc_Zora001_R_005:
    attributes: Npc_Zora001
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 19
      - text: "Oh! "
      - control:
          kind: pause
          length: short
      - text: "Du da! "
      - control:
          kind: pause
          length: short
      - text: So warte doch!
  Npc_Zora001_R_006:
    attributes: Npc_Zora001
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 3
      - text: "Hier! "
      - control:
          kind: pause
          length: short
      - text: Hier drüben!
  Npc_Zora001_R_007:
    attributes: Npc_Zora001
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 22
      - control:
          kind: text_size
          percent: 125
      - text: Oooh!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 4
            - 1
      - text: "Schön, dich kennenzulernen!"
      - control:
          kind: text_size
          percent: 100
  Npc_Zora001_R_008:
    attributes: Npc_Zora001
    contents:
      - control:
          kind: text_size
          percent: 125
      - control:
          kind: sound
          unknown:
            - 7
            - 4
      - text: "...Ich tu's. "
      - control:
          kind: pause
          length: short
      - text: Ich zwinge dich!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 11
      - text: Geh zum Prinzen!
      - control:
          kind: pause
          length: short
      - text: " Sofort!"
      - control:
          kind: text_size
          percent: 100
  Npc_Zora001_R_Near01:
    attributes: Npc_Zora001
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 26
      - text: "Laaa, la, la... ♪"
  Npc_Zora013_R_001:
    attributes: Npc_Zora013
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Verzeih mir, dass ich dich so plötzlich\nanspreche!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ich bin Letogan "
      - control:
          kind: pause
          length: short
      - text: vom Volk der Zoras!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Als ich gesehen habe, dass du ein\nHylianer bist,"
      - control:
          kind: pause
          length: short
      - text: " musste ich einfach mit\ndir sprechen.\n"
      - control:
          kind: sound
          unknown:
            - 5
            - 6
      - text: "Würdest du mir einen Moment deine\nAufmerksamkeit schenken?"
      - control:
          kind: choice
          choice_labels:
            - 8
            - 9
          selected_index: 0
          cancel_index: 2
          unknown: 6
  Npc_Zora013_R_002:
    attributes: Npc_Zora013
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: Der Turm auf diesem Berg dort...
      - control:
          kind: pause
          length: short
      - text: " Siehst\ndu ihn? "
      - control:
          kind: pause
          length: short
      - text: "Nördlich davon fließt der\nOberlauf des "
      - control:
          kind: set_colour
          colour: blue
      - text: Zora-Flusses
      - control:
          kind: reset_colour
      - text: ".\nÜber diesen Fluss spannt sich die\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Ingogo-Brücke
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: pause
          length: short
      - text: " Ich bitte dich,\ndorthin zu gehen!"
  Npc_Zora013_R_003:
    attributes: Npc_Zora013
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 1
      - text: "..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "Ich weiß, was du denkst..."
      - control:
          kind: pause
          length: short
      - text: "\n„Warum gerade ich?“"
      - control:
          kind: pause
          length: short
      - text: " Nicht wahr?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "So würde wohl jeder reagieren,"
      - control:
          kind: pause
          length: short
      - text: " wenn\ner ohne Vorwarnung von einem Zora\nbehelligt wird.\n"
      - control:
          kind: sound
          unknown:
            - 7
            - 11
      - text: "Aber glaub mir, "
      - control:
          kind: pause
          length: short
      - text: "das Ganze wird sich\nfür dich auszahlen!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 1
      - text: Es ist nämlich so...
      - control:
          kind: pause
          length: short
      - text: " Unser Prinz, "
      - control:
          kind: set_colour
          colour: red
      - text: Sidon
      - control:
          kind: reset_colour
      - text: ",\nist auf der Suche nach einem starken\nHylianer."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 1
      - text: "Da es um einen königlichen Auftrag geht,\nbin ich sicher, dass es für diesen\nHylianer eine Belohnung geben wird...\n"
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: Also nur zu!
      - control:
          kind: pause
          length: short
      - text: " Geh zum Oberlauf des\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Zora-Flusses
      - control:
          kind: reset_colour
      - text: " und sprich bei der\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Ingogo-Brücke
      - control:
          kind: reset_colour
      - text: " mit dem "
      - control:
          kind: set_colour
          colour: red
      - text: Prinzen
      - control:
          kind: reset_colour
      - text: "!"
  Npc_Zora013_R_004:
    attributes: Npc_Zora013
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 4
      - text: Der Hylianer...
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Wenn du unseren "
      - control:
          kind: set_colour
          colour: red
      - text: Prinzen Sidon
      - control:
          kind: reset_colour
      - text: " treffen\nmöchtest..."
  Npc_Zora013_R_005:
    attributes: Npc_Zora013
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 1
      - text: "Dann geh doch bitte zur "
      - control:
          kind: set_colour
          colour: blue
      - text: Ingogo-Brücke
      - control:
          kind: reset_colour
      - text: "\nüber dem "
      - control:
          kind: set_colour
          colour: blue
      - text: Zora-Fluss
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: "Es geht um einen Auftrag des "
      - control:
          kind: set_colour
          colour: red
      - text: Prinzen
      - control:
          kind: reset_colour
      - text: "\nhöchstpersönlich..."
      - control:
          kind: pause
          length: short
      - text: " Ich bin sicher, du\nwirst es nicht bereuen!"
  Npc_Zora013_R_007:
    attributes: Npc_Zora013
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 19
      - text: Dann will ich es ganz kurz machen!
  Npc_Zora013_R_008:
    attributes: Npc_Zora013
    contents:
      - control:
          kind: sound
          unknown:
            - 7
