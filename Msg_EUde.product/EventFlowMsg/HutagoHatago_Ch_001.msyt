entries:
  "0003":
    attributes: ""
    contents:
      - text: Nur zu!
  "0004":
    attributes: ""
    contents:
      - text: "Ach, lieber nicht..."
  "0005":
    attributes: ""
    contents:
      - text: "Los geht's!"
  "0006":
    attributes: ""
    contents:
      - text: Wildpferde?
  "0007":
    attributes: ""
    contents:
      - text: Dörfer in der Nähe?
  "0008":
    attributes: ""
    contents:
      - text: Schon gut.
  "0009":
    attributes: ""
    contents:
      - text: Kakariko?
  "0010":
    attributes: ""
    contents:
      - text: Hateno?
  Npc_SouthHateru006_AreaOut_01:
    attributes: Npc_SouthHateru006
    contents:
      - control:
          kind: animation
          name: Greeting_00
      - text: "Oh, wo willst du denn hin?"
  Npc_SouthHateru006_AreaOut_02:
    attributes: Npc_SouthHateru006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Wenn du dich zu weit entfernst, kann\nich deine Zeit nicht mehr messen."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Also bleib doch bitte in der Nähe dieses\nStalls, ja?"
  Npc_SouthHateru006_H_Near000:
    attributes: Npc_SouthHateru006
    contents:
      - text: Ich warte einfach.
  Talk_01:
    attributes: Npc_SouthHateru006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Bis du ein wildes Pferd gezähmt hast, ist\nes ein wilder Ritt, aber wenn du es\nschaffst, eröffnet sich dir eine neue Welt!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Das kannst du mir ruhig glauben, ich\nbin schließlich hier der zweitbeste\nPferdezähmer!"
  Talk_03:
    attributes: Npc_SouthHateru006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Hier in der Gegend gibt es jede Menge\nWildpferde, schnapp dir einfach\nirgendeines davon!"
  Talk_04:
    attributes: Npc_SouthHateru006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Meine Bestzeit liegt bei 2 Minuten, also\nmusst du innerhalb dieser Zeit mit einem\nPferd zurückkommen!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: "Los geht's!"
  Talk_05:
    attributes: Npc_SouthHateru006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Wildpferde sind scheu, du musst dich\nihnen also ruhig und vorsichtig nähern."
  Talk_06:
    attributes: Npc_SouthHateru006
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - control:
          kind: text_size
          percent: 125
      - text: Unglaublich!
      - control:
          kind: text_size
          percent: 100
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Ich hätte allerdings nicht gedacht, dass\ndu die 2 Minuten so einfach unterbietest."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ich habe zwar beim Pferdezähmen den\nzweiten Platz belegt, aber das war nur\ngegen meinen Bruder, der Vorletzter war."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Wie dem auch sei, hier ist jedenfalls dein\nPreis für den Sieg beim 2. offiziellen\nPferdezähmwettbewerb!"
  Talk_07:
    attributes: Npc_SouthHateru006
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 10
      - text: "Hahaha! Gar nicht so leicht, das in\n2 Minuten zu schaffen, was?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Aber lass dich nicht entmutigen.
      - control:
          kind: pause
          length: short
      - text: " Selbst\nich habe Jahre gebraucht, bis ich ein\nWildpferd in 2 Minuten fangen konnte!"
  Talk_08:
    attributes: Npc_SouthHateru006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Willst du noch mal versuchen, meine\n2 Minuten zu schlagen?"
      - control:
          kind: pause
          length: short
      - text: " Oder wolltest\ndu was anderes?"
      - control:
          kind: choice
          choice_labels:
            - 5
            - 6
            - 7
            - 8
          selected_index: 0
          cancel_index: 3
          unknown: 10
  Talk_09:
    attributes: Npc_SouthHateru006
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 2
      - text: Uuund die Zeit ist um!
      - control:
          kind: pause
          length: longer
      - text: "\nTut mir leid!"
  Talk_10:
    attributes: Npc_SouthHateru006
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: Du glaubst mir nicht?!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "... "
      - control:
          kind: pause
          length: short
      - text: "..."
      - control:
          kind: pause
          length: longer
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - control:
          kind: text_size
          percent: 125
      - text: Na gut!!!
      - control:
          kind: text_size
          percent: 100
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Dann versuch doch, den Rekord von\nFussaren, dem zweitbesten Pferdezähmer\nzu brechen!"
      - control:
          kind: choice
          choice_labels:
            - 3
            - 4
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Talk_11:
    attributes: Npc_SouthHateru006
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 0
      - text: Wa...
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 10
            - 0
      - text: Was?!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Du lehnst meine Herausforderung\nab?! "
      - control:
          kind: pause
          length: short
      - text: Einfach so?!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Hm... "
      - control:
          kind: pause
          length: short
      - text: "Aber da kann man wohl nichts\nmachen..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 0
      - text: "Aber lass dir gesagt sein, dass ich\nmich vor keiner Herausforderung scheue!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Wenn du irgendwann den Mut aufbringst,\ndich mir zu stellen, komm ruhig wieder\nher!"
  Talk_12:
    attributes: Npc_SouthHateru006
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: "Das wollte ich hören!\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Es gibt nur eine Regel: "
      - control:
          kind: pause
          length: short
      - text: "Bring innerhalb\ndes Zeitlimits ein Wildpferd hierher!"
  Talk_13:
    attributes: Npc_SouthHateru006
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 0
      - text: "Und, bist du bereit?"
      - control:
          kind: pause
          length: short
      - text: "\nOder musst du noch was erledigen?"
      - control:
          kind: choice
          choice_labels:
            - 5
            - 6
            - 7
            - 8
          selected_index: 0
          cancel_index: 3
          unknown: 10
  Talk_14:
    attributes: Npc_SouthHateru006
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 0
      - text: "Wenn du dich mir irgendwann gewachsen\nfühlst, komm ruhig vorbei!"
      - control:
          kind: pause
          length: short
      - text: " Dann werden\nwir sehen, wer der Beste ist!"
  Talk_16:
    attributes: Npc_SouthHateru006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Hier in der Nähe sind "
      - control:
          kind: set_colour
          colour: blue
      - text: Kakariko
      - control:
          kind: reset_colour
      - text: " und\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Hateno
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: choice
          choice_labels:
            - 9
            - 10
          selected_index: 0
          cancel_index: 2
          unknown: 6
  Talk_18:
    attributes: Npc_SouthHateru006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Hateno liegt ziemlich am Rand von\nHyrule."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Das Dorf blieb während der großen\nVerheerung weitgehend verschont und\nist auch jetzt noch recht beschaulich."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Mal von den verschrobenen Forschern\nabgesehen, die dort auch leben..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Folge einfach dem Weg nach Norden\nund gehe an der Gabelung nach Osten\nweiter."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Zu Fuß ist es ein gutes Stück weit weg,\naber mit einem Pferd schafft man die\nStrecke an einem Tag."
  Talk_19:
    attributes: Npc_SouthHateru006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Na, bist du jetzt bereit für den\nWettbewerb?"
      - control:
          kind: choice
          choice_labels:
            - 5
            - 6
            - 7
            - 8
          selected_index: 0
          cancel_index: 3
          unknown: 10
  Talk_20:
    attributes: Npc_SouthHateru006
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: So gefällt mir das!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Du musst nur innerhalb der Zeit ein\nWildpferd hierher bringen!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 1
      - text: "Los geht's!"
  Talk_22:
    attributes: Npc_SouthHateru006
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 11
      - text: "Tja, sieht so aus, als hätte die Welt\neiniges mehr an Herausforderungen\nzu bieten!"
  Talk_TimerCK_001:
    attributes: Npc_SouthHateru006
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 21
      - control:
          kind: text_size
          percent: 125
      - text: W-w-was?!
      - control:
          kind: text_size
          percent: 100
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ich habe noch nicht mal davon gehört,\ndass jemand so schnell ist..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Mein schöner Rekord als zweitbester\nPferdezähmer, einfach so gebrochen..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Gut, war eigentlich nur ein Wettstreit\nzwischen mir und meinem Bruder..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: "Aber das soll meine Bewunderung für\ndeine Leistung nicht schmälern!"
      - control:
          kind: pause
          length: short
      - text: " Ich\nwerde wohl einfach mehr üben müssen!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Wie dem auch sei, hier ist jedenfalls dein\nPreis für den Sieg beim 2. offiziellen\nPferdezähmwettbewerb!"
