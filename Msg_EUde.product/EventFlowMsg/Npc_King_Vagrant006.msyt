      - text: Fernglas
      - control:
          kind: reset_colour
      - text: " des "
      - control:
          kind: set_colour
          colour: blue
      - text: Shiekah-Steins
      - control:
          kind: reset_colour
      - text: "."
  Kotsu02:
    attributes: Npc_King_Vagrant006
    contents:
      - control:
          kind: sound2
          unknown:
            - 11
            - 205
      - text: Wie oft soll ich dir das noch sagen?
      - control:
          kind: pause
          length: short
      - text: "\nWenn es hart auf hart kommt, kannst du\ndich nicht auf andere verlassen.\nAlle Aktionen kannst du auf dem\nPausenbildschirm nachsehen."
