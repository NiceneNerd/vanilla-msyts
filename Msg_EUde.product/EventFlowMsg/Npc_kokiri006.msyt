    attributes: Npc_kokiri006
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: Guten Morgen! Prost Mahlzeit!
      - control:
          kind: pause
          length: short
      - text: "\nDein Frühstück bekommst du beim\nPilzpendler nebenan!"
  Npc_kokiri006_Talk100:
    attributes: Npc_kokiri006
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: Guten Tag! Prost Mahlzeit!
      - control:
          kind: pause
          length: short
      - text: "\nIch habe dich die ganze Zeit\nbeim Knacken beobachtet!"
  Npc_kokiri006_Talk110:
    attributes: Npc_kokiri006
