      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Halte deine Augen offen nach seltsamen\nObjekten im Wasser."
  talk20:
    attributes: Npc_SouthernVillage013
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Gefällt dir unser Dorf?
  talk21:
