          label: 7
  talk19:
    attributes: Npc_SouthernVillage013
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 4
      - text: "Vielleicht liegen auf dem Grund des\nMeeres noch immer "
      - control:
