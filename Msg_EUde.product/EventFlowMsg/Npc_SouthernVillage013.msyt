    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 0
      - text: Bist du ein Reisender?
      - control:
          kind: pause
          length: long
      - text: "\nWillkommen in Angelstedt."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Mein Name ist Rosel.
      - control:
          kind: pause
          length: long
      - text: " Ich bin hier der\nDorfvorsteher."
  talk10:
    attributes: Npc_SouthernVillage013
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Zu schade, dass es regnet."
  talk11:
    attributes: Npc_SouthernVillage013
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Wenn du Fragen hast, dann tu dir keinen\nZwang an – frag einfach."
      - control:
