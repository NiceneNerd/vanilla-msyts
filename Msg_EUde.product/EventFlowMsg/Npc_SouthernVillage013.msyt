    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Ich habe eine Tochter.
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Sie hat geheiratet und ist vor Kurzem\nnach Hateno gezogen. "
      - control:
          kind: pause
          length: short
      - text: "Ich frage mich,\nwie es ihr geht."
      - control:
          kind: single_choice
          label: 0
  talk04:
    attributes: Npc_SouthernVillage013
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: "Genau, Ralera!"
      - control:
          kind: pause
          length: long
      - text: " Es geht ihr also gut?"
      - control:
          kind: pause
          length: short
      - text: "\nDas freut mich sehr."
  talk05:
    attributes: Npc_SouthernVillage013
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Im Meer östlich des Dorfes gibt es eine\ngroße Insel..."
      - control:
          kind: pause
          length: long
      - text: " "
      - control:
          kind: set_colour
          colour: blue
      - text: Jotwerde
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Man erzählt sich Dinge über diese Insel,\ndeswegen halten die Fischer aus dem\nDorf sich von dort fern."
      - control:
          kind: single_choice
          label: 1
  talk06:
    attributes: Npc_SouthernVillage013
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 0
      - text: Was?
      - control:
          kind: pause
          length: short
      - text: " Du warst dort?"
      - control:
          kind: pause
          length: short
      - text: " Unglaublich!"
  talk07:
    attributes: Npc_SouthernVillage013
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: Auf der Insel hört man Stimmen...
      - control:
          kind: pause
          length: long
      - text: "\nAber ist das Wirklichkeit oder Traum?"
  talk08:
