    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "Meine Frau hat mich vor die Wahl\ngestellt..."
      - control:
          kind: pause
          length: long
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "„Ich oder die Hühner, was ist dir\nwichtiger?!“"
      - control:
