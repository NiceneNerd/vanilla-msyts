              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - control:
          kind: text_size
          percent: 100
      - text: "Ich bin überglücklich, dass sie alle in\nSicherheit sind."
      - control:
          kind: pause
          length: short
      - text: " Ich bin dir zu Dank\nverpflichtet, Link!\nEs ist deiner Tat nicht angemessen,"
      - control:
