                - 3
  talk12:
    attributes: Npc_Zora029
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: "Deine elegante Körperspannung beim\nSprung ins kristallklare Nass..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ich brenne darauf, sie eines Tages\nbewundern zu dürfen."
  talk13:
    attributes: Npc_Zora029
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 10
      - text: "Meine liebste Sprungklippe in letzter\nZeit"
      - control:
          kind: pause
          length: short
      - text: " ist die Spitze des "
      - control:
          kind: set_colour
          colour: blue
      - text: Donnerhorns
      - control:
          kind: reset_colour
      - text: "..."
      - control:
          kind: pause
          length: short
      - text: "\nDas "
      - control:
          kind: set_colour
          colour: blue
      - text: Kap der Prüfungen
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Die Gestalt, die mir das Wasser des\n"
      - control:
          kind: set_colour
          colour: blue
      - text: "Östlichen Stausees "
      - control:
          kind: reset_colour
      - text: "beim Hinabspringen\nreflektiert..."
      - control:
          kind: pause
          length: short
      - text: " Einfach umwerfend!\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Doch zu meinem Unglück lebt auf dem\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Donnerhorn
      - control:
          kind: reset_colour
      - text: " ein "
      - control:
          kind: set_colour
          colour: blue
      - text: Leune
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Wenn du dem begegnest, kannst du dich\nvon zwei, drei Rippen verabschieden."
  talk14:
    attributes: Npc_Zora029
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 1
      - text: "Meine Wenigkeit sucht weiterhin nach\nSprungklippen, die meinen entzückenden\nSprüngen als Bühne dienen können.\n"
      - control:
          kind: sound
          unknown:
            - 1
            - 11
      - text: "Wenn du Wasserfälle hinaufschwimmen\nkannst,"
      - control:
          kind: pause
          length: short
      - text: " solltest auch du einmal\nprobieren, in die Tiefe zu springen!"
  talk15:
    attributes: Npc_Zora029
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 10
            - 21
      - text: "...Oh!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 1
      - text: "Du bist der Hylianer, der "
      - control:
          kind: set_colour
          colour: red
      - text: Prinz Sidon
      - control:
          kind: reset_colour
      - text: "\nhilft,"
      - control:
          kind: pause
          length: short
      - text: " oder?\n"
  talk16:
    attributes: Npc_Zora029
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 19
      - text: "Erlaube mir, mich vorzustellen. "
      - control:
          kind: pause
          length: short
      - text: "Meine\nWenigkeit heißt Rytt."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Vor einiger Zeit war ich zusammen mit\n"
      - control:
          kind: set_colour
          colour: red
      - text: Prinz Sidon
      - control:
          kind: reset_colour
      - text: " unterwegs,"
      - control:
          kind: pause
          length: short
      - text: " um einen\nHylianer zu finden.\n"
      - control:
          kind: sound
          unknown:
            - 7
            - 11
      - text: "Dass du ganz allein den Weg zu uns\ngefunden hast, passt wie der Fisch aufs\nAuge!\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: Aber...
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 9
            - 12
      - text: "Ich darf nur nicht daran zurückdenken,\nwie der "
      - control:
          kind: set_colour
          colour: red
      - text: "Prinz "
      - control:
          kind: reset_colour
      - text: "mich auf unserer Hylianer-\nSuche auf einem Turm zurückließ..."
