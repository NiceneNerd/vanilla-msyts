                - 0
                - 0
                - 3
  talk10:
    attributes: Npc_Zora029
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "Ohne fremde Hilfe können Hylianer keine\nWasserfälle hinaufschwimmen, oder?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 1
      - text: "Aber wenn du eine der "
      - control:
          kind: set_colour
          colour: red
      - control:
          kind: raw
          two_hundred_one:
            dynamic:
              - 0
              - len: 4
                field_2:
                  - 2
                  - 1
                  - 1
                  - 0
      - text: Zora-Rüstungen
      - control:
          kind: reset_colour
      - text: "\nanhättest, welche die Zora-Prinzessinnen\nanfertigen,"
      - control:
          kind: pause
          length: short
      - text: " könntest selbst du das..."
  talk11:
    attributes: Npc_Zora029
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 16
      - text: Hast du noch andere Fragen?
      - control:
          kind: raw
          one:
            eight:
              unknown_1:
                - - 255
                  - 255
                  - 0
                  - 0
              field_1:
                - 3
                - 0
                - 4
