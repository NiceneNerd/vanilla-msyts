  Com_Talk_04:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - control:
          kind: sound2
          unknown:
            - 9
            - 205
      - text: "Du spielst die Wählerische, was?\nIch wette, mit der Masche wickelst du\nauch jeden Vooi um den kleinen Finger!"
  Com_Talk_05:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - control:
          kind: sound2
          unknown:
            - 9
            - 205
      - text: "Eine Vaai muss auch manchmal\nfrech sein... "
      - control:
          kind: pause
          length: short
      - text: "Das macht sie umso\nverführerischer!"
  Com_Talk_05b:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Hast du noch mehr in der Art?
      - control:
          kind: pause
          length: short
      - text: "\nIch liebe solche hübschen Sachen."
  Com_Talk_05c:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Für dich ist es überflüssig, aber für mich\nist es wunderschön..."
      - control:
          kind: pause
          length: short
      - text: " Faszinierend, wenn\nman mal darüber nachdenkt!"
  Com_Talk_05d:
    attributes: ""
    contents:
      - text: "Hast du nicht noch mehr solcher\nKleinode in deinen Taschen?"
  Com_Talk_13:
    attributes: Npc_Kakariko010
    contents:
      - text: "Ah, "
      - control:
