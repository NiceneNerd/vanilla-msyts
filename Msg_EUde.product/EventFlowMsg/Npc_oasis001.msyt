          colour: red
      - control:
          kind: variable
          variable_kind: 18
          name: ShopTradePrice
      - control:
          kind: localisation
          localisation_kind: plural
          options:
            - " Rubin"
            - " Rubinen"
            - " Rubin"
      - control:
