          unknown: 6
  Com_Talk_61:
    attributes: Npc_Kakariko010
    contents:
      - control:
          kind: sound2
          unknown:
            - 7
            - 205
      - text: Hihi...
      - control:
          kind: pause
          length: short
      - text: " Beim Auswählen muss man sich\nZeit lassen."
      - control:
          kind: pause
          length: short
      - text: " Ob es nun um Vooi geht\noder um Edelsteine."
  Talk001:
    attributes: Npc_oasis001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Dank deiner Hilfe konnte ich mein\nnormales Leben wieder aufnehmen."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Vielen Dank noch mal!
      - control:
          kind: pause
          length: short
      - text: "\nDu bekommst Sonderpreise bei mir!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound2
          unknown:
            - 7
            - 205
      - text: "Wenn dir von meinem "
      - control:
          kind: set_colour
          colour: blue
      - text: "Schmuck "
      - control:
          kind: reset_colour
      - text: "irgendein\nStück gefällt, nimm es ruhig einmal in die\nHand!"
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
            - 2
          selected_index: 0
          cancel_index: 2
          unknown: 8
  Talk002:
    attributes: Npc_oasis001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Der Laden hat wieder geöffnet, und Cara\nist schon wieder fleißig dabei, Schmuck\nherzustellen... "
      - control:
          kind: pause
          length: short
      - control:
          kind: sound2
          unknown:
            - 7
            - 205
      - text: Tausendmal Sarq-sarq!
  Talk006:
    attributes: Npc_oasis001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Hihi! Sarq-sarq!
  Talk007:
    attributes: Npc_oasis001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Edelsteine haben besondere Kräfte.
      - control:
          kind: pause
          length: short
      - text: "\nWenn man sie am Körper trägt, wird man\nvon ihnen beschützt.\nEin Beispiel? "
      - control:
          kind: pause
          length: short
      - text: "Der "
      - control:
          kind: set_colour
          colour: blue
      - control:
          kind: raw
          two_hundred_one:
            dynamic:
              - 0
              - len: 4
                field_2:
                  - 1
                  - 0
                  - 0
                  - 0
      - text: Rhodonit
      - control:
          kind: reset_colour
      - text: " lässt dich\nKälte aushalten, "
      - control:
          kind: pause
          length: short
      - text: "der "
      - control:
          kind: set_colour
          colour: blue
      - control:
          kind: raw
          two_hundred_one:
            dynamic:
              - 0
              - len: 4
                field_2:
                  - 1
                  - 0
                  - 0
                  - 0
      - text: Saphir
      - control:
          kind: reset_colour
      - text: " macht Hitze\nerträglich... "
      - control:
          kind: pause
          length: long
      - text: "Und so weiter!\nIch verarbeite diese Edelsteine in\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Stirnreifen
      - control:
          kind: reset_colour
      - text: " oder "
      - control:
          kind: set_colour
          colour: blue
      - text: Ohrringen
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Leider ist es schwierig, an die\nMaterialien zu kommen..."
      - control:
          kind: pause
          length: longer
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Deshalb fertigen wir nur Schmuck an,\nwenn der Kunde das Material mitbringt."
  Talk008:
    attributes: Npc_oasis001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Das mag dir lästig erscheinen, aber ich\nbin sicher, mit dem Endprodukt bist du\numso zufriedener!\nGerudo-Chic steht übrigens beiden\nGeschlechtern gleichermaßen."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound2
          unknown:
            - 7
            - 205
      - text: "Also kannst du hier auch ohne Bedenken\nein Geschenk für den Vooi deiner Träume\nbesorgen!"
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
            - 2
          selected_index: 0
          cancel_index: 2
          unknown: 8
  Talk009:
    attributes: Npc_oasis001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - control:
          kind: sound2
          unknown:
            - 7
            - 205
      - text: "Komm bald wieder, ja?"
      - control:
          kind: pause
          length: short
      - text: "\nWir kaufen immer gern deine Sachen an!"
  Talk012:
    attributes: Npc_oasis001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Von heute an wird gearbeitet!
      - control:
          kind: pause
          length: short
      - text: "\n"
      - control:
          kind: set_colour
          colour: blue
      - control:
          kind: raw
          two_hundred_one:
            dynamic:
              - 0
              - len: 4
                field_2:
                  - 1
                  - 0
                  - 0
                  - 0
      - text: Feuersteine
      - control:
          kind: reset_colour
      - text: " habe ich jetzt ja genug."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound2
          unknown:
            - 7
            - 205
      - text: "Wenn du dich für "
      - control:
          kind: set_colour
          colour: blue
      - text: Schmuck
      - control:
          kind: reset_colour
      - text: " interessierst,\nsieh dir die Auslagen an!"
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
            - 2
          selected_index: 0
          cancel_index: 2
          unknown: 8
  Talk013:
    attributes: Npc_oasis001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - control:
          kind: set_colour
          colour: blue
      - text: Schmuck
      - control:
          kind: reset_colour
      - text: " hebt die natürliche Schönheit\neiner Vaai noch weiter hervor..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound2
          unknown:
            - 7
            - 205
      - text: "Wenn du etwas siehst, was dich\nbesonders erstrahlen lassen könnte,\nsag Bescheid!"
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
            - 2
          selected_index: 0
          cancel_index: 2
          unknown: 8
  Talk014:
    attributes: Npc_oasis001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Edelsteine haben eine ganz besondere\nVerbindung zur Seele einer Vaai."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Wenn dir etwas ins Auge fällt und dich\nnicht mehr loslässt, ist das sicher der\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Schmuck
      - control:
          kind: reset_colour
      - text: ", der für dich bestimmt ist!\n"
      - control:
          kind: sound2
          unknown:
            - 7
            - 205
      - text: "Nimm die Stücke, die dich ansprechen,\neinfach mal in die Hand!"
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
            - 2
          selected_index: 0
          cancel_index: 2
          unknown: 8
  Talk015:
    attributes: Npc_oasis001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Wärst du nicht gewesen, hätten wir\ndiesen Laden vielleicht schließen\nmüssen."
      - control:
          kind: pause
          length: short
      - control:
          kind: sound2
          unknown:
            - 7
            - 205
      - text: " Sarq-sarq, von Herzen!"
  Talk016:
    attributes: Npc_oasis001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 9
      - text: Hihi!
      - control:
          kind: pause
          length: short
      - text: "\nIch hoffe, die Steine bringen dir Glück!"
  Talk017:
    attributes: Npc_oasis001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Sarq-sarq!\nDer Schmuck passt richtig gut zu dir!"
  Talk018:
    attributes: Npc_oasis001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Sarq-sarq!
      - control:
          kind: pause
          length: short
      - text: "\nMögen die Edelsteine dich beschützen!"
  Talk019:
    attributes: Npc_oasis001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Seit der Titan sich beruhigt hat, ist\nwieder mehr Leben in die Stadt\ngekommen!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound2
          unknown:
            - 7
            - 205
      - text: "Hihi... "
      - control:
          kind: pause
          length: short
      - text: "Und ich weiß durchaus, dass du\nan der Sache nicht ganz unbeteiligt\nwarst...\nAber du würdest das nie\nherausposaunen..."
      - control:
          kind: pause
          length: short
      - text: " Das finde ich\nwirklich bewundernswert!\n"
      - control:
          kind: sound2
          unknown:
            - 2
            - 205
      - text: Also dann...
      - control:
          kind: pause
          length: longer
      - text: " Was kann ich für die\nRetterin der Gerudo tun?"
      - control:
          kind: choice
          choice_labels:
            - 1
            - 2
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Talk020:
    attributes: Npc_oasis001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Wenn ich an deiner Stelle wäre, würde\nich verlangen, dass sie ein Standbild für\ndie Retterin von Gerudo-Stadt aufstellen!\n"
      - control:
          kind: sound2
          unknown:
            - 7
            - 205
      - text: "Hihi... "
      - control:
          kind: pause
          length: short
      - text: "Natürlich reichlich mit Edelsteinen\nverziert!"
  Talk021:
    attributes: Npc_oasis001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - control:
          kind: sound2
          unknown:
            - 28
            - 205
      - text: Vasaaq!
