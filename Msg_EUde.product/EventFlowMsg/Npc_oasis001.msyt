          variable_kind: 16
          name: Shop_SelectItemPrice
      - control:
          kind: localisation
          localisation_kind: plural
          options:
            - " Rubin"
            - " Rubine"
            - " Rubin"
      - control:
          kind: reset_colour
      - text: " bist du die neue\nBesitzerin!"
      - control:
