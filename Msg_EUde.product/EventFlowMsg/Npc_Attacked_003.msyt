  SavedLv3_00:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 9
      - text: "Ich kann kaum glauben, dass sich jemand\ndie Mühe macht, einen Niemand wie mich\nzu retten.\nDanke. Vielen Dank."
      - control:
          kind: pause
          length: short
      - text: "\nDas hier soll dir gehören."
  SavedLv3_01:
