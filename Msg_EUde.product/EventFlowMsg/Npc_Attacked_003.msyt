    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 12
      - text: Du hast mir wirklich die Haut gerettet...
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
