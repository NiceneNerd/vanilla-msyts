              field_1: 0
      - control:
          kind: text_size
          percent: 125
      - control:
          kind: sound
          unknown:
            - 8
            - 6
      - text: Du darfst da nicht rein!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: text_size
          percent: 100
      - text: "Wenn du meinen Pflaumenbäumen etwas\nantust, ist das, als würdest du mir etwas\nantun!\n"
      - control:
          kind: sound
          unknown:
            - 2
            - 5
