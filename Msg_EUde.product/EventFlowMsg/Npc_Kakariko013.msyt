    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 0
      - text: Was ist das für ein verschämter Blick?
      - control:
          kind: pause
          length: short
      - text: "\nDer Überfall ist eine ehrenhafte\nKampftechnik!\n"
      - control:
          kind: sound
          unknown:
            - 11
            - 1
      - text: "Man schleicht sich von hinten an den\nGegner heran,"
      - control:
          kind: pause
          length: short
      - text: " und ehe er sich's\nversieht...\n"
      - control:
          kind: sound
          unknown:
            - 8
            - 0
      - text: "Zack!\n\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Am besten, man nähert sich geduckt!"
      - control:
          kind: pause
          length: short
      - text: "\nEin Feind, der sich nicht verteidigt,\nerleidet schweren Schaden!"
      - control:
