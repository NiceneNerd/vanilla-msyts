  Npc_oasis013_016:
    attributes: Npc_oasis017
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 0
      - text: "Wer würde nur so etwas Schreckliches\ntun? "
      - control:
          kind: pause
          length: short
      - text: "Derjenige kann sich auf was\ngefasst machen!"
  Npc_oasis013_017:
    attributes: Npc_oasis013
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: Sagt mal...
      - control:
          kind: pause
          length: short
      - text: "\nHabt ihr schon mal einen Vooi gesehen?"
  Npc_oasis013_018:
    attributes: Npc_oasis011
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: "Also ich,"
      - control:
          kind: pause
          length: short
      - text: " wenn ich mal groß bin, "
      - control:
          kind: pause
          length: short
      - text: "dann\nlerne ich bei Frau Ashai alles über Vooi\nund so!"
  Npc_oasis013_019:
    attributes: Npc_oasis017
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Meine Mama hat gesagt, es ist komisch,\ndass eine ledige Frau wie Ashai einen\nKurs über Vooi anbietet!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: Was bedeutet eigentlich „ledig“?
  Npc_oasis013_020:
    attributes: Npc_oasis016
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 9
      - text: "Ich hab gehört, wenn man sich in einen\nVooi verliebt, ist das süßer als jedes\nObst!\nSogar süßer als "
      - control:
          kind: raw
          two_hundred_one:
            dynamic:
              - 0
              - len: 4
                field_2:
                  - 2
                  - 1
                  - 1
                  - 0
      - text: Wildbeeren?
      - control:
          kind: pause
          length: short
      - text: "\nDas glaube ich erst, wenn ich meinen\neigenen Vooi gebissen habe!"
  Npc_oasis013_021:
    attributes: Npc_oasis013
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 6
      - text: Waaah!
      - control:
          kind: pause
          length: short
      - text: "\nWas für ein riesiger Va'ad..."
  Npc_oasis013_022:
    attributes: Npc_oasis013
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: "Du bist eine von den Guten, oder?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Wer viel Geld ausgibt, ist gut!\nDas sagt meine Vaba immer!"
