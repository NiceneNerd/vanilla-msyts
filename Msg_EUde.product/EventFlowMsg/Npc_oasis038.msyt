  Talk_04:
    attributes: Npc_oasis039
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 4
      - text: "Momentchen mal! "
      - control:
          kind: pause
          length: short
      - text: "So kannst du über\nPayu nicht reden!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Sag mir nicht, dass du noch nie von\nPayu, dem "
      - control:
          kind: set_colour
          colour: blue
      - text: "Star der Sandrobbenrennen-\nSzene"
      - control:
          kind: reset_colour
      - text: " gehört hast?!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Wenn nur dieser Störenfried von einem\n"
      - control:
          kind: set_colour
          colour: blue
      - text: "Titanen "
      - control:
          kind: reset_colour
      - text: "nicht wäre, würde dir Payu\nzeigen, was sie draufhat..."
  Talk_05:
    attributes: Npc_oasis039
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "Das siehst du doch wohl, oder?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Da stampft ein "
      - control:
          kind: set_colour
          colour: blue
      - text: "Titan "
      - control:
          kind: reset_colour
      - text: "durch die Gegend,\nalso können wir unsere "
      - control:
          kind: set_colour
          colour: blue
      - text: Rennstrecke
      - control:
          kind: reset_colour
      - text: "\nnicht nutzen.\nIch kann dir nicht verbieten, dir die Beine\nin den Bauch zu stehen, "
      - control:
          kind: pause
          length: short
      - text: "aber es wäre\nnett, wenn du es woanders tun würdest."
  Talk_06:
    attributes: Npc_oasis039
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 4
      - text: Wenn du die Wahrheit wissen willst –
      - control:
          kind: pause
          length: short
      - text: "\nich war es, die aus Payu einen Champion\ngemacht hat."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Sie war immer schon etwas schüchtern.
      - control:
          kind: pause
          length: short
      - text: "\nNur in meiner Gegenwart ist sie etwas\naufgeschlossener."
  Talk_07:
