  Near03:
    attributes: Npc_Road_041
    contents:
      - text: Der Monsterladen...
  Near04:
    attributes: Npc_Road_041
    contents:
      - text: Mieses Wetter...
  Rest_00:
    attributes: Npc_Road_041
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: "Schon von dem "
      - control:
          kind: set_colour
          colour: blue
      - control:
          kind: pause
          length: short
      - text: Totenkopf-Teich
      - control:
          kind: reset_colour
      - text: " gehört?"
      - control:
          kind: pause
          length: long
      - text: "\nDem Teich in Totenkopfform?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Ich?
      - control:
          kind: pause
          length: short
      - text: " Ich habe keine Karte dabei, also\nkann ich leider nicht nachsehen."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Tja, hätte ich besser mal meine Karte\neingepackt, dann sähe es anders aus..."
  Rest_01:
    attributes: Npc_Road_041
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 12
      - text: Mein Pferd ist sehr wetterfühlig.
      - control:
          kind: pause
          length: long
      - text: "\nBei Regen bewegt es sich nur äußerst\nwiderwillig vom Fleck."
  Rest_02:
    attributes: Npc_Road_041
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 12
      - text: "Mein Pferd ist wegen des Wetters ganz\nmies gelaunt. "
      - control:
          kind: pause
          length: long
      - text: "Hoffentlich kommt die\nSonne schnell wieder heraus..."
  RetMSG_Akka_00:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: "Schon mal vom "
      - control:
          kind: set_colour
          colour: blue
      - text: "Monsterladen "
      - control:
          kind: reset_colour
      - text: "in Akkala\ngehört?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Ich?
      - control:
          kind: pause
          length: short
      - text: " Ein, zwei Dinge weiß ich darüber, ja."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 16
      - text: "Ich weiß, dass der Laden nur nachts\ngeöffnet hat und dass er am \n"
      - control:
          kind: set_colour
          colour: blue
      - text: "Totenkopf-Teich "
      - control:
          kind: reset_colour
      - text: "sein soll...\nDafür weiß ich nicht, wo der\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Totenkopf-Teich
      - control:
          kind: reset_colour
      - text: " ist. "
      - control:
          kind: pause
          length: short
      - text: "Hätte ich besser\nmal meine Karte mitgenommen..."
  RetMSG_Akka_00_1:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Der "
      - control:
          kind: set_colour
          colour: blue
      - text: "Monsterladen "
      - control:
          kind: reset_colour
      - text: "hat nur nachts\ngeöffnet."
      - control:
          kind: pause
          length: short
      - text: " Irgendwo am "
      - control:
          kind: set_colour
          colour: blue
      - text: Totenkopf-Teich
      - control:
          kind: reset_colour
      - text: "\nsoll er sein. "
      - control:
          kind: pause
          length: short
      - text: "Keine Ahnung, wo genau..."
  RetMSG_Akka_01:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: Schon mal von Nobo aus Akkala gehört?
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Ich?
      - control:
          kind: pause
          length: short
      - text: " Klar kenn ich sie."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Sie hat mir immerhin von der\n"
      - control:
          kind: set_colour
          colour: red
      - text: "Quelle der Kraft "
      - control:
          kind: reset_colour
      - text: erzählt.
  RetMSG_Akka_01_1:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "Was wohl Nobo aus Akkala gerade\nmacht?"
      - control:
          kind: pause
          length: short
      - text: " Vielleicht sollte ich sie mal\nbesuchen."
  RetMSG_Akka_02:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: "Schon gehört, wie viele Einwohner\nAkkala hat?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Ich?
      - control:
          kind: pause
          length: short
      - text: " Ich habe nicht die geringste\nAhnung."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "...Was schaust du so?"
      - control:
          kind: pause
          length: long
      - text: " Ich weiß es\nwirklich nicht!"
  RetMSG_Akka_02_1:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Weißt du, wie viele Einwohner \nAkkala hat?"
      - control:
          kind: pause
          length: short
      - text: " Ich jedenfalls nicht."
  RetMSG_Akka_03:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: "Schon vom fahrenden Koch in Akkala\ngehört?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Ich?
      - control:
          kind: pause
          length: short
      - text: " Ich leider noch nichts."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Ich hatte eigentlich gehofft, dass du\nmehr über ihn weißt."
  RetMSG_Akka_03_1:
    attributes: Npc_Road_043
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "Ich würde zu gern mal den fahrenden\nKoch in Akkala treffen."
  RetMSG_Akka_04:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: "Schon gehört, dass man in Akkala\neine besondere Version von \n„Schere, Stein, Papier“ spielt?"
      - control:
          kind: pause
          length: long
      - text: "\nIch?"
      - control:
          kind: pause
          length: short
      - text: " Nein, keine Ahnung."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Vielleicht habe ich mir die auch nur ein-\ngebildet. "
      - control:
          kind: pause
          length: short
      - text: "Oder vielleicht arbeitet jemand\ngerade an so einer Version. "
      - control:
          kind: pause
          length: short
      - text: Wer weiß.
  RetMSG_Akka_04_1:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "Mir wurde gesagt, dass es in Akkala eine\nbesondere Version von „Schere, Stein,\nPapier“ gibt, aber ob das stimmt..."
  SavedLv1_00:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 18
      - text: Danke!
      - control:
          kind: pause
          length: short
      - text: " Du hast mir echt aus der\nPatsche geholfen!"
  SavedLv1_01:
    attributes: Npc_Road_041
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 18
      - text: Danke für deine Hilfe!
      - control:
          kind: pause
          length: long
      - text: " Da bin ich dir\necht was schuldig!"
  SavedLv2_00:
    attributes: Npc_Road_041
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 18
      - text: "Puh, "
      - control:
          kind: pause
          length: short
      - text: "das war knapper, als mir lieb ist!"
      - control:
          kind: pause
          length: long
      - text: "\nDanke!"
  SavedLv2_01:
    attributes: Npc_Road_041
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 18
      - text: "Puh, zum Glück ist alles noch dran..."
      - control:
          kind: pause
          length: long
      - text: "\nDas hab ich dir zu verdanken!"
  SavedLv3_00:
    attributes: Npc_Road_043
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 18
      - text: "Diese Monster, eine richtige Plage!"
      - control:
          kind: pause
          length: long
      - text: "\nDanke für deine Hilfe!"
  SavedLv3_01:
    attributes: Npc_Road_041
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 18
      - text: Da hab ich ja wirklich Glück gehabt!
  Thanks00:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 11
      - text: Mehr kann ich dir leider nicht anbieten.
      - control:
          kind: pause
          length: short
      - text: "\nBitte, nimm es ruhig an!"
