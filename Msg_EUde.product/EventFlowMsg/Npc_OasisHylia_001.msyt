  Talk_05:
    attributes: Npc_OasisHylia_001
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 28
      - text: Savaaq!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: text_size
          percent: 80
      - control:
          kind: sound
          unknown:
            - 11
            - 1
      - text: "Savaaq bedeutet „Guten Tag“, oder?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 7
            - 4
      - control:
          kind: text_size
          percent: 100
      - text: Schöne Klamotten!
      - control:
          kind: pause
          length: short
      - text: " Die halten sicher\nangenehm kühl, oder?"
      - control:
          kind: pause
          length: short
      - text: " So was muss ich\nauch haben."
  Talk_06:
    attributes: Npc_OasisHylia_001
    contents:
      - control:
          kind: text_size
          percent: 80
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Mal überlegen...\nWas heißt noch gleich „Guten Abend“...?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Oh!
      - control:
          kind: text_size
          percent: 100
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 7
            - 29
      - text: "Sav'saaba!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Warst du schon in der Bar? "
      - control:
          kind: pause
          length: short
      - text: "Wenn du\nschon nach Gerudo-Stadt kommst, musst\ndu dort unbedingt einmal hereinschauen."
  Talk_07:
    attributes: Npc_OasisHylia_001
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 4
      - text: "Der "
      - control:
          kind: set_colour
          colour: blue
      - text: "Eiskeller "
      - control:
          kind: reset_colour
      - text: ist ein Lagerraum für Eis.
      - control:
          kind: pause
          length: short
      - text: "\nEigentlich ist er nur ein Loch, das in den\nBoden gegraben wurde.\n"
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Anscheinend werden in der nahegelege-\nnen "
      - control:
          kind: set_colour
          colour: blue
      - text: "Gerudo-Hochebene "
      - control:
          kind: reset_colour
      - text: "Eisblöcke\nherausgeschlagen und so gelagert."
      - control:
          kind: choice
          choice_labels:
            - 2
            - 3
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Talk_08:
    attributes: Npc_OasisHylia_001
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: "Ich doch nicht,"
      - control:
          kind: pause
          length: short
      - text: " ich doch nicht!"
      - control:
          kind: pause
          length: longer
      - text: "\nNur noch ein Glas..."
  Talk_09:
    attributes: Npc_OasisHylia_001
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 4
      - text: Halte dich hinter dem Haupttor links.
      - control:
          kind: pause
          length: short
      - text: "\nGehe an den Ruinen vorbei, dann\nsiehst du den "
      - control:
          kind: set_colour
          colour: blue
      - text: "Eiskeller "
      - control:
          kind: reset_colour
      - text: auch schon.
  Talk_10:
    attributes: Npc_OasisHylia_001
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Hm...\n„Auf Wiedersehen“ war..."
      - control:
          kind: pause
          length: longer
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 1
      - text: Oh!
      - control:
