      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Genau das Richtige!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Alle Drinks hier verwenden das Eis vom\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Eiskeller
      - control:
          kind: reset_colour
      - text: " in der Nähe."
      - control:
          kind: raw
