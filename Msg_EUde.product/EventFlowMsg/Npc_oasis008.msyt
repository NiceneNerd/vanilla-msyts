          percent: 100
  Talk_18:
    attributes: Npc_oasis008
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "Tut mir leid, aber kannst du leise\nsprechen? Dieser Kater bringt mich\nnoch um...\n"
      - control:
          kind: text_size
          percent: 80
      - text: Ich hatte wohl ein Glas zu viel...
      - control:
