    attributes: Npc_Road_037
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: "Tja, junger Mann, meinst wohl, ich gehör\nzum alten Eisen."
      - control:
          kind: pause
          length: short
      - text: " So schnell, wie ich\nnoch schlittere, kannst du nicht gucken."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Deswegen lass dir von einem erfahrenen\n"
      - control:
          kind: set_colour
          colour: red
      - text: "Schildschlitterer "
      - control:
          kind: reset_colour
      - text: etwas erklären...
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Auf Sand oder Schnee geht dein Schild\nnicht kaputt."
      - control:
          kind: pause
          length: short
      - text: " Aber pass auf, andere\nUntergründe tun deinem Schild nicht gut."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Eigentlich ganz logisch, oder?"
  RetMSG_Taba_01_1:
    attributes: Npc_Road_037
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - control:
          kind: set_colour
          colour: red
      - text: "Schildschlittern "
      - control:
          kind: reset_colour
      - text: "nur auf Sand und\nSchnee."
      - control:
          kind: pause
          length: short
      - text: " Andere Untergründe machen\ndeinen Schild kaputt."
      - control:
          kind: pause
          length: short
      - text: " Ist ja wohl klar."
  RetMSG_Taba_02:
    attributes: Npc_Road_037
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: "He, junger Mann!"
      - control:
          kind: pause
          length: long
      - text: " Geht's hoch in die\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Hebra-Berge
      - control:
          kind: reset_colour
      - text: "? Dann lass dir vom "
      - control:
          kind: set_colour
          colour: red
      - text: "Schild-\nschlitter"
      - control:
          kind: reset_colour
      - text: "-Veteranen einen Tipp geben:"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Denk beim Aufstieg schon an den\nWeg bergab – "
      - control:
          kind: pause
