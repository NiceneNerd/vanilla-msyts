            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 1
      - text: "Hö,"
      - control:
          kind: pause
          length: long
      - text: " nicht lustig?"
  MSG_Taba_01:
    attributes: Npc_Road_037
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 6
      - text: Du kennst dich also mit Waffen aus?
      - control:
          kind: pause
          length: long
      - text: "\nDann bist du sicher auch im Umgang\nmit Schilden geschickt...\n"
      - control:
          kind: sound
          unknown:
            - 1
            - 1
      - text: Hehe...
      - control:
          kind: pause
          length: long
      - text: " Ich seh zwar nicht so aus, aber\nich bin ein Ass im "
      - control:
          kind: set_colour
          colour: red
      - text: Schildschlittern
      - control:
          kind: reset_colour
      - text: ".\nAls ich in etwa so alt war wie du..."
      - control:
          kind: pause
          length: short
      - text: "\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Damals habe ich ein Kind aus den "
      - control:
          kind: set_colour
          colour: blue
      - text: "Hebra-\nBergen"
      - control:
          kind: reset_colour
      - text: " gerettet."
      - control:
          kind: pause
          length: short
      - text: " "
      - control:
          kind: set_colour
          colour: red
      - text: "Schildschlittern "
      - control:
          kind: reset_colour
      - text: "mit\neinem Kind huckepack war nicht einfach!"
  MSG_Taba_01_1:
    attributes: Npc_Road_037
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Einst habe ich ein Kind aus den "
      - control:
          kind: set_colour
          colour: blue
      - text: "Hebra-\nBergen"
      - control:
          kind: reset_colour
      - text: " gerettet."
      - control:
          kind: pause
          length: short
      - text: " "
      - control:
          kind: set_colour
          colour: red
      - text: "Schildschlittern "
      - control:
          kind: reset_colour
      - text: "mit\neinem Kind huckepack war nicht einfach!"
  MSG_Taba_02:
    attributes: Npc_Road_037
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: "Na, junger Mann?"
      - control:
          kind: pause
          length: short
      - text: " Bist du schön mit\ndeinem Schild die Abhänge runter-\ngebrettert?\nIch bin zum "
      - control:
          kind: set_colour
          colour: blue
      - text: Stall des Schneelands
      - control:
          kind: reset_colour
      - text: "\nunterwegs, um auch mal wieder\ngepflegt zu "
      - control:
          kind: set_colour
          colour: red
      - text: schildschlittern
      - control:
          kind: reset_colour
      - text: ". \n"
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Vielleicht bekomm ich diesmal die großen\nWalknochen an der "
      - control:
          kind: set_colour
          colour: blue
      - text: Hebra-Nordspitze
      - control:
          kind: reset_colour
      - text: "\nzu Gesicht...\nDer Anblick ist bestimmt so viel wert\nwie die perfekte Schneewehe beim\nSchlittern."
  MSG_Taba_02_1:
    attributes: Npc_Road_037
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Vielleicht bekomm ich bald die großen\nWalknochen in der Nähe der\n"
      - control:
          kind: set_colour
          colour: blue
      - text: "Hebra-Nordspitze "
      - control:
          kind: reset_colour
      - text: "zu Gesicht...\nDer Anblick ist bestimmt so viel wert\nwie die perfekte Schneewehe beim\nSchlittern."
  MSG_Taba_03:
    attributes: Npc_Road_037
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: "Was steht an, junger Mann?"
      - control:
          kind: pause
          length: long
      - text: " Willst du\nwissen, wo's für mich hingeht?"
      - control:
          kind: pause
          length: short
      - text: " Nachts\nschlafe ich im "
      - control:
          kind: set_colour
          colour: blue
      - text: Stall des Schneelands
      - control:
          kind: reset_colour
      - text: "...\nTagsüber geht's hoch in die "
      - control:
          kind: set_colour
          colour: blue
      - text: Hebra-Berge
      - control:
          kind: reset_colour
      - text: "\nzum "
      - control:
          kind: set_colour
          colour: red
      - text: Schildschlittern
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: pause
          length: short
      - text: " Die perfekte\nSchneewehe nehmen und dann runter...\n"
      - control:
          kind: sound
          unknown:
            - 1
            - 4
      - text: Na?
      - control:
          kind: pause
          length: long
      - text: " Willst du ein paar abenteuerliche\nGeschichten von einer Schlitterlegende\nauf dem Weg zum Stall hören? "
      - control:
          kind: pause
          length: longer
      - text: Höhö!
  MSG_Taba_03_1:
    attributes: Npc_Road_037
    contents:
      - control:
          kind: sound
