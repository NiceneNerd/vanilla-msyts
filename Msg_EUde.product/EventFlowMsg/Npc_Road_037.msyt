      - text: Höhö!
  MSG_Taba_04:
    attributes: Npc_Road_037
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: "Na, junger Mann?"
      - control:
          kind: pause
          length: long
      - text: " Fragst dich wohl, was\n'ne alte Schneeeule wie ich hier sucht?"
      - control:
          kind: pause
          length: short
      - text: "\nIch mach am "
      - control:
          kind: set_colour
          colour: blue
      - text: Stall des Schneelands
      - control:
          kind: reset_colour
      - text: " Halt."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Dann geht's weiter in die "
      - control:
          kind: set_colour
          colour: blue
      - text: Hebra-Berge
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: pause
          length: short
      - text: "\nEin "
      - control:
          kind: set_colour
          colour: red
      - text: Schildschlitter
      - control:
          kind: reset_colour
      - text: "-Ass, das dort lebt,\nhat mir mal gesagt:"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: „Schlittere nie einfach drauf los.
      - control:
          kind: pause
          length: short
      - text: "\nZum wahren Schlitterkönig wird man nur\ndurch harte Arbeit und Vorbereitung.“"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: So oder so ähnlich waren ihre Worte...
      - control:
          kind: pause
          length: longer
      - text: "\nNa ja."
      - control:
          kind: pause
          length: short
      - text: " Ich bereite mich demnach mal\nauf die Kälte vor..."
  MSG_Taba_04_1:
    attributes: Npc_Road_037
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Ich mach am "
      - control:
          kind: set_colour
          colour: blue
      - text: Stall des Schneelands
      - control:
          kind: reset_colour
      - text: " Halt.\nDann geht's weiter in die "
      - control:
          kind: set_colour
          colour: blue
      - text: Hebra-Berge
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: pause
          length: short
      - text: "\nEin "
      - control:
          kind: set_colour
          colour: red
      - text: Schildschlitter
      - control:
          kind: reset_colour
      - text: "-Ass sagte mir einst:"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "„Schlittere nie einfach drauflos.“\n"
      - control:
          kind: pause
          length: long
      - text: "Dann sollte ich mich wohl mal auf\ndie Kälte vorbereiten..."
