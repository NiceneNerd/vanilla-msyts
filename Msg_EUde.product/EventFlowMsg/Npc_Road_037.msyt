      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: "Hallo, junger Mann!"
      - control:
          kind: pause
          length: short
      - text: " Na, reitest du?"
      - control:
          kind: pause
          length: short
      - text: "\nLässt dir den Wind um die Ohren wehen?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ich reite oft zum "
      - control:
          kind: set_colour
          colour: blue
      - text: Stall des Schneelands
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: pause
          length: short
      - text: "\nIch schnapp mir meinen Schild, springe\ndrauf und los geht's!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Das Ganze nennt sich
      - control:
          kind: pause
          length: long
      - text: " "
      - control:
          kind: set_colour
          colour: red
      - text: Schildschlittern
      - control:
          kind: reset_colour
      - text: "!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 6
      - text: "Mit dem „Reiten“ hab ich dich zuerst aufs\nGlatteis geführt, was?"
  MSG_Taba_00_1:
    attributes: Npc_Road_037
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - control:
          kind: set_colour
          colour: red
      - text: "Schildschlittern "
      - control:
          kind: reset_colour
