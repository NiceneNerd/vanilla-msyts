    attributes: Npc_Road_037
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: "Wie steht's, junger Mann?"
      - control:
          kind: pause
          length: long
      - text: " Mal eine\nverrückte Frage:"
      - control:
          kind: pause
          length: short
      - text: " Hast du Höhenangst?\nOder kommst du mit Parasegeln klar?"
      - control:
          kind: pause
          length: long
      - text: "\nIn dem Fall solltest du mal am\n"
      - control:
          kind: set_colour
          colour: blue
      - text: "Dorf der Orni "
      - control:
          kind: reset_colour
      - text: vorbeisegeln.
      - control:
          kind: pause
          length: short
      - text: "\nDie Orni mögen wohl Höhenlagen."
      - control:
          kind: pause
          length: short
      - text: "\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Mich zieht's leider nicht hoch hinaus."
      - control:
          kind: pause
          length: short
      - text: "\nRunter ist kein Problem für mich, darum\nlieb ich das "
      - control:
          kind: set_colour
          colour: red
      - text: Schildschlittern
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: pause
          length: short
      - text: " Aber rauf..."
  RetMSG_Taba_00_1:
    attributes: Npc_Road_037
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Das "
      - control:
          kind: set_colour
          colour: blue
      - text: Dorf der Orni
      - control:
          kind: reset_colour
      - text: " ist nichts für mich."
      - control:
          kind: pause
          length: short
      - text: "\nIch hab Höhenangst."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Runter geht's immer, sag ich als\n"
      - control:
          kind: set_colour
          colour: red
      - text: Schildschlitterer
      - control:
          kind: reset_colour
      - text: "..."
      - control:
          kind: pause
