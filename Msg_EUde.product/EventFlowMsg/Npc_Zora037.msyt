      - control:
          kind: sound
          unknown:
            - 0
            - 23
      - text: Endlich kein Regen mehr...
  talk13:
    attributes: Npc_Zora037
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 23
      - text: "Du warst es, der "
      - control:
          kind: set_colour
          colour: blue
      - text: "Vah Ruta "
      - control:
          kind: reset_colour
      - text: "zur Vernunft\ngebracht hat?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ich habe meinen Vater über dich\nausgefragt..."
      - control:
          kind: pause
          length: short
      - text: " Vor 100 Jahren warst du\nalso der Recke der Hylianer!\nUnd du scheinst immer noch ein richtiger\nHeld zu sein."
      - control:
          kind: pause
          length: longer
      - text: " Ich wünsche dir viel Glück\nauf deinen weiteren Abenteuern!"
  talk14:
    attributes: Npc_Zora037
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 5
      - text: "Du warst also kein gewöhnlicher\nHylianer..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ich hoffe, du besuchst das\n"
      - control:
