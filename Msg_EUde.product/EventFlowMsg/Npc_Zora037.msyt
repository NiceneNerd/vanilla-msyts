          colour: blue
      - text: Dorf der Zoras
      - control:
          kind: reset_colour
      - text: " bald einmal wieder!"
  talk15:
    attributes: Npc_Zora037
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "König Dorephan ist die mächtigste\nPerson im "
