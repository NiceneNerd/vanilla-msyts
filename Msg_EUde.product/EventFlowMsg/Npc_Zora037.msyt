    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 13
      - text: "Unter 100, mehr sage ich dir nicht."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Schließlich sage ich nicht in jedem\nzweiten Satz „zo“, oder?!"
  talk17:
