  "0002":
    attributes: ""
    contents:
      - text: Wiedersehen.
  "0003":
    attributes: ""
    contents:
      - text: Danke!
  "0004":
    attributes: ""
    contents:
      - text: Kaufmann gesehen?
  Talk_000:
    attributes: Npc_SmallOasis011
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Das hier ist der "
      - control:
          kind: set_colour
          colour: blue
      - text: Wüstenbasar
      - control:
          kind: reset_colour
      - text: ", so etwas\nwie ein Rastplatz auf dem Weg nach\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Gerudo-Stadt
      - control:
          kind: reset_colour
      - text: ".\nAls Reisender solltest du dich hier erst\neinmal gut ausruhen."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Da du ein Vooi bist, wirst du die Stadt\naber sowieso nicht betreten dürfen."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Dazu kommt, dass im Moment dieser\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Titan
      - control:
          kind: reset_colour
      - text: " dort wütet. "
      - control:
          kind: pause
          length: short
      - text: "Ich würde dir davon\nabraten, dich der Stadt zu nähern."
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
            - 2
          selected_index: 0
          cancel_index: 2
          unknown: 8
  Talk_001:
    attributes: Npc_SmallOasis011
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 6
      - text: "Hast du noch nicht vom "
      - control:
          kind: set_colour
          colour: blue
      - text: "Titanen\nVah Naboris"
      - control:
          kind: reset_colour
      - text: " gehört?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Anscheinend wurde er früher als\nSchutzgottheit der Gerudo verehrt."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Vor einer Weile ist er plötzlich wieder\naufgetaucht und wütet seitdem herum!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Was man auch versucht, der "
      - control:
          kind: set_colour
          colour: blue
      - text: Titan
      - control:
          kind: reset_colour
      - text: " ist von\neinem Sandsturm und Blitzen umgeben,\nsodass niemand ihn erreichen kann.\nWir müssen nur hoffen, dass er\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Gerudo-Stadt
      - control:
          kind: reset_colour
      - text: " und diesem Gebiet\nhier nicht zu nahe kommt.\nDeshalb behalte ich ihn von dem Posten\nhier im Blick."
      - control:
          kind: choice
          choice_labels:
            - 0
            - 2
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Talk_002:
    attributes: Npc_SmallOasis011
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: "Wenn du mit uns Gerudo sprechen willst,\nsolltest du ein paar Worte der Gerudo-\nSprache lernen.\nAuf Wiedersehen heißt bei uns „Savorq“!"
  Talk_003:
    attributes: Npc_SmallOasis011
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Bis hier wird der "
      - control:
          kind: set_colour
          colour: blue
      - text: Titan
      - control:
          kind: reset_colour
      - text: " sicher nicht\nkommen, aber Vorsicht ist besser als\nNachsicht.\nWenn du nach "
      - control:
          kind: set_colour
          colour: blue
      - text: Gerudo-Stadt
      - control:
          kind: reset_colour
      - text: " willst, nimm\ndich in Acht! "
      - control:
          kind: pause
          length: short
      - text: "Pass vor allem auf, nicht\ndeine schönen Kleider kaputtzumachen!"
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
            - 2
          selected_index: 0
          cancel_index: 2
          unknown: 8
  Talk_004:
    attributes: Npc_SmallOasis011
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Der "
      - control:
          kind: set_colour
          colour: blue
      - text: Titan
      - control:
          kind: reset_colour
      - text: " ist zwar zur Ruhe gekommen,\naber die Wüste wimmelt trotzdem von\nMonstern.\nIch würde dir raten, stets auf der Hut\nzu sein."
  Talk_005:
    attributes: Npc_SmallOasis011
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Das hier ist der "
      - control:
          kind: set_colour
          colour: blue
      - text: Wüstenbasar
      - control:
          kind: reset_colour
