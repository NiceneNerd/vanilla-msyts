          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 7
            - 1
      - text: "Du hast deinen überragenden Schneid\nunter Beweis gestellt!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Nun bist du unser vierter Bruder!
