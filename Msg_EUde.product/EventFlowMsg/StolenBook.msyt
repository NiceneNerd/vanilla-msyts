  Npc_Assassin_0:
    attributes: NPC_PublicVoice
    contents:
      - text: "Hehehe, da ist aber jemand sauer..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ich dachte mir schon, dass das Juwel\nden Dorfbewohnern wichtig ist."
  Npc_Assassin_1:
    attributes: NPC_PublicVoice
    contents:
      - control:
          kind: sound2
          unknown:
            - 0
            - 205
      - text: Du musst hier nicht so rumschreien...
      - control:
          kind: pause
          length: short
      - text: "\nBin ja schon da."
  Npc_Assassin_2:
    attributes: NPC_PublicVoice
    contents:
      - text: "Ich bin überrascht, dass du das fragst."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Warst du nicht der erste, der uns\nverlassen wollte?"
      - control:
          kind: pause
          length: short
      - text: " Du müsstest doch am\nbesten wissen, was das bedeutet."
  Npc_Assassin_3:
    attributes: NPC_PublicVoice
    contents:
      - text: Aber...
      - control:
          kind: pause
          length: longer
      - text: "\nAm Ende hast du mir sehr geholfen."
  Npc_Assassin_4:
    attributes: NPC_PublicVoice
    contents:
      - text: Um dich kümmere ich mich gleich.
  Npc_Assassin_5:
    attributes: NPC_PublicVoice
    contents:
      - text: "Zuerst kümmere ich mich um den\nTypen da, der sich hinter dir versteckt!"
  Npc_Assassin_6:
    attributes: NPC_PublicVoice
    contents:
      - text: "Ich kümmere mich um den Typen, der\nsich dort versteckt."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Spüre den Zorn meines Meisters!
  Npc_Kakariko002_Near:
    attributes: Npc_Kakariko002
    contents:
      - control:
          kind: sound2
