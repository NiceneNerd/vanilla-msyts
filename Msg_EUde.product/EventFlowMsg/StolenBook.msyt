  Npc_Kakariko002_R_004:
    attributes: Npc_Kakariko001
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 4
      - text: „
      - control:
          kind: set_colour
          colour: red
      - text: "Dem vom Juwel Auserwählten wird\nder Segen der Alten gewährt."
      - control:
          kind: reset_colour
      - text: “
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ich denke, "
      - control:
          kind: set_colour
          colour: red
      - text: dieser Auserwählte bist du
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Paya hat kaum noch geschlafen, weil\nsie dem Geheimnis des "
      - control:
          kind: set_colour
          colour: blue
      - text: Juwels
      - control:
          kind: reset_colour
      - text: " auf den\nGrund gehen wollte."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Sie glaubt, dass der „"
      - control:
          kind: set_colour
          colour: red
      - text: Segen der Alten
      - control:
          kind: reset_colour
      - text: "“ \nsich auf die "
      - control:
          kind: set_colour
          colour: red
      - text: "antiken Schreine "
      - control:
          kind: reset_colour
      - text: "bezieht,\nwie den hier oberhalb des Dorfs..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Und dass das "
      - control:
          kind: set_colour
          colour: blue
      - text: Juwel
      - control:
          kind: reset_colour
      - text: " der "
      - control:
          kind: set_colour
          colour: red
      - text: "Schlüssel ist,\num einen dieser Schreine zu öffnen"
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 9
            - 17
      - text: "Sie hat so darauf gewartet, dir das\nalles sagen zu können..."
      - control:
          kind: pause
          length: long
      - text: " Und dann ist\ndieser ganze Schlamassel passiert."
  Npc_Kakariko002_R_006:
    attributes: Npc_Kakariko002
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 37
      - text: "Ich habe das "
      - control:
          kind: set_colour
          colour: blue
      - text: "heilige Juwel "
      - control:
          kind: reset_colour
      - text: "von morgens\nbis abends gereinigt..."
      - control:
          kind: pause
          length: short
      - text: " Jemand muss es\nnachts gestohlen haben."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 1
      - text: "Aber Außenseitern ist der Zutritt hier\nnicht erlaubt."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Und ich weigere mich zu glauben, dass\nes jemand aus dem Dorf war."
  Npc_Kakariko002_R_008:
    attributes: Npc_Kakariko002
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 12
      - text: "Ich habe das "
      - control:
          kind: set_colour
          colour: blue
      - text: Juwel
      - control:
          kind: reset_colour
      - text: " von morgens bis\nabends im Auge gehabt..."
      - control:
          kind: pause
          length: short
      - text: " Der Dieb muss\nes nachts gestohlen haben."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ich habe schon mit Dorian und Cado\ngesprochen und sie haben nur Leute\naus dem Dorf gesehen."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 12
      - text: "Aber wer aus dem Dorf würde so etwas\ntun?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Es ist noch nicht lange her, dass das\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Juwel
      - control:
          kind: reset_colour
      - text: " gestohlen wurde."
      - control:
          kind: pause
          length: short
      - text: " Vielleicht\nversteckt sich der Täter in der Nähe."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 3
            - 1
      - text: "Wenn Ihr also nachts eine verdächtige\nPerson seht, "
      - control:
          kind: set_colour
          colour: red
      - text: kommt ihr nicht zu nah
      - control:
          kind: reset_colour
      - text: "!"
  Npc_Kakariko002_R_009:
    attributes: Npc_Kakariko002
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 1
      - text: "Dieser heilige Ort wurde von jemandem\nmit bösen Absichten im Herz betreten..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 5
            - 1
      - text: "Ich bin froh, dass Großmutter in\nSicherheit ist, aber trotzdem habe ich\nAngst..."
  Npc_Kakariko002_R_010:
    attributes: Npc_Kakariko001
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 17
      - text: Link...
      - control:
          kind: pause
          length: short
      - text: " Paya ist verängstigt."
      - control:
          kind: pause
          length: short
      - text: " Vielleicht ist\nder Verbrecher auch noch in der Nähe."
      - control:
          kind: pause
          length: short
      - text: "\nKönntest du nicht kurz bei ihr bleiben?"
  Npc_Kakariko002_R_011:
    attributes: Npc_Kakariko002
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 18
      - text: Link...
      - control:
          kind: pause
          length: short
      - text: " Ich danke Euch."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Da Ihr den ganzen Tag bei mir geblieben\nseid, habe ich den Mut gefunden, diesem\nBanditen die Stirn zu bieten."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Hier zu sitzen und das Geschehene\nzu bejammern ist doch keine Lösung."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ich habe Dorian und Cado befragt,\naber außer den Dorfbewohnern haben\nsie niemanden gesehen..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 5
            - 12
      - text: "Ich werde versuchen, mich an die Details\ndes Diebstahls zu erinnern."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Bedenkt jedoch bitte eines:"
      - control:
          kind: pause
          length: short
      - text: "\nDer Dieb könnte noch in der Nähe sein."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Versprecht mir, dass Ihr euch des Nachts\nverdächtigen Personen "
      - control:
          kind: set_colour
          colour: red
      - text: "keinesfalls\nnähert"
      - control:
          kind: reset_colour
      - text: ", egal, was sie tun."
  Npc_Kakariko006_01:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: "Bist du auch hier, um dir das Schloss\nanzusehen?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 16
      - text: Dieses Licht...
      - control:
          kind: pause
          length: short
      - text: " Ich weiß, es ist ein\nZeichen des Bösen..."
      - control:
          kind: pause
          length: short
      - text: " Und doch sehe\nich sein Leuchten nachts gern."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 13
      - text: "Aber das könnte ich nie zu jemandem\naus dem Dorf sagen."
  Npc_Kakariko006_02:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 6
      - text: "Bist du auch hier, um dir das Schloss\nanzusehen?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Dieses Licht...
      - control:
          kind: pause
          length: short
      - text: " Ich weiß, es ist ein\nZeichen des Bösen..."
      - control:
          kind: pause
          length: short
      - text: " Und doch sehe\nich sein Leuchten nachts gern."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Schon komisch, wie viele Leute man hier\nheute sieht..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Gerade habe ich "
      - control:
          kind: set_colour
          colour: blue
      - text: Dorian
      - control:
          kind: reset_colour
      - text: " gesehen, er war\nauf dem Weg zu den "
      - control:
          kind: set_colour
          colour: red
      - text: Hügeln dort hinten
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Aber er schien es sehr eilig zu haben...
  Npc_Kakariko006_03:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: Was ein toller Tag.
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Vielleicht mache ich noch einen\nSpaziergang, bevor ich nach Hause gehe."
  Npc_Kakariko006_Near:
    attributes: Npc_Kakariko006
    contents:
      - text: Hrm hrm...
  Npc_Kakariko007_101:
    attributes: Npc_Kakariko007
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 14
      - text: "Ich weiß, dass du hier bist!"
      - control:
          kind: pause
          length: longer
      - text: "\nKomm schon raus!"
  Npc_Kakariko007_102:
    attributes: Npc_Kakariko007
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 0
      - text: Also du hast das Juwel gestohlen!
  Npc_Kakariko007_103:
    attributes: Npc_Kakariko007
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 14
      - text: Warum hast du es gestohlen?!
  Npc_Kakariko007_104:
    attributes: Npc_Kakariko007
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 44
      - text: Grr...
  Npc_Kakariko007_105:
    attributes: Npc_Kakariko007
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "Ich kann nicht glauben, dass mir jemand\ngefolgt ist..."
      - control:
          kind: pause
          length: short
      - text: " Ihr wisst es wahrscheinlich\nschon, aber das ist alles meine Schuld..."
  Npc_Kakariko007_106:
    attributes: Npc_Kakariko007
    contents:
      - control:
          kind: sound
          unknown:
            - 2
            - 12
      - text: "Aber dann habe ich mein letztes\nbisschen Mut zusammengenommen..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ich wollte mit denen nichts mehr zu tun\nhaben und meine Kinder beschützen!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 8
            - 45
      - text: Aber ich hatte sie unterschätzt...
      - control:
          kind: pause
          length: short
      - text: "\nUnd so haben sie auch noch Impa und\nPaya mit hineingezogen..."
  Npc_Kakariko007_107:
    attributes: Npc_Kakariko007
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Vielleicht wisst Ihr es schon, aber mit\ndiesem "
      - control:
          kind: set_colour
          colour: blue
      - text: "heiligen Juwel "
      - control:
          kind: reset_colour
      - text: "lässt sich "
      - control:
          kind: set_colour
          colour: red
      - text: "ein\nantiker Schrein öffnen"
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Wir hatten bisher kein Glück, aber Euch\nkönnte es gelingen...\n"
  Npc_Kakariko007_108:
    attributes: Npc_Kakariko007
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "Ich habe kein Recht, Euch darum zu\nbitten, aber könntet Ihr was hier\ngeschehen ist für Euch behalten?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ich will nicht, dass meine Kinder..."
      - control:
          kind: pause
          length: short
      - text: "\nIch will nicht, dass Koko und Priko\ndarunter leiden müssen."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Ich weiß, ich kann das Geschehene nicht\nwiedergutmachen..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Aber ich werde von nun an das Dorf und\nmeine Familie mit meinem Leben\nbeschützen."
  Npc_Kakariko007_109:
    attributes: Npc_Kakariko007
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 1
      - text: "Es hat keinen Sinn, es zu leugnen..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 12
      - text: "Ich bin ein Yiga, so wie er."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 5
            - 1
      - text: "Aber nachdem ich meine spätere Frau\ngetroffen habe und Vater wurde, wollte\nich mein altes Leben hinter mir lassen..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 9
            - 12
      - text: Und deswegen ist meine Frau...
  Npc_Kakariko007_110:
    attributes: Npc_Kakariko007
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 14
      - text: "Das wird nicht einfach, aber er ist allein."
      - control:
          kind: pause
          length: short
      - text: "\nAuf ihn!"
  Npc_Kakariko007_111:
    attributes: Npc_Kakariko007
    contents:
      - control:
          kind: sound
          unknown:
            - 2
            - 1
      - text: "Ich wollte nicht, dass sie meiner Familie\nnoch mehr antun, also habe ich meine\nPosition als Wächter genutzt und..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ich habe ihnen Informationen über Euch\nund das Dorf zugespielt."
  Npc_Kakariko007_112:
    attributes: Npc_Kakariko007
    contents:
      - control:
          kind: sound
          unknown:
