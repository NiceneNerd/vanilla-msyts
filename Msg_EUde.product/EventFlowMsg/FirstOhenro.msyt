  Npc_Kokiri009_S1_Near011:
    attributes: Npc_kokiri009
    contents:
      - control:
          kind: sound2
          unknown:
            - 30
            - 205
      - text: Ist das finster! Gruselig...
  Npc_Kokiri009_S1_Near012:
    attributes: Npc_kokiri009
    contents:
      - control:
          kind: sound2
          unknown:
            - 0
            - 205
