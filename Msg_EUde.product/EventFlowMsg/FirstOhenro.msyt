      - text: Waaah!
      - control:
          kind: text_size
          percent: 100
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 8
            - 0
      - text: Erschreck mich doch nicht so!
      - control:
          kind: pause
          length: short
      - text: " Was stellst\ndu mir überhaupt nach?"
      - control:
          kind: pause
