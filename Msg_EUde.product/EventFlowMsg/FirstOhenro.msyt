          kind: text_size
          percent: 100
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Moment mal, wer bist du?"
      - control:
          kind: pause
          length: short
      - text: " Bist du etwa\nschon die ganze Zeit hinter mir!?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Na toll!
      - control:
          kind: pause
          length: short
      - text: " Ich wollte das doch allein\nschaffen!"
      - control:
          kind: pause
          length: short
      - text: " Jetzt kann ich von vorn\nanfangen!"
  Npc_Kokiri009_S1_Near001:
    attributes: Npc_kokiri009
    contents:
      - control:
          kind: sound2
          unknown:
            - 10
            - 205
