          percent: 125
      - control:
          kind: sound
          unknown:
            - 10
            - 21
      - text: Hä?
      - control:
          kind: pause
          length: short
      - text: " Wer ist da?"
      - control:
          kind: text_size
          percent: 100
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Du bist doch der Held!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Werter Held, ich habe die "
      - control:
          kind: set_colour
          colour: red
      - text: Prüfung
      - control:
          kind: reset_colour
      - text: "\ngeschafft!"
      - control:
          kind: pause
          length: short
      - text: " Und zwar ganz allein!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 10
            - 19
      - text: "Oh, ach ja!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 7
            - 1
      - text: "Der Deku-Baum hat gesagt, dass\nim Schrein ein Segen auf den Helden\nwartet! "
      - control:
