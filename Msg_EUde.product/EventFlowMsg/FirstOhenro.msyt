            - 3
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Npc_Kokiri001_R_006:
    attributes: Npc_kokiri001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Sehr gut!
      - control:
          kind: pause
          length: short
      - text: " In diesem Fall könntest du mir\nvielleicht einen Gefallen tun..."
  Npc_Kokiri001_R_007:
    attributes: Npc_kokiri001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Da lag ich doch tatsächlich schon wieder\nfalsch!"
      - control:
          kind: pause
          length: short
      - text: " Verzeihung, tut mir leid!"
  Npc_Kokiri001_R_Near:
    attributes: Npc_kokiri001
    contents:
      - text: Hoffentlich kommt er klar...
  Npc_Kokiri001_S1_001:
    attributes: Npc_kokiri001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Wie läuft die "
      - control:
          kind: set_colour
          colour: red
      - text: Prüfung
      - control:
          kind: reset_colour
      - text: "?"
      - control:
          kind: pause
          length: short
      - text: " Ist alles in\nOrdnung?"
      - control:
          kind: pause
          length: short
      - text: " Denk daran, Fraxinus darf\ndich nicht sehen!"
  Npc_Kokiri001_S1_002:
    attributes: Npc_kokiri001
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Fraxinus hat dich gesehen, was?"
      - control:
          kind: pause
          length: short
      - text: "\nEr ist wirklich sehr nervös und zuckt\nbei jedem Schatten zusammen..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Aber er hat sich trotzdem wieder auf den\nWeg gemacht, wenn du ihm also wieder\nfolgen könntest?"
      - control:
          kind: pause
          length: short
      - text: " Das wäre toll!"
  Npc_Kokiri001_S1_003:
    attributes: Npc_kokiri001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Alles in Ordnung?
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Du hast dich anscheinend im Wald\nverlaufen."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ich war ganz schön platt, als Fraxinus\ndich hier abgeliefert hat, das kann ich\ndir sagen!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Fraxinus will sich wieder auf den Weg\nmachen, wenn du ihm also noch mal\nfolgen könntest?"
      - control:
          kind: pause
          length: short
      - text: " Das wäre toll!"
  Npc_Kokiri001_S1_004:
    attributes: Npc_kokiri001
    contents:
      - text: "Na, wie geht's mit der "
      - control:
          kind: set_colour
          colour: red
      - text: Prüfung
      - control:
          kind: reset_colour
      - text: " voran?"
      - control:
          kind: choice
          choice_labels:
            - 4
            - 5
          selected_index: 0
          cancel_index: 1
          unknown: 6
