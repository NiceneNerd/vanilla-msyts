    attributes: Npc_kokiri001
    contents:
      - control:
          kind: text_size
          percent: 125
      - control:
          kind: sound
          unknown:
            - 10
            - 21
      - text: Du hast ihn verloren?!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: text_size
          percent: 100
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Vielleicht hat Fraxinus sich verlaufen...
      - control:
          kind: pause
          length: short
      - text: "\nIch muss schnell nach ihm sehen!"
  Npc_Kokiri001_S1_006:
    attributes: Npc_kokiri001
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 18
      - text: Ach ja...
      - control:
          kind: pause
          length: short
      - text: " Das Geräusch des Windes hat\nihm solche Angst gemacht, dass er sich\nnicht von der Stelle rühren konnte."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Aber er will es noch mal versuchen!
      - control:
          kind: pause
          length: short
      - text: "\nWenn du also wieder auf ihn aufpassen\nkönntest, wäre das toll!"
      - control:
          kind: pause
          length: short
      - text: " Danke!"
  Npc_Kokiri001_S1_007:
    attributes: Npc_kokiri001
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: "Das freut mich! Und denk daran,\n"
      - control:
