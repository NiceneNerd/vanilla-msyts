            - 7
            - 0
      - text: Oh!
      - control:
          kind: pause
          length: short
      - text: " Puh, sehr gut."
      - control:
          kind: pause
          length: short
      - text: " In diesem Fall\nkönntest du mir vielleicht einen Gefallen\ntun."
  Npc_Kokiri001_R_003:
    attributes: Npc_kokiri001
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 1
      - text: "Es ist so:"
      - control:
          kind: pause
          length: short
      - text: " Fraxinus hat sich in den Kopf\ngesetzt, die "
      - control:
          kind: set_colour
          colour: red
      - text: Prüfungen der Krogs
      - control:
          kind: reset_colour
      - text: " zu\nmachen."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Normalerweise wäre ich ja mitgegangen,\naber er wollte unbedingt allein gehen."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Also habe ich ihn ziehen lassen...
      - control:
          kind: pause
          length: short
      - text: "\nUnd jetzt mache ich mir solche Sorgen!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 6
            - 9
      - text: Könntest du ihm nicht
      - control:
          kind: pause
          length: short
      - text: " "
      - control:
          kind: set_colour
          colour: red
      - text: heimlich folgen
      - control:
          kind: reset_colour
      - text: "?"
      - control:
          kind: pause
          length: short
      - text: "\nAlso... So, dass er dich nicht sieht?"
      - control:
          kind: pause
          length: short
      - text: "\nBittebitte..."
  Npc_Kokiri001_R_004:
    attributes: Npc_kokiri001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "N-nicht? "
      - control:
          kind: pause
          length: short
      - text: Oh... Da lag ich wohl falsch!
      - control:
          kind: pause
          length: short
      - text: "\nNix für ungut."
  Npc_Kokiri001_R_005:
    attributes: Npc_kokiri001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Oh! "
      - control:
          kind: pause
          length: short
      - text: Da bist du ja wieder!
      - control:
          kind: pause
          length: short
      - text: " Machst du jetzt\nauch die "
      - control:
