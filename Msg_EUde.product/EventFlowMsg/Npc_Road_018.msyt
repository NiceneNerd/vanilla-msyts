                  - 0
      - text: Rhodonit... ♪
  Near03:
    attributes: Npc_Road_018
    contents:
      - text: "Sterne aus "
      - control:
          kind: raw
          two_hundred_one:
            dynamic:
              - 0
              - len: 4
