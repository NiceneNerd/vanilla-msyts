  Talk_001:
    attributes: Npc_HighMountain032
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 11
      - text: "Oh, super!"
      - control:
          kind: pause
          length: short
      - text: " Du bist so mutig, dass du\nein "
      - control:
          kind: set_colour
          colour: blue
      - text: "Recke "
      - control:
          kind: reset_colour
      - text: sein könntest!
  Talk_002:
    attributes: Npc_HighMountain032
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: Aber du bist ganz viel größer als ich...
      - control:
          kind: pause
          length: short
      - text: "\nWie kannst du da Angst haben..."
      - control:
          kind: pause
          length: short
      - text: "\nWas mach ich denn jetzt?\n"
      - control:
          kind: sound
          unknown:
            - 7
            - 1
      - text: "Ich weiß! "
      - control:
          kind: pause
          length: short
      - text: "Ich werde dem Vogel ganz viele\nFische zu fressen geben, dann ist er so\nsatt, dass er mich nicht mehr mag!\n"
      - control:
          kind: sound
          unknown:
            - 10
            - 22
      - text: Aber...
      - control:
          kind: pause
          length: short
      - text: " vielleicht frisst er mich dann,\nwenn ich ihm die Fische bringe..."
      - control:
          kind: pause
          length: long
      - text: " Ach..."
  Talk_003:
    attributes: Npc_HighMountain032
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 6
      - text: Dann weißt du noch gar nichts?
      - control:
          kind: pause
          length: short
      - text: " Du\nmusst gleich mit dem Häuptling reden!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Die Geschichte macht dir bestimmt ganz\ndoll Angst, aber keine Sorge."
      - control:
          kind: pause
          length: short
      - text: " Ich höre\nsie mir mit dir zusammen an."
  Talk_004:
    attributes: Npc_HighMountain032
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Hast du keine Angst vor dem\n"
      - control:
