          unknown: 8
  Talk_010:
    attributes: Npc_HighMountain032
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 0
      - text: Ich gehe zum Häuptling!
      - control:
          kind: pause
          length: short
      - text: "\nMöchtest du mitkommen?"
  Talk_020:
    attributes: Npc_HighMountain032
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ich muss ganz schnell nach Hause,\nsonst frisst mich der Geistervogel!"
      - control:
          kind: pause
          length: long
      - text: "\nGanz schnell!"
  Talk_030:
    attributes: Npc_HighMountain032
    contents:
      - control:
          kind: sound2
          unknown:
            - 31
            - 205
      - text: Hmmm...
  Talk_100:
    attributes: Npc_HighMountain032
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 2
      - text: "Fische, Fische, ganz viele Fische! ♪"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Also, wie war es denn in dem Schrein?"
      - control:
          kind: pause
          length: short
      - text: "\nGab es da viele Fische?"
      - control:
          kind: choice
          choice_labels:
            - 3
            - 4
            - 5
          selected_index: 0
          cancel_index: 2
          unknown: 8
  Talk_102:
    attributes: Npc_HighMountain032
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 11
      - text: "Toll! "
      - control:
          kind: pause
          length: long
      - text: "Wenn ich groß und stark bin,\nwerde ich selbst einmal hineingehen!"
  Talk_103:
    attributes: Npc_HighMountain032
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: Das dachte ich mir schon...
      - control:
          kind: pause
          length: short
      - text: " Dieser\nSchrein schläft schon ganz lange."
      - control:
          kind: pause
          length: short
      - text: "\nDa gibt es nur noch Gräten..."
  Talk_104:
    attributes: Npc_HighMountain032
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 0
      - text: Wieso warst du denn noch nicht drin?
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Dann aber schnell!
      - control:
          kind: pause
          length: short
      - text: " Wenn es da drin\nFische gibt, gehe ich sie fangen!"
  Talk_105:
    attributes: Npc_HighMountain032
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: La-da-daaa! ♪ Fischi-Fischi-Fischi! ♪
  Talk_110:
    attributes: Npc_HighMountain032
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 0
      - text: "Hach, ich würde ja so gerne Fische\nfangen, aber Kihl wird sauer, wenn\nich nicht mit ihr zusammen übe."
  Talk_120:
    attributes: Npc_HighMountain032
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "Vielleicht kann ich ja morgen ein paar\nFische fangen..."
  Talk_200:
    attributes: Npc_HighMountain032
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 2
      - text: "Papa hat mir gezeigt, wie man Fische\nfängt."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Man muss ganz schnell zu ihnen hin\nschwimmen, dann fängt man sie ganz\nleicht! "
      - control:
          kind: pause
          length: short
      - text: Ich übe auch fleißig schwimmen!
  Talk_201:
    attributes: Npc_HighMountain032
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: "Wenn du einen Fisch siehst, schwimme\nganz schnell zu ihm hin und greif ihn\ndir! "
      - control:
          kind: pause
          length: short
      - text: "Ich weiß, wie das geht!"
  Talk_210:
    attributes: Npc_HighMountain032
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 0
      - text: "Ich würde so gerne Fische fangen gehen,\naber Kihl wird sauer, wenn ich nicht mit\nihr zusammen singen übe."
  Talk_220:
    attributes: Npc_HighMountain032
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "Vielleicht kann ich ja morgen ein paar\nFische fangen..."
