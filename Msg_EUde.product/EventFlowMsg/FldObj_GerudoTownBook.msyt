  talk00:
    attributes: FldObj_GerudoTownBook
    contents:
      - text: "Es liegt so weit oben, dass du es nicht\nlesen kannst..."
  talk01:
    attributes: Npc_OasisStudent_C
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: "Dieses Buch wurde von einem Meister-\nkoch namens Ra-Tahm verfasst. "
      - control:
          kind: pause
          length: short
      - text: "Leider\nweilt er nicht mehr unter uns."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: text_size
          percent: 80
      - text: "Erzähl es nicht rum, aber..."
      - control:
          kind: pause
          length: short
      - text: " Er war ein Vooi,\nin den sich unsere Lehrerin einst verliebt hat..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: text_size
          percent: 100
      - text: "Wenn das Buch zu hoch für dich liegt,\nkann ich dir daraus vorlesen."
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
          selected_index: 0
          cancel_index: 1
          unknown: 6
