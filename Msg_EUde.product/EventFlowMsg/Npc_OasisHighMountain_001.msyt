  talk11:
    attributes: Npc_OasisHighMountain_001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Das Fleisch hier riecht wirklich\nunglaublich verlockend..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Bei diesem Duft möchte ich am liebsten\ngleich drei Portionen verdrücken!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Schöner könnte es nur sein, wenn sie\nauch Hähnchen hätten..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: text_size
          percent: 80
      - control:
          kind: sound
          unknown:
            - 11
            - 1
      - text: Dann würde ich fünf Portionen runterkriegen!
      - control:
          kind: text_size
          percent: 100
  talk12:
    attributes: Npc_OasisHighMountain_001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 6
      - text: "Dieses Fleisch ist wirklich eine\nAugenweide..."
      - control:
          kind: pause
          length: short
      - text: " Hast du es auch\ndarauf abgesehen?\nDiese natürliche Marmorierung, dieser\nwilde, frische Geruch..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ich könnte den ganzen Abend hier\nverbringen!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "...Oh! Es ist ja schon Morgen!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Wenn sie auch Hähnchen hätten...
      - control:
          kind: pause
          length: longer
      - text: "\nDann würde ich mein ganzes Leben\nhier verbringen!"
  talk13:
    attributes: Npc_OasisHighMountain_001
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: Es ist kein Fleisch mehr da...
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Nur der Duft hängt noch in der Luft.
      - control:
          kind: pause
          length: long
      - text: "\nWenn ich das rieche, könnte ich direkt\nzwei Portionen verdrücken!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Schöner könnte es nur sein, wenn sie\nauch Hähnchen hätten..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: text_size
          percent: 80
      - control:
          kind: sound
          unknown:
            - 11
            - 1
      - text: Dann würde ich vier Portionen runterkriegen!
      - control:
          kind: text_size
          percent: 100
  talk14:
    attributes: Npc_OasisHighMountain_001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Es ist kein Wild mehr da, aber ich habe\nes sowieso auf Hühnchen abgesehen."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Sobald sie Hühnchen reinbekommen,\nkaufe ich den ganzen Laden leer!"
  talk15:
    attributes: Npc_OasisHighMountain_001
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: Wie soll ich sagen...
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Es ist so, als würde ich damit angeben,\ndass ich eine Fremdsprache\nbeherrsche..."
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
          selected_index: 0
          cancel_index: 2
          unknown: 6
  talk16:
    attributes: Npc_OasisHighMountain_001
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Ja, vielleicht mache ich mir da\nwirklich zu viele Gedanken..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Im Dorf der Orni, wo ich wohne, kommt\nkaum je ein Tourist vorbei."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Deshalb ist mir die Lebhaftigkeit einer\nStadt wie dieser hier ziemlich fremd..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Wo ich schon einmal hier bin, will ich\nauf jeden Fall Spaß haben."
      - control:
          kind: pause
          length: short
      - text: " Deshalb\nwollte ich auch hier Fleisch essen...\n"
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "Zu schade, dass sie nichts als Wild\nhaben..."
  talk17:
