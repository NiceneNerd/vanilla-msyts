  talk00:
    attributes: Npc_HyrulePlain005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: "Sieht heute nicht gut aus, das Wetter..."
  talk01:
    attributes: Npc_HyrulePlain005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Na, wenn du dir die Zeit nimmst, mit\neinem alten Knacker wie mir zu reden,\nbrauchst du wohl Hilfe, oder?"
  talk02:
    attributes: Npc_HyrulePlain005
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 0
      - text: "Hehe... "
      - control:
          kind: pause
          length: short
      - text: "Wenn du einen Opa wie mich\ndabei störst, sein Enkelkind im Schlaf zu\nbetrachten, brauchst du wohl Hilfe?"
  talk03:
    attributes: Npc_HyrulePlain005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Du kennst dich nicht besonders gut aus\nhier in der Umgebung, oder?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: Tja-ha-ha.
      - control:
          kind: pause
          length: short
      - text: " Wenn du einmal in meinem\nAlter bist, siehst du den Leuten so was\nan der Nasenspitze an!\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Das hier ist der "
      - control:
          kind: set_colour
          colour: blue
      - text: Stall des Waldes
      - control:
          kind: reset_colour
      - text: ". "
      - control:
          kind: pause
          length: short
      - text: "Der\nliegt genau auf der Grenze zwischen\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Eldin
      - control:
          kind: reset_colour
      - text: " und der "
      - control:
          kind: set_colour
          colour: blue
      - text: Ebene von Hyrule
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Es gibt kaum einen Reisenden, der hier\nkeine Pause einlegt!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "...Ah, und der "
      - control:
          kind: set_colour
          colour: blue
      - text: Wald von Hyrule
      - control:
          kind: reset_colour
      - text: ", von dem\nder Stall seinen Namen hat, ist auch\nnicht weit von hier."
  talk04:
    attributes: Npc_HyrulePlain005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Soll ich dir vielleicht was über die\nUmgebung hier erzählen?"
      - control:
          kind: choice
          choice_labels:
            - 2
            - 3
            - 5
            - 4
          selected_index: 0
          cancel_index: 3
          unknown: 10
  talk05:
    attributes: Npc_HyrulePlain005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: Tja...
      - control:
          kind: pause
          length: longer
      - text: " Dann wünsche ich dir eine gute\nReise."
  talk06:
    attributes: Npc_HyrulePlain005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Nordöstlich von hier beginnt die Region\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Eldin
      - control:
          kind: reset_colour
      - text: ", eine Gebirgskette um den\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Todesberg
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Man kann von hier aus sogar den\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Todesberg
      - control:
          kind: reset_colour
      - text: " sehen, aber nur seine\nRückseite."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Wenn du ihn besteigen möchtest,\nwürde ich empfehlen, es von der Region\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Akkala
      - control:
          kind: reset_colour
      - text: " im Osten aus zu versuchen."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Auf dem Weg solltest du eigentlich am\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Stall am Berge
      - control:
          kind: reset_colour
      - text: " vorbeikommen. "
      - control:
          kind: pause
          length: short
      - text: "Da in der\nNähe beginnt der "
      - control:
          kind: set_colour
          colour: blue
      - text: Pfad zum Todesberg
      - control:
          kind: reset_colour
      - text: ".\n"
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "Der "
      - control:
          kind: set_colour
          colour: blue
      - text: Todesberg
      - control:
          kind: reset_colour
      - text: " ist allerdings ein\nlodernder Vulkan."
      - control:
          kind: pause
          length: long
      - text: " Wer da unvorbereitet\nrangeht, bezahlt das mit dem Leben."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Im "
      - control:
          kind: set_colour
          colour: blue
      - text: Stall am Berge
      - control:
          kind: reset_colour
      - text: " gibt es aber sicher\njemanden, der weiß, wie man mit der\nBrandgefahr dort zurechtkommt."
  talk07:
    attributes: Npc_HyrulePlain005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Die "
      - control:
          kind: set_colour
          colour: blue
      - text: Ebene von Hyrule
      - control:
          kind: reset_colour
      - text: " liegt südlich von\nhier, hinter dem "
      - control:
          kind: set_colour
          colour: blue
      - text: Gongol-Hügel
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Früher war sie dank "
      - control:
          kind: set_colour
          colour: blue
      - text: Schloss Hyrule
      - control:
          kind: reset_colour
      - text: " und\nder umgebenden Stadt eine blühende\nRegion..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 8
            - 45
      - text: Aber geh da bloß nicht hin!
      - control:
          kind: pause
          length: short
      - text: " Schlag dir\ndas gleich wieder aus dem Kopf!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "..."
      - control:
          kind: pause
          length: short
      - text: "Und bitte denk nicht, dass das nur das\nGemaule eines alten Mannes ist."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 3
            - 12
      - text: "Viele junge Leute sind zur "
      - control:
          kind: set_colour
          colour: blue
      - text: "Ebene von\nHyrule"
      - control:
          kind: reset_colour
      - text: " aufgebrochen, aber keiner von\nihnen ist je zurückgekehrt..."
  talk08:
    attributes: Npc_HyrulePlain005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: "Ja, guten Morgen."
  talk09:
    attributes: Npc_HyrulePlain005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: "Ja, hallo."
  talk10:
    attributes: Npc_HyrulePlain005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ja, guten Abend."
  talk11:
    attributes: Npc_HyrulePlain005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: "Kann ich dir mit Informationen\nweiterhelfen?"
      - control:
          kind: choice
          choice_labels:
            - 2
            - 3
            - 5
            - 4
          selected_index: 0
          cancel_index: 3
          unknown: 10
  talk12:
    attributes: Npc_HyrulePlain005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Der "
      - control:
          kind: set_colour
          colour: blue
      - text: Wald von Hyrule
      - control:
          kind: reset_colour
      - text: " liegt im Norden und\nist der größte Wald in ganz Hyrule."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 1
      - text: "In seiner Mitte sind die "
      - control:
          kind: set_colour
          colour: blue
      - text: "Verlorenen\nWälder"
      - control:
          kind: reset_colour
      - text: ", ein unheimlicher Ort..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Als ich noch jünger war, habe ich mich\ndort mal beim Feuerholzsammeln\nverlaufen."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Ich bin immer tiefer in den Wald geraten.
      - control:
          kind: pause
          length: short
      - text: "\nEin "
      - control:
          kind: set_colour
          colour: red
      - text: starker Wind
      - control:
          kind: reset_colour
      - text: " kam auf..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Da bin ich losgerannt, so schnell wie ich\nkonnte!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 5
            - 12
      - text: "Dann war der Nebel plötzlich weg und\nich stand am Waldrand."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Irgendetwas haust in den "
      - control:
          kind: set_colour
          colour: blue
      - text: "Verlorenen\nWäldern"
      - control:
          kind: reset_colour
      - text: "..."
      - control:
          kind: pause
          length: short
      - text: " Geh da lieber nicht hin, wenn\ndu es vermeiden kannst."
