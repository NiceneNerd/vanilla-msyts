    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: Lanja ist vielleicht sauer...
      - control:
          kind: pause
          length: short
      - text: "\nNa ja, ist wohl kein Wunder."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Sie hat vor einer Weile schon wegen\nBaretta mächtig Ärger mit "
