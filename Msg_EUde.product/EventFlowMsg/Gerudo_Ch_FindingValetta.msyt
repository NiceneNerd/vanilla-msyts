    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "Wir geben kein sehr gutes Bild ab, was?"
      - control:
          kind: pause
          length: short
      - text: "\nDas ist mir wirklich peinlich..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Und jetzt ist auch Baretta schon wieder\nverschwunden..."
      - control:
          kind: pause
          length: longer
      - text: " Wenn die Kommandantin\ndavon erfährt, wird sie uns was erzählen!"
