    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: Oh! Du bist wirklich eine mutige Vaai!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Baretta hat uns alles erzählt!
      - control:
          kind: pause
          length: short
      - text: "\nWie du sie gefunden hast, meine ich!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Du warst heldenhafter als jeder Vooi,\nhat sie gesagt!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 9
      - text: "Wenn du eine Gerudo wärst, müsstest\ndu unbedingt unserer Einheit beitreten!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Sieht jedenfalls so aus, als wäre ich\ndavongekommen, ohne dass die\nKommandantin mir den Kopf abreißt!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: "Du bist sicher erschöpft, oder?"
      - control:
          kind: pause
          length: short
      - text: " Hier,\nnimm das hier zum Dank und kauf dir\nirgendetwas auf dem Markt!"
  talk03:
    attributes: Npc_oasis041
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Diesmal scheint Baretta endlich etwas\naus der Sache gelernt zu haben..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
