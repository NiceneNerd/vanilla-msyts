          colour: blue
      - text: Hateno
      - control:
          kind: reset_colour
      - text: "!"
      - control:
          kind: pause
          length: longer
      - text: "\nAber für ein bisschen Handel wäre ich\nnoch zu haben!"
  RetMSG_Futa_04:
    attributes: Npc_Road_004
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 16
      - text: "Ist schon wieder etwas her, dass ich in\nder Heimat war..."
      - control:
          kind: pause
          length: long
      - text: " Ich würde mich ja gern\nausruhen, aber das geht nicht."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
