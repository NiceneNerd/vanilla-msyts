      - control:
          kind: set_colour
          colour: blue
      - text: Hateno
      - control:
          kind: reset_colour
      - text: "."
  RetMSG_Futa_02:
    attributes: Npc_Road_004
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 20
      - text: Waaah!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 18
      - text: "Oh... "
      - control:
          kind: pause
          length: long
      - text: Nur ein Reisender...
      - control:
          kind: pause
          length: long
      - text: " Da bin ich\naber erleichtert."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Auf der anderen Seite des "
      - control:
          kind: set_colour
          colour: blue
      - text: Ciella-Sees
      - control:
          kind: reset_colour
      - text: "\nwar so ein komischer Baumgeist, und der\nSchreck sitzt mir noch in den Knochen."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Aber nachdem du kein Geist bist,\nkönnen wir ja vielleicht handeln?"
  RetMSG_Futa_02_1:
    attributes: Npc_Road_004
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 16
      - text: "Täusche ich mich, oder hat sich der\nBaum am anderen Ufer des "
      - control:
          kind: set_colour
          colour: blue
      - text: Ciella-Sees
      - control:
          kind: reset_colour
      - text: "\nbewegt?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Na ja, nicht so wichtig, ich muss  mich\nwieder ums Geschäft kümmern..."
  RetMSG_Futa_03:
    attributes: Npc_Road_004
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Du bist fast in "
      - control:
          kind: set_colour
          colour: blue
      - text: Hateno
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: pause
          length: short
      - text: " Das ist mein\nHeimatdorf!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Frisches Gemüse, nette Leute und ein\ngeheimnisvolles Institut..."
      - control:
          kind: pause
          length: long
      - text: " Für diese\ndrei Dinge ist unser Dorf bekannt!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Oh, und übrigens..."
  RetMSG_Futa_03_1:
    attributes: Npc_Road_004
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Bald bin ich wieder zu Hause, in "
