  RetMSG_Futa_04_1:
    attributes: Npc_Road_004
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Kaum bin ich in "
      - control:
          kind: set_colour
          colour: blue
      - text: Hateno
