          kind: choice
          choice_labels:
            - 2
            - 1
          selected_index: 0
          cancel_index: 2
          unknown: 6
  Appear08:
    attributes: Npc_King_Parasail004
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: Hmm.
      - control:
          kind: pause
          length: long
      - text: " Ich will versuchen, mich an\nalles zu erinnern..."
