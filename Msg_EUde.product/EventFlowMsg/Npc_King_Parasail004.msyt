    attributes: Npc_King_Parasail004
    contents:
      - control:
          kind: sound2
          unknown:
            - 0
            - 205
      - text: Hör mir gut zu...
      - control:
          kind: pause
          length: long
      - text: "\nVon diesen Schreinen gibt es noch\nweitere."
