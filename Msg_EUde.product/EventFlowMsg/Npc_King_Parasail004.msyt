    attributes: Npc_King_Parasail004
    contents:
      - text: "Allein auf diesem Plateau habe ich noch\nmindestens "
      - control:
          kind: set_colour
          colour: red
      - text: drei
      - control:
          kind: reset_colour
      - text: " weitere gesehen.\n\n"
      - control:
          kind: sound
          unknown:
            - 7
            - 1
      - control:
          kind: animation
          name: CustomLaugh
      - text: "Wenn du in jedem davon ein "
      - control:
          kind: set_colour
          colour: blue
      - text: "Zeichen\nder Bewährung "
      - control:
          kind: reset_colour
      - text: "errungen hast,"
      - control:
          kind: pause
          length: long
