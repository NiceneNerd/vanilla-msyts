          kind: text_size
          percent: 100
  Appear10:
    attributes: Npc_King_Parasail004
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Vor langer,"
      - control:
          kind: pause
          length: short
      - text: " langer Zeit lebte an diesem\nOrt eine hoch entwickelte Kultur..."
      - control:
          kind: pause
          length: long
      - text: "\nDie Shiekah!\nDurch ihr Wissen konnte dieses Land ein\nums andere Mal vor dem Untergang\nbewahrt werden."
  Appear11:
    attributes: Npc_King_Parasail004
    contents:
      - text: "Diese Kultur verschwand schon vor\nJahrtausenden von der Bildfläche."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Doch Überreste ihrer Zivilisation, wie\ndieser Schrein, scheinen bis heute\nüberdauert zu haben!"
