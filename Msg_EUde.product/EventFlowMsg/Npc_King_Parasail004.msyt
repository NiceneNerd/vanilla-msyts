          unknown: 6
  Appear02:
    attributes: Npc_King_Parasail004
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 9
      - text: Ganz einfach...
      - control:
          kind: pause
          length: long
      - control:
          kind: text_size
          percent: 125
      - text: " Ich kann hellsehen!"
      - control:
          kind: text_size
          percent: 100
  Appear03:
    attributes: Npc_King_Parasail004
    contents:
      - text: "Mit dem Alter sieht man die Dinge vor\nsich nur noch verschwommen."
      - control:
          kind: pause
          length: long
      - text: " Doch die\nverborgenen erscheinen umso schärfer."
  Appear04:
    attributes: Npc_King_Parasail004
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 1
      - control:
          kind: sound2
          unknown:
            - 8
            - 205
      - text: "Obwohl das sicher nicht bei jedem\ndahergelaufenen Greis der Fall ist."
      - control:
          kind: pause
          length: long
      - text: "\nHo ho ho..."
  Appear05:
    attributes: Npc_King_Parasail004
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 9
      - text: Nun mal ruhig mit den jungen Pferden!
      - control:
          kind: pause
          length: long
      - text: "\nIch bin noch nicht fertig mit dir."
  Appear06:
    attributes: Npc_King_Parasail004
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Es sieht schwer danach aus, als seien die\nTürme und dieser Schrein deinetwegen\naufgetaucht..."
  Appear07:
    attributes: Npc_King_Parasail004
    contents:
      - control:
          kind: sound2
          unknown:
            - 14
            - 205
      - text: "Genauer gesagt wegen dieses\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Shiekah-Steins
      - control:
          kind: reset_colour
      - text: ", den du dort\nam Gürtel trägst!"
