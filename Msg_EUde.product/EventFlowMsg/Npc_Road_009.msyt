  Atacked00:
    attributes: Npc_Road_009
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 42
      - text: "Ach, ich hab's satt... "
      - control:
          kind: pause
          length: short
      - text: Wirklich jetzt...
  Atacked01:
    attributes: Npc_Road_009
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 42
      - text: "Oje... "
      - control:
          kind: pause
          length: short
      - text: "Warum bin ich nur wieder in so\neinem Schlamassel gelandet?"
  Failed00:
    attributes: Npc_Road_009
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 14
      - text: Verfluchte Monster!
      - control:
          kind: pause
          length: short
      - text: " Das vergess ich\neuch nie!"
  Failed01:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 44
      - text: "Das tat weh! "
      - control:
          kind: pause
          length: short
      - text: Ihr verdammten Monster!
  Guardian00:
    attributes: Npc_Road_009
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 42
      - text: "Aaaaaah! "
      - control:
          kind: pause
          length: short
      - text: Er kommt direkt auf uns zu!
      - control:
          kind: pause
          length: short
      - text: "\nLauf!"
  MSG_Firo_00:
    attributes: Npc_Road_009
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 4
      - text: Was willst du?
      - control:
          kind: pause
          length: long
      - text: " Na gut, wenn es nur eine\nkleine Unterhaltung ist..."
      - control:
          kind: pause
          length: short
      - text: " Ich bin grad\nunterwegs nach "
      - control:
          kind: set_colour
          colour: blue
      - text: Angelstedt
      - control:
          kind: reset_colour
      - text: "!\nDort soll es tollen Fisch und fantastische\nFrüchte geben, das wollte ich mir nicht\nentgehen lassen..."
  MSG_Firo_00_1:
    attributes: Npc_Road_009
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 0
      - text: "Ich bin grad unterwegs nach "
      - control:
          kind: set_colour
          colour: blue
      - text: Angelstedt
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: pause
          length: short
      - text: "\nFisch und Früchte allerhöchster Güte!\n"
      - control:
          kind: pause
          length: short
      - text: Da muss ich hin!
  MSG_Firo_01:
    attributes: Npc_Road_009
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 2
      - text: "Oh, he! Du! "
      - control:
          kind: pause
          length: short
      - text: "Kann ich dich fix was\nfragen?"
      - control:
          kind: pause
          length: short
      - text: " Kennst du schon viele Rezepte,\nfür die man Fisch und Obst braucht?\n...Nicht? "
      - control:
          kind: pause
          length: short
      - text: Hm. Gibt es aber!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Du bist wohl einer von denen, die\nkeine Geduld für Experimente mit\nungewöhnlichen Zutaten haben..."
  MSG_Firo_01_1:
    attributes: Npc_Road_009
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: "Es gibt sehr wohl Rezepte für Fisch\nund Obst!"
      - control:
          kind: pause
          length: short
      - text: " Man muss nur ein wenig\nexperimentierfreudig sein..."
  MSG_Firo_02:
    attributes: Npc_Road_009
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "Ein berühmter Ort, an dem jeder seine\ngroße Liebe finden kann...?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Oh!
      - control:
          kind: pause
          length: short
      - text: " Hast du mich etwa belauscht?\nIch habe nur mit mir selbst geredet..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Aber ja, es ging um den "
      - control:
          kind: set_colour
          colour: blue
      - text: Liebesteich
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: pause
          length: short
      - text: "\nIrgendwo im Süden muss er sein..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ich hab ihn schon mal auf einer Karte\ngesehen und weißt du was?"
      - control:
          kind: pause
          length: long
      - text: " Er sieht aus\nwie ein Herz!"
  MSG_Firo_02_1:
    attributes: Npc_Road_009
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 0
      - text: "Ob der "
      - control:
          kind: set_colour
          colour: blue
      - text: "Liebesteich "
      - control:
          kind: reset_colour
      - text: "im Süden wohl auch\nin echt aussieht wie ein Herz? "
      - control:
          kind: pause
          length: short
      - text: "Auf der\nKarte tut er das jedenfalls."
  MSG_Firo_03:
    attributes: Npc_Road_009
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: "Was willst du? "
      - control:
          kind: pause
          length: short
      - text: "Etwa, dass ich dir ein\ntolles Geheimnis verrate?"
      - control:
          kind: pause
          length: short
      - text: " Ich kann dir\nallerhöchstens mit Kochtipps dienen!\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Also, Fleischgerichte sind gut für deine\nGesundheit, aber Fisch im Essen sorgt\nfür allerlei spezielle Effekte. "
      - control:
          kind: pause
          length: long
      - text: Reicht das?
  MSG_Firo_03_1:
    attributes: Npc_Road_009
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Fleischgerichte geben dir vor allem viel\nEnergie, während der Fisch im Essen für\nspezielle Effekte sorgt. "
      - control:
          kind: pause
          length: short
      - text: "Ja, so ist das!"
  MSG_Firo_04:
    attributes: Npc_Road_009
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: "Soll ich dir mal verraten, was passiert,\nwenn du dich in Angelstedt ins Boot\nsetzt und ganz lange nach Osten fährst?\nDann kommst du nach Jotwerde!"
      - control:
          kind: pause
          length: long
      - text: "\nJa, da kriegst du auf einmal ganz große\nAugen, stimmt's?!"
  MSG_Firo_04_1:
    attributes: Npc_Road_009
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Wenn du dich in Angelstedt ins Boot\nsetzt und ganz lange nach Osten fährst,\nkommst du nach Jotwerde! "
      - control:
          kind: pause
          length: short
      - text: "Ja, so ist das!"
  Near00:
    attributes: Npc_Road_009
    contents:
      - text: Heute ist mir nach...
  Near01:
    attributes: Npc_Road_009
    contents:
      - text: Ich fühle mich so...
  Near02:
    attributes: Npc_Road_009
    contents:
      - text: Kopf hoch!
  Near03:
    attributes: Npc_Road_009
    contents:
      - text: Fast geschafft...
  Near04:
    attributes: Npc_Road_009
    contents:
      - text: Nicht schon wieder Regen!
  Rest_00:
    attributes: Npc_Road_009
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Frei und ungebunden auf den Straßen\nvon Phirone!"
      - control:
          kind: pause
          length: short
      - text: " Warmes Wetter, Obst und\nFisch, besser wird's nicht, glaube mir!"
  Rest_01:
    attributes: Npc_Road_009
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "Aaah, hab ich eine schlechte Laune..."
      - control:
          kind: pause
          length: short
      - text: "\nOb die besser wird, wenn's zu regnen\naufhört?"
      - control:
          kind: pause
          length: long
      - text: " Ich glaub's eher nicht..."
  Rest_02:
    attributes: Npc_Road_009
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: "Alle wollen mir erzählen, dass ich\nangeblich ein aufbrausendes\nTemperament hab..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Dabei bin ich die Ruhe in Person!
      - control:
          kind: pause
          length: long
      - text: " Und\nwer was anderes erzählt, kriegt einen\nhinter die Löffel!!!"
  RetMSG_Firo_00:
    attributes: Npc_Road_009
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 26
      - text: Lalalalilala. ♪
      - control:
          kind: pause
          length: long
      - text: "\nAch, der Stall am See..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Da werd ich ein Weilchen bleiben und an\nder Kochstelle herumexperimentieren."
      - control:
          kind: pause
          length: long
      - text: "\nIch will neue Rezepte herausfinden!"
  RetMSG_Firo_00_1:
    attributes: Npc_Road_009
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: "Beim Stall am See kann man so schön\nkochen. "
      - control:
          kind: pause
          length: short
      - text: "Da werd ich eine Weile bleiben\nund mir neue Rezepte ausdenken!"
  RetMSG_Firo_01:
    attributes: Npc_Road_009
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 6
      - text: Was ist los?
      - control:
          kind: pause
          length: short
      - text: " Langweilig?"
      - control:
          kind: pause
          length: short
      - text: " Na, dann geh\ndoch einfach mal vom Stall am See aus\nin Richtung Westen!\nDort gibt's den Stall der Hochebene, und\nauf dem Weg dahin kannst du dich mit\njeder Menge Monstern rumärgern!"
  RetMSG_Firo_01_1:
    attributes: Npc_Road_009
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 0
      - text: "Westlich vom Stall am See liegt der Stall\nder Hochebene. "
      - control:
          kind: pause
          length: short
      - text: "Der Weg dorthin ist\nallerdings voller Monster. "
      - control:
          kind: pause
          length: short
      - text: "Tja, so ist das!"
  RetMSG_Firo_02:
    attributes: Npc_Road_009
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: "Ahaha! "
      - control:
          kind: pause
          length: short
      - text: Jippieh!
      - control:
          kind: pause
          length: short
      - text: " Heute fühl ich mich\nrichtig gut!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Vielleicht schleich ich mich einfach mal\nnach Süden,"
      - control:
          kind: pause
          length: short
      - text: " zum Liebesteich,"
      - control:
          kind: pause
          length: short
      - text: " und such\nmir einen Liebsten!"
  RetMSG_Firo_02_1:
    attributes: Npc_Road_009
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: "Heute schleich ich mich einfach mal\nnach Süden,"
      - control:
          kind: pause
          length: short
      - text: " zum Liebesteich,"
      - control:
          kind: pause
          length: short
      - text: " und such\nmir einen Liebsten! "
      - control:
          kind: pause
          length: short
      - text: Jippieh!
  RetMSG_Firo_03:
    attributes: Npc_Road_009
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 6
      - text: "Was, "
      - control:
          kind: pause
          length: short
      - text: "du sagst, mein Temperament geht\nmit mir durch?!"
      - control:
          kind: pause
          length: short
      - text: " Du denkst, ich hab völlig\nabnormale Stimmungsschwankungen?!\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Aber,"
      - control:
          kind: pause
          length: longer
      - text: " du,"
      - control:
          kind: pause
          length: long
      - text: " du kannst davon eigentlich gar\nnichts wissen..."
      - control:
          kind: pause
          length: long
      - text: " Ähm, jetzt muss ich dir\nwohl 'nen Tipp zur Ablenkung geben.\nAlso, die fettesten Fische findest du in\nAngelstedt. "
      - control:
          kind: pause
          length: short
      - text: "Wenn du die auf den Grill\nhaust, werden die waaahnsinnig lecker!"
  RetMSG_Firo_03_1:
    attributes: Npc_Road_009
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Wenn du die ganz fetten Fische aus\nAngelstedt auf den Grill haust, werden\ndie waaahnsinnig lecker!"
      - control:
          kind: pause
          length: short
      - text: " Ist echt so!"
  RetMSG_Firo_04:
    attributes: Npc_Road_009
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 6
      - text: "Was ist los? "
      - control:
          kind: pause
          length: short
      - text: Was willst du von mir?
      - control:
          kind: pause
          length: short
      - text: "\nZu essen kriegst du nichts, aber 'nen\nTipp hab ich immer für dich parat!\nAlso..."
      - control:
          kind: pause
          length: short
      - text: " Irgendwo hier bei Angelstedt\nsoll es Ruinen geben..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Was, das reicht dir nicht?"
      - control:
          kind: pause
          length: long
      - text: " Tja, war aber\nleider schon alles, was ich dir anbieten\nkann, "
      - control:
          kind: pause
          length: short
      - text: du kleiner Nimmersatt!
  RetMSG_Firo_04_1:
    attributes: Npc_Road_009
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 6
      - text: Was?
      - control:
          kind: pause
          length: short
      - text: " Du willst den Tipp noch mal hören?"
      - control:
          kind: pause
          length: short
      - text: "\nAn der Küste von Angelstedt soll es\nRuinen geben!"
  SavedLv1_00:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 32
      - text: "Es ist doch wieder sicher, oder? "
      - control:
          kind: pause
          length: long
      - text: Oder?
  SavedLv1_01:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 32
      - text: "Alles in Ordnung, ja?"
      - control:
          kind: pause
          length: long
      - text: " Bist du sicher?"
  SavedLv2_00:
    attributes: Npc_Road_009
    contents:
      - control:
          kind: sound
