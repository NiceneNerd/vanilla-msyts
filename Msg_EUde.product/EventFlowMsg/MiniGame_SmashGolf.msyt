---
group_count: 29
atr1_unknown: 4
entries:
  "0001":
    attributes: ""
    contents:
      - text: Da bin ich dabei!
  "0002":
    attributes: ""
    contents:
      - text: Bitte was?
  "0003":
    attributes: ""
    contents:
      - text: Keine Lust.
  "0005":
    attributes: ""
    contents:
      - text: Zahlen
  "0006":
    attributes: ""
    contents:
      - text: Nicht zahlen
  "0013":
    attributes: ""
    contents:
      - text: Ich hör auf.
  "0014":
    attributes: ""
    contents:
      - text: Ich will doch spielen!
  "0020":
    attributes: ""
    contents:
      - text: Spielen
  "0021":
    attributes: ""
    contents:
      - text: Nicht spielen
  AskGolf1:
    attributes: Npc_MiniGame_Golf
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 6
      - text: "Du weißt nicht, was Rumpelgolf ist?"
      - control:
          kind: pause
          length: longer
      - text: "\nEin Ründchen und du willst nie mehr\ndamit aufhören!"
  AskGolf10:
    attributes: Npc_MiniGame_Golf
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 11
      - text: "So! "
      - control:
          kind: pause
          length: short
      - text: "Auf geht's! "
      - control:
          kind: pause
          length: long
      - control:
          kind: text_size
          percent: 125
      - text: Rumpelgolf!
      - control:
          kind: text_size
          percent: 100
  AskGolf2:
    attributes: Npc_MiniGame_Golf
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Die Idee kam mir genau hier, in diesem\nTal!"
      - control:
          kind: pause
          length: short
      - text: " Von daher eigentlich klar, dass du's\nnicht kennst..."
  AskGolf3:
    attributes: Npc_MiniGame_Golf
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Es geht ganz einfach! Schlag den "
      - control:
          kind: set_colour
          colour: red
      - text: Ball
      - control:
          kind: reset_colour
      - text: " so,\ndass er in das Loch bei der Flagge fällt."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Je weniger Schläge du dafür brauchst,\ndesto höher deine Punktzahl."
      - control:
          kind: pause
          length: long
      - text: " Ein Spiel\n"
      - control:
          kind: set_colour
          colour: red
      - text: 20 Rubine
      - control:
          kind: reset_colour
      - text: "!"
  AskGolf4:
    attributes: Npc_MiniGame_Golf
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Bei Zweihand-Waffen wie Hämmern fliegt\nder Ball ein ordentliches Stück."
      - control:
          kind: pause
          length: short
      - text: " Da musst\ndu aufpassen, dass es nicht zu weit wird!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Einhand-Waffen wie Schwerter geben dir\nmehr Kontrolle über die Entfernung, aber\ndafür musst du vielleicht öfter schlagen."
  AskGolf5:
    attributes: Npc_MiniGame_Golf
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 11
      - text: "Hau ruhig ordentlich auf den "
      - control:
          kind: set_colour
          colour: red
      - text: Ball
      - control:
          kind: reset_colour
      - text: " drauf!"
      - control:
          kind: pause
          length: long
      - text: "\nJe mehr du draufhaust, desto weiter\nfliegt er!"
  AskGolf6:
    attributes: Npc_MiniGame_Golf
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Wenn der "
      - control:
          kind: set_colour
          colour: red
      - text: Ball
      - control:
          kind: reset_colour
      - text: " die Bahn verlässt, gibt's\n"
      - control:
          kind: set_colour
          colour: red
      - text: 1 Strafschlag
      - control:
          kind: reset_colour
      - text: " zur Schlagzahl hinzu und\ndu kehrst zurück zum letzten Abschlag."
  AskGolf7:
    attributes: Npc_MiniGame_Golf
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ich behalte deine "
      - control:
          kind: set_colour
          colour: red
      - text: Schlagzahl
      - control:
          kind: reset_colour
      - text: " im Auge..."
      - control:
          kind: pause
          length: long
      - text: "\nVersuche, den "
      - control:
          kind: set_colour
          colour: red
      - text: Ball
      - control:
          kind: reset_colour
      - text: " mit "
      - control:
          kind: set_colour
          colour: red
      - text: "20 oder weniger\nSchlägen"
      - control:
          kind: reset_colour
      - text: " ins Loch zu kriegen!"
  AskGolf8:
    attributes: Npc_MiniGame_Golf
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Wie du den "
      - control:
          kind: set_colour
          colour: red
      - text: Ball
      - control:
          kind: reset_colour
      - text: " schlägst, ist egal."
      - control:
          kind: pause
          length: long
      - text: "\nAm besten ist es, wenn er mit einem\nSchlag so weit wie möglich fliegt!"
  AskGolf9:
    attributes: Npc_MiniGame_Golf
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Auch wenn der "
      - control:
          kind: set_colour
          colour: red
      - text: Ball
      - control:
          kind: reset_colour
      - text: " nur 'nen kleinen\nHüpfer macht, zählt das als "
      - control:
          kind: set_colour
          colour: red
      - text: 1 Schlag
      - control:
          kind: reset_colour
      - text: "!"
      - control:
          kind: pause
          length: long
      - text: "\nPuh, war das 'ne lange Erklärung!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Na, wie sieht's aus?"
      - control:
          kind: pause
          length: short
      - text: " Lust auf 'ne Runde?"
      - control:
          kind: choice
          choice_labels:
            - 20
            - 21
          selected_index: 0
          cancel_index: 1
          unknown: 6
  AskOneMore:
    attributes: Npc_MiniGame_Golf
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 11
      - text: "Ha! "
      - control:
          kind: pause
          length: long
      - text: "Mein Kennerblick verrät mir,"
      - control:
          kind: pause
          length: long
      - text: " dass du\nnoch ein Ründchen den Ball dreschen\nmöchtest! "
      - control:
          kind: pause
          length: long
      - text: "Na, hab ich recht, oder was?"
      - control:
          kind: choice
          choice_labels:
            - 20
            - 21
          selected_index: 0
          cancel_index: 1
          unknown: 6
  AskPay:
    attributes: Npc_MiniGame_Golf
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Krieg ich noch schnell die "
      - control:
          kind: set_colour
          colour: red
      - text: 20 Rubine
      - control:
          kind: reset_colour
      - text: "\nTeilnahmegebühr?"
      - control:
          kind: choice
          choice_labels:
            - 5
            - 6
          selected_index: 0
          cancel_index: 1
          unknown: 6
  AskPlay:
    attributes: Npc_MiniGame_Golf
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Na-ha?
      - control:
          kind: pause
          length: long
      - text: " Hast du den Mumm für 'ne\ngepflegte Runde Rumpelgolf?"
      - control:
          kind: choice
          choice_labels:
            - 1
            - 2
            - 3
          selected_index: 0
          cancel_index: 2
          unknown: 8
  Cancel_End:
    attributes: Npc_MiniGame_Golf
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Schau bald mal wieder auf 'ne Runde\nRumpelgolf vorbei!"
  Clear_00:
    attributes: Npc_MiniGame_Golf
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: Saubere Arbeit!
  Clear_1:
    attributes: Npc_MiniGame_Golf
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Lass mich schnell deine "
      - control:
          kind: set_colour
          colour: red
      - text: Schlagzahl
      - control:
          kind: reset_colour
      - text: "\nausknobeln!"
  Clear_Excellent_1:
    attributes: Npc_MiniGame_Golf
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 20
      - control:
          kind: set_colour
          colour: red
      - control:
          kind: variable
          variable_kind: 17
          name: MiniGame_SmashGolf_ShotCount
      - control:
          kind: reset_colour
      - text: "!?"
      - control:
          kind: pause
          length: long
      - text: " Du hast das Zeug zum\nProfi-Rumpelgolfer!"
  Clear_Excellent_2:
    attributes: Npc_MiniGame_Golf
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Öh... "
      - control:
          kind: pause
          length: long
      - text: "Ich hab meine "
      - control:
          kind: set_colour
          colour: red
      - text: Schlagzahl
      - control:
          kind: reset_colour
      - text: "..."
      - control:
          kind: pause
          length: long
      - text: " nicht\nmehr im Kopf..."
      - control:
          kind: pause
          length: long
      - text: " Aber sie müsste ziemlich\ngenau gleich sein, jawoll!"
  Clear_Excellent_3:
    attributes: Npc_MiniGame_Golf
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 0
      - text: "Du hast ganz neue Möglichkeiten für\nRumpelgolf aufgezeigt!"
      - control:
          kind: pause
          length: long
      - text: " Immer wacker\nweiter so, versuch immer neue Rekorde!"
  Clear_High_1:
    attributes: Npc_MiniGame_Golf
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 11
      - text: "Uuund deine "
      - control:
          kind: set_colour
          colour: red
      - text: Schlagzahl
      - control:
          kind: reset_colour
      - text: " ist..."
      - control:
          kind: text_size
          percent: 80
      - control:
          kind: pause
          length: long
      - control:
          kind: text_size
          percent: 100
      - text: " "
      - control:
          kind: set_colour
          colour: red
      - control:
          kind: variable
          variable_kind: 17
          name: MiniGame_SmashGolf_ShotCount
      - control:
          kind: reset_colour
      - text: "!\nDu..."
      - control:
          kind: pause
          length: long
      - text: " bist gar nicht schlecht."
  Clear_High_2:
    attributes: Npc_MiniGame_Golf
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Aber mach weiter!
      - control:
          kind: pause
          length: long
      - text: " Ein heute noch in\nStein gemeißelter Rekord kann morgen\ngebrochen sein!"
      - control:
          kind: pause
          length: short
      - text: " Das ist Rumpelgolf!"
  Clear_Low_1:
    attributes: Npc_MiniGame_Golf
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 12
      - text: "Uuund deine "
      - control:
          kind: set_colour
          colour: red
      - text: Schlagzahl
      - control:
          kind: reset_colour
      - text: " ist..."
      - control:
          kind: pause
          length: long
      - text: " "
      - control:
          kind: set_colour
          colour: red
      - control:
          kind: variable
          variable_kind: 17
          name: MiniGame_SmashGolf_ShotCount
      - control:
          kind: reset_colour
      - text: "!"
      - control:
          kind: pause
          length: long
      - text: "\nRumpelgolf ist eben nicht so\neinfach, wie es aussieht!"
  Clear_Low_2:
    attributes: Npc_MiniGame_Golf
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: Ich erwarte große Dinge von dir!
      - control:
          kind: pause
          length: long
      - text: " Aber für\n'nen anständigen Rekord musst du "
      - control:
          kind: set_colour
          colour: red
      - text: "üben,\nüben, üben, üben und außerdem üben"
      - control:
          kind: reset_colour
      - text: "!"
  Clear_Middle_1:
    attributes: Npc_MiniGame_Golf
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: Uuund deine Schlagzahl ist...
      - control:
          kind: text_size
          percent: 80
      - control:
          kind: pause
          length: long
      - control:
          kind: text_size
          percent: 100
      - text: " "
      - control:
          kind: set_colour
          colour: red
      - control:
          kind: variable
          variable_kind: 17
          name: MiniGame_SmashGolf_ShotCount
      - control:
          kind: reset_colour
      - text: "!"
      - control:
          kind: pause
          length: long
      - text: "\nIrgendwie habe ich den Eindruck, dass\nda noch mehr geht!"
  Clear_Middle_2:
    attributes: Npc_MiniGame_Golf
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Wenn du erst mal den Dreh raus hast,\nwirst du sicher besser!"
      - control:
          kind: pause
          length: long
      - text: " "
      - control:
          kind: set_colour
          colour: red
      - text: "Jede Reise\nbeginnt mit einem ersten Schlag"
      - control:
          kind: reset_colour
      - text: " oder so!"
  GameTalk:
    attributes: Npc_MiniGame_Golf
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 19
      - text: Hö?
      - control:
          kind: pause
          length: long
      - text: " Was'n los? "
      - control:
          kind: pause
          length: long
      - text: Keine Lust mehr?
      - control:
          kind: choice
          choice_labels:
            - 13
            - 14
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Judge_OB:
    attributes: Npc_MiniGame_Golf
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 12
      - text: Huch!
      - control:
          kind: pause
          length: long
      - text: " Der "
      - control:
          kind: set_colour
          colour: red
      - text: Ball
      - control:
          kind: reset_colour
      - text: " ist außerhalb der Bahn!"
      - control:
          kind: pause
          length: long
      - text: "\nDas gibt "
      - control:
          kind: set_colour
          colour: red
      - text: 1 Strafschlag
      - control:
          kind: reset_colour
      - text: " und du musst\nzurück zum letzten Abschlag!"
  Near00:
    attributes: Npc_MiniGame_Golf
    contents:
      - text: Rumpeldipumpel...
  NoOneMore1:
    attributes: Npc_MiniGame_Golf
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 12
      - text: Schade...
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Aber jetzt weißt du wenigstens, wie viel\nLaune so 'ne Partie Rumpelgolf macht!"
  NoOneMore2:
    attributes: Npc_MiniGame_Golf
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Ich bastele hier weiter an meiner Bahn...
      - control:
          kind: pause
          length: long
      - text: "\nKannst jederzeit auf ein paar Schläge\nvorbeikommen!"
  NoRupee:
    attributes: Npc_MiniGame_Golf
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: Deine Rubine reichen nicht ganz.
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Komm wieder, wenn du mehr Geld in\nder Tasche hast."
      - control:
          kind: pause
          length: long
      - text: " Ich bin hier!"
  NotPlay:
    attributes: Npc_MiniGame_Golf
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 12
      - text: "Tja, "
      - control:
          kind: pause
          length: long
      - text: kann man nix machen.
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 6
            - 1
      - text: "Ich bin hier, wenn du doch mal Lust auf\n'ne Runde hast."
  OB_Cancell1:
    attributes: Npc_MiniGame_Golf
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 3
      - text: "'tschuldige, "
      - control:
          kind: pause
          length: short
      - text: "aber du bist "
      - control:
          kind: set_colour
          colour: red
      - text: disqualifiziert
      - control:
          kind: reset_colour
      - text: "!"
  OB_Cancell2:
    attributes: Npc_MiniGame_Golf
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 0
      - text: "Du hättest auch einfach sagen können,\ndass du aufhören möchtest... "
      - control:
          kind: pause
          length: long
      - text: "Wenn du\nnochmal spielen willst, sag Bescheid."
  OB_Warning1:
    attributes: Npc_MiniGame_Golf
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 3
      - text: "Wo willst du hin? "
      - control:
          kind: pause
          length: long
      - text: Gibst du auf?
  OB_Warning2:
    attributes: Npc_MiniGame_Golf
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Wenn du zu weit weg von der Bahn\ngehst, muss ich dich "
      - control:
          kind: set_colour
          colour: red
      - text: disqualifizieren
      - control:
          kind: reset_colour
      - text: ".\nUnd die Teilnahmegebühr behalte ich!"
  PlayOK:
    attributes: Npc_MiniGame_Golf
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: So gefällt mir das!
      - control:
          kind: pause
          length: long
      - text: " Ich mach die Bahn\nstartklar!"
  RetrunGame:
    attributes: Npc_MiniGame_Golf
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 11
      - text: "Juhu! "
      - control:
          kind: pause
          length: long
      - control:
          kind: text_size
          percent: 125
      - text: Rumpelgolf!
      - control:
          kind: text_size
          percent: 100
  StrokeOver:
    attributes: Npc_MiniGame_Golf
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 12
      - text: Oha...
      - control:
          kind: pause
          length: short
      - text: " Das waren jetzt 20 Schläge,\ndamit bist du raus..."
      - control:
          kind: pause
          length: short
      - text: " Komm zurück!"
  StrokeOver_Result:
    attributes: Npc_MiniGame_Golf
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "Schade, aber du hast es leider nicht\nmit 20 Schlägen geschafft..."
  TalkFirst:
    attributes: Npc_MiniGame_Golf
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 2
      - text: "Oh! "
      - control:
          kind: pause
          length: long
      - text: "Ich hab hier ewig niemanden mehr\ngesehen!"
      - control:
          kind: pause
          length: long
      - text: " Wer sich hier rumtreibt, muss\nhart sein wie 'ne Fuhre Eisen!"
