  DekuTree_A_01_Talk010:
    attributes: TwnObj_Village_Korok_DekuTree_A_01
    contents:
      - control:
          kind: sound2
          unknown:
            - 10
            - 205
      - text: "Egal, wie viel Kraft du vorgibst zu\nbesitzen, dieses Schwert durchschaut\ndich.\nWer dieses Schwert ziehen will, muss\nauf der Höhe seiner Kräfte sein."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Wollen wir sehen, ob du bereit bist."
  DekuTree_A_01_Talk020:
    attributes: TwnObj_Village_Korok_DekuTree_A_01
    contents:
      - control:
          kind: sound2
          unknown:
            - 2
            - 205
      - text: "Im Kampf gegen die Verheerung Ganon\nund ihre Manifestationen erstrahlt das\n"
      - control:
          kind: set_colour
          colour: blue
      - control:
          kind: raw
          two_hundred_one:
            dynamic:
              - 0
              - len: 4
                field_2:
                  - 3
                  - 2
                  - 2
                  - 0
      - text: "Master-Schwert "
      - control:
          kind: reset_colour
      - text: "in heiligem Licht.\nZu diesem Zeitpunkt zeigt sich seine\nwahre Macht..."
      - control:
          kind: pause
          length: long
      - text: " Doch verlasse dich nicht\nzu sehr auf dieses Schwert.\nSchwingst du es ohne Grund, wird es\nSchaden nehmen und mit der Zeit\nunbrauchbar werden.\n...Sollte dies geschehen, warte, bis das\nSchwert seine Kräfte erneuert hat."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Willst du dir deines Sieges gewiss sein,\nsolltest du Ganon nicht angreifen, bevor\ndu die Titanen auf deiner Seite weißt...\n"
      - control:
          kind: sound2
          unknown:
            - 14
            - 205
      - text: "Die Hohepriesterin... "
      - control:
          kind: pause
          length: long
      - text: "Was würde ich\nnicht geben, ihr Lächeln noch einmal\nzu sehen... "
