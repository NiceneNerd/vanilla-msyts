  talk18:
    attributes: Npc_Zora001
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 20
      - text: "Du hast uns einen großen Dienst\nerwiesen, Hylianer!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 1
      - text: Hauptmann Basu hat mir alles erzählt.
      - control:
          kind: pause
          length: short
      - text: "\nEr sagte, dass du der Recke bist, der\nvor 100 Jahren hier war oder so..."
  talk19:
    attributes: Npc_Zora001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 20
