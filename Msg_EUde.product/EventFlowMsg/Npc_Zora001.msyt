  talk21:
    attributes: Npc_Zora001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Nach solchen Taten bist du selbst für\ndie Alten von der Sache mit Mipha\nreingewaschen."
  talk22:
    attributes: Npc_Zora001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Wir suchen hier im "
      - control:
          kind: set_colour
          colour: blue
      - text: Dorf der Zoras
      - control:
          kind: reset_colour
      - text: " einen\nKrieger, der Prinz Sidon unterstützen\nkönnte."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "Die Alten sind in ihrer Starrköpfigkeit\ndagegen, sich von außerhalb des Dorfes\nhelfen zu lassen."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Aber den Jungen und auch König\nDorephan ist es gleich, wo die Hilfe\nherkommt."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Sprich am besten mit dem König darüber.
      - control:
          kind: pause
          length: short
      - text: "\nGeh einfach die Treppe hoch."
