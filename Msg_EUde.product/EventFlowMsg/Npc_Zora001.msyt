    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 20
      - text: Hylianer!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Der seit dem Erscheinen von "
      - control:
          kind: set_colour
          colour: blue
      - text: Vah Ruta
      - control:
          kind: reset_colour
      - text: "\nununterbrochen fallende Regen hat\nendlich aufgehört!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
