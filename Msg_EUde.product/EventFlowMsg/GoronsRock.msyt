---
group_count: 29
atr1_unknown: 4
entries:
  "0000":
    attributes: ""
    contents:
      - text: Bruder?
  "0001":
    attributes: ""
    contents:
      - text: Ist er ausgebüchst?
  "0002":
    attributes: ""
    contents:
      - text: Wo wollte er hin?
  "0003":
    attributes: ""
    contents:
      - text: Geheimnis?
  "0004":
    attributes: ""
    contents:
      - text: Ich verstehe ihn.
  "0005":
    attributes: ""
    contents:
      - text: Marugo-Tunnel?
  Goron_Father_Near01:
    attributes: Npc_Goron010
    contents:
      - text: Ab in den Topf!
  Goron_Father_Near02:
    attributes: Npc_Goron010
    contents:
      - text: Wo ist er nur?!
  Goron_Father_Talk01:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 0
      - text: "Mein kleiner Bruder, dieser Kreidekopf,\nkommt einfach nicht nach Hause!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 9
            - 1
      - text: "Ich frage mich, was er sich dabei denkt..."
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
            - 4
          selected_index: 0
          cancel_index: 3
          unknown: 8
  Goron_Father_Talk02:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound2
          unknown:
            - 9
            - 205
      - text: "Bruder!\nJetzt verstehe ich dich endlich!"
  Goron_Father_Talk03:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Du hast nur getan, was du tun\nmusstest..."
  Goron_Father_Talk04:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Ich habe eine Bitte an dich, als sein\nBruder! "
      - control:
          kind: pause
          length: short
      - text: "Hilf ihm, bitte!"
  Goron_Father_Talk05:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ich bereite hier schon mal alles zum\nKochen vor und warte auf dich!"
  Goron_Father_Talk06:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: "!!!"
      - control:
          kind: pause
          length: longer
      - text: "\nDas ist eines der schönsten Dinge, die\nich jemals gesehen habe!\nWas für ein erlesenes "
      - control:
          kind: set_colour
          colour: red
      - text: Felsenfilet
      - control:
          kind: reset_colour
      - text: "!\nIch muss sofort anfangen zu kochen!"
  Goron_Father_Talk06_2:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: raw
          four:
            three:
              field_1: 0
      - text: "Es ist vollbracht!\nDas "
      - control:
          kind: set_colour
          colour: red
      - text: Geröstete Felsenfilet
      - control:
          kind: reset_colour
      - text: " ist fertig!"
  Goron_Father_Talk07:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound2
          unknown:
            - 1
            - 205
      - text: "Bruder! Essen ist fertig!\nGuten Appetit!"
  Goron_Father_Talk08:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound2
          unknown:
            - 2
            - 205
      - text: "Das ist also das "
      - control:
          kind: set_colour
          colour: red
      - text: Geheimnis des Helden
      - control:
          kind: reset_colour
      - text: "?!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound2
          unknown:
            - 4
            - 205
      - control:
          kind: text_size
          percent: 80
      - text: "...Hm? Das kommt mir irgendwie bekannt vor."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound2
          unknown:
            - 9
            - 205
      - control:
          kind: text_size
          percent: 100
      - text: "Aber wie dem auch sei...\nEine Heldentat!\nMein Bruder! Du bist der Größte!"
  Goron_Father_Talk09:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: "Danke, dass du meinem Bruder geholfen\nhast!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ich bin so stolz auf ihn... Wir werden im\nTriumph nach Goronia zurückkehren!"
  Goron_Father_Talk10:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 0
      - text: "Wann kommt mein Bruder denn endlich\nnach Hause?!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 1
      - text: "Er wird doch nicht immer noch im\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Marugo-Tunnel
      - control:
          kind: reset_colour
      - text: " nach dem "
      - control:
          kind: set_colour
          colour: red
      - text: "Geheimnis\ndes Helden"
      - control:
          kind: reset_colour
      - text: " graben?!?"
      - control:
          kind: choice
          choice_labels:
            - 5
            - 3
            - 4
          selected_index: 0
          cancel_index: 3
          unknown: 8
  Goron_Father_Talk11:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Der "
      - control:
          kind: set_colour
          colour: blue
      - text: Marugo-Tunnel
      - control:
          kind: reset_colour
      - text: " ist eine Mine\nsüdwestlich des "
      - control:
          kind: set_colour
          colour: blue
      - text: Marugo-Teichs
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Die Arbeiten dort wurden wegen einer\nharten Felsschicht schon vor Ewigkeiten\naufgegeben.\n"
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Manche glauben, hinter dieser\nFelsschicht sei das "
      - control:
          kind: set_colour
          colour: red
      - text: "Geheimnis des\nHelden"
      - control:
          kind: reset_colour
      - text: " versteckt..."
  Goron_Father_Talk12:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Geh einfach hier den "
      - control:
          kind: set_colour
          colour: red
      - text: Hügel runter
      - control:
          kind: reset_colour
      - text: ". Dort\nsind die Specksteinklippen, wo es das\nbeste "
      - control:
          kind: set_colour
          colour: red
      - text: Felsenfilet
      - control:
          kind: reset_colour
      - text: " gibt."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Bitte bring mir ein saftiges Stück!
  Goron_Father_Talk13:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 16
      - text: "Ich würde ja selbst gehen, aber..."
      - control:
          kind: pause
          length: short
      - text: "\nMeine Schaufelschulter bringt mich um\nheute. "
      - control:
          kind: pause
          length: short
      - text: "Goronen-Volkskrankheit Nr. 1...\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ich bitte dich! "
      - control:
          kind: pause
          length: short
      - text: "Das machst du, oder?"
  Goron_Father_Talk14:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 0
      - text: "Mein geliebtes Bruderherz Gonguron\nnatürlich!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 2
            - 0
      - text: "Er hat irgendwas davon gefaselt, das\n"
      - control:
          kind: set_colour
          colour: red
      - text: Geheimnis des Helden
      - control:
          kind: reset_colour
      - text: " auszugraben,\nund schon war er weg!\n"
      - control:
          kind: sound
          unknown:
            - 9
            - 1
      - text: "Ach, Gonguron... "
      - control:
          kind: pause
          length: short
      - text: "Was denkst du dir nur\nbei solchen Sachen?"
      - control:
          kind: choice
          choice_labels:
            - 2
            - 3
            - 4
          selected_index: 0
          cancel_index: 3
          unknown: 8
  Goron_Father_Talk15:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 0
      - text: "Wenn er hätte ausbüchsen wollen, hätte\ner nicht vorher groß getönt, er würde das\n"
      - control:
          kind: set_colour
          colour: red
      - text: Geheimnis des Helden
      - control:
          kind: reset_colour
      - text: " ausgraben!\n"
      - control:
          kind: sound
          unknown:
            - 9
            - 1
      - text: "Ach, Gonguron... "
      - control:
          kind: pause
          length: short
      - text: "Was denkst du dir nur\nbei solchen Sachen?"
      - control:
          kind: choice
          choice_labels:
            - 2
            - 3
            - 4
          selected_index: 0
          cancel_index: 3
          unknown: 8
  Goron_Father_Talk16:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Er müsste zum "
      - control:
          kind: set_colour
          colour: blue
      - text: Marugo-Tunnel
      - control:
          kind: reset_colour
      - text: "\nsüdwestlich des "
      - control:
          kind: set_colour
          colour: blue
      - text: Marugo-Teichs
      - control:
          kind: reset_colour
      - text: "\ngegangen sein."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Die Mine dort wurde wegen einer\nharten Felsschicht schon vor Ewigkeiten\naufgegeben."
  Goron_Father_Talk17:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Wenn du ein "
      - control:
          kind: set_colour
          colour: red
      - text: Felsenfilet
      - control:
          kind: reset_colour
      - text: " herbringst,\nwirf es einfach in den Topf!"
  Goron_Father_Talk18:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 0
      - text: "Aber das ist sicher nur eine Ausrede,\num abzuhauen und dort faulenzen zu\nkönnen!"
      - control:
          kind: pause
          length: short
      - text: " Dieser kleine Gipsbube...!"
  Goron_Father_Talk20:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Südwestlich des "
      - control:
          kind: set_colour
          colour: blue
      - text: Marugo-Teichs
      - control:
          kind: reset_colour
      - text: ", im\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Marugo-Tunnel
      - control:
          kind: reset_colour
      - text: " ist eine aufgegebene\nMine.\n"
      - control:
          kind: sound
          unknown:
            - 6
            - 1
      - text: "Hinter einer undurchdringlichen\nFelsschicht dort ist kein Erz, sondern\ndas "
      - control:
          kind: set_colour
          colour: red
      - text: Geheimnis des Helden
      - control:
          kind: reset_colour
      - text: " versteckt!\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "...Zumindest wenn man dem Kollegen\ndes Cousins meines Vaters glauben darf,\nder das herumerzählt hat."
  Goron_Father_Talk21:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: "Endlich, der Moment ist gekommen...\nWirf es direkt in den Topf!"
  Goron_Father_Talk22:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Das "
      - control:
          kind: set_colour
          colour: red
      - text: Geheimnis des Helden
      - control:
          kind: reset_colour
      - text: " ist eine\nuralte Mär..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ein Kollege des Cousins meines Vaters\nhat uns davon erzählt."
  Goron_Father_Talk23:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 0
      - text: "Du... "
      - control:
          kind: pause
          length: short
      - text: Du verstehst ihn?!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: text_size
          percent: 80
      - control:
          kind: sound
          unknown:
            - 9
            - 1
      - text: Ich verstehe ihn ganz und gar nicht.
      - control:
          kind: pause
          length: short
      - text: "\nZiemlich traurig, so als großer Bruder..."
      - control:
          kind: text_size
          percent: 100
  Goron_Father_Talk24:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "Was sollen solche Träumereien? Er sollte\nbesser in der Realität bleiben und einer\nehrlichen Arbeit nachgehen..."
  Goron_Son_Near01:
    attributes: Npc_Goron009
    contents:
      - text: Geheimnis des Helden!
  Goron_Son_Near02:
    attributes: Npc_Goron009
    contents:
      - text: Hunger!
  Goron_Son_Talk01:
    attributes: Npc_Goron009
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 10
      - text: "Ich muss graben, graben, graben als\ngäb es kein Morgen! Ich werde das\n"
      - control:
          kind: set_colour
          colour: red
      - text: Geheimnis
      - control:
          kind: reset_colour
      - text: " lüften!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Hm? Was ich ausgrabe?\nNa, das "
      - control:
          kind: set_colour
          colour: red
      - text: Geheimnis
      - control:
          kind: reset_colour
      - text: "!"
  Goron_Son_Talk02:
    attributes: Npc_Goron009
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 43
      - text: "Hah... "
      - control:
          kind: pause
          length: short
      - text: Hah...
      - control:
          kind: pause
          length: long
      - text: " Ich kann nicht mehr..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Hinter diesen Felsen ist irgendwo das\n"
      - control:
          kind: set_colour
          colour: red
      - text: Geheimnis des Helden
      - control:
          kind: reset_colour
      - text: "..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ich hab mir vorgenommen, es zu finden,\num zu beweisen, dass ich ein starker\nGorone bin!\n"
      - control:
          kind: sound2
          unknown:
            - 40
            - 205
      - text: "Aber jetzt bin ich so hungrig, dass ich\nnicht weiterarbeiten kann..."
  Goron_Son_Talk02b:
    attributes: Npc_Goron009
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 43
      - text: "Ich krieg meinen Arm keinen Zentimeter\nmehr hoch..."
  Goron_Son_Talk04:
    attributes: Npc_Goron009
    contents:
      - text: Es ist...
      - control:
          kind: pause
          length: short
      - text: " unbeschreiblich..."
      - control:
          kind: pause
          length: short
      - text: "\nGöttlich!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Dieses raue Knirschen zwischen den\nZähnen, das sich durch den ganzen\nKörper fortsetzt..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Der beißende Geschmack der Mineralien\nversetzt den Gaumen in Ekstase... "
      - control:
          kind: pause
          length: short
      - text: "Ein\nerbarmungsloser Angriff auf alle Sinne!\nIch fühle mich groß und stark wie der\nTodesberg!"
  Goron_Son_Talk04_1:
    attributes: Npc_Goron009
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 6
      - text: "Hm? Ist das etwa der unwiderstehliche\nDuft von "
      - control:
          kind: set_colour
          colour: red
      - text: Geröstetem Felsenfilet
      - control:
          kind: reset_colour
      - text: "...?"
  Goron_Son_Talk04_2:
    attributes: Npc_Goron009
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 11
      - text: "Bei allen Gletschern! "
      - control:
          kind: pause
          length: short
      - text: "Das sieht ja zum\nSterben gut aus! "
      - control:
          kind: pause
          length: short
      - text: Ich hau sofort mal rein!
  Goron_Son_Talk04_3:
    attributes: Npc_Goron009
    contents:
      - control:
          kind: sound2
          unknown:
            - 9
            - 205
      - text: Niemand kocht so gut wie mein Bruder!
  Goron_Son_Talk04_4:
    attributes: Npc_Goron009
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 18
      - text: "Ich bin pappsatt!\nGut! Jetzt wird gearbeitet!"
  Goron_Son_Talk05:
    attributes: Npc_Goron009
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 16
      - control:
          kind: text_size
          percent: 125
      - text: Huoaaaaah!
      - control:
          kind: text_size
          percent: 100
  Goron_Son_Talk05_2:
    attributes: Npc_Goron009
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 10
      - control:
          kind: text_size
          percent: 125
      - text: GORONENANGRIFF!!!
      - control:
          kind: text_size
          percent: 100
  Goron_Son_Talk06:
    attributes: Npc_Goron009
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 1
      - control:
          kind: animation
          name: Happy_00
      - text: "Da ist es! Ich werd nicht mehr!\nDas "
      - control:
          kind: set_colour
          colour: red
      - text: Geheimnis des Helden
      - control:
          kind: reset_colour
      - text: "!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound2
          unknown:
            - 4
            - 205
      - control:
          kind: text_size
          percent: 80
      - text: "...Hm? Steht nicht in Goronia\ngenauso ein Ding?"
      - control:
          kind: text_size
          percent: 100
  Goron_Son_Talk07:
    attributes: Npc_Goron009
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 0
      - text: "Vielen Dank!\nIch gehe gleich nach Goronia und\nerzähle allen davon!"
