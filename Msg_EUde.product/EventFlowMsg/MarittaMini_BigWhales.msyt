  Near_00:
    attributes: Npc_HyruleWestHatago004
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Das war sicher so...
  Near_01:
    attributes: Npc_HyruleWestHatago004
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: Was ist mit ihnen passiert?
  Near_02:
    attributes: Npc_HyruleWestHatago004
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: Wenn wir Bilder hätten...
  Near_03:
    attributes: Npc_HyruleWestHatago003
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: Koloss...
  Near_04:
    attributes: Npc_HyruleWestHatago002
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: Wale...
  talk-Gamyo01:
    attributes: Npc_HyruleWestHatago002
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ich denke, dass die "
      - control:
          kind: set_colour
          colour: red
      - text: Kolosswale
      - control:
          kind: reset_colour
      - text: " wegen\nWassermangel aufgrund einer Dürre\nausgestorben sind!"
  talk-Gamyo04:
    attributes: Npc_HyruleWestHatago002
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 16
      - text: "Aber hier nur rumzusitzen bringt uns der\nWahrheit über die "
      - control:
          kind: set_colour
          colour: red
      - text: Kolosswale
      - control:
          kind: reset_colour
      - text: " auch nicht\nnäher..."
  talk-Gamyo05:
    attributes: Npc_HyruleWestHatago002
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 0
      - text: "Ich würde zu gern "
      - control:
          kind: set_colour
          colour: red
      - text: Kolosswal-Knochen
      - control:
          kind: reset_colour
      - text: "\naus "
      - control:
          kind: set_colour
          colour: blue
      - text: Gerudo
      - control:
          kind: reset_colour
      - text: " sehen..."
      - control:
          kind: choice
          choice_labels:
            - 2
            - 3
          selected_index: 0
          cancel_index: 1
          unknown: 6
  talk-Gamyo07:
    attributes: Npc_HyruleWestHatago002
    contents:
      - control:
          kind: sound
          unknown:
            - 2
            - 14
      - text: "Du hast gar kein "
      - control:
          kind: set_colour
          colour: red
      - text: Bild
      - control:
          kind: reset_colour
      - text: "!"
      - control:
          kind: pause
          length: short
      - text: " Komm wieder,\nwenn du eins hast!"
  talk-Gamyo08:
    attributes: Npc_HyruleWestHatago002
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 11
      - text: "Sieh an! "
      - control:
          kind: pause
          length: short
      - text: "Du hast "
      - control:
          kind: set_colour
          colour: red
      - text: Bilder
      - control:
          kind: reset_colour
      - text: " von allen\n3 Fundorten von "
      - control:
          kind: set_colour
          colour: red
      - text: Kolosswal-Knochen
      - control:
          kind: reset_colour
      - text: "!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Vielen Dank! "
      - control:
          kind: pause
          length: short
      - text: "Und hier ist die Belohnung,\nwie abgemacht!"
  talk-Gamyo09:
    attributes: Npc_HyruleWestHatago002
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Bitte hilf auch meinen Brüdern!
  talk-Gamyo10:
    attributes: Npc_HyruleWestHatago002
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 1
      - text: "Denk dran, hier warten "
      - control:
          kind: set_colour
          colour: red
      - text: 300 Rubine
      - control:
          kind: reset_colour
      - text: "\nauf dich!"
      - control:
          kind: pause
          length: short
      - text: " Du musst nur noch liefern!"
  talk-Gamyo11:
    attributes: Npc_HyruleWestHatago002
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ich arbeite weiter an meiner Theorie,\ndass die "
      - control:
          kind: set_colour
          colour: red
      - text: Kolosswale
      - control:
          kind: reset_colour
      - text: " an einer Dürre\nzugrunde gegangen sind."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Bitte hilf auch meinen Brüdern!
  talk-Gamyo12:
    attributes: Npc_HyruleWestHatago002
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Deine Bilder sind eine Riesenhilfe!
      - control:
          kind: pause
          length: short
      - text: "\nDanke!"
  talk-Negyui01:
    attributes: Npc_HyruleWestHatago003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 16
      - text: "Meiner Meinung nach wurde den\n"
      - control:
          kind: set_colour
          colour: red
      - text: Kolosswalen
      - control:
          kind: reset_colour
      - text: " der Urzeit eine Eiszeit zum\nVerhängnis!"
  talk-Negyui04:
    attributes: Npc_HyruleWestHatago003
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 16
      - text: Tja...
      - control:
          kind: pause
          length: long
      - text: " Allerdings werden wir wohl kaum\nerfahren, warum die "
      - control:
          kind: set_colour
          colour: red
      - text: Kolosswale
      - control:
          kind: reset_colour
      - text: " weg\nsind, wenn wir nur herumdiskutieren..."
  talk-Negyui05:
    attributes: Npc_HyruleWestHatago003
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 0
      - text: "Ich würde ja nur zu gern die "
      - control:
          kind: set_colour
          colour: red
      - text: "Kolosswal-\nKnochen"
      - control:
          kind: reset_colour
      - text: " in "
      - control:
          kind: set_colour
          colour: blue
      - text: Hebra
      - control:
          kind: reset_colour
      - text: " mal zu Gesicht\nbekommen..."
      - control:
          kind: choice
          choice_labels:
            - 2
            - 3
          selected_index: 0
          cancel_index: 1
          unknown: 6
  talk-Negyui07:
    attributes: Npc_HyruleWestHatago003
    contents:
      - control:
          kind: sound
          unknown:
            - 2
            - 12
      - text: "Du hast doch gar kein "
      - control:
          kind: set_colour
          colour: red
      - text: Bild
      - control:
          kind: reset_colour
      - text: "..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Tja, die Knochen in "
      - control:
          kind: set_colour
          colour: blue
      - text: Hebra
      - control:
          kind: reset_colour
      - text: " sind wohl auch\nbesonders schwierig aufzuspüren."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Sie sollen tief in einer verschlossenen\nEishöhle sein..."
      - control:
          kind: pause
          length: short
      - text: " Mehr weiß ich leider\nauch nicht."
  talk-Negyui08:
    attributes: Npc_HyruleWestHatago003
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 11
      - text: "Nicht zu glauben! "
      - control:
          kind: pause
          length: short
      - text: "Du hast "
      - control:
          kind: set_colour
          colour: red
      - text: Bilder
      - control:
          kind: reset_colour
      - text: " aller\n3 Fundorte von "
      - control:
          kind: set_colour
          colour: red
      - text: Kolosswal
      - control:
          kind: reset_colour
      - text: "-Knochen!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: Ich kann dir gar nicht genug danken!
      - control:
          kind: pause
          length: short
      - text: "\nUnd hier ist deine Belohnung!"
  talk-Negyui09:
    attributes: Npc_HyruleWestHatago003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Bitte hilf auch meinen Brüdern!
  talk-Negyui10:
    attributes: Npc_HyruleWestHatago003
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 0
      - text: "Wie solltest du auch so schnell schon\nfündig geworden sein..."
      - control:
          kind: pause
          length: short
      - text: " Ich werde mich\ndann hier weiter in Geduld üben."
  talk-Negyui11:
    attributes: Npc_HyruleWestHatago003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ich werde dein Bild gründlich auf\nBeweise für meine Eiszeittheorie\nuntersuchen!"
  talk-Negyui12:
    attributes: Npc_HyruleWestHatago003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Mit diesem Bild werde ich sicher\nbeweisen können, dass eine Eiszeit die\n"
      - control:
          kind: set_colour
          colour: red
      - text: Kolosswale
      - control:
          kind: reset_colour
      - text: " dahingerafft hat!"
  talk-Orak00:
    attributes: Npc_HyruleWestHatago004
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 16
      - text: "Ich bin überzeugt, dass die "
      - control:
          kind: set_colour
          colour: red
      - text: Kolosswale
      - control:
          kind: reset_colour
      - text: "\ndurch eine Vulkaneruption ausgerottet\nwurden!"
  talk-Orak02:
    attributes: Npc_HyruleWestHatago004
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 16
      - text: Aaalso...
      - control:
          kind: pause
          length: short
      - text: " Wenn wir hier nur diskutieren,\nkommen wir mit den "
      - control:
          kind: set_colour
          colour: red
      - text: Kolosswalen
      - control:
          kind: reset_colour
      - text: " nicht\nweiter!"
  talk-Orak04:
    attributes: Npc_HyruleWestHatago004
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: Sag mal...
      - control:
          kind: pause
          length: long
      - text: " Würdest du vielleicht einen\nkleinen Auftrag für uns erledigen?"
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
          selected_index: 0
          cancel_index: 1
          unknown: 6
  talk-Orak05:
    attributes: Npc_HyruleWestHatago004
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 19
      - text: Huch...
      - control:
          kind: pause
          length: long
      - text: " Oh, tut mir leid, dass wir dich\nnicht beachtet haben..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Wir erörtern, warum die "
      - control:
          kind: set_colour
          colour: red
      - text: Kolosswale
      - control:
          kind: reset_colour
      - text: "\nausgestorben sind."
      - control:
          kind: pause
          length: short
      - text: " Das ist die Frage,\nder wir auf den Grund gehen wollen."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 16
      - text: "Leider sind die Orte, an denen die\nKnochen der "
      - control:
          kind: set_colour
          colour: red
      - text: Kolosswale
      - control:
          kind: reset_colour
      - text: " zu finden sein\nsollen, unerreichbar für uns..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: Deswegen können wir nur spekulieren...
      - control:
          kind: pause
          length: short
      - text: "\nAber wir wissen zumindest ungefähr, wo\ndie Knochen zu finden sein müssten."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Und zwar nordöstlich in "
      - control:
          kind: set_colour
          colour: blue
      - text: "Eldin,"
      - control:
          kind: reset_colour
      - text: " "
      - control:
          kind: set_colour
          colour: blue
      - text: "Hebra "
      - control:
          kind: reset_colour
      - text: "im\nNordwesten und "
      - control:
          kind: set_colour
          colour: blue
      - text: "Gerudo "
      - control:
          kind: reset_colour
      - text: im Südwesten.
      - control:
          kind: pause
          length: short
      - text: "\nAlles gefährliche Orte."
  talk-Orak06:
    attributes: Npc_HyruleWestHatago004
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Du siehst aus wie jemand, für den Gefahr\ndas täglich Brot ist."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "Kannst du nicht für uns an diese drei\nOrte gehen und "
      - control:
          kind: set_colour
          colour: red
      - text: Bilder
      - control:
          kind: reset_colour
      - text: " der "
      - control:
          kind: set_colour
          colour: red
      - text: "Knochen der\nKolosswale"
      - control:
          kind: reset_colour
      - text: " für uns anfertigen?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Besonders gern würde ich den Kopf\nsehen. Wenn der auf dem Bild ist, ist\nalles in Ordnung.\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Wenn du das schaffst, bekommst du\n"
      - control:
          kind: set_colour
          colour: red
      - text: 300 Rubine
      - control:
          kind: reset_colour
      - text: "!"
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
          selected_index: 0
          cancel_index: 1
          unknown: 6
  talk-Orak07:
    attributes: Npc_HyruleWestHatago004
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 12
      - text: Na gut.
      - control:
          kind: pause
          length: long
      - text: " Aber das Angebot besteht weiter."
  talk-Orak08:
    attributes: Npc_HyruleWestHatago004
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 11
      - text: "Du erweist damit der Wissenschaft einen\ngroßen Dienst!"
      - control:
          kind: pause
          length: short
      - text: " Die "
      - control:
          kind: set_colour
          colour: red
      - text: 300 Rubine
      - control:
          kind: reset_colour
      - text: " warten\nhier auf dich!"
  talk-Orak09:
    attributes: Npc_HyruleWestHatago004
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 0
      - text: "Ich will die "
      - control:
          kind: set_colour
          colour: red
      - text: Kolosswal-Knochen
      - control:
          kind: reset_colour
      - text: " aus "
      - control:
          kind: set_colour
          colour: blue
      - text: Eldin
      - control:
          kind: reset_colour
      - text: "\nsehen..."
      - control:
          kind: choice
          choice_labels:
            - 2
            - 3
          selected_index: 0
          cancel_index: 1
          unknown: 6
  talk-Orak10:
    attributes: Npc_HyruleWestHatago004
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 11
      - text: Na so was!
      - control:
          kind: pause
          length: short
      - text: " Du hast tatsächlich "
      - control:
          kind: set_colour
          colour: red
      - text: Bilder
      - control:
          kind: reset_colour
      - text: "\naller 3 Fundorte von "
      - control:
          kind: set_colour
          colour: red
      - text: Kolosswal
      - control:
          kind: reset_colour
      - text: "-Knochen\nangefertigt!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: Ich danke dir!
      - control:
          kind: pause
          length: short
      - text: " Hier, nimm deine\nBelohnung, wie abgemacht!"
  talk-Orak11:
    attributes: Npc_HyruleWestHatago004
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Bitte hilf auch meinen Brüdern!
  talk-Orak12:
    attributes: Npc_HyruleWestHatago004
    contents:
      - control:
          kind: sound
          unknown:
            - 2
            - 14
      - text: Aber...
      - control:
          kind: pause
          length: short
      - text: " Du hast ja gar kein "
      - control:
          kind: set_colour
          colour: red
      - text: Bild
      - control:
          kind: reset_colour
      - text: "!"
      - control:
          kind: pause
          length: short
      - text: "\nHör auf mit diesen Scherzen!"
  talk-Orak13:
    attributes: Npc_HyruleWestHatago004
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Bringt auch nichts, sich nervös zu\nmachen. "
      - control:
          kind: pause
          length: short
      - text: "Ich warte einfach hier, bis du\nein Bild hast."
  talk-Orak14:
    attributes: Npc_HyruleWestHatago004
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ich werde weiter nach Belegen dafür\nsuchen, dass ein Vulkanausbruch den\n"
      - control:
          kind: set_colour
          colour: red
      - text: Kolosswalen
      - control:
          kind: reset_colour
      - text: " zum Verhängnis wurde!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Bitte hilf auch meinen Brüdern!
  talk-Orak15:
    attributes: Npc_HyruleWestHatago004
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ich werde das Bild mit größter Sorgfalt\nuntersuchen und an meiner Theorie\nweiterarbeiten."
