          cancel_index: 1
          unknown: 6
  Talk06:
    attributes: subchallnpc000
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 15
      - text: "...Er hat's nicht einfach vergessen. "
      - control:
          kind: pause
          length: short
      - text: "Wäre\nja auch zu schön gewesen."
  Talk07:
    attributes: subchallnpc000
