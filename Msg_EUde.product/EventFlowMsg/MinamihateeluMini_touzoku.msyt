      - text: " gehört uns allein!"
  Talk20:
    attributes: subchallnpc000_twin
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: "Mein Bruder ist der beste Schatzsucher\nüberhaupt!"
      - control:
          kind: pause
          length: short
      - text: " Der weiß immer, wo es was\nzu holen gibt!"
  Talk22:
    attributes: subchallnpc000
    contents:
      - control:
          kind: sound
          unknown:
            - 2
            - 0
      - text: "Der "
      - control:
