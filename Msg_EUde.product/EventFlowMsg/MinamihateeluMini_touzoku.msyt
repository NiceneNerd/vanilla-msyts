  Talk04:
    attributes: subchallnpc000
    contents:
      - control:
          kind: sound
          unknown:
            - 2
            - 0
      - text: "Wir haben hier jedenfalls wichtige Dinge\nzu bereden, also lass uns in Ruhe."
  Talk05:
    attributes: subchallnpc000
    contents:
      - control:
          kind: sound
          unknown:
            - 2
            - 0
      - text: "'tschuldige, mein Bruder redet manchmal\neinen Blödsinn zusammen..."
      - control:
          kind: pause
          length: short
      - text: " Vergiss es\nam besten einfach."
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
