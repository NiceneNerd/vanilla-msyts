          kind: reset_colour
      - text: "."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ich meine, diese heiße Information ist\nlocker mehr als 1000 Rubine wert."
      - control:
          kind: pause
          length: short
      - text: " Was\nsagst du?"
      - control:
          kind: choice
          choice_labels:
            - 2
            - 3
