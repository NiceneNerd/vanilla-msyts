  talk00:
    attributes: Npc_FaronWoods010
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Bist du'n Reisender?"
      - control:
          kind: pause
          length: short
      - text: "\nHier gibt's sehr viele Pferde, bin quasi\nmit denen aufgewachsen."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Deswegen bin ich auch 'n ziemlich\nguter Reiter."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Haste Lust, deine Reitfähigkeiten unter\nBeweis zu stellen?"
      - control:
          kind: pause
          length: short
      - text: " Aber ich muss dich\nwarnen, ich bin richtig gut!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Na, willste versuchen, meinen Rekord\nzu schlagen?"
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
            - 2
          selected_index: 0
          cancel_index: 2
          unknown: 8
  talk01:
    attributes: Npc_FaronWoods010
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Na?
      - control:
          kind: pause
          length: short
      - text: " Willste endlich meinen Rekord\nknacken?"
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
            - 2
          selected_index: 0
          cancel_index: 2
          unknown: 8
  talk02:
    attributes: Npc_FaronWoods010
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Dann bräuchte ich erst mal 'nen kleinen\nUnkostenbeitrag von "
      - control:
