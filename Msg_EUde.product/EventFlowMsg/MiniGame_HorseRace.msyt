      - text: 20 Rubinen
      - control:
          kind: reset_colour
      - text: "!"
      - control:
          kind: choice
          choice_labels:
            - 3
            - 4
          selected_index: 0
          cancel_index: 1
          unknown: 6
  talk04:
    attributes: Npc_FaronWoods010
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 9
      - text: Lass es krachen!
  talk05:
    attributes: Npc_FaronWoods010
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "Das ist echt 'ne Schande, aber vielleicht\nwillste ja ein andermal?"
  talk06:
    attributes: Npc_FaronWoods010
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Reite über diese Wiese und springe dabei\nüber die Hindernisse."
  talk07:
    attributes: Npc_FaronWoods010
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 7
      - text: "Was'n los?"
      - control:
          kind: choice
          choice_labels:
            - 5
            - 6
          selected_index: 0
          cancel_index: 1
          unknown: 6
  talk08:
    attributes: Npc_FaronWoods010
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 13
      - text: "So 'n bisschen Übung würde dir nicht\nschaden!"
  talk09:
    attributes: Npc_FaronWoods010
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 9
      - text: Bist ja ganz schön schnell!
      - control:
          kind: pause
          length: long
      - text: "\nAber nicht schneller als ich!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Hier, was für dein Pferd."
  talk10:
    attributes: Npc_FaronWoods010
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 21
      - text: "Ich hätte nie gedacht, dass mal wer\nmeinen Rekord brechen würde..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Hier ist die "
      - control:
          kind: set_colour
          colour: red
      - text: Pferdeausrüstung
      - control:
          kind: reset_colour
      - text: ", die ich dir\nversprochen habe."
  talk11:
    attributes: Npc_FaronWoods010
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 23
      - text: "Das war 'ne wirklich gute Zeit!"
      - control:
          kind: pause
          length: long
      - text: "\nDu wirst immer besser!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Und hier, wie versprochen –"
      - control:
          kind: pause
          length: short
      - text: " deine\n"
      - control:
          kind: set_colour
          colour: red
      - text: Pferdeausrüstung
      - control:
          kind: reset_colour
      - text: "!"
      - control:
          kind: pause
          length: short
      - text: " Und versuch weiter,\nmeinen Rekord zu erreichen!"
  talk12:
    attributes: Npc_FaronWoods010
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 9
      - text: "Üb noch 'n bisschen und versuch es\ndann wieder, würd ich sagen."
  talk13:
    attributes: Npc_FaronWoods010
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: Deine Rubine reichen nicht.
      - control:
          kind: pause
          length: short
      - text: " Kann ja mal\npassieren!"
      - control:
          kind: pause
          length: short
      - text: " Komm einfach wieder, wenn\ndu die "
      - control:
          kind: set_colour
          colour: red
      - text: 20 Rubine
      - control:
          kind: reset_colour
      - text: " hast."
  talk14:
    attributes: Npc_FaronWoods010
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Willst du's noch mal versuchen?"
      - control:
          kind: choice
          choice_labels:
            - 0
            - 2
          selected_index: 0
          cancel_index: 1
          unknown: 6
  talk15:
    attributes: Npc_FaronWoods010
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Zuerst mal musst du auf 'nem Pferd zu\nmir kommen und mit mir sprechen!"
  talk16:
    attributes: Npc_FaronWoods010
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Dein Pferd ist nicht "
      - control:
          kind: set_colour
          colour: red
      - text: registriert
      - control:
          kind: reset_colour
      - text: "..."
      - control:
          kind: pause
          length: short
      - text: "\nDu müsstest es erst hier im Stall\n"
      - control:
          kind: set_colour
          colour: red
      - text: registrieren
      - control:
          kind: reset_colour
      - text: " lassen, sonst wird das nix."
  talk17:
    attributes: Npc_FaronWoods010
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Die Gebühr kann ich dir nicht erstatten,\nist alles schon direkt in die Unkosten\ngeflossen."
      - control:
          kind: pause
          length: long
      - text: " Willste trotzdem aufhören?"
      - control:
          kind: choice
          choice_labels:
            - 5
            - 7
          selected_index: 0
          cancel_index: 1
          unknown: 6
  talk18:
    attributes: Npc_FaronWoods010
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Beeil dich, los!"
      - control:
          kind: pause
          length: short
      - text: "\nDu verlierst wertvolle Zeit!"
  talk19:
    attributes: Npc_FaronWoods010
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: Zu schade.
      - control:
          kind: pause
          length: short
      - text: " Du kannst aber jederzeit\nwiederkommen!"
  talk20:
    attributes: Npc_FaronWoods010
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Wie sieht's aus?"
      - control:
          kind: pause
          length: short
      - text: " Willst du's versuchen?"
      - control:
          kind: choice
          choice_labels:
            - 0
            - 2
          selected_index: 0
          cancel_index: 1
          unknown: 6
  talk21:
    attributes: Npc_FaronWoods010
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 0
      - text: "Sehr gut, da bist du ja wieder!"
      - control:
          kind: pause
          length: short
      - text: "\nDu willst also meinen Rekord brechen?"
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
            - 2
          selected_index: 0
          cancel_index: 2
          unknown: 8
  talk22:
    attributes: Npc_FaronWoods010
    contents:
      - text: "Du hast's geschafft!"
  talk23:
    attributes: Npc_FaronWoods010
    contents:
      - text: Vom Pferd absteigen gilt nicht!
      - control:
          kind: pause
          length: short
      - text: "\nKomm zurück!"
  talk24:
    attributes: Npc_FaronWoods010
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 13
      - text: Was ist los?
      - control:
          kind: pause
          length: short
      - text: "\nSo brichst du meinen Rekord nie!"
  talk25:
    attributes: Npc_FaronWoods010
    contents:
      - text: "Das war 'n bisschen zu langsam!"
      - control:
          kind: pause
          length: short
      - text: "\nKomm zurück!"
  talk26:
    attributes: Npc_FaronWoods010
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Bei Regen tust du dich nur unnötig\nschwer, einen Rekord aufzustellen."
      - control:
          kind: pause
          length: short
      - text: "\nKomm wieder, wenn es nicht regnet."
  talk27:
    attributes: Npc_FaronWoods010
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: Im Regen zu reiten macht keinen Spaß.
      - control:
          kind: pause
          length: short
      - text: "\nHoffentlich hört's bald auf."
  talk28:
    attributes: Npc_FaronWoods010
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "'tschuldige, aber ich hau mich jetzt aufs\nOhr."
      - control:
          kind: pause
          length: short
      - text: " Und nachts ist die Sicht sowieso zu\nschlecht."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Komm morgen wieder, wenn du dich an\nmeinen Rekord wagen willst."
  talk29:
    attributes: Npc_FaronWoods010
    contents:
      - text: Schneller...
  talk30:
    attributes: Npc_FaronWoods010
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Ich hau mich ins Heu.
      - control:
          kind: pause
          length: short
      - text: " Wenn du was\nwillst, musst du morgen wiederkommen."
  talk31:
    attributes: Npc_FaronWoods010
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Wenn du vom Pferd steigst, wirst du\ndisqualifiziert, aber das ist ja eigentlich\nlogisch, oder?"
  talk32:
    attributes: Npc_FaronWoods010
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 13
      - text: Das war wieder mal ziemlich fix...
      - control:
          kind: pause
          length: long
      - text: "\nIch sag dir, du bist nicht gut für mein\nSelbstvertrauen..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Den "
      - control:
          kind: set_colour
          colour: red
      - control:
          kind: raw
          two_hundred_one:
            dynamic:
              - 0
              - len: 4
                field_2:
                  - 1
                  - 0
                  - 0
                  - 0
      - text: Schmucksattel
      - control:
          kind: reset_colour
      - text: " hab ich dir schon\ngegeben, also kriegste jetzt das hier."
  talk33:
    attributes: Npc_FaronWoods010
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 23
      - text: Was für eine Zeit!
      - control:
          kind: pause
          length: long
      - text: " Du wirst immer besser!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Das "
      - control:
          kind: set_colour
          colour: red
      - control:
          kind: raw
          two_hundred_one:
            dynamic:
              - 0
              - len: 4
                field_2:
                  - 3
                  - 2
                  - 2
                  - 0
      - text: Schmuckzaumzeug
      - control:
          kind: reset_colour
      - text: " hab ich dir schon\ngegeben, also gibt's stattdessen jetzt\ndas hier."
  talk34:
    attributes: Npc_FaronWoods010
    contents:
      - text: "Wenn du 'ne gute Zeit schaffst, geb ich\ndir meine liebste "
      - control:
          kind: set_colour
          colour: red
      - text: Pferdeausrüstung
      - control:
          kind: reset_colour
      - text: "."
  talk35:
    attributes: Npc_FaronWoods010
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 9
      - text: Das war recht schnell...
      - control:
          kind: pause
          length: long
      - text: " Aber nicht\nschneller als mein Rekord!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Hier hast du 'nen Preis!"
      - control:
          kind: pause
          length: short
      - text: " Du kannst's\nauch gern noch mal versuchen!"
  talk36:
    attributes: Npc_FaronWoods010
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 0
      - text: "Das hab ich jetzt davon, Leute\nherauszufordern..."
      - control:
          kind: pause
          length: long
      - text: " Aber irgendwann\nbreche ich deinen Rekord, wirst sehen!"
  talk37:
    attributes: Npc_FaronWoods010
    contents:
      - control:
          kind: sound2
          unknown:
            - 7
            - 205
      - text: "Mein Rekord liegt bei "
      - control:
          kind: set_colour
          colour: red
      - text: 1 Minute
      - control:
          kind: reset_colour
      - text: " und\n"
      - control:
          kind: set_colour
          colour: red
      - text: 15 Sekunden
      - control:
          kind: reset_colour
      - text: ". Denkst du, da kannste\nmithalten?"
  talk38:
    attributes: Npc_FaronWoods010
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Deine Zeit lag also bei "
      - control:
          kind: set_colour
          colour: red
      - control:
          kind: variable
          variable_kind: 14
          name: MiniGame_HorseRace_ResultMinute
      - text: " "
      - control:
          kind: localisation
          localisation_kind: plural
          options:
            - Minute
            - Minuten
            - Minute
      - text: " "
      - control:
          kind: reset_colour
      - text: "und\n"
      - control:
          kind: set_colour
          colour: red
      - control:
          kind: variable
          variable_kind: 14
          name: MiniGame_HorseRace_ResultSecond
      - text: ","
      - control:
          kind: variable
