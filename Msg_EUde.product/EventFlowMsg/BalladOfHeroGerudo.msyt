          colour: blue
      - text: "Sache mit\ndem Donnerhelm"
      - control:
          kind: reset_colour
      - text: "..."
      - control:
          kind: pause
          length: long
      - text: " Und wieder waren\nes die "
      - control:
          kind: set_colour
