    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ich weiß, dass das "
      - control:
          kind: set_colour
          colour: red
      - text: "Juwel, das sie dem\nGerudo-Stamm gestohlen haben,"
      - control:
          kind: reset_colour
      - text: " im\nLager im ersten Stock aufbewahrt wird..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 3
            - 12
      - text: "Aber sich noch näher heranzuwagen ist\nriskant. Am Leben bleiben ist Grundvor-\naussetzung für das Schatzjäger-Dasein!"
  Npc_Attacked_006_DLC2nd_SD_001:
    attributes: Npc_Attacked_006_DLC2nd
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 0
      - text: "Ich habe gehört, dass ein reisender\nKrieger den Yiga-Clan besiegt hat, aber..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 4
            - 20
      - text: "Es gibt immer noch so viele von denen!\nUnd sie sammeln Bananen in rauen\nMengen. Steckt da ein Plan dahinter?"
  Npc_Attacked_006_DLC2nd_SD_100:
    attributes: Npc_Attacked_006_DLC2nd
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 0
      - text: "Du bist doch nicht wirklich da\nreingegangen..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Und hast dir das "
      - control:
          kind: set_colour
          colour: red
      - text: "Juwel "
      - control:
          kind: reset_colour
      - text: geholt?
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Es ihnen stibitzt?
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Wirklich!?
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 3
            - 0
      - text: "Wenn sie das herauskriegen, werden sie\ndich bis ans Ende der Welt verfolgen!"
  Npc_Attacked_006_DLC2nd_SD_200:
    attributes: Npc_Attacked_006_DLC2nd
    contents:
