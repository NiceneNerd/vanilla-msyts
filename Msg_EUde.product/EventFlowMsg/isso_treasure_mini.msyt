  NewText_00:
    attributes: Npc_NorthHatelHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: "Oh, Verzeihung, tut mir leid."
      - control:
          kind: pause
          length: short
      - text: "\nIch war ganz in Gedanken."
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
          selected_index: 0
          cancel_index: 1
          unknown: 6
  NewText_01:
    attributes: Npc_NorthHatelHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 12
      - text: "Eigentlich will ich es ja niemandem\nerzählen, aber..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Hast du bemerkt, dass da etwas auf\ndem Grund des Flusses liegt?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Wenn ich recht habe...
      - control:
          kind: pause
          length: short
      - text: " dann ist es eine\nSchatztruhe, der Form nach zu urteilen."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Jetzt überlege ich also, wie ich sie an die\nOberfläche bekomme."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Da braucht man wohl...
      - control:
          kind: pause
          length: short
      - text: " besonderes\nGerät für, denke ich. "
      - control:
          kind: pause
          length: short
      - text: Was meinst du?
  NewText_03:
    attributes: Npc_NorthHatelHatago001
    contents:
      - text: Wenn ich Recht habe...
      - control:
          kind: pause
          length: short
      - text: " dann ist es eine\nSchatztruhe, der Form nach zu urteilen."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Da braucht man wohl...
      - control:
          kind: pause
          length: short
      - text: " besonderes\nGerät für, denke ich. "
      - control:
          kind: pause
          length: short
      - text: Was meinst du?
  NewText_07:
    attributes: Npc_NorthHatelHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 0
      - text: Ein Schatz?
      - control:
          kind: pause
          length: short
      - text: "\nEin ganz toller und wertvoller?!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 8
            - 0
      - text: "Ach Menno! "
      - control:
          kind: pause
          length: longer
      - text: Jetzt bin ich total gefrustet!
  NewText_08:
    attributes: Npc_NorthHatelHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 6
      - text: Was denn?
      - control:
          kind: pause
          length: longer
      - text: " Ein tolles Schwert, sagst du?"
  NewText_09:
    attributes: Npc_NorthHatelHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ich weiß ja nicht, wieso du über solche\nKräfte verfügst, aber..."
      - control:
          kind: pause
          length: short
      - text: " du hast es echt\ndrauf!\nUnd..."
      - control:
          kind: pause
          length: short
      - text: " Stimmt ja!"
      - control:
          kind: pause
          length: longer
      - text: "\nWas war denn jetzt in der Schatztruhe?"
      - control:
          kind: choice
          choice_labels:
            - 4
            - 5
          selected_index: 0
          cancel_index: 2
          unknown: 6
  NewText_10:
    attributes: Npc_NorthHatelHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: Was mach ich nur...?
      - control:
          kind: pause
          length: longer
      - text: "\nIch bin so wasserscheu..."
  NewText_11:
    attributes: Npc_NorthHatelHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 18
      - text: "Ich bin so gespannt, was in der Truhe\nist!"
      - control:
          kind: pause
          length: short
      - text: " Da kann ich kaum schlafen... "
      - control:
          kind: pause
          length: short
      - text: Hach.
  NewText_12:
    attributes: Npc_NorthHatelHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Wenn ich nur an diese Truhe gekommen\nwäre, dann wäre ich jetzt steinreich"
      - control:
          kind: pause
          length: short
      - text: " und\ndie Damen würden sich um mich reißen..."
  NewText_13:
    attributes: Npc_NorthHatelHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Tja, jetzt, da ich weiß, was drin war,\nhabe ich wohl endlich Ruhe. Danke."
  NewText_14:
    attributes: Npc_NorthHatelHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Na ja, selbst wenn ich behalten könnte,\nwas in der Truhe war, wüsste ich doch\nnichts recht damit anzufangen..."
  NewText_15:
    attributes: Npc_NorthHatelHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Egal, jetzt weiß ich wenigstens, was drin\nwar und habe meinen Seelenfrieden\ngefunden."
      - control:
          kind: pause
          length: longer
      - text: " Danke."
  NewText_16:
    attributes: Npc_NorthHatelHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Oh! "
      - control:
          kind: pause
          length: longer
      - text: Die Schatztruhe!
  NewText_17:
    attributes: Npc_NorthHatelHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Das war fantastisch!
  NewText_18:
    attributes: Npc_NorthHatelHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: D-
      - control:
          kind: pause
          length: short
      - text: die Schatztruhe?!
  NewText_19:
    attributes: Npc_NorthHatelHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 0
      - text: Was geht da vor?!
  NewText_20:
    attributes: Npc_NorthHatelHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "...Hm?"
  NewText_23:
    attributes: Npc_NorthHatelHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 0
      - text: Du bist großartig!
      - control:
          kind: pause
          length: longer
      - text: " Ich hätte nie\nrausgefunden, wie man die Schatztruhe\naus dem Fluss fischt!\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Schnell, lass uns nachsehen, was\ndrin ist!"
  NewText_26:
    attributes: Npc_NorthHatelHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Hm...
      - control:
          kind: pause
          length: short
      - text: " Wie mach ich's nur..."
  NewText_27:
    attributes: Npc_NorthHatelHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ah. "
      - control:
          kind: pause
          length: long
      - text: Auf Wiedersehen.
  NewText_28:
    attributes: Npc_NorthHatelHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ah, bist wohl neugierig geworden, oder?"
  NewText_29:
    attributes: Npc_NorthHatelHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: Unglaublich!
      - control:
          kind: pause
          length: short
      - text: " Hast du diese Truhe echt\neinfach so gehoben?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 5
            - 0
      - text: "Und ich stehe hier schon seit Tagen und\nzerbreche mir das Hirn darüber, wie ich\nsie aus dem Fluss bekomme..."
  NewText_30:
    attributes: Npc_NorthHatelHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 0
      - text: Woah!
      - control:
          kind: pause
          length: short
      - text: " Hast du übernatürliche Kräfte\noder so?!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: "Unglaublich! "
      - control:
          kind: pause
          length: short
      - text: Einfach unglaublich!
      - control:
          kind: pause
          length: short
      - text: "\nSchnell, öffne die Truhe und hol\nden Inhalt heraus!"
  NewText_31:
    attributes: Npc_NorthHatelHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 0
      - text: "Boah! "
      - control:
          kind: pause
          length: short
      - text: "Sag mal, bist du etwa ein\nZauberer?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Und ich frage mich schon seit Tagen,\nwie ich an diese Truhe kommen soll..."
