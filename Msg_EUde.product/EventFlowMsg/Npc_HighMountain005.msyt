  talk05:
    attributes: Npc_HighMountain005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Bitte kümmere dich nicht weiter\num mich..."
  talk06:
    attributes: Npc_HighMountain005
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 11
      - text: Mein Mann hat mir alles erzählt...
      - control:
          kind: pause
          length: longer
      - text: " Vom\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Flugplatz
      - control:
          kind: reset_colour
      - text: ", vom Kampf gegen "
      - control:
          kind: set_colour
          colour: blue
      - text: Medoh
      - control:
          kind: reset_colour
      - text: "..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Jetzt, wo "
      - control:
          kind: set_colour
          colour: blue
      - text: Medoh
      - control:
          kind: reset_colour
      - text: " sich über dem Dorf\nniedergelassen hat, sei endlich alles\nvorbei.\nIch hätte kaum noch zu hoffen gewagt,\ndass mein Mann wohlbehalten zurück-\nkehrt..."
      - control:
          kind: pause
          length: longer
      - text: " Ich bin dir unendlich dankbar!"
  talk08:
    attributes: Npc_HighMountain005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Wenn der "
      - control:
          kind: set_colour
          colour: blue
      - text: Häuptling
      - control:
          kind: reset_colour
      - text: " dir vertraut, werde\nauch ich dir alles sagen, was ich weiß."
      - control:
          kind: choice
