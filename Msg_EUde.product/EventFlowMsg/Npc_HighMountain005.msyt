  talk39:
    attributes: Npc_HighMountain005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Mein Mann hat Tulin mal wieder zum\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Flugplatz
      - control:
          kind: reset_colour
      - text: " mitgenommen..."
  talk40:
