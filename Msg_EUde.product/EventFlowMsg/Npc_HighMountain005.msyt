      - control:
          kind: sound
          unknown:
            - 6
            - 1
      - text: "Es tut mir leid, aber..."
      - control:
          kind: pause
          length: short
      - text: "\nIch habe mitgehört, als du mit dem\n"
      - control:
          kind: set_colour
