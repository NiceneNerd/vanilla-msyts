      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: Verzeih mir...
      - control:
          kind: pause
          length: longer
      - text: "\nIch wollte nicht anmaßend klingen."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Wenn du irgendetwas wissen möchtest,\nkannst du mich später gerne fragen."
  talk36:
    attributes: Npc_HighMountain005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Der Revali-Platz ist ein Gedenkort für\nden "
      - control:
          kind: set_colour
          colour: blue
      - text: Recken
      - control:
          kind: reset_colour
      - text: " der Orni, Revali..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Damit wir niemals vergessen, was an\njenem unheilvollen Tag geschah."
  talk37:
    attributes: Npc_HighMountain005
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 12
      - text: Ich will jetzt schlafen.
      - control:
          kind: pause
          length: longer
      - text: "\nWenn du reden willst, komm\nspäter wieder."
  talk38:
    attributes: Npc_HighMountain005
    contents:
