          colour: blue
      - text: "Rinos-Pass "
      - control:
          kind: reset_colour
      - text: "am\nFuße der "
      - control:
          kind: set_colour
          colour: blue
      - text: Hebra-Berge
      - control:
          kind: reset_colour
      - text: "."
  talk41:
    attributes: Npc_HighMountain005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Dort üben die Orni-Krieger den\nLuftkampf."
  talk42:
    attributes: Npc_HighMountain005
    contents:
      - control:
          kind: sound2
          unknown:
            - 4
            - 205
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Wahrscheinlich wappnet sich mein\nMann dort, um "
      - control:
          kind: set_colour
          colour: blue
      - text: Medoh
      - control:
          kind: reset_colour
      - text: " nicht noch\neinmal zu unterliegen...\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Ich glaube, er ist zu Fuß zum "
      - control:
