entries:
  Deku_Cancel00:
    attributes: TwnObj_Village_Korok_DekuTree_A_01
    contents:
      - control:
          kind: sound2
          unknown:
            - 10
            - 205
      - text: "Vor 100 Jahren konntest du es mühelos\nherausziehen, aber du scheinst\nschwächlich geworden zu sein.\nDu musst wieder zu deiner alten Stärke\nzurückfinden."
      - control:
          kind: pause
          length: long
      - text: "\n"
      - control:
          kind: sound2
          unknown:
            - 8
            - 205
      - text: Ho ho ho ho!
  Deku_Cancel01:
    attributes: TwnObj_Village_Korok_DekuTree_A_01
    contents:
      - control:
          kind: sound2
          unknown:
            - 10
            - 205
      - text: "Deine Kraft scheint noch nicht ganz\nauszureichen."
      - control:
          kind: pause
          length: short
      - text: " Du musst weitere\nPrüfungen bestehen!"
  Deku_Cancel01_2:
    attributes: TwnObj_Village_Korok_DekuTree_A_01
    contents:
      - text: "...Dabei wäre es dir beinahe gelungen!"
      - control:
          kind: pause
          length: long
      - text: "\n"
      - control:
          kind: sound2
          unknown:
            - 8
            - 205
      - text: Ho ho ho!
  Deku_Cancel02:
    attributes: TwnObj_Village_Korok_DekuTree_A_01
    contents:
      - control:
          kind: sound2
          unknown:
            - 7
            - 205
      - text: Ho ho ho!
      - control:
          kind: pause
          length: long
      - text: "\nDu scheinst ängstlich geworden zu sein."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Dabei bist du schon weit stärker, als du\nes vor 100 Jahren warst. "
      - control:
          kind: pause
          length: short
      - text: "Wäre nur dein\nMut so groß wie deine Kraft!"
  Deku_Cancel03:
    attributes: TwnObj_Village_Korok_DekuTree_A_01
    contents:
      - control:
          kind: sound2
          unknown:
            - 14
            - 205
      - text: "Deine Kraft scheint noch nicht\nauszureichen, obwohl du schon viele\nPrüfungen bestanden hast.\nSicher glaubst du, die Geschenke, die du\nvon der Statue der Göttin erhalten hast,\nlassen sich nicht ändern...\nBegib dich zum Dorf Hateno im Südosten\nvon Hyrule. "
      - control:
          kind: pause
          length: short
      - text: "Dort gibt es jemanden, der\nmit diesen Geschenken handelt.\nEin ziemlicher Schurke ist das..."
      - control:
          kind: pause
          length: long
      - text: "\n"
      - control:
          kind: sound2
          unknown:
            - 8
            - 205
      - text: Ho ho ho ho!
  Deku_Cancel04:
    attributes: TwnObj_Village_Korok_DekuTree_A_01
    contents:
      - control:
          kind: sound2
          unknown:
            - 9
            - 205
      - text: "Deine Kraft scheint noch nicht\nauszureichen, obwohl du schon viele\nPrüfungen bestanden hast.\nBesuche eine der Statuen der Göttin und\nbringe ihr deine "
      - control:
          kind: raw
          two_hundred_one:
            dynamic:
              - 0
              - len: 4
                field_2:
                  - 3
                  - 2
                  - 2
                  - 0
      - text: "Zeichen der Bewährung\ndar, um mehr Herzen zu erhalten."
  Deku_Cancel05:
    attributes: TwnObj_Village_Korok_DekuTree_A_01
    contents:
      - control:
          kind: sound2
          unknown:
            - 3
            - 205
      - text: "Dachtest du etwa, du könntest mit\nsolchen Wunden das Schwert heraus-\nziehen? "
      - control:
          kind: pause
          length: short
      - text: Du solltest dich zuerst heilen!
  Deku_Cancel06:
    attributes: TwnObj_Village_Korok_DekuTree_A_01
    contents:
      - control:
          kind: sound2
          unknown:
            - 8
            - 205
      - text: Ho ho ho!
      - control:
          kind: pause
          length: long
      - text: "\nDu scheinst ängstlich geworden zu sein."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Stelle dich der Prüfung, sobald du\ngenug Mut gefasst hast."
  Deku_Finish01:
    attributes: TwnObj_Village_Korok_DekuTree_A_01
    contents:
      - control:
          kind: sound2
          unknown:
            - 14
            - 205
      - text: Der Name dieses Schwertes lautet
      - control:
          kind: pause
          length: long
      - text: "\n"
      - control:
          kind: set_colour
          colour: blue
      - control:
          kind: raw
          two_hundred_one:
            dynamic:
              - 0
              - len: 4
                field_2:
                  - 3
                  - 2
                  - 2
                  - 0
      - text: Master-Schwert
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Wenn du der "
      - control:
          kind: set_colour
          colour: red
      - text: Verheerung Ganon
      - control:
          kind: reset_colour
      - text: " und\nihren Manifestationen gegenüberstehst,\nwird es von heiligem Licht erfüllt werden.\n"
      - control:
          kind: sound2
          unknown:
            - 9
            - 205
      - text: "Du darfst es jedoch auf keinen Fall\nüberbeanspruchen!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Schwingst du es zu oft umher, wird\nes schartig werden und seine Kraft\nverlieren.\nSollte das passieren, musst du warten,\nbis es sich regeneriert hat."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Es wäre töricht, sich im Kampf mit Ganon\neinzig auf dieses Schwert zu verlassen."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Um für alle Fälle vorbereitet zu sein,\nsolltest du auch die Titanen befreien."
