      - control:
          kind: text_size
          percent: 100
      - text: "Link...\n\n\n"
      - control:
          kind: text_size
          percent: 80
      - control:
          kind: auto_advance
          frames: 275
      - control:
          kind: text_size
          percent: 100
      - text: "Ich habe jeden Schritt deiner Reise\nverfolgt. Du hast schwierige Prüfungen\nhinter dich gebracht.\n"
      - control:
          kind: text_size
          percent: 80
      - control:
          kind: auto_advance
          frames: 85
      - control:
          kind: text_size
          percent: 100
      - text: "Nun solltest du bereit sein...\n\n\n"
      - control:
          kind: text_size
          percent: 80
      - control:
          kind: auto_advance
          frames: 255
      - control:
          kind: text_size
          percent: 100
      - text: "Ja! Du bist nun gewiss bereit, dich der\nVerheerung Ganon zu stellen!"
  Demo646_0_Text012:
    contents:
