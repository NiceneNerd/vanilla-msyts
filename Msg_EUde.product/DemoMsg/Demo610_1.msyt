entries:
  Npc_DungeonPriest_HeroRitoRelief_001:
    contents:
      - text: Dein Kommen beweist deinen Mut.
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "An den Orten, die dir gezeigt werden,\nfindest du Prüfungen, die dich stärker\nmachen sollen."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Doch stelle dich ihnen nicht leichtfertig.
  Npc_Musician_AoC_HeroRitoRelief_003:
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 6
      - text: Hm?
      - control:
          kind: pause
          length: short
      - text: " Seit wann bist du denn da?"
      - control:
          kind: pause
          length: long
      - text: "\nIch habe dich gar nicht kommen gehört."
  Npc_Musician_AoC_HeroRitoRelief_004:
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 1
      - text: "So treffen wir uns wieder!\nAh... Du hast wohl gehört, wie ich\nSelbstgespräche geführt habe."
  Npc_Musician_AoC_HeroRitoRelief_100:
    contents:
      - control:
          kind: auto_advance
          frames: 275
      - control:
          kind: raw
          one:
            one:
              field_1: 4
              field_2: 1053609165
      - control:
          kind: set_colour
          colour: blue
      - text: "Medoh "
      - control:
          kind: reset_colour
      - text: "durchzieht den Himmel fern,"
      - control:
          kind: pause
          frames: 66
      - text: "\nden behänden Recken als seinen Herrn."
  Npc_Musician_AoC_HeroRitoRelief_101:
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 1
      - control:
          kind: animation
          name: Ease_00
      - text: "Oh, welche Freude, dieses Lied vor\ndiesem "
      - control:
          kind: set_colour
          colour: blue
      - text: "Monument "
      - control:
          kind: reset_colour
      - text: singen zu können...
  Npc_Musician_AoC_HeroRitoRelief_102:
    contents:
      - control:
          kind: auto_advance
          frames: 300
      - control:
          kind: raw
          one:
            one:
              field_1: 4
              field_2: 1053609165
      - text: "Größere Kräfte noch zu ergründen,"
      - control:
          kind: pause
          frames: 84
      - text: "\nfolge dem, was die "
      - control:
          kind: set_colour
          colour: blue
      - text: Monumente
      - control:
          kind: reset_colour
      - text: " künden."
  Npc_Musician_AoC_HeroRitoRelief_103:
    contents:
      - control:
          kind: auto_advance
          frames: 138
      - control:
          kind: raw
          one:
            one:
              field_1: 4
              field_2: 1053609165
      - text: "Es gilt, "
      - control:
          kind: set_colour
          colour: red
      - text: "ein Horn des Drachen aus Feuer\nzu durchbohren und zu fassen"
      - control:
          kind: reset_colour
      - text: "..."
      - control:
          kind: pause
          frames: 73
      - text: "\n"
      - control:
          kind: set_colour
          colour: red
      - control:
          kind: reset_colour
      - text: "\n"
      - control:
          kind: auto_advance
          frames: 137
      - control:
          kind: raw
          one:
            one:
              field_1: 4
              field_2: 1053609165
      - text: "Es gilt, "
      - control:
          kind: set_colour
          colour: red
      - text: "sich von Lichtringen führen\nzu lassen"
      - control:
          kind: reset_colour
      - text: "..."
  Npc_Musician_AoC_HeroRitoRelief_104:
    contents:
      - control:
          kind: auto_advance
          frames: 330
      - control:
          kind: raw
          one:
            one:
              field_1: 4
              field_2: 1053609165
      - text: "Es gilt, "
      - control:
          kind: set_colour
          colour: red
      - text: vier Ziele zu treffen
      - control:
          kind: reset_colour
      - text: " ohne zu\nzagen,"
      - control:
          kind: pause
          frames: 84
      - text: "\nPrüfungen, die nur Recken wagen."
  Npc_Musician_AoC_HeroRitoRelief_105:
    contents:
      - control:
          kind: auto_advance
          frames: 140
      - control:
          kind: sound
          unknown:
            - 1
            - 1
      - text: "Lass mich das Lied vortragen, das mein\nMeister mir hinterließ... Diese "
      - control:
          kind: set_colour
          colour: blue
      - text: "Verse\nehren Revali"
      - control:
          kind: reset_colour
      - text: ", der sich der Prüfung stellt.\n"
      - control:
          kind: auto_advance
          frames: 40
      - text: So höre...
  Npc_Musician_AoC_HeroRitoRelief_201:
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 1
      - text: "Wenn ich an jene denke, welche die\n"
      - control:
          kind: set_colour
          colour: blue
      - text: "Titanen "
      - control:
          kind: reset_colour
      - text: steuerten... Welch Tapferkeit...
  Npc_Musician_AoC_HeroRitoRelief_202:
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 11
      - text: "Es liegt nun an mir, den Traum meines\nMeisters wahr werden zu lassen und\ndas "
      - control:
          kind: set_colour
          colour: blue
      - text: unvollendete Lied
      - control:
          kind: reset_colour
      - text: " fertigzustellen."
  Npc_Musician_AoC_HeroRitoRelief_203:
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Für die Erfüllung dieses Traumes muss\nich noch viel über die Recken lernen."
  Npc_Musician_AoC_HeroRitoRelief_204:
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Bis das Lied fertig ist, ist mein\nHeimatdorf nur ein weiterer Ort auf\nmeiner Reise, den ich passiere."
  Npc_Musician_AoC_HeroRitoRelief_205:
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Da trifft es sich gut, dass "
      - control:
          kind: set_colour
          colour: blue
      - text: Teba
      - control:
          kind: reset_colour
      - text: " und\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Kaneli
      - control:
          kind: reset_colour
      - text: " mir im "
      - control:
          kind: set_colour
          colour: blue
      - text: Dorf der Orni
      - control:
          kind: reset_colour
      - text: " einiges über\nden Recken erzählen konnten."
