group_count: 11
atr1_unknown: 0
entries:
  Demo154_2_Text054:
    contents:
      - text: "Der Name der Prinzessin ist Zelda...\nSie ist meine Tochter."
  Demo154_2_Text055:
    contents:
      - text: "Und... der Ritter, der sie bis zuletzt beschützte..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Das warst du... Du selbst, Link."
  Demo154_2_Text056:
    contents:
      - text: An jenem Tag hing dein Leben am seidenen Faden.
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Doch dann wurdest du zum Schrein auf\ndiesem Plateau gebracht. Nach 100 langen\nJahren bist du endlich zu uns zurückgekehrt."
  Demo154_2_Text057:
    contents:
      - text: "Seit du erwacht bist, hast du einige Male eine\nStimme vernommen, die dir den Weg weisen will..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Das ist sie. Zelda, die bis zum heutigen Tag in\nSchloss Hyrule Ganons Macht im Zaum hält."
  Demo154_2_Text058:
    contents:
      - text: Aber... Zeldas Kraft neigt sich dem Ende zu.
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Wenn sie versiegt, kehrt Ganon zurück und dieses\nLand ist endgültig dem Untergang geweiht."
  Demo154_2_Text059:
    contents:
      - text: Link... Ich konnte mein Reich nicht beschützen.
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Somit habe ich kein Recht,\nirgendetwas von dir zu fordern."
  Demo154_2_Text061:
    contents:
      - text: "Die vier Titanen sind jedoch\nnoch immer in Ganons Gewalt..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Und rund um Schloss Hyrule\nwimmelt es nur so von Wächtern."
  Demo154_2_Text062:
    contents:
      - text: "Es wäre wohl nicht besonders klug, sich ohne\nweitere Vorbereitungen zum Schloss zu wagen."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Du solltest zuerst einem Dorf im\nOsten von hier einen Besuch abstatten."
  Demo154_2_Text063:
    contents:
      - text: "Ich zeig es dir, Kakariko heißt es."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Dort wohnt eine Weise namens Impa.\nSie wird dir sagen, was du zu tun hast."
  Demo154_2_Text064:
    contents:
      - text: "Wo Kakariko liegt, ist auf der Karte\ndeines Shiekah-Steins eingezeichnet."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Gehe zuerst zu den Zwillingsbergen, und dahinter\nnach Norden. Der Weg dort führt dich bis zum Dorf."
  Demo154_2_Text065:
    contents:
      - text: Und doch habe ich eine Bitte...
  Demo154_2_Text066:
    contents:
      - text: "Besiege Ganon... und rette sie...\nRette mein Volk und meine Tochter Zelda!"
