group_count: 13
atr1_unknown: 0
entries:
  Demo101_0_Text038:
    contents:
      - text: "Die Verheerung Ganon... Sie ist die Wiedergeburt\neines Dämonenkönigs, der vor ungezählten\nJahren in diesem Land geboren wurde."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Eine Manifestation seines Zorns.
  Demo101_0_Text039:
    contents:
      - text: "Über die Zeitalter wurde Ganon zum Stoff von\nMärchen und Legenden."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Doch eines Tages sprach ein\nköniglicher Weissager eine Prophezeiung aus."
  Demo101_0_Text040:
    contents:
      - text: "„Die Erde, auf der wir wandeln, trägt Zeugnis von der\nWiederkehr der Verheerung Ganon..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Doch sie birgt ebenfalls einen Weg,\nihr zu widerstehen...“"
  Demo101_0_Text042:
    contents:
      - text: "Vier Titanen... Gigantische Maschinen in\nForm von Tieren, die von einem Piloten\ngesteuert werden konnten..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Und die Wächter, die wie von Geisterhand\nbewegt selbsttätig gegen Feinde kämpfen sollten..."
  Demo101_0_Text046:
    contents:
      - text: "Der Prinzessin und den fünf Recken sollte es im\nHandumdrehen gelingen, die Verheerung zu\nversiegeln, so glaubten wir..."
  Demo101_0_Text047:
    contents:
      - text: "Doch Ganon war listiger, als wir angenommen hatten."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Sein Vorgehen übertraf unsere schlimmsten\nVorstellungen."
  Demo101_0_Text048:
    contents:
      - text: "Er brach aus den Tiefen unter Schloss Hyrule ans\nTageslicht und riss sofort die Gewalt über die\nWächter und die Titanen an sich."
  Demo101_0_Text049:
    contents:
      - text: "Alle im Schloss und die vier Recken in den\nTitanen ließen an jenem Tag ihr Leben..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Der Held erlitt bei der Verteidigung der Prinzessin\nlebensgefährliche Verletzungen..."
  Demo101_0_Text050:
    contents:
      - text: "So wurde Hyrule, unser schönes Reich, an nur einem\nTag von der Verheerung Ganon zerstört."
  Demo101_0_Text051:
    contents:
      - text: Doch...
  Demo101_0_Text052:
    contents:
      - text: "Die Prinzessin hatte überlebt...\nGanz allein stellte sie sich Ganon entgegen."
  Demo101_0_Text053:
    contents:
      - text: Link... Der Rest liegt an dir.
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Du bist unsere letzte Hoffnung... Bitte...
  Demo101_0_Text054:
    contents:
      - text: "Sie entsprachen auf erstaunliche Weise den\nLegenden, die in unserem Reich überdauert hatten."
  Demo101_0_Text055:
    contents:
      - text: "Die Prinzessin, welche die Kraft besitzt, das Böse\nzu versiegeln... Der vom heiligen Bannschwert\nauserwählte Held..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "In uralten Zeiten bekämpften sie zusammen mit den\nRelikten den ursprünglichen Ganon."
  Demo101_0_Text056:
    contents:
      - text: "Auch vor 100 Jahren gab es eine Prinzessin und\neinen talentierten Ritter... Wir beschlossen, uns\nnach dem Plan unserer Vorfahren zu richten."
  Demo101_0_Text057:
    contents:
      - text: "Infolge dieser Prophezeiung begannen wir mit\nAusgrabungen..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Und tatsächlich entdeckten wir zahlreiche Relikte,\ndie vor Urzeiten von unseren Vorfahren erschaffen\nworden waren."
  Demo101_0_Text065:
    contents:
      - text: "Aus den Völkern Hyrules wurden vier vortreffliche\nKrieger ausgewählt, welche die Titanen steuern\nsollten..."
  Demo101_0_Text066:
    contents:
      - text: "Wir nannten sie „Recken“, und die Prinzessin führte\nsie an."
