entries:
  Npc_DungeonPriest_HeroGerudoRelief_001:
    contents:
      - text: Dein Kommen beweist deinen Mut.
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "An den Orten, die dir gezeigt werden,\nfindest du Prüfungen, die dich stärker\nmachen sollen."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Doch stelle dich ihnen nicht leichtfertig.
  Npc_Musician_AoC_HeroGerudoRelief_000:
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 6
      - text: "Sieh an, ich hätte nicht gedacht, so hoch\noben jemanden zu treffen."
  Npc_Musician_AoC_HeroGerudoRelief_002:
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 0
      - text: "Wie peinlich... Kaum bin ich allein auf\nReisen, schon führe ich Selbstgespräche."
  Npc_Musician_AoC_HeroGerudoRelief_004:
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 1
      - control:
          kind: animation
          name: Ease_00
      - text: "Wahrlich eine Freude, dieses Lied\nunter dem wachsamen Blick des "
      - control:
          kind: set_colour
          colour: blue
      - text: Titanen
      - control:
          kind: reset_colour
      - text: "\nsingen zu können."
  Npc_Musician_AoC_HeroGerudoRelief_005:
    contents:
      - control:
          kind: auto_advance
          frames: 140
      - control:
          kind: sound
          unknown:
            - 1
            - 1
      - text: "Lass mich das Lied vortragen, das mein\nMeister mir hinterließ... Diese "
      - control:
          kind: set_colour
          colour: blue
      - text: "Verse\nehren Urbosa"
      - control:
          kind: reset_colour
      - text: ", die sich der Prüfung stellt.\n"
      - control:
          kind: auto_advance
          frames: 40
      - text: So höre...
  Npc_Musician_AoC_HeroGerudoRelief_100:
    contents:
      - control:
          kind: auto_advance
          frames: 275
      - control:
          kind: raw
          one:
            one:
              field_1: 4
              field_2: 1053609165
      - control:
          kind: set_colour
          colour: blue
      - text: "Naboris "
      - control:
          kind: reset_colour
      - text: "schickt Blitze im Sand,"
      - control:
          kind: pause
          frames: 66
      - text: "\ngeleitet von des tapferen Recken Hand."
  Npc_Musician_AoC_HeroGerudoRelief_101:
    contents:
      - control:
          kind: auto_advance
          frames: 300
      - control:
          kind: raw
          one:
            one:
              field_1: 4
              field_2: 1053609165
      - text: "Größere Kräfte noch zu ergründen,"
      - control:
          kind: pause
          frames: 84
      - text: "\nfolge dem, was die "
      - control:
          kind: set_colour
          colour: blue
      - text: Monumente
      - control:
          kind: reset_colour
      - text: " künden."
  Npc_Musician_AoC_HeroGerudoRelief_102:
    contents:
      - control:
          kind: auto_advance
          frames: 138
      - control:
          kind: raw
          one:
            one:
              field_1: 4
              field_2: 1053609165
      - text: "Es gilt, "
      - control:
          kind: set_colour
          colour: red
      - text: "die grimme Herrscherin des\nSandes vom Thron zu fegen"
      - control:
          kind: reset_colour
      - text: "..."
      - control:
          kind: pause
          frames: 90
      - text: "\n\n"
      - control:
          kind: auto_advance
          frames: 137
      - control:
          kind: raw
          one:
            one:
              field_1: 4
              field_2: 1053609165
      - text: "Es gilt, "
      - control:
          kind: set_colour
          colour: red
      - text: "sich mit treuem Reittier durch die\nLichtringe zu bewegen"
      - control:
          kind: reset_colour
      - text: "..."
  Npc_Musician_AoC_HeroGerudoRelief_103:
    contents:
      - control:
          kind: auto_advance
          frames: 330
      - control:
          kind: raw
          one:
            one:
              field_1: 4
              field_2: 1053609165
      - text: "Es gilt, "
      - control:
          kind: set_colour
          colour: red
      - text: "der Wüstenstadt Schatz zum\ngroßen Schlund zu tragen"
      - control:
          kind: reset_colour
      - text: "..."
      - control:
          kind: pause
          frames: 84
      - text: "\nPrüfungen, die nur Recken wagen."
  Npc_Musician_AoC_HeroGerudoRelief_201:
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 12
      - text: "Es ist fast so, als würden die Recken\ndurch das Lied mit mir sprechen, als\nwollten sie, dass ich es vervollständige..."
  Npc_Musician_AoC_HeroGerudoRelief_204:
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Zu meiner Freude haben Königin "
      - control:
          kind: set_colour
          colour: blue
      - text: Riju
      - control:
          kind: reset_colour
      - text: "\nund ihre Leibwächterin "
      - control:
          kind: set_colour
          colour: blue
      - text: Bylla
      - control:
          kind: reset_colour
      - text: " mir mit\nGeschichten über ihren Recken geholfen."
  Npc_Musician_AoC_HeroGerudoRelief_205:
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 1
      - text: "Es war zwar etwas, äh, aufwendig...\nAber "
      - control:
          kind: set_colour
          colour: blue
      - text: "Gerudo-Stadt "
      - control:
          kind: reset_colour
      - text: "war den Besuch auf\njeden Fall wert!"
  Npc_Musician_AoC_HeroGerudoRelief_206:
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Also ist es meine Pflicht, mehr über die\nRecken aus Hyrules Vergangenheit in\nErfahrung zu bringen."
